Method delete(obj) Class PersistWFInformation
Local lRet := .F.

BEGIN TRANSACTION

	DbSelectArea("SKW")
	DbSetOrder(1) 		//KW_FILIAL+KW_CODIGO
	If DbSeek(xFilial("SKW")+obj:id)
		Reclock( "SKW",.F.,.T.)
		DbDelete()
		MsUnlock()
		lRet := .T.
	EndIf
	
END TRANSACTION

Return lRet 


