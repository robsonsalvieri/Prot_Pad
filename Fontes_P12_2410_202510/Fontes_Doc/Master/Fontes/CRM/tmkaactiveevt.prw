			cDescResol	:= STR0004	// "Informar um n๚mero vแlido."
		Case Cause == 8
			cDescCause	:= STR0005	// "Nใo foi possํvel realizar a chamada. Todas as linhas estใo ocupadas."
			cDescResol	:= STR0006	// "Verifique se hแ tom de discagem ou tente novamente mais tarde."		
		Case Cause == 10
			cDescCause	:= STR0007	// "Nใo foi possํvel completar a chamada pois o n๚mero de telefone estแ ocupado."
			cDescResol	:= STR0008	// "Tente novamente mais tarde."
		Case Cause == 11
			cDescCause	:= STR0009	// "Nใo foi possํvel completar a chamada pois o n๚mero de telefone nใo atende."
			cDescResol	:= STR0010	// "Tente novamente mais tarde."
		Case Cause == 20
			cDescCause	:= STR0011	// "Nใo foi possํvel colocar a chamada em espera, pois todos as posi็๕es de estacionamento estใo ocupados."
			cDescResol	:= STR0012	// "Tente novamente."
		Case Cause == 25
			cDescCause	:= STR0013	// "Nใo foi possํvel completar a chamada pois a liga็ใo foi atendida por um correio de voz."
			cDescResol	:= STR0014	// "Tente novamente mais tarde."
		Otherwise
			cDescCause	:= STR0015	// "O PABX retornou uma informa็ใo nใo esperada."
			cDescResol	:= STR0016	// "Tente novamente." + Chr(13)+Chr(10) + "Se persistir o problema contate o administrador do sistema e relate a situa็ใo."
	End Case

	MsgInfo(STR0001 + CRLF + cDescCause + CRLF + CRLF + STR0002 + cDescResol, "Protheus")	// "Causa:" "Resolu็ใo:"
	SMSetShowUsrMsg(.F.)
EndIf			
Return Nil

