Method execute(parameterList) Class TSEntrancePoint
Local uRet                                         
Local aArea := GetArea()
Local cFunName := AllTrim(Self:codeBlock)
Local cPosBracket := At("(", cFunName)   
Private ParamIXB := parameterList

If cPosBracket > 0 
	cFunName := SubStr(cFunName, 1, cPosBracket - 1)   
EndIf
//uRet := Eval(&("{|aParam|" + cFunName + "(aParam)}"), parameterList)
uRet := Eval(&("{||" + cFunName + "()}"))

//Retorna os parโmetros para a rotina chamadora
parameterList := ParamIXB

RestArea(aArea)
Return uRet      

