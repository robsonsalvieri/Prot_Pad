Method save(obj) Class PersistSLAResponsible
Local lRet := .F.			//Indica retorno da funcao

BEGIN TRANSACTION 

	DbSelectArea("SK8") 
	DbSetOrder(1) //K8_FILIAL+K8_CODRESP
	If DbSeek(xFilial("SK8") + obj:id)	
		//Atualiza SK8
		RecLock("SK8",.F.)
		Self:UpdateDBFields(obj) 
		MsUnlock()
		lRet := .T. 
	Else
		//Inclui SK8
		RecLock("SK8",.T.)
		Self:UpdateDBFields(obj)
		If Empty(obj:id)
			SK8->K8_CODRESP := GetSx8Num("SK8", "K8_CODRESP")
			//ConfirmSX8()
			obj:id	:= SK8->K8_CODRESP
		Else
			SK8->K8_CODRESP := obj:id
		EndIf                                  
		MsUnlock()
		lRet	:= .T. 

	EndIf

END TRANSACTION    

Return lRet                      


