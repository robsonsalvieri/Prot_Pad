Local lRet := .F.			//Indica retorno da funcao

BEGIN TRANSACTION

	DbSelectArea("SK8")
	DbSetOrder(1) //K8_FILIAL+K8_CODRESP
	If DbSeek(xFilial("SK8")+obj:id)
		Reclock("SK8",.F.,.T.)
		DbDelete()
		MsUnlock()		   
