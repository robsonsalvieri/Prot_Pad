Method add(toolbarButton) Class TSToolbarList 
Local nMaxPos := 0  
Local nCount := 0     
Local nVal

toolbarButton:codTele := Self:id    
If Empty(toolbarButton:pos)
	For nCount :=1 To Len(Self:aButtons)    		
		nVal := Val(Self:aButtons[nCount]:pos)
		If nVal > nMaxPos
			nMaxPos := nVal
		EndIf
	Next nCount	          
	nMaxPos+=1  
	toolbarButton:pos := Str(nMaxPos)
EndIf 
Aadd(Self:aButtons, toolbarButton)
Self:sort()
Return Nil 

