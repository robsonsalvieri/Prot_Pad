Method load(obj) Class SLAResponsibleCodebase  
Local lRet := .T.         
Local cAlias :=	"SK8" 

DbSelectArea(cAlias)
DbSetOrder(1)
If DbSeek(xFilial(cAlias)+obj:id)
	obj:id				:= (cAlias)->K8_CODRESP
	obj:name			:= (cAlias)->K8_NOME
	obj:email			:= (cAlias)->K8_EMAIL
	obj:codProtheusUser	:= (cAlias)->K8_CODUSU
	obj:codSuperior		:= (cAlias)->K8_SUPERIO
Else
	lRet := .F.
EndIf

Return lRet


