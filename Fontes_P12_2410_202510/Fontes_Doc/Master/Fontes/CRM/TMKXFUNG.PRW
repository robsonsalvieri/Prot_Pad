	Local lLower := .F.
	
	//Comparo as datas
	If Self:getDate() < dateTime:getDate()
		lLower := .T.
	ElseIf Self:getDate() == dateTime:getDate()
		//Comparo as horas
		If Self:getHour() < dateTime:getHour()
			lLower := .T.
		ElseIf Self:getHour() == dateTime:getHour()
			//Comparo os minutos
			If Self:getMinute() < dateTime:getMinute()
				lLower := .T.
			ElseIf Self:getMinute() == dateTime:getMinute()
				//Comparo os segundos
				If Self:getSecond() < dateTime:getSecond()
					lLower := .T.
				EndIf				
			EndIf
		EndIf
	EndIf

Return lLower

