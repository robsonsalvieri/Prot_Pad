Method minusMinutes(minutes) Class TMKDateTime 
	Local nDif		:= 0
	Local nSeconds	:= minutes - Int(minutes)
	
	If nSeconds > 0
		minutes := minutes - nSeconds
	EndIf	
	
	If (Self:getMinute() - minutes) < 0
		nDif := minutes - Self:getMinute()
		Self:minusHours(1)
		Self:setMinute(60)
		Self:minusMinutes(nDif)
	Else
		Self:setMinute(Self:getMinute() - minutes)
	EndIf
	
	If nSeconds > 0
		Self:minusSeconds(((nSeconds * 100)*60)/100)
	EndIf	
Return

