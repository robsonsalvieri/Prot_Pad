Method plusHours(hours) Class TMKDateTime   
	Local nDif		:= 0
	Local nMinutes	:= hours - Int(hours)
	Local nSeconds	:= 0
	
	If nMinutes > 0
		hours := hours - nMinutes
	EndIf
	
	If (Self:getHour() + hours) > 23
		nDif := (Self:getHour() + hours) - 23
		Self:plusDays(1)
		Self:setHour(-1)
		Self:plusHours(nDif)
	Else      
		Self:setHour(Self:getHour() + hours)				
	EndIf
	
	If nMinutes > 0
		Self:plusMinutes(Int(nMinutes*60))		
		nSeconds := nMinutes*60 - Int(nMinutes*60)
		If nSeconds > 0
			Self:plusSeconds(Round(nSeconds*60,0))
		EndIf
	EndIf
Return

