Method minusHours(hours) Class TMKDateTime
	Local nDif	:= 0
	Local nMinutes	:= hours - Int(hours)
	Local nSeconds	:= 0
	
	If nMinutes > 0
		hours := hours - nMinutes
	EndIf	
	
	If (Self:getHour() - hours) < 0
		nDif := hours - Self:getHour()
		Self:minusDays(1)
		Self:setHour(24)
		Self:minusHours(nDif)
	Else
		Self:setHour(Self:getHour() - hours)
	EndIf
		
	If nMinutes > 0
		Self:minusMinutes((Int(nMinutes * 100)*60)/100)		
		nSeconds := (nMinutes * 100) - Int(nMinutes * 100)
		If nSeconds > 0
			Self:minusSeconds((Int(nSeconds * 100)*60)/100)
		EndIf
	EndIf	
Return

