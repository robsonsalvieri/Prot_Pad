*/
Method getMDays(month) Class TMKDateTime
	Local nDays := 0
	If	month == 1 .Or.;
		month == 3 .Or.;
		month == 5 .Or.;
		month == 7 .Or.;
		month == 8 .Or.;
		month == 10 .Or.;
		month == 12
		nDays := 31
	ElseIf month == 2
		nDays := If(Self:isLeapYear(), 29, 28)
	ElseIf	month == 4 .Or.;
			month == 6 .Or.;
			month == 9 .Or.;
			month == 11
		nDays := 30
	EndIf
Return nDays

