*/
Method minusDays(days) Class TMKDateTime  
	Local nDif	:= 0
	Local nDaysMonth:= 0
	Local nHours	:= days - Int(days)
	Local nMinutes	:= 0
	Local nSeconds	:= 0	
	
	If nHours > 0
		days := days - nHours
	EndIf			
	
	If (Self:getDay() - days) <= 0
		nDif := days - Self:getDay()
		Self:minusMonths(1)
		nDaysMonth := Self:getMDays(Self:getMonth())	
		Self:setDay(nDaysMonth)
		Self:minusDays(nDif)
	Else
		Self:setDay(Self:getDay() - days)		
	EndIf	
	
	If nHours > 0
		Self:minusHours((Int(nHours * 100)*24)/100)		
		nMinutes := (nHours*100) - Int(nHours * 100)
		If nMinutes > 0
			Self:minusMinutes((Int(nMinutes * 100)*60)/100)		
			nSeconds := (nMinutes * 100) - Int(nMinutes * 100)
			If nSeconds > 0
				Self:minusSeconds((Int(nSeconds * 100)*60)/100)
			EndIf
		EndIf
	EndIf	
Return			

