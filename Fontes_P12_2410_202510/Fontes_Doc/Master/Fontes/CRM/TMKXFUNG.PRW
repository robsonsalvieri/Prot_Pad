Method plusMinutes(minutes) Class TMKDateTime
	Local nDif	:= 0
	Local nSeconds := minutes - Int(minutes)
	
	If nSeconds > 0
		minutes := minutes - nSeconds
	EndIf
	
	If (Self:getMinute() + minutes) > 59
		nDif := (Self:getMinute() + minutes) - 59
		Self:plusHours(1)
		Self:setMinute(-1)
		Self:plusMinutes(nDif)
	Else
		Self:setMinute(Self:getMinute() + minutes)
	EndIf
	
	If nSeconds > 0
		Self:plusSeconds(Round(nSeconds * 60,0))
	EndIf
Return

