*/
Method plusDays(days) Class TMKDateTime
	Local nDif		:= 0
	Local nDaysMonth:= 0
	Local nHours	:= (days - Int(days)) * 24
	Local nMinutes	:= 0
	Local nSeconds	:= 0	
	
	If nHours > 0
		days := Int(days)
	EndIf	
	
	nDaysMonth := Self:getMDays(Self:getMonth())	
	
	If (Self:getDay() + days) > nDaysMonth
		nDif := (Self:getDay() + days) - nDaysMonth
		Self:plusMonths(1)
		Self:setDay(0)
		Self:plusDays(nDif)
	Else
		Self:setDay(Self:getDay() + days)		
	EndIf
	
	If nHours > 0
		Self:plusHours(Int(nHours))	 
		nMinutes := nHours - Int(nHours)
		If nMinutes > 0
			Self:plusMinutes(Int(nMinutes*60))		
			nSeconds := nMinutes*60 - Int(nMinutes*60)
			If nSeconds > 0
				Self:plusSeconds(Round(nSeconds*60,0))
			EndIf		
		EndIf
	EndIf
Return			

