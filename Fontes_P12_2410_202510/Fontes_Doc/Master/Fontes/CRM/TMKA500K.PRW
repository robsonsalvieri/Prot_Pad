*/    
Method getValue() Class TSData 
Local oObj := Self //Objeto valor a ser retornado pela propriedade

If ValType(Self:value) == "U"	
	TMKFree( oObj )
	If Self:datatype == 1 //CARACTER				
		oObj := TSCHAR():NEW()		 
	ElseIf Self:datatype == 2 //NUMERICO
		oObj := TSNUMBER():NEW()
	ElseIf Self:datatype == 3 //LOGICO
		oObj := TSBOOLEAN():NEW()
	ElseIf Self:datatype == 4 //DATA
		oObj := TSDATE():NEW()
	ElseIf Self:datatype == 5 //MEMO
		oObj := TSMEMO():NEW()
	Else
		oObj := TSCHAR():NEW()
	EndIf                        
EndIf
Return oObj

