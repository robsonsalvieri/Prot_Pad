Method clone() Class TSData
Local obj


If Self:datatype == 1 //CARACTER
	obj := TSCHAR():NEW()	 
ElseIf Self:datatype == 2 //NUMERICO
	obj := TSNUMBER():NEW()
ElseIf Self:datatype == 3 //LOGICO
	obj := TSBOOLEAN():NEW()
ElseIf Self:datatype == 4 //DATA
	obj := TSDATE():NEW()
ElseIf Self:datatype == 5 //MEMO
	obj := TSMEMO():NEW()
Else
	obj := TSCHAR():NEW()
EndIf         
obj:value 	:= Self:value    
obj:size	:= Self:size
obj:decimal	:= Self:decimal
Return obj

