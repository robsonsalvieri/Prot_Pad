*/
Static Function FS_GrvABO(nReg)
Local aArea := GetArea()

DbSelectArea("GH9")
If nOpcE <> 3
	DbGoTo(nReg)
Endif

If nOpcE == 3 .Or. nOpcE == 4 //Inclusao e Alterar
	RecLock("GH9", (nOpcE == 3))
	HS_GrvCpo("GH9")
	GH9->GH9_LOGARQ := HS_LOGARQ()
	MsUnlock()
	
ElseIf nOpcE == 5 //Exclusao
	RecLock("GH9", .F.)
	DbDelete()
	MsUnlock()
Endif

Restarea(aArea)
Return()


