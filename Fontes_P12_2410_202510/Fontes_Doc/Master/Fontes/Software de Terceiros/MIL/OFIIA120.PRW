*/
Function FS_INTVII()

Local ixi    := 0
Local _CDERR := Space(03)
Local _MENSA := Space(30)

Local nRegTrans := 0

DbSelectArea("VII")                                                                           
dbSeek( xFilial("VII"))

Begin Transaction

   While xFilial("VII") == VII->VII_FILIAL .and. VII->VII_CODMAR == cCodMar .and. !Eof()

      For ixi := 1 to 3

         _CDERR := "VII->VII_CDERR"+Str(ixi,1)
         _MENSA := "VII->VII_MENSA"+Str(ixi,1)

         If Empty(&_CDERR)
            Loop
         EndIf

         dbSelectArea("VGD")
         dbSetOrder(1)
         dbSeek( xFilial("VGD")+cCodMar+&_CDERR)

         RecLock("VGD", !Found() )
         VGD_FILIAL := xFilial("VGD")
         VGD_CODMAR := cCodMar
         VGD_CODERR := &_CDERR
         VGD_DESERR := &_MENSA

      Next

      DbSelectArea("VII")
      DbSkip()

   EndDo

End Transaction

FS_DELIA120()

If lMsErroAuto
   MostraErro()
EndIf   

return

