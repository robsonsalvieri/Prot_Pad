/*/{Protheus.doc} fOsTQB
Subselect para buscar ordens relacionadas a TQB e Executante
@author	Maria Elisandra de Paula
@since 27/12/2024
@return string
/*/
//---------------------------------------------------------------------
Static Function fOsTQB()

	Local cQuery	:= ""
	Local lMultiple := SuperGetMv( 'MV_NGMULOS', .F., 'S' ) == 'S'

	If lMultiple
		cQuery := " SELECT TT7.TT7_ORDEM "
		cQuery += " FROM " + RetSqlName("TT7") + " TT7"
		cQuery += " JOIN " + RetSqlName("TQB") + " TQB ON "
		cQuery += " 	TQB.TQB_FILIAL = " + ValtoSql(FwxFilial("TQB"))
		cQuery += " 	AND TT7.TT7_SOLICI = TQB.TQB_SOLICI "
		cQuery += " 	AND TQB.TQB_CDEXEC = " + ValtoSql( cUserTQ4 ) 
		cQuery += " WHERE TT7.TT7_FILIAL = " + ValtoSql(FwxFilial("TT7"))
		cQuery += "	AND TT7.D_E_L_E_T_ = ' ' "
	Else
		cQuery := " SELECT TQB.TQB_ORDEM "
		cQuery += " FROM " + RetSqlName("TQB") + " TQB"
		cQuery += " LEFT JOIN " + RetSqlName("TQ3") + " TQ3 " 
		cQuery += "		ON TQ3.TQ3_FILIAL = " + ValtoSql(FwxFilial("TQ3"))
		cQuery += " 	AND TQ3.TQ3_CDSERV = TQB.TQB_CDSERV"
		cQuery += " 	AND TQ3.D_E_L_E_T_ = ' '"
		cQuery += " WHERE TQB.TQB_FILIAL = " + ValtoSql(FwxFilial("TQB"))"
		cQuery += fOpenTQB()
		cQuery += " AND TQB.TQB_CDEXEC = " + ValtoSql( cUserTQ4 )

	EndIf

Return cQuery 

//---------------------------------------------------------------------
/*/{Protheus.doc} fExists
Verifica se o insumo já está gravado no banco de dados

@author	Maria Elisandra de Paula
@since 24/02/2024

@param cOrder, string, código da ordem
@param cPlan, string, plano da ordem
@param aInputAux, array, dados do insumo
@param lIsDone, boolean, se é insumo realizado
@return boolean
/*/
//---------------------------------------------------------------------
Static Function fExists(cOrder , cPlan, aInputAux, lIsDone)

	Local lRet := .F.
	Local cKey := FwxFilial( 'STL' ) + cOrder + cPlan + aInputAux[_TASK_] + aInputAux[_TYPE_] + aInputAux[_CODE_]

	dbSelectArea( 'STL' )
	dbSetOrder( 1 )
	If msSeek( cKey )

		While !Eof() .And. STL->TL_FILIAL + STL->TL_ORDEM + STL->TL_PLANO + STL->TL_TAREFA + STL->TL_TIPOREG + STL->TL_CODIGO == cKey
			lRet := STL->TL_DTINICI == aInputAux[_STARTDATE_] ;
					.AND. STL->TL_DTFIM == aInputAux[_ENDDATE_];
					.AND. STL->TL_HOINICI == aInputAux[_STARTHOUR_];
					.AND. STL->TL_HOFIM == aInputAux[_ENDHOUR_];
					.And. ( Val(STL->TL_SEQRELA) == 0 .And. !lIsDone .Or. Val(STL->TL_SEQRELA) > 0 .And. lIsDone )

			If lRet
				Exit
			EndIf

			STL->( dbSkip() )
	
		EndDo
	EndIf

Return lRet

//---------------------------------------------------------------------
/*/{Protheus.doc} fUpdtSint
Salva sintomas no banco de dados

@author	Pedro Henrique Perez Trebilcock
@since 06/06/2025

@param aSymptoms, array de sintomas de ordem de serviço.
@param cPlan, string, plano da ordem.
@param cCode, string, código da ordem de serviço.
@param cEquipment, string, código do equipamento.
@param cDate, string, data da ordem de serviço.
@return boolean
/*/
//---------------------------------------------------------------------

Static Function fUpdtSint( aSymptoms, cCode, cPlan, cEquipment, cDate )

	Local nI := 0

	For nI := 1 To Len( aSymptoms )

		IF aSymptoms[nI]["OPERATION"] = "C"

			dbSelectArea( 'TTC' )
			
			If !dbSeek( FwxFilial( 'TTC' ) + aSymptoms[nI]["ORDERCODE"] + aSymptoms[nI]["ORDERPLAN"] + aSymptoms[nI]["SYMPTOMCODE"])


				RecLock( 'TTC', .T. )

				TTC->TTC_FILIAL := FwxFilial( 'TTC' )
				TTC->TTC_CODBEM := cEquipment
				TTC->TTC_ORDEM  := cCode
				TTC->TTC_PLANO  := cPlan
				TTC->TTC_CDSINT := aSymptoms[nI]["SYMPTOMCODE"]
				TTC->TTC_DATA   := StoD( cDate )
				
				MsUnlock()

			EndIf

		ELSE

			dbSelectArea( 'TTC' )
			dbSetOrder( 1 )

			If dbSeek( FwxFilial( 'TTC' ) + aSymptoms[nI]["ORDERCODE"] + aSymptoms[nI]["ORDERPLAN"] + aSymptoms[nI]["SYMPTOMCODE"])

				RecLock( 'TTC', .F. )
				dbDelete()
				MsUnlock()

			EndIF
		
		EndIf
		
	NEXT nI

return


