/*/{Protheus.doc} fWriteTpq
Cria um objeto com valores da TPQ / respostas de etapas

@author	Maria Elisandra de Paula
@since 17/12/2024
@param cAliasQry, string, alias em execução
@return array, [1] objeto json em string
               [2] objeto json
/*/
//---------------------------------------------------------------------
Static Function fWriteTpq( cAliasQry )

	Local oData := JsonObject():New()

	oData["order"] := (cAliasQry)->TPQ_ORDEM
	oData["plan"] :=  (cAliasQry)->TPQ_PLANO
	oData["task"] := NGEscape( (cAliasQry)->TPQ_TAREFA )
	oData["checklist"] := NGEscape( (cAliasQry)->TPQ_ETAPA )
	oData["code"] := NGEscape( (cAliasQry)->TPQ_OPCAO )

	//Há uma O.S ou S.S gerada quando o campo (cAliasQry)->TPQ_ORDEMG estiver preenchido
	If !Empty((cAliasQry)->TPQ_ORDEMG)
		If (cAliasQry)->TPC_TPMANU == "1"
			oData["serviceOrder"] := (cAliasQry)->TPQ_ORDEMG
		Else
			oData["serviceRequest"] := (cAliasQry)->TPQ_ORDEMG
		EndIf
	EndIf

	If (cAliasQry)->TPC_TIPRES == "1" .Or. ((cAliasQry)->TPC_TIPCAM == "L" )
		If Alltrim((cAliasQry)->TPQ_RESPOS) == ".T."
			oData["value"] := "true"
		Else
			oData["value"] := "false"
		EndIf
	ElseIf (cAliasQry)->TPC_TIPCAM == "N"
		oData["value"] := Val((cAliasQry)->TPQ_RESPOS)
	Else
		oData["value"] := NGEscape((cAliasQry)->TPQ_RESPOS)
	EndIf

Return { oData:toJson(), oData }

