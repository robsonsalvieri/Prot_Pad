/*/{Protheus.doc} fQueryGen
executa abastecimento, criação, edição ou exclusão.
@autor Maria Elisandra de Paula
@since 08/10/2024
@param oWS, objeto, referência ao webservice
@param nOpcx, caracter, número que verifica qual operação será realizada.

/*/
//---------------------------------------------------------------------
Static Function fExecAbast(oWs, nOpcx)

	Local bError
	Local oError
	Local oRet    := JsonObject():New()
	Local lRet    := .F.
	Local aValid  := {}
	Local aAbast  := {}
	Local cError  := ""
	Local nScan   := 0
	Local cFrota  := ''
	Local dDtAbas := ''
	Local cHrAbas := ''
	Local lSeek   := .F.

	Private lMSErroAuto := .F.

	printf('Validando abastecimento... ')

    oWs:ClearResponse()
    oWs:SetContentType('application/json')

    bError := ErrorBlock( { |oError| MntWSError( oError ), lRet:= .F., Break( oError ) } )

    Begin Sequence

		//---------------
		// Valida campos 
		//---------------
		aValid := fVldAbast(oWs)
		If !aValid[1]
			SetRestFault(500, FwhttpEncode(aValid[2]))
			Return .F.
		EndIf

		aAbast := aValid[3]

		nScan := aScan( aAbast, { |x| x[1] == 'TQN_FROTA' } )
		cFrota := PadR( aAbast[nScan,2], TAMSX3('TQN_FROTA')[1] )

		nScan := aScan( aAbast, { |x| x[1] == 'TQN_DTABAS' } )
		dDtAbas := aAbast[nScan,2]

		nScan := aScan( aAbast, { |x| x[1] == 'TQN_HRABAS' } )
		cHrAbas := aAbast[nScan,2]

		If nOpcX == 5
			DbSelectArea('TQN')
			DbSetOrder(1)
			If DbSeek(FWxFilial('TQN') + cFrota + dtos( dDtAbas ) + cHrAbas )
				oRet := fJFueling('TQN')
			EndIf		
		EndIf

		If nOpcX == 3
			DbSelectArea('TQN')
			DbSetOrder(1)
			If DbSeek(FWxFilial('TQN') + cFrota + dtos( dDtAbas ) + cHrAbas )
				nOpcX := 4
			EndIf		
		EndIf

		printf('Realizando operacao de abastecimento: ' + CValToChar(nOpcX))

		//------------------------------------
		// Executa a operação do abastecimento
		//------------------------------------
		MSExecAuto( { | v, x, y | MNTA655( v, x, y ) }, , aAbast, nOpcx )

		If lMsErroAuto

			cError := MostraErro( GetSrvProfString( 'Startpath', '' ) )
			printf('Operacao nao realizada !!! Bem: ' + cFrota + ' - data/hora: ' +  dtos( dDtAbas ) + ' ' + cHrAbas + CRLF + cError)
			SetRestFault(533, FwhttpEncode(cError))

		Else

			DbSelectArea('TQN')
			DbSetOrder(1)
			lSeek := DbSeek(FWxFilial('TQN') + cFrota + dtos( dDtAbas ) + cHrAbas )

			If lSeek .And. nOpcX == 3 .Or. nOpcX == 4
				oRet := fJFueling('TQN')
				oWs:SetResponse(oRet)
				lRet := .T.
			EndIf

			If !lSeek .And. nOpcX == 5
				oWs:SetResponse(oRet)
				lRet := .T.
			EndIf

			If lRet
				printf('Operacao realizada com sucesso! Bem: ' + cFrota + ' - data/hora: ' +  dtos( dDtAbas ) + ' ' + cHrAbas )
			Else
				SetRestFault(500, 'UNKNOWN')
				printf('Operacao nao realizada !!! Bem: ' + cFrota + ' - data/hora: ' +  dtos( dDtAbas ) + ' ' + cHrAbas )
			EndIf

		EndIf

		FreeObj(oRet)

	End Sequence

	ErrorBlock( bError )

Return lRet

