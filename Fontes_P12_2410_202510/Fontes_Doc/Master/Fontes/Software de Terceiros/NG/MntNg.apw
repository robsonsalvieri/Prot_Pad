//---------------------------------------------------------------------
/*/{Protheus.doc} fListObj
Traz informações do banco de conhecimento sobre as imagens.

@author Pedro Henrique Perez Trebilcock
@since 26/06/2024
@param oSelf, objeto, instância do webservice
@param cType, String, define o tipo de requisição: order/checklist
@param cCode, string, código do registro
@param cPlan, String, plano da ordem.
@param cTask, string, código da tarefa.
@param cStep, string, código da etapa.
@return logic, se obteve sucesso na operação
/*/
//---------------------------------------------------------------------
Static Function fListObj( oSelf, cType, cCode, cPlan, cTask, cStep )

	Local aResponse := {}
	Local cSolic    := ''

	Default cPlan := ''
	Default cTask := ''
	Default cStep := ''

	If cType == 'order'

		dbSelectArea( 'STJ' )
		dbSetOrder( 1 )
		If dbSeek( FwxFilial( 'STJ' ) + cCode )

			cSolic := STJ->TJ_SOLICI

			aResponse := fListImg( cCode, 'STJ')

			If !Empty( cSolic )
				aResponse := fListImg( cSolic, 'TQB', aResponse, .F. )
			EndIf
		Else
			Return .F.
		EndIf

	ElseIf cType == 'checklist'

		dbSelectArea( 'STQ' )
		dbSetOrder( 1 )
		If dbSeek( FwxFilial( 'STQ' ) + ;
			Padr(cCode, TamSx3('TQ_ORDEM')[1]) +  ;
			Padr(cPlan, TamSx3('TQ_PLANO')[1]) + ;
			Padr(cTask, TamSx3('TQ_TAREFA')[1]) + ;
			Padr(cStep, TamSx3('TQ_ETAPA')[1]))

			aResponse := fListImg( STQ->TQ_ORDEM, 'STQ',,, STQ->TQ_PLANO, STQ->TQ_TAREFA, STQ->TQ_ETAPA )
		Else
			printf("Etapa não localizada")
			Return .F.
		EndIf

	ElseIf cType == 'request'

		If !NGIFDBSEEK("TQB", cCode, 1)
			Return .F.
		EndIf

		aResponse := fListImg( cCode, 'TQB' )

	EndIf

	oSelf:SetStatus( 200 )
	oSelf:SetResponse( FWJsonSerialize( aResponse, .F. ) )

Return .T.
//---------------------------------------------------------------------
/*/{Protheus.doc} fListImg
Retorna lista de imagens da SS ou OS

@author Maria Elisandra de Paula
@since 05/07/2022
@param cKey, string, chave para buscar entidade
@param cTable, string, tabela de ss ou os
@param aListiIni, array, lista inicial
@param lIsOwner, boolean, utilizado para o app identificar se a imagem 
pertence à entidade solicitada pois uma OS pode apresentar uma imagem de uma SS
@return array, lista de imagens em objeto 
/*/
//---------------------------------------------------------------------
Static Function fListImg( cKey, cTable, aListIni, lIsOwner, cPlan, cTask, cStep )

	Local cFilEnt := FwxFilial( cTable )
	Local cCodEnt := ''
	Local cRtrim := ''
	Local cAliasQry := GetNextAlias()
	Local aResponse := {}
	Local oAttachment

	Default aListIni := {}
	Default lIsOwner := .T.

	aResponse := aListIni

	If cTable == 'STQ'

		cCodEnt :=  '% ( AC9.AC9_CODENT = ' + ValtoSql( cKey + cPlan + cTask + cStep) + ')%
		cRtrim := "%AND RTRIM(SUBSTRING(ACB.ACB_DESCRI, 1, 9)) IN ('CHECKLIST')%"

	Else

		cCodEnt	:= '% ( AC9.AC9_CODENT = ' + ValtoSql(cKey) + ;
		IIf(cTable == 'TQB', ' OR AC9.AC9_CODENT = ' + ValtoSql( FwxFilial('TQB') +cKey ) , '') + ')%'  // em alguns casos foi gravado chave com filial

		cRtrim := "% AND RTRIM(SUBSTRING(ACB.ACB_DESCRI, 1, 8)) IN ('PROBLEM', 'SOLUTION', 'SIGNATUR', 'GENERIC') %"

	EndIf

	BeginSql Alias cAliasQry
		SELECT ACB.ACB_CODOBJ, ACB.ACB_OBJETO, ACB.ACB_DESCRI
		FROM %table:AC9% AC9
		INNER JOIN %table:ACB% ACB
			ON ACB.ACB_FILIAL = %xFilial:ACB%
			AND ACB.ACB_CODOBJ = AC9.AC9_CODOBJ
		WHERE AC9.AC9_FILIAL = %xFilial:AC9%
			AND AC9.AC9_ENTIDA = %exp:cTable%
			AND AC9.AC9_FILENT = %exp:cFilEnt%
			AND %exp:cCodEnt%
			AND AC9.%NotDel%
			AND ACB.%NotDel%
			%exp:cRtrim%
	EndSql
	

	While !(( cAliasQry )->( EoF() ))

		oAttachment := MntOrderAttachment():New( ( cAliasQry )->ACB_CODOBJ )
		oAttachment:Type     := AllTrim( Lower( SubStr( ( cAliasQry )->ACB_DESCRI, 1, 8 ) ) )
		oAttachment:Date     := AllTrim( SubStr( ( cAliasQry )->ACB_DESCRI, 9 ) )
		oAttachment:FileName := AllTrim( ( cAliasQry )->ACB_OBJETO )
		oAttachment:Path     := '/picture/' + oAttachment:FileName
		oAttachment:IsOwner  := lIsOwner
		aAdd( aResponse, oAttachment )

		( cAliasQry )->( dbSkip() )
	EndDo

	( cAliasQry )->( dbCloseArea() )

Return aResponse

