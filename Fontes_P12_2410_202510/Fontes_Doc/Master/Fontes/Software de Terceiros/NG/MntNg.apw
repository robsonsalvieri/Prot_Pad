				[2] mensagem de erro/sucesso
				[3] Etapas não geradas
				[4] Código de erro MNTNG.
@return logic, se operações foram realizadas com sucesso de acordo com os parâmetros
/*/
//---------------------------------------------------------------------
Static Function PrintPostLog( oWs, lRet, aRet )

	Local nSize     := 0
	Local nCodeRest := 400

	Default aRet := {}

	// Verifica tamanho do array de informações
	nSize := Len( aRet )

	If nSize > 0

		If !aRet[1]
			lRet := .F.
			If nSize == 3 .And. aRet[3] > 0
				nCodeRest := aRet[3]
			EndIf
			SetRestFault( nCodeRest, FwhttpEncode( aRet[ 2 ] ), , 400 )
			printf( '-------------------------------------------' )
			printf('SetRestFault: ' + cValToChar( nCodeRest ) )
			printf( aRet[ 2 ] )
			printf( '-------------------------------------------' )
		Else
			oWs:SetStatus( 200 )
			If !Empty( aRet[2] )
				oWs:SetResponse( FwhttpEncode( aRet[2] ) )
			Else
				oWs:SetResponse( '{}' )
			EndIf
		EndIf

	ElseIf !lRet
		printf('-------------------------------------------')
		printf('SetRestFault: 500')
		printf("Houve um problema ao executar a requisição.")
		printf('-------------------------------------------')
		SetRestFault( 500, FwhttpEncode(STR0010) )//"Houve um problema ao executar a requisição."
	EndIf

Return lRet

