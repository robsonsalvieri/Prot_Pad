/*/{Protheus.doc} sInputSO
Gera um JSON contendo as informações de Insumos da OS para a sincronização
com o aplicativo Mobile

@author	Larissa Thaís de Farias
@since 02/2016
@param oWS, objeto, referência ao webservice
@param cDate, caracter, data da última sincronização
@return array, [1] indica se obteve sucesso na operação,
               [2,1] nome do arquivo de delete,
			   [2,2] nome do arquivo de update
/*/
//---------------------------------------------------------------------
Static Function sInputSO( oWs, cDate )

	Local cDelete    := ''
	Local lEmptySTN  := .F.
	Local lFirst     := .T.
	Local lFirstD    := .T.
	Local nCount     := 0
	Local cHrIn      := Time()
	Local cSubJSON   := ""
	Local cQuery     := ChangeQuery( QInputSO( oWs, cDate, "sInputSO" ) + " ORDER BY TL_ORDEM " )
	Local cAliasQry  := GeraTemp( cQuery, 'InputSO' )
	Local cEntity    := 'inputso'
	Local oData

	fInitWrite(oWs, cEntity)
	
	dbSelectArea(cAliasQry)
	While !(cAliasQry)->(EoF())

		If fCondDel(cAliasQry)
			// Itens a criar ou atualizar
			cSubJSON := ','

			If lFirst
				lFirst := .F.
				cSubJSON := ''
			EndIf

			oData := fWriteStl( cAliasQry )

			If fHasPerms('occurrences') .And. ( lEmptySTN := !Empty( (cAliasQry)->TN_CODOCOR ) )
				oData["occurrences"] := fWriteStn( cAliasQry )
			EndIf

			cSubJSON += oData:toJson()

			fWriteJson(cSubJSON, , oWs)

		Else

			If !lFirstD
				cDelete += ','
			Else
				lFirstD := .F.
			EndIf

			cDelete += '"' + NGEscape( (cAliasQry)->TL_ORDEM ) + "+" +;
				NGEscape( (cAliasQry)->TL_PLANO ) + "+" +;
			 	NGEscape( (cAliasQry)->TL_TAREFA ) + "+" + ;
				NGEscape( (cAliasQry)->TL_TIPOREG ) + "+" +;
				NGEscape( (cAliasQry)->TL_CODIGO ) + "+" +;
				NGEscape( (cAliasQry)->TL_SEQRELA ) + "+" +;
				NGEscape( (cAliasQry)->TL_SEQTARE ) + '"'

			lEmptySTN := .F.

		EndIf

		nCount++

		// Caso não exista ocorrência relacionada a este registro, pula para o proximo.
		If !lEmptySTN

			(cAliasQry)->( dbSkip() )

		EndIf

	EndDo

	( cAliasQry )->( dbCloseArea() )

	fEndWrite(oWs, cDelete, cEntity, 0, 0, nCount, cHrIn)

Return { .T., {} }

//---------------------------------------------------------------------
/*/{Protheus.doc} fWriteStl
Cria um objeto com valores da STL

@author	Maria Elisandra de Paula
@since 17/12/2024
@param cAliasQry, string, alias em execução
@return array, [1] objeto json em string
               [2] objeto json
/*/
//---------------------------------------------------------------------
Static Function fWriteStl( cAliasQry )

	Local cTlObserva := ""
	Local lUsaLote   := SuperGetMV("MV_RASTRO",.F.,'N') == "S" //Indica se utiliza controle de rastro nos produtos
	Local lUsaLocal  := SuperGetMV("MV_LOCALIZ") == "S" //Indica se utiliza controle de localização nos produtos
	Local lUsaEsto   := SuperGetMv( "MV_NGMNTES", .F.,'N' ) == 'S'
	Local lGeraSa    := lUsaEsto .And. SuperGetMv( 'MV_NGGERSA', .F., 'N' ) $ 'S/P'
	Local oInput     := JsonObject():New()

	oInput["order"] := NGEscape( (cAliasQry)->TL_ORDEM )
	oInput["plan"] := NGEscape( (cAliasQry)->TL_PLANO )
	oInput["sequence"] := NGEscape( (cAliasQry)->TL_SEQRELA )
	oInput["task"] := NGEscape( (cAliasQry)->TL_TAREFA )
	oInput["taskSequence"] := NGEscape( (cAliasQry)->TL_SEQTARE )
	
	If (cAliasQry)->TL_PERMDOE != Nil
		oInput["percent"] := cValtoChar( (cAliasQry)->TL_PERMDOE )
	Else
		oInput["percent"] := ""
	EndIf
	

	oInput["type"] := NGEscape( (cAliasQry)->TL_TIPOREG )
	oInput["code"] := NGEscape( (cAliasQry)->TL_CODIGO )

	If (cAliasQry)->TL_TIPOREG != 'P' .And. Alltrim((cAliasQry)->TL_UNIDADE) == 'H' .And. (cAliasQry)->TL_TIPOHOR == 'S'
		oInput["amount"] := cValtoChar(NGRETHORDDH((cAliasQry)->TL_QUANTID)[2])
	Else
		oInput["amount"] := cValtoChar((cAliasQry)->TL_QUANTID)
	EndIf

	oInput["resourceAmount"] := (cAliasQry)->TL_QUANREC
	oInput["unity"] := NGEscape( (cAliasQry)->TL_UNIDADE )
	oInput["destiny"] := NGEscape( (cAliasQry)->TL_DESTINO )
	oInput["startDateTime"] := NGEscape( (cAliasQry)->TL_DTINICI ) + ' ' + NGEscape( (cAliasQry)->TL_HOINICI )
	oInput["endDateTime"] := NGEscape( (cAliasQry)->TL_DTFIM ) + ' ' + NGEscape( (cAliasQry)->TL_HOFIM)
	oInput["warehouse"] := NGEscape( (cAliasQry)->TL_LOCAL )

	If (cAliasQry)->TL_TIPOREG == "P"
		//Controle de rastro: 'L':controla somente Lote,'S':controla lote e sub-lote
		If lUsaLote .And. ! Empty((cAliasQry)->TL_LOTECTL)
			oInput["lot"] := NGEscape((cAliasQry)->TL_LOTECTL)
			oInput["sublot"] := NGEscape((cAliasQry)->TL_NUMLOTE)
		EndIf

		//Controle de localização: sim ou não
		If lUsaLocal .And. ( !Empty((cAliasQry)->TL_LOCALIZ) .Or. ! Empty((cAliasQry)->TL_NUMSERI) )
			oInput["location"] := NGEscape((cAliasQry)->TL_LOCALIZ)
			oInput["serialnumber"] := NGEscape((cAliasQry)->TL_NUMSERI)
		EndIf
	EndIf

	//Os campos abaixo só serão enviados caso os insumos possuam pre requisição
	If lGeraSa .And. !Empty( (cAliasQry)->CP_PREREQU ) .And. !Empty( (cAliasQry)->CP_OK  )
		oInput["warehouseRequest"] := (cAliasQry)->TL_NUMSA
		oInput["warehouseItem"] := (cAliasQry)->TL_ITEMSA
	EndIf

	If !Empty( (cAliasQry)->TL_NUMSA ) .And. !Empty( (cAliasQry)->TL_ITEMSA )
		oInput["saNum"] := (cAliasQry)->TL_NUMSA
		oInput["saItem"] := (cAliasQry)->TL_ITEMSA
	EndIf

	If !Empty( (cAliasQry)->TL_NUMSC ) .And. !Empty( (cAliasQry)->TL_ITEMSC )
		oInput["scNum"] := (cAliasQry)->TL_NUMSC
		oInput["scItem"] := (cAliasQry)->TL_ITEMSC
	EndIf

	//Caso tenha cotação de compras envia quantidade ---
	If (cAliasQry)->QUOTATION > 0
		oInput["quotation"] := (cAliasQry)->QUOTATION
	EndIf

	If (cAliasQry)->TL_USACALE == 'S'
		oInput["useCalendar"] := .T.
	EndIf

	If !Empty((cAliasQry)->TL_LOJA)
		oInput["store"] := (cAliasQry)->TL_LOJA
	EndIf

	cTlObserva := ""

	dbSelectArea("STL")
	dbGoTo( (cAliasQry)->R_E_C_N_O_ )
	If !Empty( STL->TL_OBSERVA )
		cTlObserva := Alltrim( STL->TL_OBSERVA )
		cTlObserva := StrTran( cTlObserva, '"', '\"' )
	EndIf
	
	oInput["note"] := NGEscape( cTlObserva )

Return oInput

//---------------------------------------------------------------------
/*/{Protheus.doc} fWriteStn
Cria um objeto com valores da STN / ocorrencias da ordem

@author	Maria Elisandra de Paula
@since 17/12/2024
@param cAliasQry, string, alias em execução
@return array, [1] objeto json em string
               [2] objeto json
/*/
//---------------------------------------------------------------------
Static Function fWriteStn( cAliasQry )

	Local cTNObserva := ""
	Local cOrder := (cAliasQry)->TL_ORDEM
	Local cPlan := (cAliasQry)->TL_PLANO
	Local cTask := (cAliasQry)->TL_TAREFA
	Local cSeqRe := (cAliasQry)->TL_SEQRELA
	Local aOccurrences := {}
	Local oOccurrence

	Do While (cAliasQry)->( !EoF() ) .And. (cAliasQry)->TL_ORDEM == cOrder .And. (cAliasQry)->TL_PLANO == cPlan .And.;
		(cAliasQry)->TL_TAREFA == cTask .And. (cAliasQry)->TL_SEQRELA == cSeqRe

		oOccurrence := JsonObject():New()

		oOccurrence["problem"] := NGEscape( (cAliasQry)->TN_CODOCOR )
		
		If !Empty( (cAliasQry)->TN_CAUSA )
			oOccurrence["cause"] := NGEscape( (cAliasQry)->TN_CAUSA )
		EndIf

		If !Empty( (cAliasQry)->TN_SOLUCAO ) 
			oOccurrence["solution"] := NGEscape( (cAliasQry)->TN_SOLUCAO )
		EndIf

		cTNObserva := ''

		dbSelectArea( 'STN' )
		dbGoTo( (cAliasQry)->RECNO_STN )
		If !Empty( STN->TN_DESCRIC )
			oOccurrence["note"] := NGEscape( StrTran( AllTrim( STN->TN_DESCRIC ), '"', '\"' ) )
		EndIf

		aAdd ( aOccurrences, oOccurrence )
		oOccurrence := nil

		(cAliasQry)->( dbSkip() )

	EndDo

Return aOccurrences

