/*/
//---------------------------------------------------------------------
Static Function fMessage( cOrder, cPlan, cError, aOSGeradas, aSSGeradas, aNoStQ  )

 	Local nX       := 0
	Local oItem
	Local oResponse 
	Local aResponse := {}
	Local oAux
	Local aItems    := {}

	//----------------------------------------
	// SS geradas pelas respostas de checklist
	//----------------------------------------
	aItems := fJsonRes( aOSGeradas )
	If !Empty( aItems )
		oAux := JsonObject():New()
		oAux["id"] := "serviceOrder"
		oAux["items"] := aItems
		aAdd( aResponse, oAux )
	EndIf

	//----------------------------------------
	// OS geradas pelas respostas de checklist
	//----------------------------------------
	aItems := fJsonRes( aSSGeradas )
	If !Empty( aItems )
		oAux := JsonObject():New()
		oAux["id"] := "serviceRequest"
		oAux["items"] := aItems
		aAdd( aResponse, oAux )	
	EndIf

	//----------------------------------------
	// checklist (STQ) não incluídos
	//----------------------------------------
	If Len( aNoSTQ ) > 0
		aItems := {}
		For nX := 1 to Len( aNoSTQ )
			oItem := JsonObject():New()
			oItem["task"] := Alltrim(aNoSTQ[nX][1])
			oItem["checklist"] := Alltrim(aNoSTQ[nX][2])
			oItem["answer"] := Alltrim(aNoSTQ[nX][3])
			aAdd( aItems, oItem )
		Next nX

		oAux := JsonObject():New()
		oAux["id"] := "checklist"
		oAux["items"] := aItems
		aAdd( aResponse, oAux )	
	EndIf

	oItem := JsonObject():New()
	oItem["code"] := cOrder
	oItem["plan"] := cPlan
	oItem["message"] := aResponse

	oResponse := JsonObject():New()
	oResponse["data"] := oItem

	If Empty( cError )
		oResponse["hasError"] := .F.
		oResponse["hasStack"] := .F.
	Else
		oResponse["error"] := cError
	EndIf

	oResponse["order"] := cOrder
	oResponse["osGenerated"] := aOSGeradas
	oResponse["ssGenerated"] := aSSGeradas
	oResponse["notGenerated"] := aNoSTQ

Return oResponse

