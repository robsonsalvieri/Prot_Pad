		cQuery += " OR " + dateQuery( "B2_USERLGI" ) + " >= " + ValtoSql( cDate ) + ") "
	Else
		cQuery += " AND SB2.D_E_L_E_T_ = ' '"
	EndIf

	If lPeMntNg
		cFilter := ExecBlock("MNTNG",.F.,.F.,{"FILTER_WAREHOUSE",oWS})
		If !Empty(cFilter)
			cQuery += cFilter
		EndIf
	EndIf

	cQuery := ChangeQuery(cQuery)

Return cQuery

