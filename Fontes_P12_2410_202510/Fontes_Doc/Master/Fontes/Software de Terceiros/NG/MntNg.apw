	cQuery += " 	AND SB1.D_E_L_E_T_ = ' ' "

	cQuery += "	WHERE SB2.B2_FILIAL = " + ValtoSql( FwxFilial("SB2") )

	// trecho que corresponde às condições left join da tabela HP3 logo acima
	If lMigrated
		cQuery += " AND HP3_SB1.HP3_CODE IS NULL "
