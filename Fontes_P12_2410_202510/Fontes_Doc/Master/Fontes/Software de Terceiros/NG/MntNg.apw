/*/{Protheus.doc} sQueryGen
Envio de uma entidade

@author Maria Elisandra de Paula
@since 08/10/2024
@param oWS, objeto, referência ao webservice
@param cDate, caracter, data da última sincronização
@return array, [1] indica se obteve sucesso na operação,
               [2,1] nome do arquivo de delete,
			   [2,2] nome do arquivo de update
/*/
//---------------------------------------------------------------------
Static Function sQueryGen( oWs, cDate, cTable, cSelect, cEntity, aFields, cKeyDel )

	Local cDelete   := ''
	Local lFirst    := .T.
	Local lFirstD   := .T.
	Local nCount    := 0
	Local cHrIn     := Time()
	Local cQuery    := fQueryGen( oWs, cDate, cTable, cSelect )
	Local cAliasQry := GeraTemp( cQuery, cEntity )
	Local nDelete   := 0
	Local nUpdate   := 0
	Local aFileName := {}	
	Local oOJson
	Local nIndex    := 0
	Local cDesc     := ""
	Local cField    := ""

	fInitWrite(oWs, cEntity)

	dbSelectArea(cAliasQry)
	While !(cAliasQry)->(EoF())

		If fCondDel(cAliasQry)

			If !lFirst
				oWs:SetResponse(",")
			Else
				lFirst := .F.
			EndIf

			oOJson := JsonObject():New()
			
			For nIndex := 1 To Len( aFields )
				
				cDesc  := aFields[nIndex,1]
				cField := aFields[nIndex,2]

				oOJson[ cDesc ] := &cField.

			Next nIndex

			If (cAliasQry)->BLOCKED == "TRUE"
				oOJson["blocked"] := true
			EndIf

			oWs:SetResponse(oOJson)

			FreeObj( oOJson )

		Else

			If !lFirstD
				cDelete += ','
			Else
				lFirstD := .F.
			EndIf

			cDelete += '"' + &cKeyDel. + '"'

		EndIf

		nCount++

		( cAliasQry )->( dbSkip() )

	EndDo

	( cAliasQry )->( dbCloseArea() )

	fEndWrite(oWs, cDelete, cEntity, nDelete, nUpdate, nCount, cHrIn)

Return { .T., aFileName }

