@since 17/12/2024
@param cOrder, string, código da ordem
@return array [1] detalhes de respostas
			[2] opções/perguntas relacionadas
/*/
//---------------------------------------------------------------------
Static Function fObjTpq( cOrder ) // respostas DA OS

	Local cQuery := ''
	Local cAliasQry := ''
	Local aAnswers := {}

    cQuery := " SELECT TPQ_ORDEM, TPQ_PLANO, TPQ_ETAPA,TPQ_OPCAO,TPC_TIPCAM,"
	cQuery += "  TPQ_TAREFA, TPQ_ORDEMG, TPC_TPMANU,TPC_TIPRES,"
    cQuery += "  CASE "
    cQuery += "   WHEN TPC_TIPRES = '1' THEN '.T.'"
    cQuery += "   ELSE TPQ_RESPOS "
    cQuery += "   END AS TPQ_RESPOS,"
    cQuery += fCaseDel('TPQ',.T.)
    cQuery += " FROM " + RetSQLName('TPQ') + " TPQ "
    cQuery += " JOIN " + RetSQLName('TPC') + " TPC "
    cQuery += "  ON TPC_FILIAL = " + ValtoSql(FwxFilial("TPC"))
    cQuery += "  AND TPC_ETAPA = TPQ_ETAPA "
    cQuery += "  AND TPC_OPCAO = TPQ_OPCAO "
    cQuery += "  AND TPC.D_E_L_E_T_ = ' ' "
	cQuery += " WHERE TPQ.TPQ_FILIAL = " + ValtoSql(FwxFilial("TPQ"))
	cQuery += " AND TPQ.TPQ_ORDEM = "+ ValtoSql(cOrder)
	cQuery += " AND TPQ.D_E_L_E_T_ = ' ' "
	cQuery := ChangeQuery( cQuery )

	cAliasQry := GeraTemp( cQuery, 'Answer' )

	dbSelectArea(cAliasQry)
	While !(cAliasQry)->(EoF())

		oData := fWriteTpq( cAliasQry )[2]

		aAdd( aAnswers, oData )

		( cAliasQry )->( dbSkip() )
	EndDo

	( cAliasQry )->( dbCloseArea() )

Return aAnswers

