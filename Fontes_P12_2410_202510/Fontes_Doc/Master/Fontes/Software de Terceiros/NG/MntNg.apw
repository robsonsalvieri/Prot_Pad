			EndIf

			Begin Transaction

				oResponse := fUpdateSo( Self:GetContent(), Self )
				lRet := fCheckError( oResponse )

			Recover
				DisarmTransaction()

			End Transaction

			CloseTransactions( aRet )

		EndIf

		//---------------------------------------------------
		// trecho precisa ser acionado após close transaction UPDATE
		//---------------------------------------------------
		If lRet
			fSendTrue( oResponse, Self )
		EndIf

	End Sequence
	If lTmpTbl
		oTempIns:Delete()
		oTempSLP:Delete()
	EndIf
	ErrorBlock(bLastError)

Return lRet

