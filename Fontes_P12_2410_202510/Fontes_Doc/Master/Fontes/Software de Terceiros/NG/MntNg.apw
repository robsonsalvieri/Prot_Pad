//---------------------------------------------------------------------
/*/{Protheus.doc} fDelImg
Exclusão de imagens vinculadas a registros deletados

@author Maria Elisandra de Paula
@since 04/10/24

@param cTable, string, tabela entidade
@param cKey, string, numero da ordem
@param cPlan, string, plano
@param cTask, string, tarefa
@param cStep, string, etapa

@return nulo
/*/
//---------------------------------------------------------------------
Static Function fDelImg(cTable, cKey, cPlan, cTask, cStep)

	Local cFilEnt := FwxFilial( cTable )
	Local cCodEnt := ''
	Local cAliasQry := GetNextAlias()

	If cTable == 'STQ'
		cCodEnt :=  '% ( AC9.AC9_CODENT = ' + ValtoSql( cKey + cPlan + cTask + cStep) + ')%
	Else

		cCodEnt	:= '% ( AC9.AC9_CODENT = ' + ValtoSql(cKey) + ;
		IIf(cTable == 'TQB', ' OR AC9.AC9_CODENT = ' + ValtoSql( FwxFilial('TQB') +cKey ) , '') + ')%'  // em alguns casos foi gravado chave com filial

	EndIf

	BeginSql Alias cAliasQry
		SELECT AC9.R_E_C_N_O_ AS R_ECNO FROM %table:AC9% AC9
		INNER JOIN %table:ACB% ACB
			ON ACB.ACB_FILIAL = %xFilial:ACB%
			AND ACB.ACB_CODOBJ = AC9.AC9_CODOBJ
		WHERE AC9.AC9_FILIAL = %xFilial:AC9%
			AND AC9.AC9_ENTIDA = %exp:cTable%
			AND AC9.AC9_FILENT = %exp:cFilEnt%
			AND %exp:cCodEnt%
			AND AC9.%NotDel%
			AND ACB.%NotDel%
	EndSql
	
	While !(( cAliasQry )->( EoF() ))

		dbSelectArea('AC9')
		dbGoTo( (cAliasQry)->R_ECNO )
		
		RecLock('AC9', .F.)
		dbDelete()
		MsUnLock()

		( cAliasQry )->( dbSkip() )
	EndDo

	( cAliasQry )->( dbCloseArea() )

Return

//---------------------------------------------------------------------
/*/{Protheus.doc} fNameUpd
Modifica nome de arquivo no banco de conhecimento

@param cName, string, novo nome do objeto
@param cOldName, string, nome antigo do objeto
@param cTable, string, tabela da entidade relacionada a alteração da imagem
@param cCodeEnt, string, AC9_CODENT.

@author Pedro Henrique Perez Trebilcock
@since 03/09/2024
@return nul
/*/
//---------------------------------------------------------------------
Static Function fNameUpd( cName, cOldName, cTable, cCodeEnt )

	Local aNewName := {}
	Local cMsDocPath := ''
	Local cAliasQry  := ''
	Local cKey := ''
	Local cOldKey := ''
	Local cDescri := ''
	Local nError := 0
	Local oResponse
	local fileEx := StrTokArr(cOldName, '.')[2]

	If cTable == 'TQB'

		cCodEnt := FwxFilial( cTable ) + cCodeEnt

	EndIF

	cAliasQry := GetNextAlias()

	If isSRVunix()
		cMsDocPath := StrTran( MsDocPath(),'\', '/')
	Else
		cMsDocPath := MsDocPath()
	EndIf

	aNewName := nameCheck(cName + "." + fileEx)

	DbSelectArea( 'ACB' )
	DbSetOrder( 2 )

	If dbSeek( FwxFilial( 'ACB' ) + cOldName )

		BeginSql Alias cAliasQry
		SELECT COUNT(*) AS AC9_QUANT
		FROM %table:AC9% AC9
		LEFT JOIN %table:ACB% ACB
			ON ACB_OBJETO = %exp:cOldName%
		WHERE AC9.AC9_CODOBJ = ACB.ACB_CODOBJ
		EndSql

		IF ( cAliasQry )->AC9_QUANT > 1

			__CopyFile( cMsDocPath + cBarra + cOldName, cMsDocPath + cBarra + aNewName )

			cOldKey := ACB->ACB_CODOBJ
			cDescri := ACB->ACB_DESCRI

			RecLock( 'ACB', .T. )
			cKey := GetSXEnum( 'ACB', 'ACB_CODOBJ' )
			ACB->ACB_FILIAL := FwxFilial( 'ACB' )
			ACB->ACB_CODOBJ := cKey
			ACB->ACB_OBJETO := aNewName[1] + '.' + aNewName[2]
			ACB->ACB_DESCRI := cDescri
			MsUnlock()

			DbSelectArea( 'AC9' )
			DbSetOrder( 1 )

			IF dbSeek( FwxFilial( 'AC9' ) + cOldKey + cTable + FwxFilial( cTable ) + cCodEnt)

				RecLock( 'AC9' , .F. )

				AC9->AC9_CODOBJ := cKey 

				MsUnlock()

			Endif

			

		ELSE

			DbSelectArea( 'ACB' )
			DbSetOrder( 2 )

			If dbSeek( FwxFilial( 'ACB' ) + cOldName )

				cKey := ACB_CODOBJ

				RecLock( 'ACB' , .F. )

				ACB->ACB_OBJETO := aNewName[1] + '.' + aNewName[2]

				MsUnlock()
						
				nError := FRename( cMsDocPath + cBarra + cOldName, cMsDocPath + cBarra + aNewName[1] + '.' + aNewName[2] )

				IF nError < 0 

					return {.F., STR0018 + " " + AllTrim( Str( FError() ) ) }// "Erro ao criar arquivo no servidor"

				EndIf

			
			EndIF
			

		EndIf

	Else

		return {.F., "arquivo não localizado"}
	
	Endif

	dbCloseArea()

	oResponse := MntOrderAttachment():New( cKey )
	oResponse:Path     := '/picture/' + AllTrim( aNewName[1] + '.' + aNewName[2] )
	oResponse:Date     := ''
	oResponse:Type     := ''
	oResponse:FileName := aNewName[1] + '.' + aNewName[2]

Return { .T., FWJsonSerialize( oResponse, .F. ) }

