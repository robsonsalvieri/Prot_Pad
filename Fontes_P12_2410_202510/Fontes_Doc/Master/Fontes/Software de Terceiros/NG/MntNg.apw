//---------------------------------------------------------------------
/*/{Protheus.doc} fCheckError
Verifica se exite erro e retorna requisição do ws

@author	Maria Elisandra de Paula
@since 16/12/2024
@param oResponse, objeto, retorno da requisição
@return boolean, se validou
/*/
//---------------------------------------------------------------------
Static Function fCheckError( oResponse )

	Local lRet := .T.
	Local nAux := 400

	If oResponse:hasProperty("error") .And. !Empty(oResponse["error"])
		If oResponse:hasProperty("errorCode") .And. oResponse["errorCode"] > 0
			nAux := oResponse["errorCode"]
		EndIf

		SetRestFault( nAux, FwhttpEncode( oResponse["error"] ), , 400)
		lRet := .F.

		DisarmTransaction()

		printf( '-------------------------------------------' )
		printf('SetRestFault: ' + oResponse["error"] )
		printf( '-------------------------------------------' )

	EndIf

Return lRet

//---------------------------------------------------------------------
/*/{Protheus.doc} fCreateErr
Verifica se exite erro e retorna requisição do ws

@author	Maria Elisandra de Paula
@since 16/12/2024
@param xValid, string ou array, detalhes do erro
@return object, detalhes com erro
/*/
//---------------------------------------------------------------------
Static Function fCreateErr( xValid )

	Local oResponse := JsonObject():New()

	If Valtype( xValid ) == "C"
		oResponse["error"] := xValid
	ElseIf Valtype( xValid ) == "A"
		oResponse["error"] := xValid[2]
		If Len( xValid ) == 4
			oResponse["errorCode"] := xValid[4]
		EndIf
	EndIf

Return oResponse

