/*/{Protheus.doc} QFueling
Query para abastecimento

@autor Maria Elisandra de Paula
@since 08/10/2024
@param oWS, objeto, referência ao webservice
@param cDate, caracter, data da última sincronização

/*/
//---------------------------------------------------------------------
Static Function QFueling( oWs, cDate )

	Local lBlock := TQN->(FieldPos('TQN_MSBLQL')) > 0
	Local cQuery := ""
	Local cCutDate := ""
	Local aNg1Key := fGet1Key()

	IF len(aNg1Key) == 4 .AND. !Empty(aNg1Key[4])

		cCutDate := dtos(dDataBase - Val(aNg1Key[4]))

	EndIF

	cQuery += " SELECT TQN.TQN_FROTA, TQN.TQN_NABAST, TQN.TQN_CODCOM,TQN.TQN_POSTO, TQN.TQN_LOJA, TQN.TQN_CNPJ, TQN.TQN_DTABAS, TQN.TQN_HRABAS,"
	cQuery += " TQN.TQN_TANQUE, TQN.TQN_BOMBA,TQN.TQN_QUANT, TQN.TQN_VALUNI, TQN.TQN_VALTOT, TQN.TQN_HODOM, TQN.TQN_POSCO2,TQN.TQN_CODMOT, TQN.TQN_NOTFIS,"
	cQuery += fCaseDel('TQN')

	If lBlock
		cQuery += " CASE WHEN TQN.TQN_MSBLQL = '1' THEN 'TRUE' ELSE '' END AS BLOCKED "
	Else
		cQuery += " '' AS BLOCKED "
	EndIf

	cQuery += "FROM " + RetSQLName('TQN') + " TQN "
	cQuery += "WHERE TQN.TQN_FILIAL = " + ValtoSql( FwxFilial("TQN") )
	cQuery += " AND NOT EXISTS( SELECT 1 FROM " + RetSQLName('TTA') + " TTA " 
	cQuery += "   WHERE TTA.D_E_L_E_T_ = '' "
	cQuery += "   AND TTA_FILIAL || TQN_POSTO || TQN_LOJA || TQN_NOTFIS = TTA.TTA_FILIAL || TTA.TTA_POSTO || TTA.TTA_LOJA || TTA.TTA_FOLHA)"

	If cDate <> "0"
		cQuery += " AND (" + dateQuery( "TQN_USERGA" ) + " >= " + ValtoSql( cDate )
		cQuery += " OR " + dateQuery( "TQN_USERGI" ) + " >= " + ValtoSql( cDate ) + ") "
	Else
		cQuery += " AND TQN.D_E_L_E_T_ = ' '"
	EndIf

	IF !Empty(cCutDate)

		cQuery += "AND TQN_DTABAS >= " + ValtoSql(cCutDate)
	
	EndIf

	cQuery := ChangeQuery( cQuery )

Return cQuery

