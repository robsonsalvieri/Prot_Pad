/*/
//---------------------------------------------------------------------
Static Function sEquipment( oWS, cDate )

	Local cDelete     := ''
	Local aFileName   := {}
	Local cLocTree    := ""
	Local lFirst      := .T.
	Local lFirstD     := .T.
    Local nCount      := 0
    Local cHrIn       := Time()
	Local nDelete     := 0
	Local nUpdate     := 0	
	Local cSubJSON    := ""
	Local lOcurrences := !lPortalOrigin .And. fHasPerms('occurrences')
	Local lDbMSSQL    := "MSSQL" $ Upper(AllTrim(TcGetDB()))
	Local cQuery      := ChangeQuery( QEquipment( oWS, cDate ) )
