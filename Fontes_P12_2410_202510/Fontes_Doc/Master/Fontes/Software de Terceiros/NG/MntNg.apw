/*/{Protheus.doc} nameCheck
Verifica se existe arquivo com o mesmo nome e adiciona "(n)" se já existir.

@param cName, string, novo nome do objeto
@param nCopy, numerico, número de cópias do objeto

@author Pedro Henrique Perez Trebilcock
@since 03/09/2024
@return string
/*/
//---------------------------------------------------------------------

Static Function nameCheck( cName )

	local nameArray := StrTokArr( cName, '.' )
	local fileName := nameArray[1]
	local fileEx := nameArray[2]
	Local nCopy := 1
	local cCopy := '(' + cValToChar( nCopy ) + ')'
	local aRet := nameArray

	dbSelectArea( 'ACB' )
	dbSetOrder( 2 )

	IF dbSeek( FwxFilial( 'ACB' ) + cName)

		WHILE dbSeek( FwxFilial( 'ACB' ) + fileName + cCopy + '.' + fileEx )

			nCopy++

			cCopy := '(' + cValToChar( nCopy ) + ')'

		EndDo

		aRet := { fileName + cCopy , fileEx }

	EndIf


Return aRet

