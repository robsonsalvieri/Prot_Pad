/*/{Protheus.doc} fQueryGen
Gera uma query para qualquer tabela 

@autor Maria Elisandra de Paula
@since 08/10/2024
@param oWS, objeto, referência ao webservice
@param cDate, caracter, data da última sincronização

/*/
//---------------------------------------------------------------------
Static Function fQueryGen(oWs, cDate, cTable, cSelect)
	
	Local cPreName := cTable + "." + cTable + "_"
	Local lBlock   := NGCADICBASE( cTable + "_MSBLQL","A", cTable,.F.)
	Local cQuery   := cSelect

	cQuery += fCaseDel(cTable)

	If lBlock
		cQuery += " CASE WHEN " + cPreName + "MSBLQL = '1' THEN 'TRUE' ELSE '' END AS BLOCKED "
	Else
		cQuery += " '' AS BLOCKED "
	EndIf

	cQuery += "FROM " + RetSQLName(cTable) + " " + cTable
	cQuery += "WHERE " + cPreName + "FILIAL = " + ValtoSql( FwxFilial(cTable) )

	If cDate <> "0"
		cQuery += " AND (" + dateQuery( cTable + "_USERGA" ) + " >= " + ValtoSql( cDate )
		cQuery += " OR " + dateQuery( cTable + "_USERGI" ) + " >= " + ValtoSql( cDate ) + ") "
	Else
		cQuery += " AND " + cTable + ".D_E_L_E_T_ = ' '"
	EndIf

	cQuery := ChangeQuery( cQuery )

Return cQuery

