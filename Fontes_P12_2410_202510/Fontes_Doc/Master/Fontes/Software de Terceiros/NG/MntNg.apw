/*/{Protheus.doc} fVldAbast
Valida abastecimento

@param oJson, Objeto, corpo da requisição
@param nOpcx, numeric, operação
@author Maria Elisandra de Paula
@since 10/10/2024
@return	array [1], lógico, se requisição está ok
			  [2], string, erro
			  [3], array, campos e conteúdo para exec
			  [4], numeric, numero do abastecimento para opc 5

/*/
//---------------------------------------------------------------------

Static Function fVldAbast(oWs)

	Local nIndex := 0
    Local aAbast := {}
    Local cError := ''

	Local aNum := {{'TQN_QUANT'   ,'quantity'   },;
					{'TQN_VALUNI' ,'unityValue' },;
					{'TQN_VALTOT' ,'totalValue' },;
					{'TQN_HODOM'  ,'counter1'   },;
					{'TQN_POSCO2' ,'counter2'   }}

	Local aStr := {{'TQN_PLACA'   ,'plate'      },;
					{'TQN_FROTA'  ,'equipment' },;
					{'TQN_CODCOM' ,'fuel'       },;
					{'TQN_POSTO'  ,'station' },;
					{'TQN_LOJA'   ,'store'      },;
					{'TQN_CNPJ'   ,'cnpj'       },;
					{'TQN_TANQUE' ,'tank'       },;
					{'TQN_BOMBA'  ,'pump'       },;
					{'TQN_CODMOT' ,'driver'     },;
					{'TQN_NOTFIS' ,'invoice'    }}


	Local oJson  := JsonObject():New()

    cError := oJson:FromJson( oWs:GetContent() )

    If Valtype( cError ) == 'C' 
		Return {.F.,STR0022 + CRLF + cError, {}}
	EndIf

	cError := ''

    If ValType(oJson['equipment']) == 'U'
        cError += "TQN_FROTA" + CRLF 
    EndIf

	xValue := oJson['dateTime']
	If ValType(xValue) == 'U'
        cError += "TQN_DTABAS" + CRLF 
		cError += "TQN_HRABAS" + CRLF
	Else
		cDate := Stod( StrTokArr(xValue,' ')[1] )
		cHra  := StrTokArr(xValue,' ')[2]
	EndIf

	If Empty(cError)

		For nIndex := 1 To Len(aStr)
			xValue := oJson[ aStr[nIndex,2] ]
			If ValType(xValue) == 'C'
				aAdd( aAbast, { aStr[nIndex,1], PadR( xValue, TAMSX3( aStr[nIndex,1] )[1] ), Nil } )			    
			EndIf
		Next nIndex

		aAdd( aAbast, { 'TQN_DTABAS', cDate, Nil } )
		aAdd( aAbast, { 'TQN_HRABAS', cHra, Nil } )

		For nIndex := 1 to Len(aNum)
			xValue := oJson[ aNum[nIndex,2] ]
			If ValType(xValue) == 'N'
				aAdd( aAbast, { aNum[nIndex,1], xValue, Nil } )
			EndIf
		Next nIndex

	Else
		Return {.F.,'Campo(s) obrigatório(s) não informado(s): ' + cError, {}}
	EndIf

	/*
		If nOpcx == 5

			nScan := aScan( aAbast, { |x| x[1] == 'TQN_FROTA' } )
			cFrota := aAbast[nScan,1]

			nScan := aScan( aAbast, { |x| x[1] == 'TQN_DTABAS' } )
			cDtAbas := aAbast[nScan,1]

			nScan := aScan( aAbast, { |x| x[1] == 'TQN_HRABAS' } )
			cHrAbas := aAbast[nScan,1]


			DBSelectArea('TQN')
			DBSetOrder(1)
			If MsSeek(FwxFilial('TQN') + cFrota + cDtAbas + cHrAbas)
				nAbast := TQN->TQN_NABAST
			EndIf

		EndIf
	*/
Return {.T.,'',aAbast}

