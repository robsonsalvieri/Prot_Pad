/*/{Protheus.doc} GetUserCfg
Carrega variáveis vinculadas ao usuário

@author	Alexandre Santos
@since 04/03/2020
@return Nil
/*/
//---------------------------------------------------------------------------------------
Static Function GetUserCfg()

	Local cMail     := ''
	Local cAliasST1 := ''
	Local cAliasTQ4 := ''
	Local lFilterOs := fHasPerms('onlyThisUser');
						.Or. fHasPerms('user-only-or-unlinked');
					 	.Or. fHasPerms('specialty-only-or-unlinked');
					 	.Or. fHasPerms('onlyUserSpecialties')

	Local lFilterSS := fHasPerms('onlyUserRelatedRequests')

	If lFilterOs
	
		cAliasST1 := GetNextAlias()
	
		// Busca vínculo com funcionário da manutenção - ST1
		BeginSql Alias cAliasST1

			SELECT ST1.T1_CODFUNC, ST1.T1_CODUSU
			FROM
				%table:ST1% ST1
			WHERE
				Upper(ST1.T1_CODUSU) = %exp:Upper( cUserLog )% AND
				ST1.T1_FILIAL = %xFilial:ST1% AND
				ST1.%NotDel%

		EndSql

		If (cAliasST1)->(!EoF()) .And. Upper((cAliasST1)->T1_CODUSU) == Upper( cUserLog )
			cUserST1 :=  AllTrim((cAliasST1)->T1_CODFUNC)
		EndIf

		(cAliasST1)->(dbCloseArea())

	EndIf

	If lFilterSS

		cMail := Trim( UsrRetMail( cUserLog ) )

		If !Empty(cMail)

			cAliasTQ4 := GetNextAlias()

			// Busca vínculo com executante de solicitações - TP4
			BeginSql Alias cAliasTQ4

				SELECT
					TQ4.TQ4_CDEXEC,
					TQ4.TQ4_EMAIL1
				FROM
					%table:TQ4% TQ4
				WHERE
					Rtrim( UPPER( TQ4.TQ4_EMAIL1 ) ) = %exp:Upper( cMail )% AND
					TQ4.TQ4_FILIAL = %xFilial:TQ4% AND
					TQ4.%NotDel%

			EndSql

			If (cAliasTQ4)->(!EoF())
				cUserTQ4 := AllTrim( (cAliasTQ4)->TQ4_CDEXEC )
			EndIf

			(cAliasTQ4)->(dbCloseArea())
		
		EndIf

	EndIf

Return

