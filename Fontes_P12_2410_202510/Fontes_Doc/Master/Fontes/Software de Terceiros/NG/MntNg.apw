Static Function fJFueling(cTable, cDate, lBlock)

	Default cDate  := dtos(TQN->TQN_DTABAS)
	Default lBlock := TQN->(FieldPos('TQN_MSBLQL')) > 0 .And.TQN->TQN_MSBLQL == '1'

	oOJson := JsonObject():New()
	oOJson["equipment"]  := (cTable)->TQN_FROTA
