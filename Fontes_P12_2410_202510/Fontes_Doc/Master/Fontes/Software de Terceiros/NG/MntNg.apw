	oOJson["store"]      := (cTable)->TQN_LOJA
	oOJson["dateTime"]   := cDate + " " + (cTable)->TQN_HRABAS
	oOJson["tank"]       := (cTable)->TQN_TANQUE
	oOJson["pump"]       := (cTable)->TQN_BOMBA
	oOJson["quantity"]   := (cTable)->TQN_QUANT
	oOJson["unityValue"] := (cTable)->TQN_VALUNI
	oOJson["totalValue"] := (cTable)->TQN_VALTOT
	oOJson["counter1"]   := (cTable)->TQN_HODOM
	oOJson["counter2"]   := (cTable)->TQN_POSCO2
	oOJson["driver"]     := (cTable)->TQN_CODMOT

	If !Empty((cTable)->TQN_CNPJ)
		oOJson["cnpj"] := (cTable)->TQN_CNPJ
	EndIf

	If !Empty((cTable)->TQN_NOTFIS)
		oOJson["invoice"]    := (cTable)->TQN_NOTFIS
	EndIf

	If lBlock
		oOJson["blocked"] := true
	EndIf

Return oOJson

