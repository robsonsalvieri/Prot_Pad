//---------------------------------------------------------------------
/*/{Protheus.doc} fSt9All
Query para equipamentos quando tem permissão para incluir OS ou SS

@param oWs, objeto, web service
@param cDate, string, data da sinc.
@author	Maria Elisandra de Paula
@since 26/06/2024
@return string, trecho da query
/*/
//---------------------------------------------------------------------
Static Function fSt9All(oWs,cDate)

	Local cQuery := ""
	Local lFieldTaf := NGCADICBASE( 'TAF_USERGI', 'A', 'TAF', .F. ) .And. NGCADICBASE( 'TAF_USERGA', 'A', 'TAF', .F. )

	cQuery += fFieldSt9(ows)
	cQuery += " FROM " + RetSQLName( 'ST9' ) + " ST9"
	cQuery += " INNER JOIN " + RetSQLName( 'CTT' ) + " CTT "
	cQuery += "    ON CTT_FILIAL = " + ValToSql( FwxFilial( "CTT" ) )
	cQuery += "		AND CTT_CUSTO = T9_CCUSTO "

	cQuery += fWhereCtt( oWs )
	cQuery += " AND CTT.D_E_L_E_T_ = ' ' "

	cQuery += fJoinTPE()
	cQuery += fJoinSHB()

	cQuery += "  INNER JOIN " + RetSQLName('ST6') + " ST6  "
	cQuery += " 	ON ST6.T6_FILIAL = " + ValtoSql( FwxFilial("ST6") )
	cQuery += " 	AND  ST6.T6_CODFAMI = T9_CODFAMI "

	cQuery += fWhereSt6( oWs )
	cQuery += " AND ST6.D_E_L_E_T_ = ' '"

	If lMigrated
		cQuery += fHP3OnST6()
		cQuery += fHP3OnCTT()
	EndIf

	cQuery += " WHERE T9_FILIAL = " + ValtoSql( FwxFilial('ST9') )

	// trecho que corresponde às condições left join da tabela HP3 logo acima
	If lMigrated
		cQuery += " AND HP3_CTT.HP3_CODE IS NULL "
		cQuery += " AND HP3_ST6.HP3_CODE IS NULL "
	EndIf

	cQuery += fWhereSt9( oWs )

	If cDate != "0"

		cQuery += " AND (" + dateQuery( "T9_USERLGA" ) + " >= " + ValtoSql( cDate )
		cQuery += " OR " + dateQuery( "T9_USERLGI" ) + " >= " + ValtoSql( cDate )

		//------------------------------------------------
		// verifica se houve alteração na taf do bem
		//------------------------------------------------
		If lFieldTaf
        	cQuery += " OR EXISTS( "
            cQuery += " SELECT 1 "
			cQuery += " FROM " + RetSQLName( 'TAF' ) + " TAF "
			cQuery += " WHERE TAF.TAF_FILIAL = " + ValToSql( FwxFilial( 'TAF') )
			cQuery += "     AND TAF.TAF_CODCON = T9_CODBEM "
			cQuery += "     AND TAF.TAF_INDCON = '1' "
			cQuery += "     AND TAF.TAF_MODMNT = 'X' "
			cQuery += "     AND (" + dateQuery( "TAF_USERGA" ) + " >= " + ValtoSql( cDate )
			cQuery += "         OR " + dateQuery( "TAF_USERGI" ) + " >= " + ValtoSql( cDate ) + " ) )"
		EndIf

		cQuery += " ) "

	Else
		cQuery += " AND ST9.D_E_L_E_T_ = ' '"
	EndIf

Return cQuery 

//---------------------------------------------------------------------
/*/{Protheus.doc} fFieldSt9
Campos da query para equipamentos

@autor Maria Elisandra de Paula
@param oWs, objeto, web service
@since 24/06/2024
@return string, trecho da query para equipamentos
/*/
//---------------------------------------------------------------------
Static Function fFieldSt9(ows)

	Local cSearc := ""
	Local cQuery := ""
	Local lBlock := NGCADICBASE("T9_MSBLQL","A","ST9",.F.)

	cQuery := " SELECT DISTINCT T9_CODBEM, T9_NOME, T9_CCUSTO, T9_BARCODE, T9_LOCAL, T9_CODFAMI, T9_PLACA, "

	If lFuelingPerm
		cQuery += " CASE WHEN EXISTS( SELECT 1 FROM " + RetSqlName('TT8') + " TT8 WHERE TT8_FILIAL = " + ValToSql(FWxFilial('TT8'))
		cQuery += " AND T9_CODBEM = TT8_CODBEM AND TT8.D_E_L_E_T_ = '' ) THEN 'TRUE' ELSE ' ' END  AS HASTANK,"
	Else
		cQuery += " ' ' AS HASTANK,"
	EndIf

	If lPeMntNg

		cSearc := ExecBlock("MNTNG",.F.,.F.,{"SEARCH_FIELD_ST9",oWS})
		
	EndIf

	If Empty(cSearc) // tratamento para resultado nulo

		cSearc := "''"
	
	EndIf

	cQuery += cSearc + " as PRD_SEARCH, "

	//Busca data e hora do último apontamento de contador
	cQuery += cIsNull + "((SELECT MAX(TP_DTLEITU || '' || TP_HORA) "
	cQuery += "   FROM " + RetSQLName('STP') + " STP "
	cQuery += "   WHERE TP_FILIAL =  " + ValToSql(FwxFilial("STP"))
	cQuery += "     AND TP_CODBEM = T9_CODBEM "
	cQuery += "		AND STP.D_E_L_E_T_ = ' ' ) , '')  AS DTHRCOUNT , "

	//Busca data e hora do último apontamento do SEGUNDO contador
	cQuery += cIsNull + "((SELECT MAX(TPP_DTLEIT || '' || TPP_HORA) "
	cQuery += "   FROM " + RetSQLName('TPP') + " TPP "
	cQuery += "   WHERE TPP_FILIAL =  " + ValToSql(FwxFilial("TPP"))
	cQuery += "     AND TPP_CODBEM = T9_CODBEM " 
	cQuery += " 	AND TPP.D_E_L_E_T_ = ' ' ), '') AS DTHRCOUNT2 , "

	cQuery += cIsNull + "(CTT_DESC01,'') AS CCNAME,"
	cQuery += cIsNull + "(HB_NOME,'') AS CTNAME,"
	cQuery += cIsNull + "(TPE_TPCONT,'') TPE_TPCONT,"
	cQuery += cIsNull + "(TPE_POSCON,0) TPE_POSCON,"
	cQuery += cIsNull + "(TPE_LIMICO,0) TPE_LIMICO,"
	cQuery += cIsNull + "(T9_POSCONT,0 ) T9_POSCONT,"
	cQuery += cIsNull + "(T9_TPCONTA,'') T9_TPCONTA,"
	cQuery += cIsNull + "(T9_DTULTAC,'') T9_DTULTAC,"
	cQuery += cIsNull + "(T9_LIMICON,0)  T9_LIMICON,"
	cQuery += " T9_TEMCONT,"
	cQuery += " T9_CENTRAB,"
	cQuery += " T9_ESTRUTU,"

	cQuery += fCaseDel('ST9')

	cQuery += "CASE "
	cQuery += 	"WHEN T9_BITMAP = ' ' THEN 'FALSE' "
	cQuery += "ELSE "
	cQuery += 	"'TRUE' "
	cQuery += "END AS HASIMAGE,"

	cQuery += "CASE "
	cQuery += " WHEN EXISTS( SELECT 1 FROM " + RetSQLName('TAF') + " TAF "
	cQuery += "   WHERE TAF_FILIAL = T9_FILIAL "
	cQuery += "     AND TAF_MODMNT = 'X' AND TAF_INDCON = '1' "
	cQuery += "     AND TAF_CODCON = T9_CODBEM AND D_E_L_E_T_ = ' ' ) "
	cQuery += " THEN 'TRUE' " 
	cQuery += " ELSE 'FALSE' "
	cQuery += " END AS HAS_TAF , "

	If lBlock
		cQuery += " CASE WHEN T9_MSBLQL = '1' THEN 'TRUE' ELSE '' END AS BLOCKED "
	Else
		cQuery += "'' AS BLOCKED "
	EndIf

Return cQuery

//---------------------------------------------------------------------
/*/{Protheus.doc} fSt9NotAll
Query para equipamentos quando não tem permissão para incluir OS ou SS
Neste caso serão enviados apenas ST9 das OS ou SS já existentes

@author	Maria Elisandra de Paula
@since 26/06/2024
@return string, query para equipamentos
/*/
//---------------------------------------------------------------------
Static Function fSt9NotAll(oWs, cDate)

	Local cQuery := " SELECT DISTINCT T9_CODBEM, PRD_SEARCH, T9_NOME, T9_CCUSTO, T9_BARCODE, T9_LOCAL, T9_CODFAMI,DTHRCOUNT, T9_PLACA, HASTANK, "
	
	cQuery += "  DTHRCOUNT2 , CCNAME,CTNAME,TPE_TPCONT,TPE_POSCON,TPE_LIMICO, T9_POSCONT, T9_TPCONTA,"  
	cQuery += "  T9_DTULTAC,T9_LIMICON, T9_TEMCONT,T9_CENTRAB,T9_ESTRUTU, IS_DELETED, HASIMAGE, HAS_TAF, BLOCKED "
	cQuery += " FROM ("

	If fPerEditOS()

		cQuery += "( " + fFieldSt9(ows) 
		cQuery += "	FROM " + RetSQLName('STJ') + " STJ "
		cQuery +=	fJoinSTJ( oWs, .F. )

		cQuery += fJoinTPE()
		cQuery += fJoinSHB()

		cQuery += fWhereSTJ()

		cQuery += "    AND STJ.TJ_SITUACA IN('L', 'P') "
		cQuery += "    AND STJ.TJ_TERMINO = 'N' "
		cQuery += "    AND STJ.D_E_L_E_T_ = ' ' "
		// sempre será baseado pela STJ e não pela data de userlga pois nem todos os bens foram enviados no primeiro sync
		cQuery += " AND ST9.D_E_L_E_T_ = ' '" + ")" 

		If fPerEditSS()
			cQuery += "UNION"
		EndIf

	EndIf

	If fPerEditSS()
		
		cQuery += "( " + fFieldSt9(ows) 
		cQuery += "	FROM " + RetSQLName('TQB') + " TQB "

		cQuery += fJoinTQB(oWs, .T.)
		cQuery += fJoinTPE()
		cQuery += fJoinSHB()
		cQuery += fWhereTQB(oWs)
		cQuery += fOpenTQB()
		// sempre será baseado pela TQB e não pela data de userlga pois nem todos os bens foram enviados no primeiro sync
		cQuery += " AND ST9.D_E_L_E_T_ = ' '" + ")" 

	EndIf

	cQuery += ") TBL"


Return cQuery

//---------------------------------------------------------------------
/*/{Protheus.doc} fOpenTQB
condição da query que indica apenas SS abertas

@author	Maria Elisandra de Paula
@since 26/06/2024
@return string, trecho da query
/*/
//---------------------------------------------------------------------
Static Function fOpenTQB()

	Local cQuery := "	AND ( TQB.TQB_SOLUCA IN('A','D') "
	cQuery += " 	OR ( TQB.TQB_SOLUCA = 'E' AND TQ3.TQ3_PESQST = '1' "
	cQuery += " 		AND TQB.TQB_CDSOLI = " + ValToSql( cUserLog )
	cQuery += " 		AND TQB.TQB_PSAP = ' ' ) )" //pendente de feedback
	cQuery += " AND TQB.D_E_L_E_T_ = ' ' "

Return cQuery

//------------------------------------------------------------------
/*/{Protheus.doc} fJoinTPE
Concatena trecho para query de sincronização

@type function

@author Maria Elisandra de Paula
