Function Sg160Pen()
	
	Local cAlsTAQ := GetNextAlias()	
	Local cResult := ''

	BeginSQL Alias cAlsTAQ

		SELECT
			MAX( TAQ.TAQ_PENDEN ) AS RESULT
		FROM
			%table:TAQ% TAQ
		WHERE
			TAQ.TAQ_FILIAL = %xFilial:TAQ% AND
			TAQ.%NotDel%

	EndSQL

	cResult := StrZero( ( Val( (cAlsTAQ)->RESULT ) + 1 ), 6)

	(cAlsTAQ)->( dbCloseArea() )

Return cResult

