Function Sg160Des(cTipo, cOrdem)
Local cDesc := ""

If cTipo == '1'
	cDesc := STR0007  //"Reavaliar Aspectos x Impactos"
ElseIf cTipo == '2'
	cDesc := STR0008  //"Revisar Documentos"
ElseIf cTipo == '3'
	cDesc := STR0009  //"Realizar Plano de Acao"
ElseIf cTipo == '4'
	cDesc := "O Requisito está prestes a vencer ou ja venceu"
ElseIf cTipo == '5'
	cDesc := "O Requisito está esgotando o seu prazo de vigência ou ja esgotou"
ElseIf cTipo == '6'
	cDesc := STR0012 //"O Almoxarifado não foi informado para o Destino"
ElseIf cTipo == '7'
   DbSelectArea("TCK")
	DbSetOrder(1)
	If (Dbseek(xFilial("TCK") + cOrdem ))
   	cDesc := STR0013 + " " + AllTrim(TCK->TCK_CODPRO) + " " + STR0014 + " " + AllTrim(TCK->TCK_CODLEG)  // Alerta do protocolo XXXX relacionado ao requisito XXXXX
   EndIf
EndIf

Return cDesc

