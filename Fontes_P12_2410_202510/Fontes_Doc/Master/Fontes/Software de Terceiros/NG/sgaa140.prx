		nPosTipo := aScan( oGet02:aHeader,{|x| Trim(Upper(x[2])) == "TB7_TIPO"})
		If nPosTipo > 0
			M->TB7_TIPO := oGet02:aCols[nAt][nPosTipo]
		EndIf
	EndIf

