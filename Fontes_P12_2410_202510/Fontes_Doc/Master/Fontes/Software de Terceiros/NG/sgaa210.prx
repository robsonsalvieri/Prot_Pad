Static Function fNewQuest()

	Private cQuestSGA := (cAliasCE)->TBQ_CHKLIS //Variavel que identifica qual o questionrio

	dbSelectArea( "TJ1" )
	dbSetOrder( 2 )
	If dbSeek( xFilial( "TJ1" ) + (cAliasCE)->TBQ_CHKLIS + (cAliasCE)->TBQ_ORDEM  )

		RegToMemory( "TJ1" , .F. )

		MDTB001C( "TJ1" , TJ1->( Recno() ) , 4 )

	Else

		aRotSetOpc( "TJ2" , 0 , 3 )

	EndIf

Return .T.

