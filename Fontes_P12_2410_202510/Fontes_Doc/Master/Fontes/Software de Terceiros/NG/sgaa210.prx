Function SGAA210Efi()
Dbselectarea(cAliasCE)
Dbgotop()
While !Eof()
   If !Empty((cAliasCE)->TBQ_OK)
      Dbselectarea("TBD")
      Dbsetorder(1)
      If Dbseek(xfilial("TBD")+(cAliasCE)->TBQ_CHKLIS)
         If TBD->TBD_TIPOPC <> "1"
            Dbselectarea(cAliasOC)
            Dbsetorder(1)
            If Dbseek((cAliasCE)->TBQ_CHKLIS)
               lREPOS := .F.
               While !Eof() .And. (cAliasOC)->TBE_CODCHK = (cAliasCE)->TBQ_CHKLIS
                  If !Empty((cAliasOC)->TBE_OK)
                     lREPOS := .T.
                     Exit
                  Endif
                  Dbskip()
               End

               If !lREPOS
                  Msginfo(STR0022+" "+(cAliasCE)->TBQ_CHKLIS,STR0023)  //"Nao foi selecionado as respostas das opcoes do CheckList"##"No Conformidade"
                  Return .f.
               Endif
            Else
               Msginfo(STR0022+" "+(cAliasCE)->TBQ_CHKLIS,STR0023)  //"Nao foi selecionado as respostas das opcoes do CheckList"##"No Conformidade"
               Return .f.
            Endif
         Endif
      Endif
   Endif
   Dbselectarea(cAliasCE)
   Dbskip()
End
Return .t.

