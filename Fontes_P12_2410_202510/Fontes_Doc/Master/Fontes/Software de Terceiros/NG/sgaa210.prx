/*/
Static Function Sg210Usu( cCodUsu )

If !Empty( cCodUsu )

	If M->TBQ_TIPUSU == '1'
		DbSelectArea( "QAA" )
		DbSetOrder(1)
		If !DbSeek( xFilial( "QAA" ) + cCodUsu )
			Help( " ", 1, "REGNOIS" )
			Return .F.
		Else
			cNomUsu	 := QAA->QAA_NOME
			lRefresh := .T.
		EndIf
	Else
		DbSelectArea( "SRA" )
		DbSetOrder(1)
		If !DbSeek( xFilial("SRA") + ALLTRIM( cCodUsu ) )
			Help( " ", 1, "REGNOIS" )
			Return .F.
		Else
			cNomUsu := SRA->RA_NOME
			lRefresh := .T.
		EndIf
	EndIf

	If Type( "oGetNom" ) == "O"

		oGetNom:cText := cNomUsu
		oGetNom:Refresh()

	EndIf

EndIf

Return

