//---------------------------------------------------------------------
/*/{Protheus.doc} ReportPrint
Realiza a impressão do relatorio no modelo TReport.
@param 		oReport, Objeto, Instancia de objeto oReport.
@return  	.T. -> Impressao concluida.
			.F. -> Nao imprimiu.
@sample ReportPrint(oReport)
@author Alexandre Santos
@since 11/07/2017
/*/
//---------------------------------------------------------------------
Static Function ReportPrint(oReport)
	Local oSecCcusto 	:= oReport:Section(1)
	Local oSecFunc 		:= oReport:Section(2)
	Local oSecOrdem 	:= oReport:Section(3)
	Local oSecEtapa 	:= oReport:Section(4)
	Local nFor, nOrdem, cFunc, cOrdem, cCCusto
	Private cArqTrab
	Private cTRB 		:= GetNextAlias()
	//Cria tabela temporária
	MontaTRB()
	dbSelectArea(cTRB)
	dbGotop()
	oReport:SetMeter(RecCount())
	While !EOF() .And. !oReport:Cancel()
		cCCusto := (cTRB)->Custo
		oSecCcusto:Init()
		oSecCcusto:PrintLine()
		While !EOF() .And. cCCusto == (cTRB)->CUSTO
			cFunc  := (cTRB)->CODFUNC
			oSecFunc:Init()
			oSecFunc:PrintLine()
			While !EOF() .And. cCCusto == (cTRB)->CUSTO .And. cFunc == (cTRB)->CODFUNC
				oOrdem:lVisible := .T.
				cOrdem  := (cTRB)->ORDEM
				cPlano	:= (cTRB)->PLANO
				nOrdem  := 0
				aEtapas := {}
				While !EOF() .And. cCCusto == (cTRB)->CUSTO .And.;
			 	 cFunc == (cTRB)->CODFUNC .And. cOrdem == (cTRB)->ORDEM .And. cPlano == (cTRB)->PLANO
					nOrdem++
					If nOrdem > 1
						oOrdem:Hide()
					EndIf
					oSecOrdem:Init()
					oSecOrdem:PrintLine()
					If !Empty((cTRB)->ETAPA)
						Aadd(aEtapas,(cTRB)->ETAPA)
					EndIf
					DbSelectArea(cTRB)
					Dbskip()
				End
				If Len(aEtapas) > 0
					oSecEtapa:Init()
					For nFor := 1 To Len(aEtapas)
						nX := nFor
						oSecEtapa:PrintLine()
					Next x
					oSecEtapa:Finish()
				EndIf
				oSecOrdem:Finish()
			End
			oSecFunc:Finish()
		End
		oSecCcusto:Finish()
	End
	oTempTable:Delete()//Deleta tabela temporária
Return .T.
