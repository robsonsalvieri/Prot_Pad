							Dbselectarea("STT")
							Dbskip()
						End

						MNTR730CHP(STS->TS_DTPRINI,STS->TS_HOPRINI,STS->TS_DTMRINI,;
						STS->TS_HOMRINI,STS->TS_DTPRFIM,STS->TS_HOPRFIM,;
