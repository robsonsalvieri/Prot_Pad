Function NG120VLOC(cPARAME1,cPARAME2,nTIPO)

	Local lTEMFACI := NGINTESTORG()

	If !lTEMFACI
		Return .T.
	Else
		If nTIPO = 1
			If Empty(cPARAME1)
				Return .T.
			Else
				dbSelectArea("TAF")
				dbSetOrder(07)
				If !dbSeek(xFilial("TAF") + "X" + "2" + cPARAME1)
					MsgInfo(STR0021,STR0022) //"Codigo da localização invalido."###"ATENÇÃO"
					Return .F.
				EndIf
			EndIf
		Else
			If cPARAME2 = "ZZZ"
				IIF( AteCodigo('TAF',cPARAME1,cPARAME2,16), .T., .F.)
			Else
				If Empty(cPARAME2)
					Help(" ",1,"ATEINVALID")
					Return .f.
				Elseif cPARAME2 < cPARAME1
					Help(" ",1,"DEATEINVAL")
					Return .f.
				EndIf

				dbSelectArea("TAF")
				dbSetOrder(07)
				If !dbSeek(xFilial("TAF") + "X" + "2" + cPARAME2)
					MsgInfo(STR0021,STR0022) //"Codigo da localização invalido."###"ATENÇÃO"
					Return .F.
				EndIf
			EndIf
		EndIf
	EndIf
Return .T.

