Static Function MNT120CHRE(aListaSS)

	Default aListaSS := {}

	Private oPrint := TMSPrinter():New(OemToAnsi(STR0001))  //"Solicitacao de Servico"

	oPrint:Setup()

	MNTR120IMP(oPrint,aListaSS)

	If Len(aListaSS) > 0
		oPrint:Preview()
	Else
		If MV_PAR16 == 2
			oPrint:Print()
		Else
			oPrint:Preview()
		Endif
	Endif
Return .T.

