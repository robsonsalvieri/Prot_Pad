/*/
Function MNR545MED(cMedida,cPosicao,cNumfogo)

	Local nI := 1

	While nI < 11
		dbSelectArea("TQS")
		dbSetOrder(01)
		If !Empty(aPneusQry[nI])
			If dbSeek((cAliasQry)->TRC_FILIAL+aPneusQry[nI])
				If cMedida != TQS->TQS_MEDIDA
					MNR545ARR(cPosicao,cNumfogo,6,,TQS->TQS_SULCAT,nI)
					Return 1
				Endif
			Endif
		Endif
		nI++
	End

Return 0

/*/


Ŀ
Funo    |MNR545FIL | Autor  Marcos Wagner Junior   Data 18/12/2006
Ĵ
Descrio Verifica se o pneu	consta em outra filial						  
Ĵ
 Uso      MNTR545                                                     
ٱ


/*/
Function MNR545FIL(cNumfogo,cPosicao,nI)

	Local cQryAlias := GetNextAlias()
	Local cQryTQS := ""

	Local cCodBem := ""

	//--------------------------------------------------
	// Busca o Cdigo do Bem pelo Nmero do Fogo
	//--------------------------------------------------
	// SELECT
	cQryTQS := "SELECT TQS.TQS_CODBEM "
	// FROM
	cQryTQS += "FROM " + RetSQLName("TQS") + " TQS "
	// WHERE                                                      \
	cQryTQS += "WHERE TQS.TQS_NUMFOG = " + ValToSQL(cNumfogo) + " AND TQS.D_E_L_E_T_ <> '*' "
	// ORDER BY
	cQryTQS += "ORDER BY TQS.TQS_CODBEM "

	// Utiliza a Tabela
	cQryTQS := ChangeQuery(cQryTQS)
	dbUseArea(.T., "TOPCONN", TCGenQry(,,cQryTQS), cQryAlias, .F., .T.)

	// Recebe o Cdigo do Bem
	dbSelectArea(cQryAlias)
	dbGoTop()
	If !Eof()
		cCodBem := (cQryAlias)->TQS_CODBEM
	EndIf
	dbCloseArea()

	//--------------------------------------------------
	// Verifica Inconsistncia com o Bem
	//--------------------------------------------------
	If !Empty(cCodBem)
		dbSelectArea("ST9")
		dbSetOrder(16)
		If dbSeek(cCodBem+"A")
			While !Eof() .And. cCodBem == ST9->T9_CODBEM .And. ST9->T9_SITBEM == "A"
				If FWxFilial('ST9', (cAliasQry)->TRC_FILIAL) != ST9->T9_FILIAL
					MNR545ARR(cPosicao,cNumfogo,5,,,nI)
					Return 1
				EndIf

				dbSelectArea("ST9")
				dbSkip()
			End
		EndIf
	EndIf

Return 0

/*/


Ŀ
Funo    |MNR545VEI | Autor  Marcos Wagner Junior   Data 18/12/2006
Ĵ
Descrio Verifica se o pneu	consta em outro veiculo						  
Ĵ
 Uso      MNTR545                                                     
ٱ


/*/
Function MNR545VEI(cPosicao,cNumfogo,nI)
	Local aArea := GetArea()

	DbSelectArea("TR9")
	DbSetOrder(01)
	If DbSeek((cAliasQry)->TRC_FILIAL+(cAliasQry)->TRC_ORDEM)
		DbSelectArea("TQS")
		DbSetOrder(01)
		If DbSeek((cAliasQry)->TRC_FILIAL+cNumfogo)
			If TR9->TR9_PLACA != TQS->TQS_PLACA .And. !Empty(TQS->TQS_PLACA)
				MNR545ARR(cPosicao,cNumfogo,4,,TQS->TQS_SULCAT,nI)
				RestArea(aArea)
				Return 1
			Endif
		Endif
	Endif
	RestArea(aArea)

Return 0

