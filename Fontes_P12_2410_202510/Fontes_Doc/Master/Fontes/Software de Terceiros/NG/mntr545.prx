 Uso      MNTA750                                                     
ٱ


/*/
Function MNTR545MME(nI)
	Local nMedida := 10000
	Local i

	cMed1 := "(cAliasQry)->TRC_PN"+AllTrim(Str(nI))+"M1"
	cMed2 := "(cAliasQry)->TRC_PN"+AllTrim(Str(nI))+"M2"
	cMed3 := "(cAliasQry)->TRC_PN"+AllTrim(Str(nI))+"M3"

	For i:= 1 to 3
		cMedida := "cMed"+Str(i,1)
		nMedMin := &cMedida
		nMedida := MIN(nMedida,&nMedMin)
	Next

Return nMedida

