	Local lOk		:= .T.
	Local aColsSup	:= BlankGetd( aHeader )
	Local aResul	:= { STR0025 , STR0026 , "3=Inativos" }  //"1=Todos" ## "2=Ativos"
	Local aTamSX3   := TAMSX3("TN0_QTAGEN")

	//Montagem do TRB
