		dbSelectArea( "TMA" )
		dbSetOrder( 1 )
		dbseek( xFilial( "TMA" ) + TN0->TN0_AGENTE )
		aCols[ n , nPosNom ] := TMA->TMA_NOMAGE
	Else
		dbSelectArea( "TN0" )
		dbSetOrder( 1 )
