	cQuery += " AND   TRH.TRH_CODBEM = ST9.T9_CODBEM "
	cQuery += " AND   ST9.T9_CODTMS  = DAK.DAK_CAMINH "
	cQuery += " AND   DAK.D_E_L_E_T_ <> '*' "
	cQuery += " AND   TRH.D_E_L_E_T_ <> '*' "
	cQuery += " AND   ST9.D_E_L_E_T_ <> '*' "
