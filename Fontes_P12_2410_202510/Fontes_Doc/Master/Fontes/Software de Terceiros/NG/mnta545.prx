//---------------------------------------------------------------------
Function MNTA545TRK()

	Local cCODCAR := ""
	dbSelectArea("TRK")
	cCODCAR := Space(Len(TRK->TRK_CODCAR))

	cAliasQry := GetNextAlias()
	cQuery := " SELECT DAK.DAK_COD, DAK.DAK_VALOR"
	cQuery += " FROM " + RetSqlName("TRH") + " TRH," + RetSqlName("ST9") + " ST9," + RetSqlName("DAK") + " DAK"
