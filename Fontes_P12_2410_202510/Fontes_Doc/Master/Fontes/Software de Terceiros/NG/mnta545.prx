Function MNT545SERV()

	Local cRetorno := ' '
	Local aOldArea := GetArea()
	Local cParamSera := GetMv( "MV_NG1SERA" )

	dbSelectArea("ST4")
	dbSetorder(1)
	If !dbseek(xFilial("ST4")+cParamSera)
		cRetorno := STR0072 //"Código do Serviço não cadastrado!"
	Else
		If NGFUNCRPO("NGSERVBLOQ",.F.)
			vRetBlq := NGSERVBLOQ(cParamSera,.f.)
			If !vRetBlq[1]
				cRetorno := vRetBlq[2]
			Endif
		Else
			dbSelectArea("STE")
			dbSetorder(1)
			If !dbseek(xFilial("STE")+ST4->T4_TIPOMAN)
				cRetorno := STR0073 //"Tipo de Serviço não cadastrado!"
			Elseif STE->TE_CARACTE <> 'C'
				cRetorno := STR0071 //"O serviço deverá ser do tipo Corretivo!"
			Endif
		Endif
	Endif

	RestArea(aOldArea)

Return cRetorno

