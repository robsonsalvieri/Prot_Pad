		//----------------------------
		RecLock("TM5",.F.)
		TM5->TM5_DTRESU := M->TYB_DTRESU
		TM5->TM5_CODRES := M->TYB_CODRES
		TM5->TM5_INDRES := M->TYB_INDRES
