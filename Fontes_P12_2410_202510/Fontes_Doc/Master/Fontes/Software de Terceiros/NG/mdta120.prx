Cadastro de exames do funcionário

@author Gabriel Sokacheski
@since 26/09/2023

/*/
//---------------------------------------------------------------------
Function Mdta120()

	Local aNgBeginPrm 	:= NgBeginPrm() // Guarda conteúdo e declara variáveis padroes

    Local oBrowse		:= Nil

	Private aAC  		:= { STR0001, STR0002 } // "Abandona" // "Confirma"
	Private aCRA 		:= { STR0002, STR0003, STR0001 } // "Confirma" // "Redigita" // "Abandona"
	Private asMenu 		:= Nil
	Private aIndSTP		:= {}

	Private bFiltraBrw 	:= { || Nil }

	Private cCadastro	:= Nil
	Private cAlias    	:= 'TM5'
	Private cNUMFIC   	:= TM0->TM0_NUMFIC
	Private cPrograma	:= 'MDTA120'

	Private lENumFic 	:= .F.
	Default lMdta110 	:= .F.
	Private lSuperUsr 	:= .F.
	Private lSigaMdtPS	:= If( SuperGetMv( 'MV_MDTPS', .F., 'N' ) == 'S', .T. , .F. )

	If FindFunction( 'MdtRestri' ) .And. !MdtRestri( cPrograma )
		NGRETURNPRM(aNGBEGINPRM)
		Return .F.
	Endif

    If AMiIn( 35 ) // Somente autorizado para SIGAMDT

        oBrowse := FWMBrowse():New()

        oBrowse:SetAlias( 'TM0' )
        oBrowse:SetMenuDef( 'Mdta120' )
        oBrowse:SetDescription( STR0007 ) // "Exames do Funcionario"

		oBrowse:AddLegend( { || Posicione( 'SRA', 1, TM0->TM0_FILFUN + TM0->TM0_MAT, 'RA_RESCRAI' ) $ '30/31' }		, 'Pink'	, STR0239 ) // "Transferido"
        oBrowse:AddLegend( { || Posicione( 'SRA', 1, TM0->TM0_FILFUN + TM0->TM0_MAT, 'RA_SITFOLH' ) == Space( 1 ) }	, 'Green'	, STR0238 ) // "Situação normal"
		oBrowse:AddLegend( { || Posicione( 'SRA', 1, TM0->TM0_FILFUN + TM0->TM0_MAT, 'RA_SITFOLH' ) == 'D' }		, 'Red'		, STR0240 ) // "Desligado"
		oBrowse:AddLegend( { || Posicione( 'SRA', 1, TM0->TM0_FILFUN + TM0->TM0_MAT, 'RA_SITFOLH' ) == 'A' }		, 'Yellow'	, STR0241 ) // "Afastado"
		oBrowse:AddLegend( { || Posicione( 'SRA', 1, TM0->TM0_FILFUN + TM0->TM0_MAT, 'RA_SITFOLH' ) == 'F' }		, 'Blue'	, STR0242 ) // "Férias"

		MDTX3Tam() //**DEVE SER RETIRADA APÓS LIBERAÇÃO DA PRÓXIMA RELEASE 12.1.2410**

        oBrowse:Activate()

    EndIf

	// Retorna conteúdo de variáveis padrões
	NgReturnPrm( aNgBeginPrm )

Return

