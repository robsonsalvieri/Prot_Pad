@param vCplano
@param vPreCh
@param cVTar, characters
@type function
/*/
//------------------------------------------------------------------------------
Static Function MNTR780IT(vCplano,vPreCh,cVTar)
	If Trim(cVTar) == "0"
		cNOMTAR := STR0041
	Else
		If lUSATAR
			lCORRET := If(Val(vCplano) = 0,.t.,.f.)
			cNOMTAR := NGNOMETAR(vPreCh,cVTar)
		Else
			cNOMTAR := NGSEEK("ST5",vPreCh+cVTar,1,"T5_DESCRIC")
		EndIf
	Endif
Return cNOMTAR
