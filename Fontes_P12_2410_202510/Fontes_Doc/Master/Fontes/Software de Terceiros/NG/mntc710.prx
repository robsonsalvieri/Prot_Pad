FUNCTION MNTC710()
PRIVATE aRotina := MenuDef()

PRIVATE aEstru := { }
Private cSavtela
Private LTLTTIPINS := .F.
cCadastro := OemToAnsi(STR0003) //"Servico"

SetKey( VK_F9, { | | NGVersao( "MNTC710" , 1 ) } )

//Ŀ
// Endereca a funcao de BROWSE                                  
//

mBrowse( 6, 1,22,75,"ST4")

//Ŀ
// Desativa tecla F12 e F9                                        
//
