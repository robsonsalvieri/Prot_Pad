Static Function MenuDef()

Local nCont
Local aRotina := { { STR0001  ,"AxPesqui"               ,  0 , 1},; //"Pesquisar"
                    { STR0002   ,"MNTC040()"  , 0 , 2}} //"Consulta"

If ExistBlock("MNTC7101")
	aRot := ExecBlock("MNTC7101",.F.,.F.)
	For nCont := 1 To Len(aRot)
		AADD(aRotina, aRot[nCont])
	Next
EndIf

Return(aRotina)