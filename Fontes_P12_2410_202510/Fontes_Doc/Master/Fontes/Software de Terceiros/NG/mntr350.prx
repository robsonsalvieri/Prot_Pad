/*/
//--------------------------------------------------------
Static Function fGetContI(cBem)

	Local aArea := GetArea()
	Local nContIni := 0

	dbSelectArea("STP")
	dbSetOrder(8)
	If dbSeek(xFilial("STP") + cBem + 'I')
		nContIni := STP->TP_ACUMCON
	EndIf

	RestArea(aArea)

Return nContIni
