/*/{Protheus.doc} MNTR350TRB1
Grava arquivo auxiliar

@type static function

@source MNTR350.prx

@author Deivys Joenck
@since 15/08/2001

@sample MNTR350TRB1()

@return Logico.
/*/
//---------------------------------------------------------------------
Static Function MNTR350TRB1()

	Local nContBem
	Local nCont1
	Local cCODBE

	dbSelectArea(cTrb1)
	DbGotop()
	While !Eof()
		IncProc()
		cCODBE := (cTRB1)->CBEM
		nContBem := 1
		While !Eof() .And. (cTRB1)->CBEM = cCODBE
			If nContBem == 1
				nDif := (cTRB1)->CONTA - fGetContI(cCODBE)
				nRef := 1
			Else
				nDif := (cTRB1)->CONTA - nCONT1
				nRef++
			EndIf
			DbSelectArea(cTRB2)
			lRecLock := !DbSeek((cTRB1)->FORNEC+(cTRB1)->FAMI+(cTRB1)->CBEM)
			RecLock(cTRB2, lRecLock)
				(cTRB2)->FORNECE := (cTRB1)->FORNEC
				(cTRB2)->FAMILIA := (cTRB1)->FAMI
				(cTRB2)->CODBEM  := (cTRB1)->CBEM
				(cTRB2)->TOTKM   := nDif
				(cTRB2)->REFORMA := nRef
			MsUnlock()
			nCONT1 := (cTRB1)->CONTA
			nContBem ++
			DbSelectArea(cTRB1)
			dbSkip()
		EndDo
	EndDo

Return .T.

