/*/
Function ValidaOk

	Local xx := 0
	nn := n

	If !Empty(aCols[nn][nOK])
		M->TBS_OK := space(1)
	Else
		M->TBS_OK := 'X'
	Endif

	aCols[nn][nOK] := M->TBS_OK
	lRefresh := .t.
	If ltpopcao = "2"
	If !Empty(M->TBS_OK)
		For xx := 1 to nItens
			If xx <> nn
			If !empty(aCols[xx][nOK])
				HELP(" ",1,"NGMUOPCEXC") //"Somente uma opcao podera ser marcada na etapa exclusiva.. "###"Atencao"

				M->TBS_OK := space(1)
				aCols[nn][nOK] := M->TBS_OK
				lRefresh := .t.
				n := nn
				Return .f.
			Endif
			Endif
		Next
	Else
		DbSelectArea("TBE")
			DbSetOrder(1)
			If DbSeek(xFilial("TBE")+cCodChk)
			If TBE->TBE_TIPRES == '2'
				aCols[nn][nRESPOS] := Space(len(TBT->TBT_CONDIN))
				lRefresh := .t.
		Endif
		EndIf
	Endif
	Else
		DbSelectArea("TBE")
		DbSetOrder(1)
		If DbSeek(xFilial("TBE")+cCodChk)
		If Empty(M->TBS_OK) .and. TBE->TBE_TIPRES == '2'
			aCols[nn][nRESPOS] := Space(len(TBT->TBT_CONDIN))
			lRefresh := .t.
		Endif
		EndIf
	Endif

	ValidaInf()

Return .F.

