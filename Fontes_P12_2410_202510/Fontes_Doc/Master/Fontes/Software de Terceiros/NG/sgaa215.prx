Function Sg215Tudo()

	Local yx
	For yx := 1 to nItens
		dbSelectArea("TBE")
		dbSetOrder(1)
		If dbSeek(xFilial("TBE")+cCodChk+aCols[yx][nnOPCA])
			If !Empty(aCols[yx][nOK]) .and. TBE->TBE_TIPRES == '2' .and. Empty(aCols[yx][nRESPOS])
			MsgInfo("Informe a resposta do item "+AllTrim(aCols[yx][nnOPCA]),"Atenção")
		Return .f.
			Endif
		EndIf
	Next

Return .t.

