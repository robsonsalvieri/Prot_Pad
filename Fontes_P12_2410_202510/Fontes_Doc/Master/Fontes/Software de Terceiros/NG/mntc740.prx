Function MNC740PAR()

    Pergunte("MNT74C",.T.)

    cFilterNew := 'ST9->T9_FILIAL = "' + xFilial("ST9") + '" .And. C740FILBR2()'
    NGFILBROINI("ST9", aIndST9, cFilterNew)
