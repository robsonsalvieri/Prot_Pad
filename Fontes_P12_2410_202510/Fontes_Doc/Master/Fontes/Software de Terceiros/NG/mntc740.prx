    aRotina := { {STR0002,"NGCAD01" , 0, 1},;    //"Visual."
                    {STR0028,"OSDETALH" , 0, 2},;    //"Detalh."
                    {STR0029,"OSHISTOCO", 0, 4},;    //"Ocorren."
                    {STR0030,"OshistPro", 0, 4},;    //"proBlem."
                    {STR0070,"NGATRASOS", 0, 4, 0},; //"Motivo Atraso"
                    {STR0031,"OshistEta", 0, 4}}      //"Etapas"

    If (cTrb)->RECNO > 0
        dbSelectArea(cAlias)
