Function C740FILBR2()

    Local aArea := GetArea()
    Local lRetFil := .f.

    Pergunte("MNT74C",.F.)

    dbSelectArea("STZ")
    dbSetOrder(02)
    dbSeek(xFilial("STZ")+ST9->T9_CODBEM+DTOS(MV_PAR01),.T.)
    While !Eof() .And. STZ->TZ_FILIAL == xFILIAL("STZ") .And.;
            STZ->TZ_CODBEM == ST9->T9_CODBEM .And. STZ->TZ_DATAMOV <= MV_PAR02
        lRetFil := .t.
        dbSelectArea("STZ")
        dbSkip()
    EndDo

    If IsInCallStack("MNC740IMP")
        Pergunte("MNT740",.F.)
    EndIf

    RestArea(aArea)

Return lRetFil