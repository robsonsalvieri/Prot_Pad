/*/
Function D655CHK()
Local xx := 0, npos, lRET := .T.
Local nX

nPOS := aSCAN( aHEADER, { |x| Trim( Upper(x[2]) ) == "TNK_AGENTE"})

If nPOS > 0
   For nX := 1 to Len(aCOLS)
       If n != nX
	       If aCOLS[nX][nPOS] == M->TNK_AGENTE
	          xx := xx + 1
	       Endif
       Endif
   Next
Endif

If xx > 0
   Help(" ",1,"JAEXISTINF")
   lRet := .f.
Endif

Return lRET

