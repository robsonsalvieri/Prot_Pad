If nCodPro > 0 .And. nDtEntr > 0
	//Percorre aCols
	For f = 1 to Len(aColsTCK)
		If !aColsTCK[f][Len(aColsTCK[f])]
			If f <> nAt
				If aColsTCK[f][nCodPro] == aColsTCK[nAt][nCodPro] .and. aColsTCK[f][nDtEntr] == M->TCK_DTENTR
					Help(" ",1,"JAEXISTINF")
					Return .F.
				Endif
			Endif
		Endif
	Next i
