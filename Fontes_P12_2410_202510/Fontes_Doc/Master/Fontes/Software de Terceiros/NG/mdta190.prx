Static Function MDT190AFOR()

	Local oDlg
	Local aButton := {}
	Local nOpcao, nX, nExame, nFornec, nLoja
	Local cLINOK := "AllwaysTrue()"
	Local cTUDOK := "AllwaysTrue()"
	Local cKey   := TMW->TMW_PCMSO

