	Else
		oPesquisar:Enable()
		oPesquisar:SetFocus()             // Define foco no campo de pesquisa
		oBtnPesq:Enable()               // Habilita botao de pesquisa
	EndIf

	oMark:oBrowse:Refresh()

Return

