	Local bMontCols := {|| fMonColsFo() }
	Local aYesFields:= {"TM5_EXAME","TM5_NOMEXA","TM5_FORNEC","TM5_LOJA"}
	Local aAlter    := { "TM5_FORNEC" , "TM5_LOJA" }

	Local aOldCols

	Local lAlterou

	Private aHeader := {}
	Private aCols   := {}

	//Se nao houver exames para serem alterados
	If Empty(aAltForEx)
		Return
	EndIf

	//Coloca TM5 na memoria
	dbSelectArea("TM5")
