*/
Function MNTR975FL(nOpc)
	Local cVERFL

	cVERFL := Mv_Par07

	If Empty(mv_par07)
		Return .t.
	Else
		If nOpc == 1
			lRet := IIf(Empty(Mv_Par07),.t.,ExistCpo('SM0',SM0->M0_CODIGO+Mv_par07))
			If !lRet
				Return .f.
			EndIf
		EndIf

		If nOpc == 2
			lRet := IIF(ATECODIGO('SM0',SM0->M0_CODIGO+Mv_par07,SM0->M0_CODIGO+Mv_Par08,02),.T.,.F.)
			If !lRet
				Return .f.
			EndIf
		EndIf
	EndIf

Return .t.   
