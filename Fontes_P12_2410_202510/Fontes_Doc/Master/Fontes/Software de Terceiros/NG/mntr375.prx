	Elseif par2 < par1
		Help(" ",1,STR0032,,STR0054,3,1) //"ATENO"###"Cdigo do Posto final informado  invlido."
		Return .f.
	Endif

	If par2 = replicate('Z',Len(PAR2))
		Return .t.
	Else  
		If !Atecodigo('TQF',Par1+Mv_Par04,Par2+Mv_Par06,08)
			Return .f.	
		Endif
	Endif 

Return .t. 

