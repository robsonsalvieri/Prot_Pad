Function MNTA975SER()

If !Empty(M->TSY_SERVIC)
	aCols[n][nTSYDESERV] := NGSEEK("TS4",M->TSY_SERVIC,1,"TS4_DESCRI")
	aCols[n][nTSYVALOR]  := NGSEEK("TS6",M->TSX_FORNEC+M->TSX_LOJA+M->TSY_SERVIC+TS6->TS6_DOCTO,1,"TS6_VALOR")
Else
	aCols[n][nTSYDESERV] := ''
	aCols[n][nTSYVALOR]  := 0
Endif

Return .t.

