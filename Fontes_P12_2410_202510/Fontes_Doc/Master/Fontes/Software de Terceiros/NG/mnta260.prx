/*/{Protheus.doc} GeraCon
Gera consulta de Ordens de Serviço a serem canceladas.
@type static

@author Tainã A. Cardoso
@since 07/11/2010

@obs Reescrito por: Alexandre Santos, 04/02/2019.
/*/
//------------------------------------------------------
Static Function GeraCon()

	Local lFilIns   := !Empty( M->TL_TIPOREG )
	Local cWhere    := ''
	Local cQuery    := ''

	MNT260LF() //Limpa o Filtro

	cQuery := 'SELECT '
	cQuery += "' ' AS OK, "
	cQuery += 'STJ.TJ_ORDEM AS ORDEM, '
	cQuery += 'STJ.TJ_PLANO AS PLANO, '
	cQuery += 'CASE WHEN '
	cQuery += 	"STJ.TJ_TIPOOS = 'B' THEN " + ValToSQL( NGRETSX3BOX( 'TJ_TIPOOS', 'B' ) ) + ' '
	cQuery += 'ELSE '
	cQuery += 	ValToSQL( NGRETSX3BOX( 'TJ_TIPOOS', 'L' ) ) + ' '
	cQuery += 'END AS TIPO, '
	cQuery += 'STJ.TJ_CODBEM AS BEM, '
	cQuery += 'CASE WHEN '
	cQuery += 	"STJ.TJ_TIPOOS = 'L' THEN TAF.TAF_NOMNIV "
	cQuery += 'ELSE '
	cQuery += 	'ST9.T9_NOME '
	cQuery += 'END AS NOMEBEM, '
	cQuery += 'CASE WHEN '
	cQuery += 	"STJ.TJ_TIPOOS = 'L' THEN ' ' "
	cQuery += 'ELSE '
	cQuery += 	'ST9.T9_CODFAMI '
	cQuery += 'END AS FAMILIA, '
	cQuery += 'STJ.TJ_SERVICO AS SERVICO, '
	cQuery += 'ST4.T4_NOME AS NOMESER, '
	cQuery += 'STJ.TJ_DTORIGI AS DTORIGI, '
	cQuery += 'STJ.TJ_SEQRELA AS SEQUENC '

	cQuery += 'FROM ' + RetSqlName( 'STJ' ) + ' STJ '
    
	cQuery += 'INNER JOIN ' + RetSqlName( 'ST4' ) + ' ST4 ON '
	cQuery += 	NGMODCOMP( 'ST4', 'STJ', '=' ) + ' '
	cQuery += 	'AND ST4.T4_SERVICO = STJ.TJ_SERVICO '
	cQuery += 	"AND ST4.D_E_L_E_T_ = ' ' "

	cQuery += 'LEFT JOIN ' + RetSqlName( 'ST9' ) + ' ST9 ON '
	cQuery += 	NGMODCOMP( 'ST9', 'STJ', '=' ) + ' '
	cQuery += 	'AND ST9.T9_CODBEM = STJ.TJ_CODBEM '
	cQuery += 	"AND ST9.D_E_L_E_T_ = ' ' "

	cQuery += 'LEFT JOIN ' + RetSqlName( 'TAF' ) + ' TAF ON '
	cQuery += 	NGMODCOMP( 'TAF', 'STJ', '=' ) + ' '
	cQuery += 	'AND TAF.TAF_CODNIV = STJ.TJ_CODBEM '
	cQuery += 	"AND TAF.D_E_L_E_T_ = ' ' "
	
	If lFilIns

		cQuery += 'LEFT JOIN ' + RetSqlName( 'STL' ) + ' STL ON '
		cQuery += 'STJ.TJ_FILIAL = STL.TL_FILIAL '
		cQuery += 'AND STJ.TJ_ORDEM = STL.TL_ORDEM '
		cQuery += 'AND STJ.TJ_PLANO = STL.TL_PLANO '
		cQuery += "AND STL.D_E_L_E_T_ = ' ' "
	
	EndIf

	cQuery += 'WHERE '
	cQuery += 	'STJ.TJ_FILIAL = ' + ValToSQL( FWxFilial( 'STJ' ) )
	cQuery +=	"AND STJ.TJ_SITUACA <> 'C' "
	cQuery += 	"AND STJ.TJ_TERMINO <> 'S' "
	cQuery +=	"AND STJ.TJ_TIPORET <> 'S' "
	cQuery +=	'AND (STJ.TJ_CODBEM  BETWEEN ' + ValToSQL( cDeBem ) + ' '
	cQuery +=		'AND ' + ValToSQL( cAteBem ) + ') '
	cQuery +=	'AND (STJ.TJ_DTORIGI  BETWEEN ' + ValToSQL( DtoS( dDeData ) ) + ' '
	cQuery +=		'AND ' + ValToSQL( DtoS( dAteData ) ) + ') '
	cQuery +=	'AND (SELECT COUNT(SCP.CP_OP) QNT '
	cQuery +=		'FROM ' + RetSqlName( 'SCP' ) + ' SCP '
	cQuery +=		'WHERE SCP.CP_FILIAL = ' + ValToSQL( FWxFilial( 'SCP' ) ) + ' '
	cQuery +=		'AND SCP.CP_OP = STJ.TJ_ORDEM || ' + ValToSQL( Padr( 'OS001', TAMSX3( 'CP_OP' )[ 1 ] ) ) + ' '
	cQuery +=		"AND SCP.CP_PREREQU = 'S' "
	cQuery +=		'AND ((SCP.CP_QUJE > 0 '
	cQuery +=			"AND SCP.CP_STATUS = 'E') "
	cQuery += 			"OR SCP.CP_STATUS = '') "
	cQuery += 		"AND SCP.D_E_L_E_T_= ' ' ) = 0 "
	cQuery +=	"AND STJ.D_E_L_E_T_ = ' ' "

	If lFilIns

		cQuery += 'AND STL.TL_TIPOREG = ' + ValToSql( M->TL_TIPOREG ) + ' '

		If !Empty( M->TL_CODIGO )
			cQuery += 'AND STL.TL_CODIGO  = ' + ValToSql( M->TL_CODIGO ) + ' '
		EndIf
	
	EndIf

	If ExistBlock( 'MNTA2602' )

		cWhere := ExecBlock( 'MNTA2602', .F., .F. )
		cQuery += 'AND ' + cWhere + ' '

	EndIf

	cQuery += 'GROUP BY STJ.TJ_ORDEM, '
	cQuery += 'STJ.TJ_PLANO, '
	cQuery += 'STJ.TJ_FILIAL, '
	cQuery += 'STJ.TJ_TIPOOS, '
	cQuery += 'STJ.TJ_CODBEM, '
	cQuery += 'STJ.TJ_SERVICO, '
	cQuery += 'STJ.TJ_SEQRELA, '
	cQuery += 'STJ.TJ_DTORIGI, '
	cQuery += 'ST4.T4_NOME, '
	cQuery += 'ST9.T9_CODFAMI, '
	cQuery += 'ST9.T9_NOME, '
	cQuery += 'TAF.TAF_NOMNIV '

	cQuery += 'ORDER BY STJ.TJ_ORDEM'
    
	cQuery := ChangeQuery( cQuery )
	SQLToTRB( cQuery, aDBF, cTRBP )

	( cTRBP )->( dbGoTop() )
	lContFil := .T.

Return

