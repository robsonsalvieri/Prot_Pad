			If TAA->TAA_DTINPR < dDtIni .Or. Empty(dDtIni)
				dDtIni := TAA->TAA_DTINPR
			EndIf
		Next
		//Modifica datas do objetivo
		M->TBH_ABERTU := dDtIni
		M->TBH_FECHAM := aQdGDoc[Len(aQdgDoc)][4]
