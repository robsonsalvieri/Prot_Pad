/*/
Function Sg300ExMe(nOpcx)
Local i, dDtIni

If Len( aQDGDoc ) <= 0
	MsgStop(STR0025,STR0023)
	Return .F.
EndIf

If nOpcx == 3 .or. nOpcx == 4
	dbSelectArea("TBK")
	dbSetOrder(1)
	If dbSeek(xFilial("TBK")+M->TBH_CODOBJ+aQdGDoc[oQdg:nAt][2])
		MsgInfo(STR0034,STR0035) //"J existem resultados lanados para as metas. No  possvel excluir o objetivo"###"Ateno"
		Return .F.
	Endif
	If MsgYesNo(STR0018,STR0019) //"Confirmar excluso?"
		aDel( aQDGDoc,oQdg:nAt)
		aSize( aQDGDoc, Len( aQDGDoc )-1)
		If Len( aQdgDoc ) > 0
			aQdGDoc:= aSort( aQdGDoc,,,{ |x,y| x[4] < y[4] } )
			oQdg:SetArray( aQDGDoc )
			oQdg:bLine:= bQdgLine1
			dDtIni := CTOD("  /  /  ")
			For i := 1 To Len(aQdgDoc)
				dbSelectArea("TAA")
				dbSetOrder(1)
				dbSeek(xFilial("TAA")+aQdGDoc[i][2])
				If TAA->TAA_DTINPR < dDtIni .Or. Empty(dDtIni)
					dDtIni := TAA->TAA_DTINPR
				EndIf
			Next
			M->TBH_ABERTU := dDtIni
			M->TBH_FECHAM := aQdGDoc[Len(aQdgDoc)][4]
		Else
			aQdGDoc := {}
			M->TBH_ABERTU := CTOD("  /  /  ")
			M->TBH_FECHAM := CTOD("  /  /  ")
		EndIf
		oEnc01:oBox:Refresh()
	EndIf
EndIf

Return .t.

