Function Sg300Situac(nOpcao)
Local lRet := .F.

If nOpcao = 4
	DbSelectArea("TBH")
	DbSetOrder(1)
	If DbSeek(xFilial("TBH")+M->TBH_CODOBJ)
		RecLock("TBH",.f.)
		If M->TBH_SITUAC == "1"
			TBH->TBH_SITUAC := "2"
		ElseIf M->TBH_SITUAC == "2"
			TBH->TBH_SITUAC := "3"
		EndIf
		MsUnLock("TBH")
		lRet := .T.
	EndIf
Else
	MsgAlert(STR0026) //"Opo apenas disponvel no modo de alterao."
EndIf

Return lRet

