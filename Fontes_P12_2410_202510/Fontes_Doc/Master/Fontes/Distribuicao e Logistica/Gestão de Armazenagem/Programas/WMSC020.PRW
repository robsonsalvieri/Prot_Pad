Function WMSC020FIL()
  
If Pergunte("WMSC020",.T.)
 
	cFiltraSB8 := "  B8_FILIAL == '" + xFilial("SB8") + "' "
	cFiltraSB8 += " .And. B8_PRODUTO >= '"+mv_par03+"' "
	cFiltraSB8 += " .And. B8_PRODUTO <= '"+mv_par04+"' "
	cFiltraSB8 += " .And. B8_LOCAL   >= '"+mv_par05+"' "
	cFiltraSB8 += " .And. B8_LOCAL   <= '"+mv_par06+"' "
	cFiltraSB8 += " .And. DTOS(B8_DTVALID)    >= '"+DTOS(mv_par01)+"' "
	cFiltraSB8 += " .And. DTOS(B8_DTVALID)    <= '"+DTOS(mv_par02)+"' "
	cFiltraSB8 += " .And. B8_LOTECTL >= '"+mv_par07+"' "
	cFiltraSB8 += " .And. B8_LOTECTL <= '"+mv_par08+"' "

	If mv_par09 == 2
		cFiltraSB8 += " .And. B8_SALDO > 0"
	EndIf
Else       
	Return .F.
EndIf
  
Return .T. 
