			ACTIVATE MSDIALOG oDlg1 ON INIT EnchoiceBar( oDlg1,	{|| ( Eval(bOk1), If(lVldAD,Eval(bOk2),Eval(bOk3)) ) }, {|| nOpca := 0,oDlg1:End()},, aButtons ) CENTER
		Else
