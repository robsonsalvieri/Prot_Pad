																	cLog := cValToChar(oModelBxPA:GetErrorMessage()[4]) + ' - '
																	cLog += cValToChar(oModelBxPA:GetErrorMessage()[5]) + ' - '
																	cLog += cValToChar(oModelBxPA:GetErrorMessage()[6])
