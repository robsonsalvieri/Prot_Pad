												If nPisCalc > 0 .And. nPisBaseC > 0
													aadd(aImpostos,{"PIS", nPisCalc ,cMV_PISNAT,"", nPis, nPisBaseC, nPisBaseR, '', SA2->A2_COD, SA2->A2_LOJA, SA2->A2_CGC, aCodRet[1]})
