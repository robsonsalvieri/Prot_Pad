									Else//Valores acessorios
										If	oSubFKA:SeekLine({{'FKA_TABORI',"FK2"}})
											If cTpDoc == "VA" .and. nVA != 0
												// Grava novos valores acessórios (FKD)
												FSetFK6FKD(oSubFK6, cChaveFK7, cIdFK2)
