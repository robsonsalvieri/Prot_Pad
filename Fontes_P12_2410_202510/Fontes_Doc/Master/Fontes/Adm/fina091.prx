																RestArea(aAreaAnt)
															EndIf

															dbSelectArea("SFQ")

															If Empty( _cFilFwSFQ )   //Multi-Filial
																cMFilial := xFilial("SFQ")
															Else
																If Empty( _cFilFwSE2 )
																	cMFilial := xFilial("SFQ")
