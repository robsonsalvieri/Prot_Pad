Function FA091PcoLan(cMarca)
	Local lRet	:= .T.
	Local aArea := GetArea()

	(cAliasSE2)->(dbSetOrder(__nIdxE2OK))
	(cAliasSE2)->(dbSeek(cMarca))
	SE2->(DBGOTO((caliasSE2)->NUM_REG))
	Do While (cAliasSE2)->(!Eof() .AND. E2_OK == cMarca)
		SE2->(DBGOTO((caliasSE2)->NUM_REG))
		RegToMemory("SE2",.F.)
		lRet := PcoVldLan("000024", "01", "FINA091")
		If !lRet
			Exit
		EndIf
		(cAliasSE2)->(dbSkip())
	EndDo
	RestArea( aArea )
Return lRet

