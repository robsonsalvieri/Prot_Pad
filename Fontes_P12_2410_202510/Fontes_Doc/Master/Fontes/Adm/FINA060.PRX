	Local lUseClass  	As Logical
	Local jTitFilho     As Json

    cFormula      := CriaVar("M4_CODIGO",.F.)
    lPadrao       := .F.
    nTotal        := 0
    nHdlPrv       := 0
    nOpca         := 0
    lDesc         := .F.
    cAgen060      := CriaVar("EF_AGENCIA",.F.)
    cContrato     := ""
    cNumBco       := ""
    cConta060     := CriaVar("EF_CONTA",.F.)
    cPort060      := CriaVar("EF_BANCO",.F.)
    cPadrao       := Space(3)
    cChavEA       := ""
