        cTitulo	:= SE1->E1_PREFIXO + " " + SE1->E1_NUM 	 + " " + SE1->E1_PARCELA
        cContrat := Subs(SE1->E1_CONTRAT, 1, 3)

        //Se situacao de cobranca possui portador (nao e carteira)
