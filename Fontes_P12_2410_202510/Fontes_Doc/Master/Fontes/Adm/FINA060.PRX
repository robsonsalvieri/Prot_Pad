/*/{Protheus.doc} RetFilSEA
    Validação da Especie e Sub Conta quando a rotina for executada de forma automatica
    (MSEXECAUTO())

    @type Static Function
    @author rafael.rondon
    @since 03/01/2023
    @version 1.0
    @param cChaveSE1, Character, Chave do SE1 para indice 4 da SEA
    @return cFilSEA, Character, EA_FILIAL
/*/
Static Function RetFilSEA(cChaveSE1 As Character) As Character

    Local cFilSEA       As Character
    Local aArea         As Array
    Local aAreaSEA      As Array

    cFilSEA     := ""
    aArea       := FwGetArea()
    aAreaSEA    := SEA->(FwGetArea())

    DbSelectArea("SEA")
    SEA->(DbSetOrder(4))    //EA_FILORIG+EA_NUMBOR+EA_CART+EA_PREFIXO+EA_NUM+EA_PARCELA+EA_TIPO+EA_FORNECE+EA_LOJA                                                                            
    If SEA->(dbSeek(cChaveSE1))
        cFilSEA := SEA->EA_FILIAL
    EndIf
    
    FWRestArea(aAreaSEA)
	FWRestArea(aArea)

Return cFilSEA

/*/{Protheus.doc} BuscaFI2
    Função que realiza a busca dos registros existentes na tabela FI2. 
    OBS: Criada com intuito de verificar instruções de cobranças já geradas com as mesmas ocorrencias.

    @type Static Function
    @author matheus.monteiro
    @since 13/02/2025
    @version 1.0
    @param cFil, cOcorrencia , cPrefixo, cTitulo, cParcela, cTipo
    @return nQuant
/*/
Static Function BuscaFI2(cFil As Character, cOcorrencia As Character, cPrefixo As Character, cTitulo As Character, cParcela As Character, cTipo As Character, cCarteira As Character) As Numeric 
    Local cQuery    As Character
    Local oQryFI2   As Object
    Local nParam    As Numeric 
    Local nQuant    As Numeric

    Default cFil        := ""
    Default cOcorrencia := ""
    Default cPrefixo    := ""
    Default cTitulo     := ""
    Default cParcela    := ""
    Default cTipo       := ""
    Default cCarteira   := ""

    cQuery     := ""
    nParam     := 1
    nQuant     := 0
    __lCachQry := IIf(__lCachQry == Nil, FwLibVersion() >= "20211116", __lCachQry)

    If oQryFI2 == Nil 
        cQuery := "SELECT COUNT(R_E_C_N_O_) NQTDFI2 "
        cQuery += "FROM ? "
        cQuery += "WHERE FI2_FILIAL = ? AND "
        cQuery += "FI2_OCORR = ? AND "
        cQuery += "FI2_PREFIX = ? AND "
        cQuery += "FI2_TITULO = ? AND "
        cQuery += "FI2_PARCEL = ? AND "
        cQuery += "FI2_TIPO = ? AND "
        cQuery += "FI2_CARTEI = ? "

        cQuery := ChangeQuery(cQuery)
        oQryFI2:= IIf(__lCachQry, FwExecStatement():New(cQuery), FWPreparedStatement():New(cQuery))
    EndIf 

    oQryFI2:SetUnsafe(nParam++, RetSqlName("FI2"))
    oQryFI2:SetString(nParam++,  cFil)
    oQryFI2:SetString(nParam++,  cOcorrencia)
    oQryFI2:SetString(nParam++,  cPrefixo)
    oQryFI2:SetString(nParam++,  cTitulo)
    oQryFI2:SetString(nParam++,  cParcela)
    oQryFI2:SetString(nParam++,  cTipo)
    oQryFI2:SetString(nParam++,  cCarteira)

    nQuant := IIf(__lCachQry, oQryFI2:ExecScalar('NQTDFI2'), MpSysExecScalar(cQuery, "NQTDFI2"))

    If oQryFI2 != Nil 
        oQryFI2:Destroy()
    EndIf

Return nQuant

/*/{Protheus.doc} TCheckInst
	Apresenta uma tela pergutando como será o comportamento para a Instrução de Cobrança.
	@type  Function
	@author Francisco Oliveira
	@since 22/07/2025
	@version 1.0
	@return Array, com variaveis lCheck1, lCheck2, lCheck3
/*/
Static Function TCheckInst() As Array

    Local oSay1         As Object
    Local oSay2         As Object
    Local oSay3         As Object
    Local oCheck1       As Object
    Local oCheck2       As Object
    Local oCheck3       As Object
    Local oModal        As Object
    Local oContainer    As Object
    Local lCheck1       As Logical
    Local lCheck2       As Logical
    Local lCheck3       As Logical
    Local aCheckInst    As Array

    lCheck1     := .F.
    lCheck2     := .F.
    lCheck3     := .F.
    aCheckInst  := {}
    
    oModal := FWDialogModal():New()
    oModal:SetCloseButton( .F. )
    oModal:SetEscClose( .F. )
    oModal:setTitle(STR0195) // "Informe como será o comportamento para a Instrução de Cobrança"

    //define a altura e largura da janela em pixel
    oModal:setSize(180, 250)

    oModal:createDialog()

    oModal:AddButton( STR0006, {||oModal:DeActivate()}, , , .T., .F., .T., ) // "Confirmar"

    oContainer := TPanel():New( ,,, oModal:getPanelMain() )
    oContainer:Align := CONTROL_ALIGN_ALLCLIENT
    
    oSay1 := TSay():New( 010,10,{||STR0189 },oContainer,,,,,,.T.,,,220,20,,,,,,.T.) // "Todas as ocorrências deste processo serão realizadas conforme a opção escolhida."
    oSay2 := TSay():New( 020,10,{||STR0190 },oContainer,,,,,,.T.,,,220,20,,,,,,.T.) // "Se nenhuma opção for marcada, a tela de instrução será apresentada para todas as ocorrências."
    oSay3 := TSay():New( 050,10,{||STR0191 },oContainer,,,,,,.T.,,,220,20,,,,,,.T.) // "Deseja cadastrar instrução de cobrança para posterior envio ao  banco?"

    oCheck1 := TCheckBox():New(070,10,STR0192 ,{|x|IIf(Pcount() == 0,lCheck1 ,VlCheckBox(1, @lCheck1, @lCheck2, @lCheck3))},oContainer,220,21,,,,,,,,.T.,,,) // "1 – Nenhuma instrução será cadastrada para nenhum dos títulos."
    oCheck2 := TCheckBox():New(080,10,STR0193 ,{|x|IIf(Pcount() == 0,lCheck2 ,VlCheckBox(2, @lCheck1, @lCheck2, @lCheck3))},oContainer,220,21,,,,,,,,.T.,,,) // "2 – A instrução informada será cadastrada para todas os títulos."
    oCheck3 := TCheckBox():New(090,10,STR0194 ,{|x|IIf(Pcount() == 0,lCheck3 ,VlCheckBox(3, @lCheck1, @lCheck2, @lCheck3))},oContainer,220,21,,,,,,,,.T.,,,) // "3 – A tela de instrução de cobrança será apresentar para cada título."
    
    oModal:Activate()

    Aadd(aCheckInst, lCheck1)
    Aadd(aCheckInst, lCheck2)
    Aadd(aCheckInst, lCheck3)

Return aCheckInst

/*/{Protheus.doc} VlCheckBox
	Valida os cliques dando em tela, qdo clicar em uma opção, inverte as outras variações das opções.
	@type  Function
	@author Francisco Oliveira
	@since 25/07/2025
	@version 1.0
	@return Nil
/*/
Static Function VlCheckBox(nClick As Numeric, lCheck1 As Logical, lCheck2 As Logical, lCheck3 As Logical)

    Default nClick  := 1
    Default lCheck1 := .F.
    Default lCheck2 := .F.
    Default lCheck3 := .F.
    
    Do Case
        Case nClick == 1
            lCheck1 := .T.
            lCheck2 := .F.
            lCheck3 := .F.
        Case nClick == 2
            lCheck1 := .F.
            lCheck2 := .T.
            lCheck3 := .F.
        Case nClick == 3
            lCheck1 := .F.
            lCheck2 := .F.
            lCheck3 := .T.
    EndCase

Return

/*/{Protheus.doc} brCanUse
