Function a922ChkInc()
Local lRet			:=	.T.

AHH->(DbSetOrder(1))
SX5->(DbSetOrder(1))
If AHH->(dbSeek(xFilial("AHH")+M->AHH_CODPUB+M->AHH_CODELE))
   Help(" ",1,"ESPEXISTE",,STR0002,1,0) //"Esp้cie da Publica็ใo jแ existe"
   lRet := .F.
ElseIf !(SX5->(DbSeek(xFilial("SX5")+"LN"+M->AHH_CODELE)))
	Help(" ",1,"ELEMENTO",,STR0003,1,0) //"Elemento da Publica็ใo nใo existe"
	lRet := .F.
ElseIf !Empty(M->AHH_CODPAP) .and. !(AHI->(DbSeek(xFilial("AHI")+M->AHH_CODPAP)))
	Help(" ",1,"PAPEL",,STR0004,1,0) //"Tipo de Papel nใo existe"
	lRet := .F.
Endif
Return lRet

