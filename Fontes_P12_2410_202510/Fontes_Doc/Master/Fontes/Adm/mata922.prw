Function a922Alter()
Local nReg	:=AHH->(Recno())
Local nOpc	:=4
Local cAlias:="AHH"
Local nX    :=0
Local aCpos :={}

For nX := 1 To FCount()
	M->&(FieldName(nX)) := CriaVar(FieldName(nX))
    // Campos nao editaveis
	If !(FieldName(nX)) $ "AHH_CODPUB/AHH_CODELE"
		Aadd(aCpos,FieldName(nX))
	EndIf
Next nX

AxAltera( cAlias, nReg, nOpc,,aCpos)
Return(.T.)

