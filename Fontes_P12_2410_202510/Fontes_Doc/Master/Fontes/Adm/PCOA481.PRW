cQuery := "SELECT MAX(AM8_ID) AS ID FROM " + RetSqlName("AM8") + " AM8"
cQuery := ChangeQuery(cQuery)
dbUseArea( .T., "TOPCONN", TcGenQry(,,cQuery), "TMPAM8", .T., .T. )
nRet := If( VALTYPE(TMPAM8->ID)<>"N" , 1 , TMPAM8->ID + 1 )
TMPAM8->(dbCloseArea())

Return nRet


