        //Decrescimo -----------------------------------------------------------------------------------
		If SE1->E1_DECRESC > 0 .And. SE1->E1_SALDO == SE1->E1_VALOR //Se não houver baixa, componho com o decréscimo
			nValue := GetxMoeda(SE1->E1_DECRESC,SE1->E1_MOEDA,1,dDataConv,,If(cPaisLoc=="BRA",SE1->E1_TXMOEDA,0))
