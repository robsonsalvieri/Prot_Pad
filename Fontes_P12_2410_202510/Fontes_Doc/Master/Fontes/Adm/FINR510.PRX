			cHistor := STR0052  //"ACRESC.S/EM.TITULO"
			cDC := "C"    //Abatimentos/RA/NCC
			IF !SE1->E1_TIPO $ (MVABATIM+"/"+MVRECANT+"/"+MV_CRNEG)
    			cDC := "D"
			Endif
            SE1->(InsRec("cNomeArq",nValue,cHistor,cDC))
