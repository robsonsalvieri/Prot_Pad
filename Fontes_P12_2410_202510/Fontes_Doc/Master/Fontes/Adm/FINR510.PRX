			Endif
            SE1->(InsRec("cNomeArq",nValue,cHistor,cDC))
		EndIf

        //Valores Acessórios ---------------------------------------------------------------------------
		If SE1->E1_SALDO == SE1->E1_VALOR
            SE1->(ValidFKD(cIdFK7,dDataConv))
		EndIf
		
		dbSelectArea("SE1")
		SE1->(DBSKIP())
