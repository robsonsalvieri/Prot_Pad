			((cAliasTmp)->E5_TIPO $ MVPAGANT+"/"+MV_CPNEG .AND. (cAliasTmp)->E5_TIPODOC =="ES") .or. ; //Estorno da Baixa de PA
			(( (cAliasTmp)->E5_TIPO $ MVRECANT) .AND. mv_par11 == 2).or. ;
			(!((cAliasTmp)->E5_TIPO $ MVRECANT) .AND. mv_par11 == 3)

			(cAliasTmp)->(dbSkip())
			Loop
