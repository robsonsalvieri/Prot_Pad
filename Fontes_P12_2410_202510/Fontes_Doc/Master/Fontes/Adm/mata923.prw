Local lRet			:=	.T.

AHI->(DbSetOrder(1))
SX5->(DbSetOrder(1))
If AHI->(dbSeek(xFilial("AHI")+M->AHI_CODPAP))
   Help(" ",1,"PAPEXISTE",,STR0002,1,0) //"C๓digo de papel jแ existe"
   lRet := .F.
ElseIf !(SX5->(DbSeek(xFilial("SX5")+"LO"+M->AHI_FORMAT)))
	Help(" ",1,"FORMATO",,STR0003,1,0) //"Formato do papel nใo existe"
	lRet := .F.
ElseIf !(SX5->(DbSeek(xFilial("SX5")+"LR"+M->AHI_UNIMED)))
	Help(" ",1,"UNIMED",,STR0004,1,0) //"Unidade de Medida do papel nใo existe"
	lRet := .F.
Endif
Return lRet


