Function a923Alter()
Local nReg	:=AHI->(Recno())
Local nOpc	:=4
Local cAlias:="AHI"
Local nX    :=0
Local aCpos :={}

For nX := 1 To FCount()
	M->&(FieldName(nX)) := CriaVar(FieldName(nX))
    // Campos nao editaveis
	If !(FieldName(nX)) $ "AHI_CODP	AP"
		Aadd(aCpos,FieldName(nX))
	EndIf
Next nX

AxAltera( cAlias, nReg, nOpc,,aCpos)
Return(.T.)

