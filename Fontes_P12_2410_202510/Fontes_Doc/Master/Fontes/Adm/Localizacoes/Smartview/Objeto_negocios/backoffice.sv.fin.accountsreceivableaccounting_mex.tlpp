@totvsFrameworkTReportsIntegratedProvider( active=.F., team='SIGAFIN', tables='SA2,SEK', name='Recibos de cobro Conciliadas', country='MEX', initialRelease='12.1.2410', customTables='SA2,SEK,CT2' )
class accountsreceivableaccountingTReportsBusinessObjectMEX from custom.financeiro.accountsreceivableaccounting.integratedprovider.accountsreceivableaccountingTReportsBusinessObject
	public method new() as object
	public method preData() as object
	public method processData() as object
	public method preSchema() as object
endclass

method new() class accountsreceivableaccountingTReportsBusinessObjectMEX
	_Super:new()
return self

method preData() as object class accountsreceivableaccountingTReportsBusinessObjectMEX
return self:oData

method processData() as object class accountsreceivableaccountingTReportsBusinessObjectMEX

	Local cQuery as character
	Local oExecMEX as object
	Local nX as numeric
    Local cAliasMEX as character

    aFieldsMEX := { 'EL_UUID','EL_FECTIMB','EL_COMPEN' }
	aStructMEX := getStrutObj( aFieldsMEX, {} )

	cQuery := "SELECT SEL.EL_COMPEN, SEL.EL_UUID, SEL.EL_FECTIMB"
	cQuery += "   FROM " + RetSqlName("SEL") + " SEL"
	cQuery += "   WHERE SEL.R_E_C_N_O_ = ?
	cQuery := ChangeQuery(cQuery)
	oExecMEX := FwExecStatement():New( cQuery )
    oExecMEX:SetString( 01, AllTrim(Str(self:nRecnoSEL)) )
	cAliasMEX := oExecMEX:OpenAlias( )
	For nX := 1 To Len(aStructMEX)
		If ( aStructMEX[nX][3] == 'date' )
			self:jItems[aStructMEX[nX][1]] := totvs.framework.treports.date.dateToTimeStamp( StoD( ( cAliasMEX )->&( aStructMEX[nX][5] ) ) )
		Else
			self:jItems[aStructMEX[nX][1]] := ( cAliasMEX )->&( aStructMEX[nX][5] )
		Endif
	Next

	// fecha tabela
	( cAliasMEX )->( DbCloseArea( ) )
	oExecMEX:Destroy( )
	oExecMEX := Nil

return

method preSchema() as object class accountsreceivableaccountingTReportsBusinessObjectMEX

	Local nX as numeric

	aFieldsMEX := { 'EL_UUID','EL_FECTIMB','EL_COMPEN' }
	aStructMEX := getStrutObj( aFieldsMEX, {} )
	For nX := 1 To Len(aStructMEX)
		self:addProperty( aStructMEX[nX][1], aStructMEX[nX][2], aStructMEX[nX][3], aStructMEX[nX][4], aStructMEX[nX][5] )
	Next

return self:oSchema

