cCliFor := If( cLiq=="0",cFornece,cCliente )
cLjCliFor := cLoja

nOpca := 0
DEFINE MSDIALOG oDlg TITLE cCadastro PIXEL FROM 97,0.5 TO 398,630 OF oMainWnd

