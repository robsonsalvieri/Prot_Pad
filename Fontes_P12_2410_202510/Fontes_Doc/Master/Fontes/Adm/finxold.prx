	If lRet
		While .T.
			cTit  := SE2->E2_PREFIXO+"-"+SE2->E2_NUM+"-"+SE2->E2_PARCELA+"-"+SE2->E2_TIPO
			cForne:= SE2->E2_FORNECE
			cLoja := SE2->E2_LOJA
