	LOCAL aMoedas := { "01-"+GetMv("MV_MOEDA1"),"02-"+GetMv("MV_MOEDA2"),"03-"+GetMv("MV_MOEDA3"),"04-"+GetMv("MV_MOEDA4"),"05-"+GetMv("MV_MOEDA5") }
