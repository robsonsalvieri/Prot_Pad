	Local oDlg := NIL
	Local lDadosDig := .F.

	If lRet .And. SE2->E2_TIPO $ MVPROVIS
		Help(" ",1,"FA590PROV")
		lRet := .F.
	Endif

	If lRet .And. SE2->E2_TIPO $ MVENVBCOR
		Help(" ",1,"F590ENVBCO")
		lRet := .F.
	Endif

	If lRet .and. !Empty(SE2->E2_NUMBOR)
