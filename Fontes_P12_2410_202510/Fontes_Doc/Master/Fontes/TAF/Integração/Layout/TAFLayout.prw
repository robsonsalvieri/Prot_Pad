#INCLUDE "PROTHEUS.CH" 
#INCLUDE "TAFLAYOUT.CH"
/**/
//-------------------------------------------------------------------
/*/{Protheus.doc} TAFLayout

Funcionalidade para criar arquivo de extensão .def, utilizado como
layout de integração com o TAF.

@Param 		Nil

@Return	cFile	-	Diretório e nome referente ao arquivo .def criado

@Author	Felipe C. Seolin
@Since		11/11/2015
@Version	1.0
/*/
//-------------------------------------------------------------------
Function TAFLayout() 

Local cStartPath	as character
Local cBarra		as character
Local cBarraInv		as character
Local cLayout		as character
Local cFile			as character
Local cString		as character
Local nHandle		as numeric
Local nError		as numeric
Local nI			as numeric
Local aLayout		as array
Local lOk			as logical

cStartPath	:=	GetSrvProfString( "Startpath", "" )
cBarra		:=	Iif( IsSrvUnix(), "/", "\" )
cBarraInv	:=	Iif( cBarra == "/", "\", "/" )
cLayout	:=	"LAYOUT_" + cEmpAnt + "_" + AllTrim( Str( ThreadID() ) ) + ".DEF"
cFile		:=	""
cString	:=	""
nHandle	:=	FCreate( cFile )
nError		:=	0
nI			:=	0
aLayout	:=	GetLayout()
lOk			:=	.T.

If SubStr( cStartPath, 1, 1 ) <> cBarra
	If SubStr( cStartPath, 1, 1 ) == cBarraInv
		cStartPath := cBarra + SubStr( cStartPath, 2 )
	Else
		cStartPath := cBarra + cStartPath
	EndIf
EndIf

If SubStr( cStartPath, Len( cStartPath ) ) <> cBarra
	If SubStr( cStartPath, Len( cStartPath ) ) == cBarraInv
		cStartPath := SubStr( cStartPath, 1, Len( cStartPath ) - 1 ) + cBarra
	Else
		cStartPath := cStartPath + cBarra
	EndIf
EndIf

cFile := cStartPath + cLayout

If nHandle < 0

	nI := 1

	While ( nHandle := FCreate( cFile ) ) < 0
		Sleep( 1000 )
		nI += 1
		If nI == 10
			lOk := .F.
			Aviso( STR0001, STR0002 + ": " + cFile + "." + Chr( 13 ) + Chr( 10 ) + STR0003 + ": " + AllTrim( Str( FError() ) ) + Chr( 13 ) + Chr( 10 ) + STR0006 + ": http://tdn.totvs.com/display/tec/FError", { STR0004 }, 3 ) //##"Atenção" ##"Falha na criação do arquivo" ##"Código do erro" ##"Fechar" ##"Mais informações"
			Exit
		EndIf
	EndDo

EndIf

If lOk

	ProcRegua( Len( aLayout ) )

	For nI := 1 to Len( aLayout )

		cString += aLayout[nI] + Chr( 13 ) + Chr( 10 )

		IncProc()

	Next nI

	nError := FWrite( nHandle, cString, Len( cString ) )

	If nError < 0
		Aviso( STR0001, STR0005 + ": " + cFile + "." + Chr( 13 ) + Chr( 10 ) + STR0003 + ": " + AllTrim( Str( FError() ) ) + Chr( 13 ) + Chr( 10 ) + STR0006 + ": http://tdn.totvs.com/display/tec/FError", { STR0004 }, 3 ) //##"Atenção" ##"Falha no processamento do arquivo" ##"Código do erro" ##"Fechar" ##"Mais informações"
	EndIf

	FClose( nHandle )

EndIf

Return( cFile )

