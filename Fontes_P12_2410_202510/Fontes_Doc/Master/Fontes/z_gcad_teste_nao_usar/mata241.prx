	oPanel1:= TSay():New(oSize:GetDimension("CABECALHO","LININI"),oSize:GetDimension("CABECALHO","COLINI"),{||''},oDlg,,,,,,.T.,,,oSize:GetDimension("CABECALHO","XSIZE"),oSize:GetDimension("CABECALHO","YSIZE"))
