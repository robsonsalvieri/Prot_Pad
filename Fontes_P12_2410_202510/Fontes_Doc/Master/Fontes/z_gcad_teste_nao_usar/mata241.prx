Function a241TudoOk(o)
Static lExistMT241OK := NIL
Local  lRetorno      := .T.
Local  zi            := 0
Local  nLinha        := n

If Empty(cTM)
	Help(" ",1,"D3_TM")
	lRetorno := .F.
Else
	If ! A241Check()
		lRetorno := .F.
	Else
		For zi:=1 to Len(aCols)
			If !A241LinOk(nil,zi,.T.)
				lRetorno:=.F.
				Exit
			EndIf
		Next zi
		n := nlinha
		If lRetorno
			If lExistMT241OK == NIL
				lExistMT241OK:=ExistBlock("MT241TOK")
