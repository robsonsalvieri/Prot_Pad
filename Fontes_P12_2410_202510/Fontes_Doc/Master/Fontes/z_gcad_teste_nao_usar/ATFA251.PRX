Local aRatZeroB	 := {}
Local nPosRatZB	 := 0

Local aRatZeroC	 := {}
Local nPosRatZC	 := 0

Local aRatZeroG	 := {}

Local nPosSn3		:= 0
Local cPadAnt		:= ""
Local lPadAnt

//AVP
Local lAvpAtf 		:= AFAvpAtf()
Local aRetBxAVP	    :={}
Local cIdProcAVP	:= ""
Local dDataAvp		:= STOD("")
Local cIndAvp		:= ""
Local nTaxaAvp		:= 0
