	Else
		nOpcA := 1 
	EndIf

	If nOpca == 0 .or. nOpca == 2
		A251Desmar(nIndex)
		While GetSx8Len() > nSaveSx8Len
			RollBackSx8()
		EndDo
	Elseif nOpca == 1
		AF251GRAVA(cBaseDe,cBaseAte,nSaveSx8Len, nIndex,nOpca, aAF251Imp, aAF251Desc,aHeadDadoC,aColsDadoC,aRateio,aRateioOrig)
