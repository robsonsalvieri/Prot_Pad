			ATFXMOV(cFilAnt,@cIDMOV,SN3->N3_AQUISIC,cOcorr,SN3->N3_CBASE,SN3->N3_ITEM,SN3->N3_TIPO,SN3->N3_BAIXA,SN3->N3_SEQ,SN3->N3_SEQREAV,"4",0,cTpSaldo,,aValores,aDadosComp,,,,,lOnOff,cPadrao)

