		 aOrdFields := {"F1_FORNECE", "F1_LOJA", "cNome", "F1_SERIE", "F1_DOC", "F1_NATUREZ", "F1_ESPECIE", "F1_EMISSAO", "F1_DTDIGIT", "F1_MOEDA", "F1_TXMOEDA", "F1_DIACTB", "F1_NUMAUT", "F1_ESTABL", "F1_PTOEMIS", "F1_TIPOPE", "F1_CODCTR"}

   		self:AlterOrder(aOrdFields)
	else
		self:NumAut()
		self:ESTABL()
		self:PtoEmis()
		self:TIPOPE()
		self:CODCTR()
	EndIf

