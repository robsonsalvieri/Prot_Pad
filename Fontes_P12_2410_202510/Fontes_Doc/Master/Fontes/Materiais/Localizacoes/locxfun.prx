Function EstorSX5( cAlias, cSerie, cNumero )
Local aTamRemito := TamSX3("CN_REMITO")
Local cChaveRem  := ""

cAlias  := If(cAlias  == NIL,"SCN",cAlias)
cSerie  := If(cSerie  == NIL,"   ",cSerie)
cNumero := If(cNumero == NIL,"",cNumero)
If cAlias == "SCN"
	aTamRemito := TamSX3("CN_REMITO")
	cChaveRem := "REMSAI"
Else
	aTamRemito := TamSX3("CM_REMITO")
	cChaveRem := "REMENT"
EndIf
DbSelectArea("SX5")
DbGoTop()
If cPaisLoc == "ARG"
	If MsSeek( xFilial("SX5")+"SR"+cSerie,.F. )
		cNumero := StrZero( Val(cNumero)-1, aTamRemito[1] )
		RecLock("SX5",.F.)
		Replace X5_DESCRI  With cNumero
		Replace X5_DESCSPA With cNumero
		Replace X5_DESCENG With cNumero
		MsRUnLock()
		DbCommitAll()
	EndIf
Else
	If MsSeek( xFilial("SX5")+"99"+cChaveRem,.F. )
		If cPaisLoc == "ARG"
			cNumero := StrZero( Val(cNumero)-1, 12 )
		Else
			cNumero := StrZero( Val(cNumero)-1, aTamRemito[1] )
		EndIf
		MsRLock()
		Replace X5_DESCRI  With cNumero
		Replace X5_DESCSPA With cNumero
		Replace X5_DESCENG With cNumero
		MsRUnLock()
	Else
		MsRUnLock()
		Return(.F.)
	EndIf
EndIf
Return
