Function MaFisF3Eic( nCaso )
Local aArea      := GetArea()
Local aArraySF3  := {}
Local cNumHawb   := CriaVar("WN_HAWB")
Local cDespacho  := CriaVar("W6_DI_NUM")
Local aCposBase  := {}
Local nValorCont := 0
Local cCpoValor  := ""
Local nPosTes 	 := Ascan(aHeader,{|x| Alltrim(x[2]) ==  "D1_TES"  } )
Local cTes 		 := ""
Local nI		 :=	0
Local lMAF3EIC   := ExistBlock("MAF3EIC")

If nPosTes > 0
	cTes := aCols[Len(aCols)][nPosTes]
EndIf
SWN->(DbSetOrder(2))
SWN->(DbSeek(xFilial("SWN")+SF1->F1_DOC+SF1->F1_SERIE+SF1->F1_FORNECE+SF1->F1_LOJA))
If SWN->(Found())
	cNumHawb := SWN->WN_HAWB
EndIf
If ! Empty(cNumHawb)
   SW6->(DbSetOrder(1))
   SW6->(DbSeek(xFilial("SW6")+cNumHawb))
   If SW6->(Found())
   		cDespacho := SW6->W6_DI_NUM
   EndIf
EndIf
//Inclusao do SF3 para a NF de despacho com o codigo do Proveedor Original...
If nCaso == 1
	DbSelectArea("SWN")
	DbSetOrder(2)
	aCposBase := {"",""}
	SFB->(DbSetOrder(1))
	If SFB->(DbSeek(xFilial("SFB")+"EDI"))
		aCposBase[1] := SFB->FB_CPOLVRO
	EndIf
	If SFB->(DbSeek(xFilial("SFB")+"ETE"))
		aCposBase[2] := SFB->FB_CPOLVRO
	EndIf
	cAuxPaisL := "ANG|ARG|AUS|BOL|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"
	While !Eof() .and. WN_FILIAL==xFilial("SF1") .and. WN_DOC==SF1->F1_DOC .and. WN_SERIE==SF1->F1_SERIE .And.;
		WN_FORNECE+WN_LOJA==SF1->F1_FORNECE+SF1->F1_LOJA
		//If SWN->WN_DI_NUM == cDespacho
	   		nPosTES := ASCAN(aArraySF3,{ |x| x[7] == cTes })
	   		If nPosTES == 0
	   			nBasImp7 := If(cPaisLoc $ "ANG|ARG|BOL|COL|EQU|HAI|URU",SWN->WN_BASIMP7,0.00)
	   			nValImp7 := If(cPaisLoc $ "ANG|ARG|BOL|COL|EQU|HAI|URU",SWN->WN_VALIMP7,0.00)
	   			nBasImp8 := If(cPaisLoc $ "ANG|ARG|BOL|EQU|HAI|URU",SWN->WN_BASIMP8,0.00)
	   			nValImp8 := If(cPaisLoc $ "ANG|ARG|BOL|EQU|HAI|URU",SWN->WN_VALIMP8,0.00)
	   			nBasImp9 := If(cPaisLoc $ "ARG" ,SWN->WN_BASIMP9,0.00)
	   			nValImp9 := If(cPaisLoc $ "ARG|BOL|URU",SWN->WN_VALIMP9,0.00)
	   			nBasImpI := If(cPaisLoc $ cAuxPaisL , SWN->WN_BASIMPI,0.00)
	   			nValImpI := If(cPaisLoc $ cAuxPaisL , SWN->WN_VALIMPI,0.00)
	   			nBasImpH := If(cPaisLoc $ cAuxPaisL , SWN->WN_BASIMPH,0.00)
	   			nValImpH := If(cPaisLoc $ cAuxPaisL , SWN->WN_VALIMPH,0.00)
	   			nBasImpJ := If(cPaisLoc $ "PAR|URU" , SWN->WN_BASIMPJ,0.00)
	   			nValImpJ := If(SWN->(FieldPos("WN_VALIMPJ"))>0.and. cPaisLoc == "PAR" ,SWN->WN_VALIMPJ,0.00)
	       		If Len(aCposBase) > 0 .and. !Empty(aCposBase[1])
	       			cCpoValor := "SWN->WN_BASIMP"+AllTrim(aCposBase[1])
		       		nValorCont := &(cCpoValor)
		       	EndIf
		       	If Len(aCposBase) > 1 .and. !Empty(aCposBase[2])
		       		cCpoValor := "SWN->WN_BASIMP"+AllTrim(aCposBase[2])
		       		nValorCont := &(cCpoValor)
	            EndIf
	   			AADD(aArraySF3,{"N",SF1->F1_DTDIGIT,Subs(cDespacho,4,12),"A ",SWN->WN_FORNECE,SWN->WN_LOJA,cTes,;
	       				   SF1->F1_EST,SF1->F1_EMISSAO,nValorCont,"N","C",SWN->WN_DOC,"NF ",;
	       				   SWN->WN_BASIMP1,SWN->WN_VALIMP1,SWN->WN_BASIMP2,SWN->WN_VALIMP2,SWN->WN_BASIMP3,;
	       				   SWN->WN_VALIMP3,SWN->WN_BASIMP4,SWN->WN_VALIMP4,SWN->WN_BASIMP5,SWN->WN_VALIMP5,;
	       				   SWN->WN_BASIMP6,SWN->WN_VALIMP6,nBasImp7,nValImp7,;
	       				   nBasImp8,nValImp8,nBasImp9,nValImp9,;
	       				   nBasImpI,nValImpI,nBasImpH,nValImpH,;
	       				   nBasImpJ,nValImpJ})
	    	Else
   	       		aArraySF3[nPosTES][01] := "N"		//Reprocessar Livro
   	      	 	aArraySF3[nPosTES][02] := SF1->F1_DTDIGIT
	       		aArraySF3[nPosTES][03] := Subs(cDespacho,4,12)
	       		aArraySF3[nPosTES][04] := "A"
	       		aArraySF3[nPosTES][05] := SWN->WN_FORNECE
	       		aArraySF3[nPosTES][06] := SWN->WN_LOJA
	       		aArraySF3[nPosTES][07] := cTes
	       		aArraySF3[nPosTES][08] := SF1->F1_EST
	       		aArraySF3[nPosTES][09] := SF1->F1_EMISSAO
	       		If Len(aCposBase) > 0 .and. !Empty(aCposBase[1])
	       			cCpoValor := "SWN->WN_BASIMP"+AllTrim(aCposBase[1])
		       		aArraySF3[nPosTES][10] += &(cCpoValor)
		       	EndIf
		       	If Len(aCposBase) > 0 .and. !Empty(aCposBase[2])
		       		cCpoValor := "SWN->WN_BASIMP"+AllTrim(aCposBase[2])
		       		aArraySF3[nPosTES][10] += &(cCpoValor)
		       	EndIf
	       		aArraySF3[nPosTES][11] := "N"		//Tipo do do NF
   	       		aArraySF3[nPosTES][12] := "C"		//Tipo do Movimento
	       		aArraySF3[nPosTES][13] := SWN->WN_DOC
	       		aArraySF3[nPosTES][14] := "NF "
	       		aArraySF3[nPosTES][15] += SWN->WN_BASIMP1
	       		aArraySF3[nPosTES][16] += SWN->WN_VALIMP1
	       		aArraySF3[nPosTES][17] += SWN->WN_BASIMP2
	       		aArraySF3[nPosTES][18] += SWN->WN_VALIMP2
