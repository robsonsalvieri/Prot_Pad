Function GrvOrigem( cCodigo, cSequenc )
Local aSaveArea := GetArea()
Local cPrefixo  := CriaVar("E1_PREFIXO")
Local cSerie 	:= CriaVar("F1_SERIE")
Local cParcela 	:= CriaVar("E1_PARCELA")
Local cDocto 	:= CriaVar("F1_DOC")
Local cOrigem 	:= ""

cCodigo  := If( cCodigo == NIL,CriaVar("I5_CODIGO"),cCodigo)
cSequenc := If( cSequenc == NIL,CriaVar("I5_SEQUENC"),cSequenc)

If cCodigo $ "500|501|502"
	cOrigem := SE1->E1_PREFIXO+SE1->E1_SERIE+SE1->E1_NUM+SE1->E1_PARCELA
ElseIf cCodigo $ "510|513|514|515|597"
	cOrigem := cPrefixo+SE2->E2_SERIE+SE2->E2_NUM+SE2->E2_PARCELA
ElseIf cCodigo $ "560|561|562|563|564|565"
	cOrigem := cPrefixo+SE5->E5_SERIE+SE5->E5_NUM+SE5->E5_PARCELA
ElseIf cCodigo $ "570|571"
	cOrigem := cPrefixo+cSerie+SEK->EK_NUM+SEK->EK_PARCELA
ElseIf cCodigo $ "575|576"
	cOrigem := cPrefixo+cSerie+SEL->EL_NUM+SEL->EL_PARCELA
ElseIf cCodigo $ "580|581"
	cOrigem := cPrefixo+cSerie+SEH->EH_NUM+SEH->EH_PARCELA
ElseIf cCodigo $ "590|591"
	cOrigem := cPrefixo+SEF->EF_SERIE+SEF->EF_NUM+SEF->EF_PARCELA
ElseIf cCodigo $ "610|620|630|635|640"
	cOrigem := cPrefixo+SF2->F2_SERIE+SF2->F2_DOC+cParcela
ElseIf cCodigo $ "650|655|660|665|950|955"
	cOrigem := cPrefixo+SF1->F1_SERIE+SF1->F1_DOC+cParcela
ElseIf cCodigo $ "901|902|903|904|905|906|907|908"
	cOrigem := cPrefixo+cSerie+SW6->W6_HAWB+cParcela
ElseIf cCodigo < "500"
	cOrigem := cPrefixo+cSerie+cDocto+cParcela
EndIf
cOrigem += "|"+cCodigo+"|"+cSequenc
If ExistBlock("CA050ORI")
	cOrigem := ExecBlock("CA050ORI",.F.,.F.)
EndIf
RestArea( aSaveArea )
Return( cOrigem )
