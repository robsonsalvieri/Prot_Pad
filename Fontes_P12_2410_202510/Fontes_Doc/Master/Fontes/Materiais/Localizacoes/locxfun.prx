		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE INSCRIPT","NDP",cImp,"M","E","I"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE INSCRIPT","NCE",cImp,"M","S","I"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE NO INSCR","NCE",cImp,"M","S","N"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE INSCRIPT","NCP",cImp,"M","S","I"})
