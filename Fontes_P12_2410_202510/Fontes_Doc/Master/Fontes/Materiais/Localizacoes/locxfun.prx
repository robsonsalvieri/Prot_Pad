				MsAguarde({|| dbUseArea(.T.,"TOPCONN",TCGenQry(,,cQuery),cSFY,.F.,.T.)},STR0097)
				DbSelectArea(cSFY)
				lRet:=(cSFY)->NUMCOR==0
				DbCloseArea()
			#ELSE
				lRet:=!MsSeek(xFilial("SFY")+cPeriodo+cCorrelat)
			#ENDIF
		Endif
		If lMsg
			If !lRet
				MsgAlert(cCorrelat+" - "+STR0093) //"Correlativo ja utilizado."
