	cImp := "IBM"
Elseif cProv = "SJ" //San Juan
	cTit := "PERC. I.BRUTOS SAN JUAN  "
	cImp := "IBN"
Elseif cProv = "TF" //Tierra del Fuego
	cTit := "PERC. I.BRUTOS TIERRA FUE"
	cImp := "IBO"
