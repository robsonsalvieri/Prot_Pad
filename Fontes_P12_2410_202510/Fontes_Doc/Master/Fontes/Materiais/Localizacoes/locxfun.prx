±±ÈÍÍÍÍÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¼±±
ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß
*/
Static Function OldTesXIp( aImpVarSD2, nItemValTot, nTotBaseImp, cProduto,nPosCols)
Local cFileRdMk
Local aImposto 	:= {}
Local cNewArea 	:= Alias()
Local nC		:= 0
/*
Array Imposto:
aImpVarSD2[5][n][01] -> C¢digo do Imposto
[n][02] -> Al¡quota do Imposto
[n][03] -> Base de C lculo do Imposto
[n][04] -> Valor Calculado do Imposto
[n][05] -> "___" = Pos.: 1-Inclui no Valor da Duplicata   (S/N)
2-Inclui no Total da Nota Fiscal (S/N)
3-Credita se do imposto para c lculo do Custo (SN)
[n][06] -> Cpo.Grava‡Æo SD2 (Valor Imposto)
[n][07] ->                  (Base de C lculo)
[n][08] -> Cpo.Grava‡Æo SF2 (Valor Imposto)
[n][09] ->                  (Base de C lculo)
[n][10] -> C¢digo dos Impostos Incidentes no C lculo.
[n][11] -> Valor do Frete e Despesas Rateados.
[n][12] -> Valor Calculado do Imposto sobre Valor do Frete e Despesas.
[n][13] -> Valor Calculado do Imposto sobre Despesas.
[n][14] -> Valor Calculado do Imposto sobre Gastos Totales.
[n][15] ->
[n][16] -> Codigo do Produto.
[n][17] -> Campo do Livro Fiscal.
*/
SFB->( DbSetOrder( 1 ) )
If Len( aImpVarSD2 ) == 5
	aadd( aImpVarSD2, {} )
EndIf
DbSelectArea("SFC")
DbSetOrder(1)
DbSeek( xFilial("SFC")+SF4->F4_CODIGO )
While !Eof() .And. FC_FILIAL+FC_TES == xFilial("SFC")+SF4->F4_CODIGO
	SFB->( DbSeek( xFilial("SFB")+SFC->FC_IMPOSTO ) )
	cFileRdMk := Alltrim(SFB->FB_FORMSAI)
	If SFC->FC_CALCULO $ "T"
		aImpVarSD2[1] := 1
		aImpVarSD2[2] := nItemValTot
		aImpVarSD2[3] := nItemValTot
		aImpVarSD2[4] := nFrete
		aImpVarSD2[5] := 0.00
	EndIf
	aImposto  := {}
	AAdd( aImposto, SFB->FB_CODIGO )   // [1] - Cod Imposto
	AAdd( aImposto, SFB->FB_ALIQ )	  // [2] - Aliquota do Imposto - [JLO]
	AAdd( aImposto, 0.00 )             // [3] -
	AAdd( aImposto, 0.00 )             // [4] -
	AAdd( aImposto, SFC->FC_INCDUPL + SFC->FC_INCNOTA + SFC->FC_CREDITA + "S") // [5] -
	AAdd( aImposto, "D2_VALIMP"+SFB->FB_CPOLVRO)  // [6]
	AAdd( aImposto, "D2_BASIMP"+SFB->FB_CPOLVRO)  // [7]
	AAdd( aImposto, "F2_VALIMP"+SFB->FB_CPOLVRO)  // [8]
	AAdd( aImposto, "F2_BASIMP"+SFB->FB_CPOLVRO)  // [9]
	AAdd( aImposto, SFC->FC_INCIMP  )  // [10]-Cod.Imposto sobre o cual Incide.
	AAdd( aImposto, 0.00 )             // [11]
	AAdd( aImposto, 0.00 )             // [12]
	AAdd( aImposto, 0.00 )             // [13]
	AAdd( aImposto, 0.00 )             // [14]
	AAdd( aImposto, SFC->FC_CALCULO )  // [15]
	AAdd( aImposto, cProduto )         // [16]-Cod. Produto
	AAdd( aImposto, SFB->FB_CPOLVRO )  // [17]-Campo do Livro Fiscal.
	AAdd( aImposto, 0  )  				// [18]-Campo do Livro Fiscal.
	AADD( aImposto, {1,-1,0} ) 		//[19] Define se imposto vai somar, subtrair ou indiferente
	AADD( aImposto, SFC->FC_PROV )
	If IsAlpha(Subs(aImposto[5],1,1))
		cConverte	:=	aImposto[5]
		aImposto[5]	:=	IIf(Subs(cConverte,1,2)=="SN".Or.Subs(cConverte,1,2)=="NS","3",;
		IIf(Subs(cConverte,1,2)=="SS","1","2"))
		aImposto[5]	+=	IIf(Subs(cConverte,2,1)=="S" ,"1",IIf(Subs(cConverte,2,1)=="R" ,"2","3"))
		aImposto[5]	+=	IIf(Subs(cConverte,2,2)=="SN","1",IIf(Subs(cConverte,2,2)=="NS","2","3"))
		aImposto[5]	+=	Subs(cConverte,4,1)
	EndIf
	If !Empty( cFileRdMk ) .and. ExistBlock( cFileRdMk,.t. )
		aImposto := ExecBlock( cFileRdMk,.F.,.F., { aImpVarSD2, aImposto,;
			nPosCols,"D2_VALIMP"+SFB->FB_CPOLVRO},.t.)
		aadd( aImpVarSD2[6], AClone( aImposto ) )
	Else
		//Help(" ",1,"NO_IX",,cFileRdMk,1,30)
	EndIf
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Ratear base de calculo e valor do imposto quando  ³
	//³ c lculo for com base no Total das Mercadorias...  ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	If SFC->FC_CALCULO $ "T"
		nVlrImposto := 0
		For nC := 1 To Len( aImpVarSD2[6] )
			If aImpVarSD2[6,nC,1] == SFC->FC_IMPOSTO
				If aImpVarSD2[6,nC,3] > 0.00 .or. SFC->FC_IMPOSTO="IBK"
					aImpVarSD2[6,nC,3] := ( nItemValTot / nTotBaseImp ) * nTotBaseImp
				EndIf
				If aImpVarSD2[6,nC,4] > 0.00 .or. SFC->FC_IMPOSTO="IBK"
					nVlrImposto := aImpVarSD2[6,nC,4]
					aImpVarSD2[6,nC,4] := nVlrImposto
				EndIf
			EndIf
		Next nC
	EndIf
	SFC->( DbSkip() )
End
DbSelectArea(cNewArea)
Return
