Function CORR_PROX( dCorrData )
Local aArea		:= GetArea()
Local nCorrelat := 0
Local cCorrelat := ""
Local cPeriodo  := ""

#IFDEF TOP
	Local cQuery := ""
	Local cSFY	 := "FYTMP"
#ENDIF

DEFAULT dCorrData=dDataBase

If CORR_USO()
	DbSelectArea("SFY")
	DbSetOrder(1)
	If Upper(SuperGetMV("MV_CTRLCOR",,"M"))=="M"
		cPeriodo:=Left(Dtos(dCorrData),6)
	Else
		cPeriodo:=Left(Dtos(dCorrData),4)+"00"
	Endif
	#IFDEF TOP
		cQuery:="select MAX(FY_NUMCOR) as NUMCOR from "+RetSqlName("SFY")+" where FY_FILIAL='"+xFilial("SFY")+"'"
		cQuery+=" and FY_PERIODO='"+cPeriodo+"' and D_E_L_E_T_=' '"
		cQuery:=ChangeQuery(cQuery)
		MsAguarde({|| dbUseArea(.T.,"TOPCONN",TCGenQry(,,cQuery),cSFY,.F.,.T.)},STR0097) //"Verificando o numero correlativo"
		DbSelectArea(cSFY)
		nCorrelat:=Val((cSFY)->NUMCOR)
		DbCloseArea()
	#ELSE
		cCorrelat:=Padl(AllTrim(cCorrelat),TamSX3("FY_NUMCOR")[1],"z")
		MsSeek(xFilial("SFY")+cPeriodo+cCorrelat,.T.)
		DbSkip(-1)
		If FY_PERIODO==cPeriodo
			nCorrelat:=Val(FY_NUMCOR)
		Endif
	#ENDIF
	nCorrelat++
	cCorrelat:=Padl(AllTrim(Str(nCorrelat)),TamSX3("FY_NUMCOR")[1],"0")
Endif
RestArea(aArea)
Return(cCorrelat)
