Function CriaSES()
Local aSaveArea := GetArea()
Local nI 		:= 0
Local cFilSES 	:= xFilial("SES")
Local aRecnoSES := {}
// Registros exclusivos avaliados pela Rotina de Recibo...
AAdd(aRecnoSES,{cFilSES,"TF ","3","2","2","1","DEPOSITO            ","2","1","+","TF ","1"})
AAdd(aRecnoSES,{cFilSES,"CC ","1","2","1","1","TARJETA DE CREDITO  "," ","1","+","CC ","1"})
AAdd(aRecnoSES,{cFilSES,"CD ","1","2","1","1","TARJETA DE DEBITO   "," ","1","+","CD ","1"})
AAdd(aRecnoSES,{cFilSES,"CH ","3","2","1","1","CHEQUES             ","1","1","+","CH ","1"})
AAdd(aRecnoSES,{cFilSES,"DP ","3","2","1","1","TITULO              "," ","1","+","DP ","1"})
If cPaisLoc == "CHI"
	AAdd(aRecnoSES,{cFilSES,"LT ","3","2","1","1","LETRA                 ","1","1","+","LT ","1"})
	AAdd(aRecnoSES,{cFilSES,"SE ","1","2","1","1","SEGUROS               ","1","1","+","SE ","1"})
	AAdd(aRecnoSES,{cFilSES,"EFE","3","2","2","1","EFECTIVO              ","2","1","+","EF ","1"})
Else
   If cPaisLoc == "ARG"
      AAdd(aRecnoSES,{cFilSES,"RG ","2","2","1","1","RETENCIONES           ","1","1","+","RG ","1"})
      AAdd(aRecnoSES,{cFilSES,"CT ","2","2","2","1","CHEQUES TERCEIROS   ","1","1","+","CT ","1"})
      AAdd(aRecnoSES,{cFilSES,"CP ","2","2","2","1","CHEQUES PROPRIOS    ","1","1","+","CP ","1"})
   EndIf
   AAdd(aRecnoSES,{cFilSES,"EF ","3","2","2","1","EFECTIVO              ","2","1","+","EF ","1"})
EndIf
// Registros exclusivos avaliados pela Rotina de Ordem de Pago
AAdd(aRecnoSES,{cFilSES,"NF ","3","2","1","1","FACTURAS            "," ","1","+","NF ","1"})
AAdd(aRecnoSES,{cFilSES,"NCC","1","2","1","1","NOTA CREDITO CLIENTE"," ","1","-","NCC","1"})
AAdd(aRecnoSES,{cFilSES,"NDC","1","2","1","1","NOTA DEBITO CLIENTE "," ","1","+","NDC","1"})
AAdd(aRecnoSES,{cFilSES,"NCP","2","2","1","1","NOTA CREDITO PROVEED"," ","1","-","NCP","1"})
AAdd(aRecnoSES,{cFilSES,"NDP","2","2","1","1","NOTA DEBITO PROVEED."," ","1","+","NDP","1"})
If SES->(RecCount()) > 0
	aRecnoSES := {}
EndIf
For nI := 1 To Len(aRecnoSES)
	DbSelectArea("SES")
	DbSetOrder(1)
	If !DbSeek(xFilial("SES")+aRecnoSES[nI][2]+aRecnoSES[nI][3])
		RecLock("SES",.T.)
		Replace ES_FILIAL	With aRecnoSES[nI][01]
		Replace ES_TIPO		With aRecnoSES[nI][02]
		Replace ES_CARTEIR	With aRecnoSES[nI][03]
		Replace ES_ABATIM	With aRecnoSES[nI][04]
		Replace ES_BXRCOP	With aRecnoSES[nI][05]
		Replace ES_CTACTE	With aRecnoSES[nI][06]
		Replace ES_DESC  	With aRecnoSES[nI][07]
		Replace ES_RCOPGER	With aRecnoSES[nI][08]
		Replace ES_SALDUP	With aRecnoSES[nI][09]
		Replace ES_SINAL   	With aRecnoSES[nI][10]
		Replace ES_TIPORIG	With aRecnoSES[nI][11]
		Replace ES_TRANSFE	With aRecnoSES[nI][12]
		MsUnLock()
	EndIf
Next nI
RestArea( aSaveArea )
Return
