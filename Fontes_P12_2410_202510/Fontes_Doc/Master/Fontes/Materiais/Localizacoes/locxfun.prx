±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´±±
±±³Uso       ³ Portugal                                                   ³±±
±±ÀÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ±±
*/
Function NIF(cNumNif,lShowHelp)
Local cNumNifAux  := ""
Local nInd
Local lValido 	  :=.F.
Local nDigito     :=0
Local cCodMun     := ""
Local cVar        := Subs(ReadVar(),4,10)

Default cNumNif   := Iif ("CGC" $ ReadVar(), &(ReadVar()),"")
Default lShowHelp :=.T.

SX3->( dbSetOrder(2) )
SX3->( dbSeek(cVar) )
If SX3->( Found() )
   cAliasCur := SX3->X3_ARQUIVO
   If cAliasCur $ "SA1"  // Clientes
		cCodMun := M->A1_COD_MUN
   ElseIf cAliasCur $ "SA2"  // Clientes
		cCodMun := M->A2_COD_MUN
   ElseIf cAliasCur $ "SA3"  // Clientes
		cCodMun := M->A3_CODMUN
   ElseIf cAliasCur $ "SA4"  // Clientes
		cCodMun := M->A4_COD_MUN
   EndIf
EndIf

If !Empty(cNumNif) .And. !Empty(cCodMun) .And. ( cCodMun <> "9999" .And. cCodMun <> "9800" )
	For nInd:=1 To Len(cNumNIf)
	  If IsDigit(Substr(cNumNIf,nInd,1))//Somente Numeros
	    cNumNifAux+=Substr(cNumNIf,nInd,1)
	  EndIf
	Next

	cNumNif:=cNumNifAux

	If Len(cNumNif)==9

	  For nInd:=0 To 7
	    nDigito+=Val(Substr(cNumNIf,nInd+1,1))*(10 - (nInd + 1))
	  Next

	  nDigito:=11-(nDigito%11)

	  If nDigito>9
	    nDigito:=0
	  EndIf

	  lValido:=AllTrim( Str(nDigito))==Substr(cNumNif,9,1)

	EndIf

	If lShowHelp .And. !lValido
	  Help(OemToAnsi(STR0104),1,OemToAnsi(STR0105),,OemToAnsi(STR0106),1,0)
	EndIf
Else
	lValido:=.T.
EndIf

Return lValido
/*
ÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜ
±±ÉÍÍÍÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍ»±±
±±ºPrograma  ³PtgAliq   ºAutor  ³Mary C. Hergert     º Data ³  27/05/2008 º±±
±±ÌÍÍÍÍÍÍÍÍÍÍØÍÍÍÍÍÍÍÍÍÍÊÍÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÊÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍ¹±±
±±ºDesc.     ³Retorna o tipo de aliquota IVA utilizada nos documentos     º±±
±±º          ³de entrada e saida                                          º±±
±±ÌÍÍÍÍÍÍÍÍÍÍØÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¹±±
±±ºParametros³nExp01: Aliquota da operacao                                º±±
±±ÌÍÍÍÍÍÍÍÍÍÍØÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¹±±
±±ºRetorno   ³aExp01: Array com [01]: tipo da aliquota                    º±±
±±º          ³                        1 = Reduzida                        º±±
±±º          ³                        2 = Intermedia                      º±±
±±º          ³                        3 = Normal                          º±±
±±º          ³                        9 = Outras                          º±±
±±º          ³                  [02]: regiao para uso da aliquota         º±±
±±º          ³                        1 = Continente                      º±±
±±º          ³                        2 = Madeira                         º±±
±±º          ³                        3 = Acores                          º±±
±±º          ³                        4 = Intracomunitaria                º±±
±±º          ³                        5 = Extracomunitaria                º±±
±±º          ³                        9 = Outras                          º±±
±±ÌÍÍÍÍÍÍÍÍÍÍØÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¹±±
±±ºUso       ³SigaFis                                                     º±±
±±ÈÍÍÍÍÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¼±±
ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß
*/
Function PtgAliq(nAliq,cGrpTrib)
Local aRetorno	:= {"",""}
Local aAliq 	:= {GetNewPar("MV_CMP21","4,5"),GetNewPar("MV_CMP23","8,12"),GetNewPar("MV_CMP22","15,21")}
Local cRegiao	:= GetNewPar("MV_REGTRIB","CON=001;MAD=002;ACO=003;INT=004;EXT=005")
Local cAliq		:= Alltrim(Str(nAliq))
Local cCodReg	:= ""
Local nX        := 0
Local nPos		:= 0
Default cGrpTrib := ""
//Verifica se a aliquota eh reduzida, normal ou intermedia
For nX := 1 To Len(aAliq)
	If At(cAliq,aAliq[nX]) > 0
		aRetorno[01] := Alltrim(Str(nX))
		Exit
	Endif
Next
//Se nao for nenhum dos tipos eh outras
If Empty(aRetorno[01])
	aRetorno[01] := "9"
Endif
//Verifica se a aliquota eh para Continente, Acores, Madeira ou Intracomunitaria, de acordo com a regiao
nPos := At(cGrpTrib,cRegiao)
cCodReg := SubStr(cRegiao,nPos-4,3)
Do Case
Case cCodReg == "CON"
	aRetorno[02] := "1"
Case cCodReg == "MAD"
	aRetorno[02] := "2"
Case cCodReg == "ACO"
	aRetorno[02] := "3"
Case cCodReg == "INT"
	aRetorno[02] := "4"
Case cCodReg == "EXT"
	aRetorno[02] := "5"
OtherWise
	aRetorno[02] := "9"
EndCase
Return aRetorno
