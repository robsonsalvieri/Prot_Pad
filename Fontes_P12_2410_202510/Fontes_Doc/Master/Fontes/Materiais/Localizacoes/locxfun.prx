/*/

Function CriaSF4()

Local aAreaOld   := GetArea()                         // Area atual
Local aRecnoSF4  := {}                                // Dados dos TES
Local aRecSF4EIC := {}                                // Dados dos TES do SIGAEIC
Local nI         := 0                                 // Controle de loop
Local cFilSF4    := xFilial("SF4")                    // Filial do arquivo SF4
Local lIntEIC    := ( SuperGetMV("MV_EASY") == "S" ) // Controla se tem integracao com SIGAEIC
Local lAchouFil  :=.T.
Local aAreaSF4   := {}

If nModulo <> 23
	If cPaisLoc == "ARG"
		SF4ARG( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "CHI"
		SF4CHI( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "EUA"
		SF4EUA( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "MEX"
		SF4MEX( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "PAR"
		SF4PAR( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "POR"
		SF4POR( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "COL"
		SF4COL( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "URU"
		SF4URU( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "VEN"
		SF4VEN( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "DOM"
		SF4DOM( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "PER"
		SF4PER( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "COS"
		SF4COS( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "SAL"
		SF4SAL( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "GUA"
		SF4GUA( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "BOL"
		SF4BOL( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "PTG"
		SF4PTG( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "ANG"
		SF4ANG( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "EQU"
		SF4EQU( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	ElseIf cPaisLoc == "AUS"
		SF4AUS( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "RUS"
		SF4RUS( @aRecnoSF4 , cFilSF4 , lIntEIC , @aRecSF4EIC )
	EndIf
	//Se o arquivo estiver vazio faz a carga com todos os registros, senao apenas com
	//aqueles da integracao com o SIGAEIC
	DbSelectArea("SF4")
	lAchouFil:=.T.
	aAreaSF4:=SF4->(GetArea())
	If !SF4->(DbSeek(xFilial("SF4")))
		lAchouFil:=.F.
	EndIf
    SF4->(RestArea(aAreaSF4))


	If SF4->(RecCount()) > 0 .And. lAchouFil
		aRecnoSF4 := {}
	EndIf
	For nI := 1 to Len(aRecSF4EIC)
		AADD(aRecnoSF4,aRecSF4EIC[nI])
	Next nI
	If Len(aRecnoSF4) > 0
		DbSelectArea("SF4")
		DbSetOrder(1)
		For nI := 1 to Len(aRecnoSF4)
			If !DbSeek(aRecnoSF4[nI][1]+aRecnoSF4[nI][2])
				RecLock("SF4",.T.)
				Replace F4_FILIAL  With aRecnoSF4[nI][01]
				Replace F4_CODIGO  With aRecnoSF4[nI][02]
				Replace F4_TIPO    With aRecnoSF4[nI][03]
				Replace F4_IPI     With aRecnoSF4[nI][04]
				Replace F4_CREDIPI With aRecnoSF4[nI][05]
				Replace F4_DUPLIC  With aRecnoSF4[nI][06]
				Replace F4_ESTOQUE With aRecnoSF4[nI][07]
				Replace F4_CF      With aRecnoSF4[nI][08]
				Replace F4_TEXTO   With aRecnoSF4[nI][09]
				Replace F4_BASEICM With aRecnoSF4[nI][10]
				Replace F4_BASEIPI With aRecnoSF4[nI][11]
				Replace F4_PODER3  With aRecnoSF4[nI][12]
				Replace F4_CIAP    With aRecnoSF4[nI][13]
				Replace F4_LIVRO   With aRecnoSF4[nI][14]
				Replace F4_ATUTEC  With aRecnoSF4[nI][15]
				Replace F4_ATUATF  With aRecnoSF4[nI][16]
				Replace F4_TPIPI   With aRecnoSF4[nI][17]
				Replace F4_DESTACA   With aRecnoSF4[nI][18]
				Replace F4_ICM     With aRecnoSF4[nI][19]
				Replace F4_CREDICM With aRecnoSF4[nI][20]
				Replace F4_LFICM   With aRecnoSF4[nI][21]
				Replace F4_GERALF  With aRecnoSF4[nI][23]
				If !Empty(aRecnoSF4[nI][24])
					Replace F4_TESDV   With aRecnoSF4[nI][24]
				EndIf
				Replace F4_QTDZERO With aRecnoSF4[nI][25]
				If Len(aRecnoSF4[nI]) > 25 .And. FieldPos("F4_TESNCC") > 0
					Replace F4_TESNCC   With aRecnoSF4[nI][26]
				EndIf
				If Len(aRecnoSF4[nI]) > 26 .And. cPaisLoc $ "ANG|EQU|HAI"
					Replace F4_PERCEMP   With aRecnoSF4[nI][27]
				EndIf
				If Len(aRecnoSF4[nI]) > 27  .And. cPaisLoc == "COL"
					Replace F4_PERCIVA   With aRecnoSF4[nI][28]
				EndIf
				If cPaisLoc=="VEN|COL|EQU|URU" .And. Len(aRecnoSF4[nI]) > 28
					Replace F4_CALCIVA   With aRecnoSF4[nI][29]
				EndIf
				If cPaisLoc=="VEN" .And. Len(aRecnoSF4[nI]) > 29
					Replace F4_TPALIQ   With aRecnoSF4[nI][30]
				EndIf
				MsUnLock()
			EndIf
		Next nI
	Endif
Endif
RestArea( aAreaOld )

Return (.T.)
