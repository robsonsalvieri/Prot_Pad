/*/
Function AtuaTroco( nValor	, nMoedaTrc	, nOpcao	, cCliente	,;
                    cLoja	, aTitulo	, cNumCaixa	, cOrigem   ,;
                    cLjPref	)

Local aAreaIni   := GetArea()
Local aAreaSL4   := SL4->(GetArea())
Local nIndSE5    := 0
Local nRegSE5    := 0
Local nI         := 0
Local nMoeda     := 1
Local nValAux    := 0
Local nPerRat    := 0
Local nValTrc    := 0
Local nValTot    := 0
Local nDecs      := MsDecimais(nMoedaTrc)
Local nPosForma  := 0
Local nValDif    := 0
Local nX
Local aValSE5    := {},aSL4:={}
Local cDup       := SuperGetMV("MV_1DUP")
Local cFormaPgto
Local cNatTroc   := IIf( cPaisLoc <> "BRA", SuperGetMV("MV_NATTROC"), GetNewPar("MV_NATTROC",'"TROCO"') )
Local dSE5data   := dDatabase
Local cBanco     := ""
Local cPrefixo   := ""
Local cNumero    := ""
Local cParcela   := ""
Local cSeqBx	 := StrZero(1,TamSX3("E5_SEQ")[1])
Local lUsaNewKey := TamSX3("F1_SERIE")[1] == 14
Local nLastPos	 := 0
Local cFilOrig   := cFilAnt
Local cSimbMoeda     := SuperGetMV("MV_SIMB"+AllTrim(Str(nMoedaTrc)))

DEFAULT nOpcao   := 1
DEFAULT cCliente := ""
DEFAULT cLoja	 := ""
DEFAULT aTitulo  := {}
DEFAULT cNumCaixa:= ""
DEFAULT cOrigem  := Space(TamSX3("E5_ORIGEM")[1])
DEFAULT cLjPref	 := ""

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³Os dados do SE5 de troco gravados abaixo (Numero, parcela e prefixo) devem ser   ³
//³identicos ao dos titulos a receber que se referem. Isso permite que, com as      ³
//³informacoes de um titulo a receber, e possivel verificar se houve troco para o   ³
//³mesmo                                                                            ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
If nOpcao == 1
	dSE5Data := SL1->L1_EMISNF
	cCliente := SL1->L1_CLIENTE
	cLoja    := SL1->L1_LOJA
    cBanco   := SL1->L1_OPERADO
    If lUsaNewKey
    	cPrefixo := IIf( Empty(subStr(cLjPref,1,3)), SL1->L1_SDOC, subStr(cLjPref,1,3) )
    Else
    	cPrefixo := IIf( Empty(cLjPref), SL1->L1_SERIE, cLjPref )
    Endif
    cNumero  := SL1->L1_DOC
ElseIf nOpcao == 2 .Or. nOpcao == 3
	dSE5data := dDatabase
	cCliente := cCliente
	cLoja    := cLoja
	cBanco   := cNumCaixa
	cPrefixo := SE1->E1_PREFIXO
	cNumero  := SE1->E1_NUM
	cParcela := SE1->E1_PARCELA
EndIf
If nOpcao <> 3
	cFilOrig := SL1->L1_FILIAL

	DbSelectArea("SL4")
	DbSetOrder(1)
	DbSeek(SL1->L1_FILIAL+SL1->L1_NUM)
	While !Eof() .and. (SL4->L4_NUM == SL1->L1_NUM)
		nMoeda  := Max(L4_MOEDA,1)
		nValTot += xMoeda(L4_VALOR,nMoeda,nMoedaTrc,dDataBase,nDecs+1)
	 	AAdd(aSL4,{Round(xMoeda(L4_VALOR,nMoeda,nMoedaTrc,dDataBase,nDecs+1),nDecs),Trim(SL4->L4_FORMA),cPrefixo,cNumero,cParcela,cSeqBx, cFilOrig})
		DbSkip()
	End
Else
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³Tratamento para troco para recebimento de titulos³
	//³Simula o array aSL4								³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
    For nX := 1 to Len(aTitulo)
        If aTitulo[nX,1]
        dbSelectArea("SE1")
		   dbSetOrder(2)
		   If dbSeek(xFilial("SE1")+aTitulo[nX][13]+aTitulo[nX][14]+aTitulo[nX][02]+aTitulo[nX][03]+aTitulo[nX][04]+aTitulo[nX][11])
           		cSeqBx := FaNxtSeqBx("SE1")
           		cSeqBx := StrZero(Val(cSeqBx)-1,TamSX3("E5_SEQ")[1])
           		cFilOrig := SE1->E1_FILORIG
           Else
           		cSeqBx := StrZero(1,TamSX3("E5_SEQ")[1])
           		cFilOrig := cFilAnt
	       EndIf
	       nMoeda  := Max(nMoedaTrc,1)
		   nValTot += xMoeda(aTitulo[nX,10],nMoeda,nMoedaTrc,dDataBase,nDecs+1)
	 	   AAdd(aSL4,{Round(xMoeda(aTitulo[nX,10], nMoeda, nMoedaTrc, dDataBase, nDecs+1),nDecs), Trim(aTitulo[nX,11]), aTitulo[nX,2], aTitulo[nX,3], aTitulo[nX,4], cSeqBx, cFilOrig})
 	    EndIf
    Next nX

    //Tratamento para alimentar sequencia do SE5 caso array aTitulo nao seja enviado
    If Len(aTitulo) == 0
    	cSeqBx := FaNxtSeqBx("SE1")
    	cSeqBx := StrZero(Val(cSeqBx)-1,TamSX3("E5_SEQ")[1])
    	cFilOrig := SE1->E1_FILORIG
    EndIf
EndIf
nValTot := Round(nValTot,nDecs)
If nValor > 0
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³No caso de pagamento com NCC, nao tem registro no SL4 ,³
	//³vai gravar no SE5 de acordo com o gravado no L1_TROCOn ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
    If Len(aSL4) == 0
		AAdd(aValSE5,{nValor , cSimbMoeda , cPrefixo , cNumero , cParcela , cSeqBx, cFilOrig})
	Else
    	// Grava um unico registro para o troco na venda
		nLastPos := Len(aSL4)
		AAdd(aValSE5,{nValor,aSL4[nLastPos][2],aSL4[nLastPos][3],aSL4[nLastPos][4],aSL4[nLastPos][5],aSL4[nLastPos][6],aSL4[nLastPos][7]})
    EndIf
	For nI := 1 To Len(aValSE5)
		DbSelectArea("SE5")
		nIndSE5 := IndexOrd()
		nRegSE5 := RecNo()
		cFormaPgto := aValSE5[nI][2]
		nPosForma  := 0
		For nX := 1 To Len(aSL4)
		   If (aSL4[nX][2] == cFormaPgto .Or. IIf(IsMoney(cFormaPgto),IsMoney(aSL4[nX][2]),.F.));
		      .And. nX <= nI
		      nPosForma++      //nPosForma contem a qtde de parcelas da forma de pagto a ser gravada
		   EndIf               //Usada na geracao da parcela(E5_PARCELA) do movimento banc.
		Next nX
		Reclock("SE5",.T.)
		Replace E5_FILIAL    With xFilial("SE5"),;
	    		E5_VALOR	 With aValSE5[nI][1],;
				E5_MOEDA	 With "TC",;
				E5_NATUREZ	 With &(cNatTroc),;
				E5_BANCO	 With cBanco
		IF nMoedaTrc > 1
	   		Replace E5_AGENCIA With cSimbMoeda
		Else
	   		Replace E5_AGENCIA With "."
		EndIf

		Replace E5_ORIGEM   With cOrigem

		Replace E5_CONTA	With ".",;
	    		E5_DATA	    With dSE5Data,;
				E5_DTDIGIT  With dSE5Data,;
				E5_DTDISPO  With dSE5Data,;
				E5_PREFIXO  With aValSE5[nI][3],;
				E5_NUMERO   With aValSE5[nI][4],;
				E5_CLIFOR   With cCliente,;
				E5_LOJA	    With cLoja,;
				E5_RECPAG   With "P",;
				E5_HISTOR   With STR0028,; //"Registro de Troco"
				E5_TIPODOC  With "VL",;
				E5_SEQ      With aValSE5[nI][6],;
				E5_MOTBX    With "NOR",;
				E5_VLMOED2	With Round(xMoeda(E5_VALOR,nMoedaTrc,1,E5_DATA,MsDecimais(1)+1),MsDecimais(1))
		If nOpcao == 3
		 	Replace E5_PARCELA  With aValSE5[nI][5]
		ElseIf ( !IsMoney(cFormaPgto) .AND. ( nPosForma > 0 ) ) .OR. ( cPaisLoc <> "BRA" .AND. IsMoney(cFormaPgto) .AND. nPosForma > 0 .AND. nOpcao == 1 )
		   Replace E5_PARCELA  With LJParcela(nPosForma,cDup)
		ElseIf nOpcao == 2
		 	Replace E5_PARCELA  With aValSE5[nI][5]
		EndIf
		Replace E5_FILORIG With aValSE5[nI][7]
		MsUnlock()
		dbCommit()
	Next nI
	AtuSalBco(SE5->E5_BANCO,SE5->E5_AGENCIA,SE5->E5_CONTA,SE5->E5_DATA,nValTrc,"-")
	SA6->(DbSetOrder(1))
	//Atualiza a moeda do novo Caixa criado devido ao tratamento de multi-moeda
	If nMoedaTrc > 1 .And. SA6->(DbSeek(xFilial("SA6")+SE5->E5_BANCO+SE5->E5_AGENCIA+SE5->E5_CONTA))
	   RecLock("SA6",.F.)
	   SA6->A6_MOEDA  := nMoedaTrc
	   MsUnlock()
	EndIf
	DbSelectArea("SE5")
	DbSetOrder(nIndSE5)
	MsGoTo(nRegSE5)
EndIf
RestArea(aAreaSL4)
RestArea(aAreaIni)
Return .T.
