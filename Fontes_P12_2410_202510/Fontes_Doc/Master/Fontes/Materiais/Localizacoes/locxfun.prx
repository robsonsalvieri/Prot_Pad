±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´±±
±±³ Uso      ³LOJA920                                                     ³±±

*/
Function A920WHeader( nOpcx, lTotais, lImpostos, lLivros, aLivro )
Local cAlias 	:= Alias()
Local nI		:= 0
Local nPos		:= 0
Local nAliq		:= 0.00
Local nCpos		:= 0
Local nC 		:= 0
Local aCposTG 	:= {}
Local aCposIP 	:= {}
Local aCposLF 	:= {}

If lTotais
	aHeadTotais:={}
	aFieldTotais:={}
	aCpos := {"F2_VALMERC","F2_FRETE","F2_DESPESA","F2_DESCONT","F2_VALBRUT"}
ElseIf lImpostos
	aHeadImpostos:={}
	aFieldImpostos:={}
	aCpos := {"FB_CODIGO","FB_DESCR","ALIQUOTA","VALORBASE","VALORIMP"}
ElseIf lLivros
	aHeadLivros:={}
	aFieldLivros:={}
	aColsLbx3:={}
	aCpos := A100CposLivro()
EndIf
//Desconsiderar o Cpo F3_FILIAL qdo o tratamento for do Livro
If lLivros
	nPos := Ascan(aCpos,"F3_FILIAL")
	If nPos > 0
		ADel(aCpos,nPos)
		ASize(aCpos,Len(aCpos)-1)
	EndIf
EndIf
//Monta o aHeader para utiliza‡Æo no Browse.
DbSelectArea("Sx3")
DbSetOrder(2)
If lTotais
	nCpos:=0
	For nI := 1 To Len(aCpos)
		If DbSeek(aCpos[nI])
			If x3uso(x3_usado) .AND. cNivel >= x3_nivel
				nCpos++
				AADD(aHeadTotais,{ TRIM(X3Titulo()), x3_campo, AllTrim(x3_picture),;
				x3_tamanho, x3_decimal, AllTrim(x3_valid),;
				x3_usado, x3_tipo, x3_arquivo, x3_context} )
				AADD(aFieldTotais,TRIM(X3Titulo()))
			EndIf
		EndIf
	Next nI
	aColsTotais := Array(1,nCpos)
EndIf
If lImpostos
	nCpos:=0
	For nI := 1 To Len(aCpos)
		If aCpos[nI] == "ALIQUOTA"
			nCpos ++
			AADD(aHeadImpostos,{ STR0001, "ALIQUOTA", "@E 999.99",7,2,"AlwaysFalse()","S","N","   ","N"})  //"Al¡quota"
			AADD(aFieldImpostos, STR0001)                                                                    //"Al¡quota"
		ElseIf aCpos[nI] == "VALORBASE"
			nCpos ++
			AADD(aHeadImpostos,{ STR0002, "VALORBASE", "@E 9,999,999,999.99",16,2,"AlwaysFalse()","S","N","   ","N"}) //"Valor Base"
			AADD(aFieldImpostos, STR0002)                                                                                //"Valor Base"
		ElseIf aCpos[nI] == "VALORIMP"
			nCpos ++
			AADD(aHeadImpostos,{ STR0003, "VALORIMP", "@E 9,999,999,999.99",16,2,"A100ValImp(o)","S","N","   ","N"})  //"Imposto"
			AADD(aFieldImpostos, STR0003)                                                                               //"Imposto"
		Else
			If DbSeek(aCpos[nI])
				If x3uso(x3_usado) .AND. cNivel >= x3_nivel
					nCpos ++
					AADD(aHeadImpostos,{ TRIM(X3Titulo()), x3_campo, x3_picture,;
					x3_tamanho, x3_decimal, x3_valid,;
					x3_usado, x3_tipo, x3_arquivo, x3_context} )
					AADD(aFieldImpostos, TRIM(X3Titulo()))
				EndIf
			EndIf
		EndIf
	Next nI
	If Len(aImpVarSF2) == 0
		aColsImpostos := Array(1,nCpos)
	Else
		aColsImpostos := Array(Len(aImpVarSF2)/2,nCpos)
	EndIf
EndIf
If lLivros
	nCpos:=0
	For nI := 1 To Len(aCpos)
		If DbSeek(aCpos[nI])
			If x3uso(x3_usado) .AND. cNivel >= x3_nivel
				nCpos++
				AADD(aHeadLivros,{ TRIM(X3Titulo()), x3_campo, PesqPict(x3_arquivo,x3_campo,,1),;
				x3_tamanho, x3_decimal, x3_valid,;
				x3_usado, x3_tipo, x3_arquivo, x3_context } )
				AADD(aFieldLivros, TRIM(X3Titulo()))
				AADD(aColsLbx3,(x3_tamanho+x3_decimal)*12)
			EndIf
		EndIf
	Next nI
	If Len(aLivro) > 2
		aColsLivros := Array(Len(aLivro)-1,nCpos)
	Else
		aColsLivros := Array(1,nCpos)
	EndIf
EndIf
//Monta o aCols para utiliza‡Æo no Browse dentro do Folder.
nCnt  := 0
nUsado:= 0
If lTotais
	For nI := 1 To Len(aColsTotais)
		nCnt++
		For nC := 1 To nCpos
			nUsado++
			Do Case
				Case AllTrim(aHeadTotais[nC][2]) ==	"F2_VALMERC"
					aColsTotais[nCnt][nUsado] := IIf(nOpcx==3.or.nOpcx==4,nTotMerc,SF2->F2_VALMERC)
				Case AllTrim(aHeadTotais[nC][2]) == "F2_FRETE"
					aColsTotais[nCnt][nUsado] := IIf(nOpcx==3.or.nOpcx==4,nValFrete,SF2->F2_FRETE)
				Case AllTrim(aHeadTotais[nC][2]) == "F2_DESPESA"
					aColsTotais[nCnt][nUsado] := IIf(nOpcx==3.or.nOpcx==4,nValDesp,SF2->F2_DESPESA)
				Case AllTrim(aHeadTotais[nC][2]) == "F2_DESCONT"
					aColsTotais[nCnt][nUsado] := IIf(nOpcx==3.or.nOpcx==4,nValDesc,SF2->F2_DESCONT)
				Case AllTrim(aHeadTotais[nC][2]) == "F2_VALBRUT"
					aColsTotais[nCnt][nUsado] := IIf(nOpcx==3.or.nOpcx==4,nTotNot,SF2->F2_VALBRUT)
			EndCase
		Next nC
	Next nI
EndIf
If lImpostos
	nUsado:=5
	For nI := 1 To Len(aImpVarSF2) Step 2
		nCnt++
		SFB->( DbSeek(xFilial("SFB")+AllTrim(aImpVarSF2[nI][1])) )
		nPos := Ascan(aColsImpostos,{|x| x[1] == aImpVarSF2[nI][1]})
		If nPos <> 0
			aColsImpostos[nPos][1] := aImpVarSF2[nI][1]
			aColsImpostos[nPos][2] := SFB->FB_DESCR
			aColsImpostos[nPos][4] := aImpVarSF2[nI+1][3]
			aColsImpostos[nPos][5] := aImpVarSF2[nI][3]
			nAliq := ((aColsImpostos[nPos][5])/(aColsImpostos[nPos][4])*100)
			aColsImpostos[nPos][3] := IIf( nAliq <= 0, 0.00, nAliq )
		Else
			aColsImpostos[nCnt][1] := aImpVarSF2[nI][1]
			aColsImpostos[nCnt][2] := SFB->FB_DESCR
			aColsImpostos[nCnt][4] := aImpVarSF2[nI+1][3]
			aColsImpostos[nCnt][5] := aImpVarSF2[nI][3]
			nAliq := ((aColsImpostos[nCnt][5])/(aColsImpostos[nCnt][4])*100)
			aColsImpostos[nCnt][3] := IIf( nAliq <= 0, 0.00, nAliq )
		EndIf
	Next nI
EndIf
If lLivros
	For nI := 1 To Len(aLivro)-1
		nCnt++
		nUsado:=0
		For nC := 1 To nCpos
			nUsado++
			aColsLivros[nCnt][nUsado] := aLivro[nI+1][nC]
			If aColsLivros[nCnt][nUsado] == NIL
				aColsLivros[nCnt][nUsado] := CriaVar(AllTrim(aHeadLivros[nC][2]))
			EndIf
		Next nC
	Next nI
EndIf

Return
