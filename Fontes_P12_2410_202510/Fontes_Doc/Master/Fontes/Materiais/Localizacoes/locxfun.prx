[n][12] -> Valor Calculado do Imposto sobre Valor do Frete e Despesas.
[n][13] -> Valor Calculado do Imposto sobre Despesas.
[n][14] -> Valor Calculado do Imposto sobre Gastos Totales.
[n][15] ->
[n][16] -> Codigo do Produto.
[n][17] -> Campo do Livro Fiscal.
[n][18] -> Desconto por item ou total
[n][19] -> Define se imposto vai somar, subtrair ou indiferente
*/
If cFunName == "MATA468"
	//Funcao mantida para compatibilidade com o programa MATA468
	//A funcao a460TesXIp mudou para atender o calculo de impostos por total
	// e virar a padrao para todos os programas de saida.
	// Bruno. REX 10/08/2001
	OldTesXIP(aImpVarSD2, nTotBaseImp, nItemValTot, cProduto , nPosCols )
Else
	If Type("aCols") <> "U"
		For nC := 1 To Len(aCols)
			If !aCols[nC][Len(aCols[nC])]
				nMaxArray++
			Endif
		Next nC
	Endif
	SFC->( DbSetOrder( 1 ) )
	SFB->( DbSetOrder( 1 ) )
	If Len( aImpVarSD2 ) == 5
		AADD( aImpVarSD2, {} )
	Endif
	lCpoLiquid := SFC->(ColumnPos("FC_LIQUIDO"))
	SFC->( DbSeek( xFilial("SFC")+SF4->F4_CODIGO ) )
	While SFC ->( !Eof() .And. SFC->FC_FILIAL+SFC->FC_TES == xFilial("SFC")+SF4->F4_CODIGO )
		//Tratamento especifico para localizado PERU
		//Abaixo é verificado se os impostos IGV, PIV e DIG vão ser
		//calculados para entrarem no demosntrativo da nota fiscal
        If cPaisLoc $ "PER"
			If SFC->FC_IMPOSTO $ "IGV"
				If (SF4->F4_CALCIGV == "2" .Or. SF4->F4_CALCIGV == "3")
					SFC->(DbSkip())
					Loop
				EndIf
			EndIf
			If SFC->FC_IMPOSTO $ "PIV"
				DbSelectArea("SA1")
				DbSetOrder(1)
				If DbSeek(xFilial("SA1")+clCodCli+clCodLoj)
					If !(SubStr(cMV_AGENTE,1,1) == "N" .And.  SA1->A1_AGENTE == "2")
						SFC->(DbSkip())
						Loop
					EndIf
				EndIf
			EndIf
			If SFC->FC_IMPOSTO $ "DIG"
				DbSelectArea("SA1")
				DbSetOrder(1)
				If DbSeek(xFilial("SA1")+clCodCli+clCodLoj)
					If !(SubStr(cMV_AGENTE,1,1) == "N" .And.  SA1->A1_AGENTE == "3" )
						SFC->(DbSkip())
						Loop
					EndIf
				EndIf
			EndIf
        EndIf
        If cPaisLoc == "ARG"
        	If SFC->FC_LIQUIDO == "N" .And. nDescItem > 0
        		aImpVarSD2[3] += nDescItem
        		nDescItem := 0
        	EndIf
        EndIf
		If cPaisLoc=="ARG" .And. lProc .And. Alltrim(Substr(SFC->FC_IMPOSTO,1,2))=="IB"
		    If SFC->FC_PROV == aImpVarSD2[7] .Or. aImpVarSD2[7] == "99"
		    	lGera := .T.
		    Else
			   	lGera := .F.
			Endif
		Else
			lGera := .T.
		Endif
		If lGera
			SFB->( DbSeek( xFilial("SFB")+SFC->FC_IMPOSTO ) )
			cFileRdMk := AllTrim(SFB->FB_FORMSAI)
			aImposto  := {}
			AAdd( aImposto, SFB->FB_CODIGO )
			AAdd( aImposto, 0.00 )
			AAdd( aImposto, 0.00 )
			AAdd( aImposto, 0.00 )
			AAdd( aImposto, SFC->FC_INCDUPL + SFC->FC_INCNOTA + SFC->FC_CREDITA + If(lCpoLiquid, SFC->FC_LIQUIDO, "S"))
			AAdd( aImposto, "D2_VALIMP"+SFB->FB_CPOLVRO)
			AAdd( aImposto, "D2_BASIMP"+SFB->FB_CPOLVRO)
			AAdd( aImposto, "F2_VALIMP"+SFB->FB_CPOLVRO)
			AAdd( aImposto, "F2_BASIMP"+SFB->FB_CPOLVRO)
			AAdd( aImposto, SFC->FC_INCIMP )
			AAdd( aImposto, 0.00 )
			AAdd( aImposto, 0.00 )
			AAdd( aImposto, 0.00 )
			AAdd( aImposto, 0.00 )
			AAdd( aImposto, SFC->FC_CALCULO )
			AAdd( aImposto, cProduto)
			AAdd( aImposto, SFB->FB_CPOLVRO )  // [17]-Campo do Livro Fiscal.
			AAdd( aImposto, nDescontos )
			AADD( aImposto, {1,-1,0} )        //[19] Define se imposto vai somar, subtrair ou indiferente
			AADD( aImposto, SFC->FC_PROV )
			AADD( aImposto, 0.00 ) //Validar limite SFF - Colombia
			Iif(cPaisLoc == "ARG", cCalculo := SFC->FC_CALCULO, "")
			nRecnoSFC := SFC->( Recno() )
			nOrderSFC := SFC->( IndexOrd() )
			If FindFunction(cFileRdMk )
				SB1->(DbSetOrder(1))
				SB1->(DbSeek(xFilial("SB1")+cProduto))
				aImposto := ExecBlock( cFileRdMk,.F.,.F., { aImpVarSD2, aImposto,;
					nPosCols, "D2_VALIMP"+SFB->FB_CPOLVRO,nTotBaseImp} ,.T. )
					If cPaisLoc == "ARG" .And. cFunName == "MATA468N"
						aImposto[15] := cCalculo
					EndIf
				AAdd( aImpVarSD2[6], AClone( aImposto ) )
			Else
				Help(" ",1,"NO_IX",,cFileRdMk,1,30)
			EndIf
			If SFC->FC_CALCULO $ "T"
				//procura por imposto + aliquota
				nPosImp := Ascan(aImpTotal,{|imp| imp[1] == aImposto[1] .And. imp[2] == aImposto[2]})
				If nPosImp == 0
					//aImpTotal[x] = {Codigo do imposto, aliquota, aimpvarsd2, rdmake do calculo,base do imposto,reg no SFC,reg no sfb}
					If cPaisLoc $ "COL" .And. cFunName $ "MATA468N|MATA461" .And. cFileRdMk $ "M460ICA"
						Aadd(aImpTotal,{ aImposto[1], aImposto[2], Aclone(aImpVarSD2), cFileRdMk, 0, SFC->(Recno()), SFB->(Recno()), aImposto[21]})
					Else
						Aadd(aImpTotal,{aImposto[1],aImposto[2],Aclone(aImpVarSD2),cFileRdMk,0,SFC->(Recno()),SFB->(Recno())})
					EndIf
					nPosImp := Len(aImpTotal)
					//zera o valor do desconto pois ele ja foi usado para o calculo da base para este item
					aImposto[18] := 0
					aImpTotal[nPosImp,3,6] := {Aclone(aImposto)}
				Endif
				aImpTotal[nPosImp,5] += aImposto[3]		//acumulo da base para imposto sobre total da nota
			EndIf
			SFC->( DbSetOrder(nOrderSFC) )
			SFC->( dbGoTo(nRecnoSFC) )
		Endif
		SFC->( DbSkip() )
	End
	//Ratear base de calculo e valor do imposto quando calculo for com base no Total das Mercadorias...
	If nPosCols == nMaxItem
		lNoRed	:= .F.
		Aadd(aImps,aClone(aImpVarSD2))
		nPosICA := Ascan(aImpTotal,{|imp| imp[1] == "ICA"})
		If cPaisLoc $ "COL" .And. nPosICA > 0 .And. Len(aImpTotal) >= 8
			AjImpCol(aImpTotal, @aImps, nPosICA)
		EndIf

		For nC := 1 To Len(aImpTotal)
			//posiciona os arquivos de impostos para uso do programa de calculo
			SFB->(DbGoto(aImpTotal[nC,7]))
			SFC->(DbGoto(aImpTotal[nC,6]))
			SF4->(MSSEEK(Xfilial("SF4")+SFC->FC_TES))
			cFileRdMk := aImpTotal[nC,4]
			aImpVarSD2 := Aclone(aImpTotal[nC,3])
			aImposto := Aclone(aImpTotal[nC,3,6,1])
			aImpVarSD2[1] := 1
			aImpVarSD2[2] := aImpTotal[nC,5]
			aImpVarSD2[3] := aImpTotal[nC,5]
			If cPaisLoc == "PTG"
				//Para Portugal, passa o valor da base acumulada nos campos de frete, despesa,
				//seguro e tara pois, esses valores sao usados como base (veja m460iva).
				aImpVarSD2[04] := aImpTotal[nC,5]
				aImpVarSD2[05] := aImpTotal[nC,5]
				aImpVarSD2[08] := aImpTotal[nC,5]
				aImpVarSD2[09] := aImpTotal[nC,5]
				aImpVarSD2[10] := aImpTotal[nC,5]
				aImpVarSD2[11] := aImpTotal[nC,5]
			Else
				//zera os valores do frete e das despesas pois ja foram utilizados para o calculo da base
				aImpVarSD2[04] := 0
				aImpVarSD2[05] := 0
			Endif
			aImpVarSD2[6] :={}
			if cPaisLoc == "COL" .AND. cFileRdMk == "M460RIVA"
				for ni := 1 To Len(aImps)
					nPosImp := Ascan(aImps[ni][6],{|imp| Substr(imp[1],1,2) == "IV"}) // Considerar códigos de impuesto IVx = IVA
					If nPosImp > 0
						Aadd(aImpVarSD2[6],aImps[ni][6][nPosImp])
					EndIf
				next
			Endif
			If FindFunction(cFileRdMk )
				aImposto := ExecBlock( cFileRdMk,.F.,.F., { aImpVarSD2, aImposto,nPosCols, aImposto[6],0} ,.T. )
				AAdd( aImpVarSD2[6], AClone( aImposto ) )
				nTotBase := 0
				nTotMerc := 0
				nTotImp  := 0
				nQtdImp  := Len( aImps )
				For nZ := 1 To nQtdImp
					If aImps[nZ] <> Nil .And. Len(aImps[nZ]) > 0
						nPosImp  := Ascan(aImps[nZ][6],{|x| x[1] == aImpVarSD2[6][Len(aImpVarSD2[6])][1] .And. x[2] == aImpVarSD2[6][Len(aImpVarSD2[6])][2] })
						If cPaisLoc=="ARG" .And. nPosImp > 0 .And. aImps[nZ][6][nPosImp][1]=="IBK"
							If nPosImp > 0 .And. aImps[nZ][6][nPosImp][15]=="T"

							 	nRat := (aImps[nZ][6][nPosImp][3]/aImpVarSD2[6][Len(aImpVarSD2[6])][3])
							 	aImps[nZ][6][nPosImp][4] := aImpVarSD2[6][Len(aImpVarSD2[6])][4] * nRat
								aImps[nZ][6][nPosImp][3] := aImpVarSD2[6][Len(aImpVarSD2[6])][3] * nRat
							 	nBasIBK  += aImps[nZ][6][nPosImp][3]
								nValIBK  += aImps[nZ][6][nPosImp][4]
 								nTotImp  += aImps[nZ][6][nPosImp][4]
								nTotBase += aImps[nZ][6][nPosImp][3]
								nBasTIbk += aImps[nZ][6][nPosImp][3]
								nImpIBK  += aImps[nZ][6][nPosImp][4]
							Endif
						Else
							If nPosImp > 0 .And. aImps[nZ][6][nPosImp][15]=="T"
								If aImpVarSD2[6][Len(aImpVarSD2[6])][4] <> 0
									nRat := (aImps[nZ][6][nPosImp][3]/aImpVarSD2[6][Len(aImpVarSD2[6])][3])
									aImps[nZ][6][nPosImp][4] := aImpVarSD2[6][Len(aImpVarSD2[6])][4] * nRat
									aImps[nZ][6][nPosImp][3] := aImpVarSD2[6][Len(aImpVarSD2[6])][3] * nRat
									nTotImp += aImps[nZ][6][nPosImp][4]
									nTotBase += aImps[nZ][6][nPosImp][3]
								ElseIf cPaisLoc!="ARG" .And. !(aImps[nZ][6][nPosImp][1] $ "IB|RV0")
									aImps[nZ][6][nPosImp][4] := 0
									aImps[nZ][6][nPosImp][3] := aImps[nZ][6][nPosImp][3]
									aImpVarSD2[6][Len(aImpVarSD2[6])][3] := 0
								Endif
							Endif
						EndIf
					Endif
					If nZ == nMaxItem .and. nImpIBK > 0
					  For nX:= 1 to nMaxItem
						nPosIBK  := Ascan(aImps[nx][6],{|x| x[1] == aImpVarSD2[6][Len(aImpVarSD2[6])][1] })
						If cPaisLoc=="ARG" .And. nPosIBK > 0 .And. aImps[nX][6][nPosIBK][1]=="IBK"
							if aImps[nx][6][nPosIBK][4] == 0
								nValorT:= aImps[nX][6][nPosIBK][3] * (aImps[nX][6][nPosIBK][2]/100)
								aImps[nx][6][nPosIBK][4] := nValorT
							EndIf
						EndIf
						next

					EndIf
				Next nZ
				//Se o imposto foi relamente calculado para este item...
				If cPaisLoc=="ARG" .And. aImpVarSD2[6][Len(aImpVarSD2[6])][1]<>"IBK"
					If aImpVarSD2[6][Len(aImpVarSD2[6])][4] <> 0
						aImpVarSD2[6][Len(aImpVarSD2[6])][3]   -= nTotBase
						aImpVarSD2[6][Len(aImpVarSD2[6])][4]   -= nTotImp
						//Se nao foi, zero a base
					Else
						aImpVarSD2[6][Len(aImpVarSD2[6])][3]   :=	0
					Endif
				EndIf
				//Restaurar os valores originais
				aImpVarSD2[1]	:=	aBase[1]
				aImpVarSD2[2]	:=	aBase[2]
				aImpVarSD2[3]	:=	aBase[3]
				aImpVarSD2[4]	:=	aBase[4]
				aImpVarSD2[5]	:=	aBase[5]
			Endif
		Next
	Endif
Endif

Return
/*
±±ÚÄÄÄÄÄÄÄÄÄÄÂÄÄÄÄÄÄÄÄÄÄÂÄÄÄÄÄÄÄÂÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÂÄÄÄÄÄÄÂÄÄÄÄÄÄÄÄÄÄ¿±±
±±³Fun‡…o    ³BaixaLote ³ Autor ³ Ary Medeiros          ³ Data ³ 02.07.93 ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄ´±±
±±³Descri‡…o ³ Faz a Baixa de sub-lote usando FIFO                        ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´±±
±±³Sintaxe   ³ BaixaLote(ExpC1,ExpC2,ExpN1)                               ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´±±
±±³Parametros³ ExpC1 = Alias Oigem da Movimentacao                        ³±±
±±³          ³ ExpC2 = Sub-Lote                                           ³±±
±±³          ³ ExpL1 = Flag que indica se baixa empenho (Producao)        ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´±±
±±³ Uso      ³ Generico                                                   ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÂÄÄÄÄÄÄÄÂÄÄÄÄÄÄÄÄÄ´±±
±±³Revisao   ³ Rodrigo de A. Sartorio / Sergio Silveira ³ Data  ³ 22/10/97³±±
±±ÀÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÙ±±

*/
Function BaixaLote( cAlias, cNumLote, lBaixaEmp )
Local xAlias 		:= Alias()
Local cProduto		:= ""
Local cLocal        := ""
Local cCliFornec    := ""
Local cLoj          := ""
Local cDocto        := ""
Local cSer          := ""
Local cSequenc      := ""
Local nBaixa		:= 0
Local nSaldo		:= 0
Local nQuant		:= 0
Local nEmpenho		:= 0
Local nMultiplic 	:= 1
Local cSeek			:= ""
Local cCompara		:= ""

If (cAlias == "SD3" .And. SD3->D3_TM > "500") .Or. cAlias == "SD2" .Or. cAlias == "SCN"
	nMultiplic := -1
EndIf
IF cAlias == "SD3"
	cProduto := SD3->D3_COD
	cLocal   := SD3->D3_LOCAL
	cLoteCtl := SD3->D3_LOTECTL
	nQuant   := SD3->D3_QUANT
	If SD3->D3_CF == "DE3"
		cLocal := SuperGetMV("MV_LOCPROC")
		nMultiplic := -1
	Endif
ElseIf cAlias == "SD2"
	cProduto := SD2->D2_COD
	cLocal   := SD2->D2_LOCAL
	cLoteCtl := SD2->D2_LOTECTL
	nQuant   := SD2->D2_QUANT
	cDocto    := SD2->D2_DOC
	cSer      := SD2->D2_SERIE
	cCliFornec:= SD2->D2_CLIENTE
	cLoj      := SD2->D2_LOJA
	cSequenc  := SD2->D2_NUMSEQ
ElseIf cAlias == "SD1"
	cProduto := SD1->D1_COD
	cLocal   := SD1->D1_LOCAL
	cLoteCtl := SD1->D1_LOTECTL
	nQuant   := SD1->D1_QUANT
	cDocto    := SD1->D1_DOC
	cSer      := SD1->D1_SERIE
	cCliFornec:= SD1->D1_FORNECE
	cLoj      := SD1->D1_LOJA
	cSequenc  := SD1->D1_NUMSEQ
ElseIf cAlias == "SCN" .And. cPaisLoc <> "BRA"
	cProduto := SCN->CN_PRODUTO
	cLocal   := SCN->CN_LOCAL
	cLoteCtl := SCN->CN_LOTECTL
	nQuant   := SCN->CN_QUANT
	cDocto    := SCN->CN_REMITO
	cSer      := SCN->CN_SERIE
	cCliFornec:= SCN->CN_CLIENTE
	cLoj      := SCN->CN_LOJA
	cSequenc  := SCN->CN_NUMSEQ
Endif
lBaixaEmp:=IIf(lBaixaEmp == NIL,.F.,lBaixaEmp)
If Rastro(cProduto,"S")
	cNumLote    := IIf(cNumLote == Nil,Space(6),cNumLote)
Else
	cNumLote    := Space(6)
EndIf
IF Empty(cNumLote)
	DbSelectArea("SB8")
	If Rastro(cProduto,"S")
		DbSetOrder(1)
		cSeek:=xFilial("SB8")+cProduto+cLocal
		cCompara:="SB8->B8_FILIAL+SB8->B8_PRODUTO+SB8->B8_LOCAL"
		dbSeek(cSeek)
		While nQuant > 0 .And. cSeek == &(cCompara)
			nSaldo := SB8->B8_SALDO - SB8->B8_EMPENHO
			If nSaldo > 0
				nBaixa := IIF(nSaldo >= nQuant,nQuant,nSaldo)
				GravaSD5(cAlias,SB8->B8_PRODUTO,SB8->B8_LOCAL,SB8->B8_LOTECTL,SB8->B8_NUMLOTE,cSequenc,cDocto,cSer,,SB8->B8_ORIGLAN,cCliFornec,cLoj,SB8->B8_LOTEFOR,nBaixa,,SB8->B8_DATA,SB8->B8_DTVALID)
				Reclock("SB8",.F.)
				Replace SB8->B8_SALDO With SB8->B8_SALDO - nBaixa
				nQuant -= nBaixa
			Endif
			DbSelectArea("SB8")
			DbSkip()
		End
	Else
		// Qdo for saida do estoque
		If nMultiplic == -1
			DbSetOrder(3)
			If !Empty(cLoteCtl)
				cSeek:=xFilial("SB8")+cProduto+cLocal+cLoteCtl
				cCompara:="SB8->B8_FILIAL+SB8->B8_PRODUTO+SB8->B8_LOCAL+SB8->B8_LOTECTL"
			Else
				cSeek:=xFilial("SB8")+cProduto+cLocal
				cCompara:="SB8->B8_FILIAL+SB8->B8_PRODUTO+SB8->B8_LOCAL"
			EndIf
			dbSeek(cSeek)
			While nQuant > 0 .and. cSeek == &(cCompara)
				nSaldo:=IIf(lBaixaEmp,SB8->B8_EMPENHO,SB8->B8_SALDO - SB8->B8_EMPENHO)
				If nSaldo > 0
					nBaixa := IIf(nSaldo >= nQuant,nQuant,nSaldo)
					GravaSD5(cAlias,SB8->B8_PRODUTO,SB8->B8_LOCAL,SB8->B8_LOTECTL,SB8->B8_NUMLOTE,cSequenc,cDocto,cSer,,SB8->B8_ORIGLAN,cCliFornec,cLoj,SB8->B8_LOTEFOR,nBaixa,,SB8->B8_DATA,SB8->B8_DTVALID)
					Reclock("SB8",.F.)
					Replace SB8->B8_SALDO With SB8->B8_SALDO - nBaixa
					If lBaixaEmp
						Replace SB8->B8_EMPENHO With SB8->B8_EMPENHO - nBaixa
					EndIf
					nQuant -= nBaixa
				EndIf
				DbSelectArea("SB8")
				DbSkip()
			End
		Else
			// Qdo for devolucao p/ Lote
			CriaLote("SD3",,NextLote(cProduto,"S"))
		EndIf
	EndIf
Else
	DbSelectArea("SB8")
	DbSetOrder(2)
	MsSeek(cFilial+cNumLote+cLoteCtl+cProduto+cLocal)
	If Found()
		GravaSD5(cAlias,SB8->B8_PRODUTO,SB8->B8_LOCAL,SB8->B8_LOTECTL,SB8->B8_NUMLOTE,cSequenc,cDocto,cSer,,SB8->B8_ORIGLAN,cCliFornec,cLoj,SB8->B8_LOTEFOR,nBaixa,,SB8->B8_DATA,SB8->B8_DTVALID)
		Reclock("SB8",.F.)
		Replace SB8->B8_SALDO With SB8->B8_SALDO + (nQuant * nMultiplic)
		//OMB - 02/05/99 - Nao baixava empenho para Remito (ARG)
		If cPaisLoc <> "BRA"
			If (cAlias == "SD2" .Or. cAlias == "SCN" .Or. cAlias == "SD3")
				If lBaixaEmp
					Replace SB8->B8_EMPENHO With B8_EMPENHO - nQuant
				EndIf
			EndIf
		EndIf
	EndIf
Endif
DbSelectArea("SB8")
DbSetOrder(1)
DbSelectArea(xAlias)

Return NIL
