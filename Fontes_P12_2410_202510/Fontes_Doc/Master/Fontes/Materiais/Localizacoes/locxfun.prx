	//Archivo temporal datos C.costo
	dbSelectArea("SEZTMP")

	SEZTMP ->(DbSetOrder(1))// EZ_NATUREZ+ EZ_CCUSTO

	//Busca natureza en archivo Temporal TMP de Mult Nat C.Custo
	If SEZTMP ->(dbSeek(cNaturez))
		While !SEZTMP->(Eof()) .and. SEZTMP->EZ_NATUREZ == cNaturez
			RecLock("SEZ",.T.)
			SEZ->EZ_FILIAL := cFilSEZ
			SEZ->EZ_PREFIXO:= SEV->EV_PREFIXO
			SEZ->EZ_NUM    := SEV->EV_NUM
			SEZ->EZ_PARCELA:= SEV->EV_PARCELA
			SEZ->EZ_CLIFOR := SEV->EV_CLIFOR
			SEZ->EZ_LOJA   := SEV->EV_LOJA
			SEZ->EZ_TIPO   := SEV->EV_TIPO
			SEZ->EZ_PERC   := SEZTMP->EZ_PERC
			SEZ->EZ_VALOR  := Round(NoRound(SEV->EV_VALOR*SEZ->EZ_PERC,nTEZ_PERC),2)
			SEZ->EZ_NATUREZ:= SEV->EV_NATUREZ
			SEZ->EZ_CCUSTO := SEZTMP->EZ_CCUSTO
			SEZ->EZ_ITEMCTA:= SEZTMP->EZ_ITEMCTA
			SEZ->EZ_CLVL   := SEZTMP->EZ_CLVL
			SEZ->EZ_RECPAG := SEV->EV_RECPAG
			SEZ->EZ_LA     := ""
			SEZ->EZ_IDENT  := SEV->EV_IDENT
			SEZ->EZ_SEQ    := SEV->EV_SEQ
			SEZ->EZ_SITUACA:= SEV->EV_SITUACA
			SEZ->EZ_CONTA := SEZTMP->EZ_CONTA
			nRateioSEZ += SEZ->EZ_VALOR
			if nRateioSEZ > SEV->EV_VALOR
				SEZ->EZ_VALOR := SEV->EV_VALOR - cParcela* Round(NoRound(SEV->EV_VALOR*SEZ->EZ_PERC,nTEZ_PERC),2)
			Endif
			cParcela += 1
			SEZ->(MsUnLock())
			SEZTMP->(DbSkip())
		EndDo
	Endif
	RestArea(cArea)
Return

/*
Funcion: xActLivFis() - Función de actualización del libro fiscal.
Parametros:
aAuxOri - Arreglo usado por las rutinas de los libros fiscales.
aNfItem - Items de los documentos.
aNfCabLiv - Libro fiscal.
aTes - TES actual de la matxfis.
nITDelet - Posición del item que indica si está eliminado.
cTipo - E->Entrada o S->Salida.
lActLivFis - Indica si rehace el libro fiscal o si hace la conversión de moneda.
*/
Function xActLivFis (aAuxOri,aNfItem,aNfCabLiv,aTes,nITDelet,cTipo,lActLivFis)
Local nX			:= 0
Local aTesResp		:= {}
Local nMoedaTasa	:= 1

Default	aAuxOri	:= {}
Default	aNfItem	:= {}
Default	aTes	:= {}
Default	cTipo	:= ""

Private lNoConv := .T. //Variable usada en las rutinas M100Livr y M460Livr para no hacer conversión de moneda

	If IsInCallStack("gravaimposto") .or. IsInCallStack("gravanfgeral")
		Return
	EndIf

	If lActLivFis
		aTesResp	:= aClone(aTes)  //Se respalda la TES actual usada en matxfis
		aNfCabLiv 	:= {}			//Se borra el libro fiscal
		//Se rehace el libro fiscal completo, sin hacer conversión de moneda.
		For nX := 1 to Len(aNfItem)
			If !aNfItem[nX][nITDelet] //Verificación, si es un item borrado no se procesa
				MaFisLFToLivro(nX,@aAuxOri,.T.)
			EndIf
		Next nX
		aTes := aClone(aTesResp) //Se restaura la TES usada en la matxfis.
	Else
		nMoedaTasa := If(Type("nTaxa")=="N",nTaxa,If(Type("nTXMoeda")=="N",nTXMoeda,1)) //Tasa de la moneda.

		If nMoedaTasa <> 1
			//Se verifica si hay datos en los libros fiscales
			If Len(aNfCabLiv) > 0 .and. Len(aNfCabLiv[1]) > 0
				If Len(aCposImpF3) == 0 //Arreglo statico con las posiciones de los impuestos dentro de los libros fiscales.
					aCposImpF3 := CposImpF3(aNfCabLiv[1])
				EndIf
				ConvLivFis(@aNfCabLiv, nMoedaTasa,cTipo) //Conversión
			EndIf
		EndIf

	EndIf


	lNoConv := .F.

Return

/*
Funcion: CposImpF3() - Función de llenado para arreglo con posiciones de campos de impuestos del libro fiscal.
Parametros:
aHeader- Arreglo con los nombres de los campos del libro fiscal

Return:
aCposImp - Arreglo con las posiciones de los campos de impuestos de los libros fiscales.
*/
Function CposImpF3(aHeader)
Local nI 		:= 0
Local aCposImp	:= {}
Local cCpoLiv	:= ""
Local nAlqImp	:= 0
Local nValImp	:= 0

	For nI := 1 To Len(aHeader)
		If Substr(aHeader[nI],1,9) == "F3_BASIMP"
			cCpoLiv := Substr(aHeader[nI],10,1)
			nAlqImp	:= aScan(aHeader,{|x| x == "F3_ALQIMP"+cCpoLiv})
			nValImp := aScan(aHeader,{|x| x == "F3_VALIMP"+cCpoLiv})
			aAdd(aCposImp,{nI,nAlqImp,nValImp})
		EndIf
	Next nI

Return aCposImp

/*
Funcion: ConvLivFis() - Función de conversión del libro fiscal.
Parametros:
aNfLivro - Arreglo con los nombres de los campos del libro fiscal
nTasa - Tasa de conversión
cTipo - E->Entrada o S->Salida.
*/
Function ConvLivFis (aNfLivro, nTasa, cTipo)
Local nI			:= 1
Local nJ			:= 1
Local nIdValCont	:= 0
Local nIdExentas	:= 0
Local nIdFrete	 	:= 0
Local nIdValMer 	:= 0
Local nAuxMoeda		:= IIf(Type("nMoedaNF")	=="N",nMoedaNf,IIf(Type("nMoedaCor")=="N",nMoedaCor,1))
Local nDecs			:= MsDecimais( 1 )
Local dDataTax		:= ddatabase

	If cTipo == "E"
		dDataTax := SF1->F1_DTDIGIT
	ElseIf cTipo == "S"
		dDataTax := SF2->F2_EMISSAO
	EndIf

	//Posiciones de los campos que son convertidos.
	If Len(aCposLivr) == 0
		nIdValCont := AScan( aNfLivro[1],{|x| x == "F3_VALCONT" } )
		nIdExentas := AScan( aNfLivro[1],{|x| x == "F3_EXENTAS" } )
		nIdFrete   := AScan( aNfLivro[1],{|x| x == "F3_FRETE" 	} )
		nIdValMer  := AScan( aNfLivro[1],{|x| x == "F3_VALMERC" })
		aCposLivr  := { nIdValCont, nIdExentas, nIdFrete, nIdValMer }
	Else
		nIdValCont := aCposLivr[1]
		nIdExentas := aCposLivr[2]
		nIdFrete   := aCposLivr[3]
		nIdValMer  := aCposLivr[4]
	EndIf

	If Len(aNfLivro) > 1
		For nI := 2 To Len(aNfLivro)
			aNfLivro[nI][nIdValCont] := 0																					//Se reinicia el valor contable
			aNfLivro[nI][nIdExentas] :=	Round(xMoeda(aNfLivro[nI][nIdExentas],nAuxMoeda,1,dDataTax,nDecs+1,nTasa),nDecs) //Conversión del valor de exento
			aNfLivro[nI][nIdFrete]	 := Round(xMoeda(aNfLivro[nI][nIdFrete]  ,nAuxMoeda,1,dDataTax,nDecs+1,nTasa),nDecs) //Conversión del valor de flete
			aNfLivro[nI][nIdValMer]  := Round(xMoeda(aNfLivro[nI][nIdValMer] ,nAuxMoeda,1,dDataTax,nDecs+1,nTasa),nDecs) //Conversión del valor de valor de mercado

			//Suma del valor de mercado convertido al valor contable.
			aNfLivro[nI][nIdValCont] += aNfLivro[nI][nIdValMer]

			For nJ := 1 To Len(aCposImpF3)
				//Conversión de base de impuesto.
				If aNfLivro[nI][aCposImpF3[nJ][1]] > 0
					aNfLivro[nI][aCposImpF3[nJ][1]] := Round(xMoeda(aNfLivro[nI][aCposImpF3[nJ][1]],nAuxMoeda,1,dDataTax,nDecs+1,nTasa),nDecs)
				EndIf
				//Aplicación de alicuota a la base convertida.
				If aNfLivro[nI][aCposImpF3[nJ][3]] > 0
					aNfLivro[nI][aCposImpF3[nJ][3]] := Round((aNfLivro[nI][aCposImpF3[nJ][1]] * (aNfLivro[nI][aCposImpF3[nJ][2]]/100)),nDecs)

					//Suma del valor del impuesto convertido al valor contable.
					aNfLivro[nI][nIdValCont] += aNfLivro[nI][aCposImpF3[nJ][3]]
				EndIf
			Next nJ
		Next nI
	EndIf

Return

/*/{Protheus.doc} AjImpCol
Ajusta impuestos cuando cálculo es por total o ítem (FC_CALCULO)
@type function
@parametros
@author oscar.lopez
@since 18/Junio/2021
@version 1.0
@param aImpTotal, caracter, Array con el totál de impuesto cuando FC_CALCULO $ "T"
@param aImps, caracter, Array con los impuesto de los ítems
@param nImp, caracter, Posicion en la cual se encuentra el total del impeusto en array aImpTotal
@return
/*/
Static Function AjImpCol(aImpTotal, aImps, nImp)
	Local nX		:= 0
	Local nPosImp	:= 0

	Default aImpTotal	:= {}
	Default aImps		:= {}

	If aImpTotal[nImp][8] > aImpTotal[nImp][5]
		For nX := 1 to Len(aImps)
			nPosImp := Ascan(aImps[nX][6],{|imp| imp[1] == "ICA"})
			If nPosImp > 0 .And. aImps[nX][6][nPosImp][15] = "T"
				aImps[nX][6][nPosImp][2] := 0
				aImps[nX][6][nPosImp][4] := 0
			EndIf
		Next nX
	EndIf
Return

/*/{Protheus.doc} LxGetAlic
Función utilizada para retornar las alicuotas de los impuestos utilizados en las
TES del documento.

@type function
@author Marco Augusto González Rivera
@since 09/07/2021
@version 1.0
@param cTabla, caracter, Codigo de la tabla en la que se está trabajando.
@param cSerie, caracter, Serie del documento.
@param cNumDoc, caracter, Numero del documento.
@param cCliProv, caracter, Codigo del Cliente o Proveedor.
@param cTienda, caracter, Tienda del Cliente o Proveedor.
@param cEspecie, caracter, Especie el documento.
@return aReturn, Array, Arreglo con las alicuotas de las TES utilizadas en el documento.
@example LxGetAlic("SF1", cSerie, cNumDoc, cCliProv, cTienda, cEspecie)
/*/
Static Function LxGetAlic(cTabla, cSerie, cNumDoc, cCliProv, cTienda, cEspecie)
	Local aArea			:= GetArea()
	Local aReturn 		:= {}
	Local nIteracion	:= 0
	Local nPosImpTes	:= 0
	Local cFilSFC		:= xFilial("SFC")
	Local cFilSFB		:= xFilial("SFB")
	Local aListTES		:= {}
	Local aListImps		:= {}

	aListTES := LxGetItTES(cTabla, cSerie, cNumDoc, cCliProv, cTienda, cEspecie)

	If Len(aListTES) > 0
		DBSelectArea("SFC")
		SFC->(DBSetOrder(1)) //FC_FILIAL+FC_TES+FC_SEQ+FC_IMPOSTO
		For nIteracion := 1 To Len(aListTES)
			If SFC->(MSSeek(cFilSFC+aListTES[nIteracion]))
				While SFC->(!Eof()) .And. SFC->FC_FILIAL == cFilSFC .And. SFC->FC_TES == aListTES[nIteracion]
					nPosImpTes := aScan(aListImps, {|x| x == SFC->FC_IMPOSTO })
					If nPosImpTes == 0 //Se descartan impuestos que ya existan en el arreglo
						aAdd(aListImps, SFC->FC_IMPOSTO)
					EndIf
					SFC->(DBSkip())
				EndDo
			EndIf
		Next nIteracion
	EndIf

	If Len(aListImps) > 0
		DBSelectArea("SFB")
		SFB->(DBSetOrder(1)) //FB_FILIAL+FB_CODIGO
		For nIteracion := 1 To Len(aListImps)
			If SFB->(MsSeek(cFilSFB+aListImps[nIteracion]))
				While SFB->(!Eof()) .And. SFB->FB_FILIAL == cFilSFB .And. SFB->FB_CODIGO == aListImps[nIteracion]
					aAdd(aReturn, {SFB->FB_CPOLVRO, SFB->FB_ALIQ, aListImps[nIteracion]})
					SFB->(DBSkip())
				EndDo
			EndIf
		Next nIteracion
	EndIf

	RestArea(aArea)

Return aReturn

/*/{Protheus.doc} LxGetItTES
Función utilizada para retornar los codigo de las TES
utilizadas en el documentos a nivel ítems.

@type function
@author Marco Augusto González Rivera
@since 09/07/2021
@version 1.0
@param cTabla, caracter, Codigo de la tabla en la que se está trabajando.
@param cSerie, caracter, Serie del documento.
@param cNumDoc, caracter, Numero del documento.
@param cCliProv, caracter, Codigo del Cliente o Proveedor.
@param cTienda, caracter, Tienda del Cliente o Proveedor.
@param cEspecie, caracter, Especie el documento.
@return aRetItTES, Array, Arreglo con las TES utilizadas en los ítems.
@example LxGetItTES("SF1", cSerie, cNumDoc, cCliProv, cTienda, cEspecie)
/*/
Static Function LxGetItTES(cTabla, cSerie, cNumDoc, cCliProv, cTienda, cEspecie)
	Local lTabSF2	:= cTabla == "SF2"
	Local aAreaSD	:= IIf(lTabSF2, SD2->(GetArea()), SD1->(GetArea()))
	Local cFilSD	:= IIf(lTabSF2, xFilial("SD2"), xFilial("SD1"))
	Local aRetItTES	:= {}
	Local cChaveSD	:= ""
	Local nPosTES	:= 0

	cChaveSD := cFilSD+cSerie + cNumDoc + cCliProv + cTienda + cEspecie

	If lTabSF2
		DBSelectArea("SD2")
		SD2->(DBSetOrder(3)) //D2_FILIAL+D2_DOC+D2_SERIE+D2_CLIENTE+D2_LOJA+D2_COD+D2_ITEM
		If SD2->(MSSeek(cFilSD+cNumDoc+cSerie+cCliProv+cTienda))
			While SD2->(!Eof()) .And. SD2->(D2_FILIAL+D2_SERIE+D2_DOC+D2_CLIENTE+D2_LOJA+D2_ESPECIE) == cChaveSD
				nPosTES := aScan(aRetItTES, {|x| x == SD2->D2_TES })
				If nPosTES == 0 //Se descartan TES que ya existan en el arreglo
					aAdd(aRetItTES, SD2->D2_TES)
				EndIf
				SD2->(DBSkip())
			EndDo
		EndIf
		SD2->(RestArea(aAreaSD))
	Else
		DBSelectArea("SD1")
		SD1->(DBSetOrder(1)) //D1_FILIAL+D1_DOC+D1_SERIE+D1_FORNECE+D1_LOJA+D1_COD+D1_ITEM
		If SD1->(MSSeek(cFilSD+cNumDoc+cSerie+cCliProv+cTienda))
			While SD1->(!Eof()) .And. SD1->(D1_FILIAL+D1_SERIE+D1_DOC+D1_FORNECE+D1_LOJA+D1_ESPECIE) == cChaveSD
				nPosTES := aScan(aRetItTES, {|x| x == SD1->D1_TES })
				If nPosTES == 0 //Se descartan TES que ya existan en el arreglo
					aAdd(aRetItTES, SD1->D1_TES)
				EndIf
				SD1->(DBSkip())
			EndDo
		EndiF
		SD1->(RestArea(aAreaSD))
	EndIf

Return aRetItTES

/*/{Protheus.doc} LxValAlqIm
Función utilizada para retornar el valor del campo Alqimp
para el libro enviado.

