Function CriaCCJ()
	Local alAreaOld		:= GetArea()						// Area atual
	Local alRecnoCCJ	:= {}								// Dados Concepto de retencao
	Local alRcCCJEIC	:= {}
	Local nlI			:= 0								// Controle de loop
	Local clFilCCJ		:= xFilial("CCJ")					// Filial do arquivo CCJ
	Local llIntEIC		:= ( SuperGetMV("MV_EASY") == "S" )	// Controla se tem integracao com SIGAEIC
	Local lAchouFil  :=.T.
	Local aAreaCCJ   := {}


	If nModulo <> 23
		If cPaisLoc == "VEN"
			CCJVEN(@alRecnoCCJ,clFilCCJ,llIntEIC,@alRcCCJEIC)
		EndIf

		DbSelectArea("CCJ")
		lAchouFil:=.T.
		aAreaCCJ:=CCJ->(GetArea())
		If !CCJ->(dbSeek(xFilial("CCJ")))
			lAchouFil:=.F.
		EndIf
    	CCJ->(RestArea(aAreaCCJ))

		If CCJ->(RecCount()) > 0 .And. lAchouFil
			alRecnoCCJ := {}
		EndIf


		If Len(alRecnoCCJ) > 0
			DBSelectArea("CCJ")
			DBSetOrder(1)
			For nlI := 1 to Len(alRecnoCCJ)
				If !dbSeek(alRecnoCCJ[nlI][1] + alRecnoCCJ[nlI][02] + alRecnoCCJ[nlI][03])
					RecLock("CCJ",.T.)
					Replace CCJ_FILIAL	With alRecnoCCJ[nlI][01]
					Replace CCJ_CODIGO	With alRecnoCCJ[nlI][02]
					Replace CCJ_REGIAO	With alRecnoCCJ[nlI][03]
					MsUnLock()
				EndIf
			Next nlI
		EndIf
		RestArea(alAreaOld)
	EndIf
Return .T.
