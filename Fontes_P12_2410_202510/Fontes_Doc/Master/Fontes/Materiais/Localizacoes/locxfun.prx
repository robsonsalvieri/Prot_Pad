Function CriaSFZ()
Local aAreaOld   := GetArea()
Local aRecnoSFZ  := {}
Local aRecSFZEIC := {}
Local nI 		 := 0
Local cFilSFZ 	 := xFilial("SFZ")
Local lIntEIC 	 := ( SuperGetMV("MV_EASY")="S" )

dbSelectArea("SFZ")
If SFZ->(RecCount()) == 0 .or. !SFZ->(dbSeek(cFilSFZ))

	If cPaisLoc == "ARG"
		SFZARG( @aRecnoSFZ , cFilSfz , lIntEIC , @aRecSFZEIC )
	EndIf

EndIf

If Len(aRecnoSFZ) > 0
	DbSelectArea("SFZ")
	DbSetOrder(1)
	For nI := 1 to Len(aRecnoSFZ)
		If !DbSeek(aRecnoSFZ[nI][1]+aRecnoSFZ[nI][8]+aRecnoSFZ[nI][05]+aRecnoSFZ[nI][09]+aRecnoSFZ[nI][07]+aRecnoSFZ[nI][06])
			RecLock("SFZ",.T.)
			Replace FZ_FILIAL  With aRecnoSFZ[nI][01]
			Replace FZ_DESIMPO With aRecnoSFZ[nI][02]
			Replace FZ_DESTACA With aRecnoSFZ[nI][03]
			Replace FZ_DESTIPO With aRecnoSFZ[nI][04]
			Replace FZ_ESPECIE With aRecnoSFZ[nI][05]
			Replace FZ_IMPOSTO With aRecnoSFZ[nI][06]
			Replace FZ_SERIE   With aRecnoSFZ[nI][07]
			Replace FZ_TIPMOV  With aRecnoSFZ[nI][08]
			Replace FZ_TIPO    With aRecnoSFZ[nI][09]
			MsUnLock()
		EndIf
	Next nI
EndIf
RestArea( aAreaOld )
Return(.T.)

