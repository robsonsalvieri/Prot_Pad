/*/
Function ESldRemSai( cMovto , cRemito, cItem, lAprob, lEstSD3, lEstSC6 )
Local lRet 	 	:= .T.
Local aCusto 	:= Array(5)
Local cSavAlias := Alias()
Local cSavOrder := IndexOrd()

lAprob 	:= Iif((lAprob<>NIL),lAprob,.t.)
lEstSD3 := IIf( lEstSD3==Nil,.T.,lEstSD3)
lEstSC6 := IIf( lEstSC6==Nil,.T.,lEstSC6)
//So movimentar Stock mediante a exclusao por Remito, nunca cuando el for Factura...
If cMovto $ "R"
	SCN->(DbSetOrder(1))
	SCN->(dbSeek(xFilial("SCN")+cRemito+cItem))
	SB1->(DbSetOrder(1))
	SB1->(MsSeek(xFilial("SB1")+SCN->CN_PRODUTO))
	SA1->(DbSetOrder(1))
	SA1->(MsSeek(xFilial("SA1")+SCN->CN_CLIENTE+SCN->CN_LOJA))
	//Verifica se havera movimenta‡„o de Estoques...
	SF4->(DbSetOrder(1))
	SF4->(MsSeek(xFilial("SF4")+SCN->CN_TES))
	DbSelectArea("SC9")
	DbSetOrder(4)
	MsSeek(xFilial("SC9")+SCN->CN_REMITO+SCN->CN_ITEM)
	If Found()
		If  !Empty(SCN->CN_CARGA)
			//Estorna a carga (Integracao com o OMS)
			//Oms200Grv("SCN",SCN->CN_CARGA,SCN->CN_SEQCAR,,SCN->CN_CLIENTE,SCN->CN_LOJA,,,,,,,,,,2,SCN->CN_QUANT,(SC9->C9_QTDLIB*SC9->C9_PRCVEN),SC9->C9_PEDIDO)Funciones no Compiladas en RPO
			RecLock("SC9",.F.)
			SC9->C9_CARGA   := ""
			SC9->C9_SEQCAR  := ""
			SC9->C9_SEQENT  := ""
			SC9->C9_ENDPAD  := ""
		Else
			RecLock("SC9",.F.)
		Endif
		Replace C9_REMITO  		With " "
		Replace C9_ITEMREM		With " "
		Replace C9_DTREMIT		With CTOD("")
		If	!lAprob
			Replace C9_BLEST       	With "  "
			Replace C9_BLCRED   		With "  "
		EndIf
		MsUnLock()
	Else
		DbSetOrder(1)
		dbSeek(xFilial("SC9")+SCN->CN_PEDIDO+SCN->CN_ITEMPED)
		If Found()
			If !Empty(SCN->CN_CARGA)
				//Estorna a carga (Integracao com o OMS)
				//Oms200Grv("SCN",SCN->CN_CARGA,SCN->CN_SEQCAR,,SCN->CN_CLIENTE,SCN->CN_LOJA,,,,,,,,,,2,SCN->CN_QUANT,(SC9->C9_QTDLIB*SC9->C9_PRCVEN),SC9->C9_PEDIDO)Funciones no Compiladas en RPO
				RecLock("SC9",.F.)
				SC9->C9_CARGA   := ""
				SC9->C9_SEQCAR  := ""
				SC9->C9_SEQENT  := ""
				SC9->C9_ENDPAD  := ""
			Else
				RecLock("SC9",.F.)
			Endif
			Replace C9_REMITO  		With " "
			Replace C9_ITEMREM		With " "
			Replace C9_DTREMIT		With CTOD("")
			If	!lAprob
				Replace C9_BLEST       	With "  "
				Replace C9_BLCRED   		With "  "
			EndIf
			MsUnLock()
		EndIf
	EndIf
	If lEstSC6
		SC6->(DbSetOrder(1))
		SC6->(MsSeek(xFilial("SC6")+SCN->CN_PEDIDO+SCN->CN_ITEMPED+SCN->CN_PRODUTO))
		If SC6->( Found() )
			RecLock("SC6",.F.)
			Replace C6_QTDENT		With ( C6_QTDENT - SCN->CN_QUANT )
			If lAprob
				Replace C6_QTDEMP	With ( C6_QTDEMP + SCN->CN_QUANT )
			Endif
			MsUnLock()
		EndIf
	Endif
	DbSelectArea("SCN")
	If lEstSD3 .And. SCN->CN_TIPOREM $ "1"
		If ! Empty(SCN->CN_LOCDEST)
			TransfCons("E")
		EndIf
	EndIf
	aCusto[1] := SCN->CN_CUSTO1
	aCusto[2] := SCN->CN_CUSTO2
	aCusto[3] := SCN->CN_CUSTO3
	aCusto[4] := SCN->CN_CUSTO4
	aCusto[5] := SCN->CN_CUSTO5
	//B2AtuComCN(aCusto,5,lAprob.And.lEstSC6) //Bruno Kluber 03/05/2000  //Se comenta  por Funciones Descontinuadas
EndIf
DbSelectArea( cSavAlias )
DbSetOrder( cSavOrder )

Return(lRet)
