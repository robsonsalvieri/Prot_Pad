*/
Function CalcImpGer( cTes , nQuant, nPrcVen, nValTot  ,;
					 nDesp, nFrete, cNitc  , aDespesas,;
					 cTec , nTotal, lLast )
Local aImpVar  := Array(11)
Local aImposto := {}

nQuant   := If(nQuant   == Nil, 0 ,nQuant  )
nPrcVen  := If(nPrcVen  == Nil, 0 ,nPrcVen )
nValTot  := If(nValTot  == Nil, 0 ,nValTot )
nDesp    := If(nDesp    == Nil, 0 ,nDesp   )
nFrete   := If(nFrete   == Nil, 0 ,nFrete  )
cNitc    := If(cNitc    == Nil, "",cNitc   )
lLast    := If(lLast    == Nil,.F.,lLast   )
//Inicializar array aImpVar com as propriedades do iten
aImpVar[1] := nQuant
aImpVar[2] := nPrcVen
aImpVar[3] := nValTot
aImpVar[4] := nDesp
aImpVar[5] := nFrete
aImpVar[6] := {}
If cPaisLoc == "COL" //Tratamento de Terceiros - Local.Colombia
	aImpVar[7] := cNitc
Endif
aImpVar[08] := IIf(cTec==Nil,"",cTec)
aImpVar[09] := IIf(aDespesas==Nil,{},aDespesas)
aImpVar[10] := IIf(nTotal ==Nil,1,nTotal)
aImpVar[11] := lLast
//Posicionar o arquivo de TES
SF4->( DbSeek( xFilial("SF4")+cTes           ) )
//Posicionar na amarracao TESxImposto
SFC->( DbSeek( xFilial("SFC")+cTes ) )
While SFC->( !Eof() .And. FC_FILIAL+FC_TES == xFilial("SFC")+cTes )
	SFB->( DbSeek( xFilial("SFB")+SFC->FC_IMPOSTO ) )
	// Preparar array de Impostos a ser tratados no RdMake do usuario.
	aImposto  := {}
	AAdd( aImposto, SFB->FB_CODIGO )
	AAdd( aImposto, SFB->FB_ALIQ )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, SFC->FC_INCDUPL + SFC->FC_INCNOTA + SFC->FC_CREDITA + SFC->FC_LIQUIDO )
	AAdd( aImposto, "D1_VALIMP"+SFB->FB_CPOLVRO )
	AAdd( aImposto, "D1_BASIMP"+SFB->FB_CPOLVRO )
	AAdd( aImposto, "F1_VALIMP"+SFB->FB_CPOLVRO )
	AAdd( aImposto, "F1_BASIMP"+SFB->FB_CPOLVRO )
	AAdd( aImposto, SFC->FC_INCIMP )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, SFC->FC_CALCULO )
	AAdd( aImposto, ""       )
	AAdd( aImposto, SFB->FB_CPOLVRO )
	AAdd( aImposto, 0 )
	AAdd( aImposto, {1,-1,0} )
	AADD( aImposto, SFC->FC_PROV )
	// Disparar o RdMake do Usuario com base em FB_FORMENT e FB_FORMSAI
	If FindFunction(AllTrim(SFB->FB_FORMENT))
		aImposto := ExecBlock( AllTrim(SFB->FB_FORMENT),.F.,.F., { aImpVar, aImposto,Nil;
			,"D1_VALIMP"+SFB->FB_CPOLVRO,Nil},.T. )
		AAdd( aImpVar[6], AClone( aImposto ) )
	EndIf
	SFC->(DbSkip())
End
Return	aImpVar
