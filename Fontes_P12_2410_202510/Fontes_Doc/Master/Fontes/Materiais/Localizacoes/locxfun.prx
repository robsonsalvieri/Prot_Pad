Local aAreaOld   := GetArea()                         // Area atual
Local aRecnoCCR  := {}                                // Dados Concepto de retencao
Local aRecCCREIC := {}
Local nI         := 0                                 // Controle de loop
Local cFilCCR    := xFilial("CCR")                    // Filial do arquivo CCR
Local lIntEIC    := ( SuperGetMV("MV_EASY") == "S" ) // Controla se tem integracao com SIGAEIC

If nModulo <> 23
	dbSelectArea("CCR")
	CCR->(dbGoTop())
	If CCR->(!dbseek(xFilial("CCR")))
		If cPaisLoc == "EQU"
			CCREQU( @aRecnoCCR , cFilCCR, lIntEIC , @aRecCCREIC )
		ElseIf cPaisLoc == "VEN"
			CCRVEN( @aRecnoCCR , cFilCCR, lIntEIC , @aRecCCREIC )
		ElseIf cPaisLoc == "DOM"
			CCRDOM( @aRecnoCCR , cFilCCR, lIntEIC , @aRecCCREIC ) // Movido para LOCXFUNA
		ElseIf cPaisLoc == "CHI"
			CCRCHI( @aRecnoCCR , cFilCCR, lIntEIC , @aRecCCREIC ) // Movido para LOCXFUNA
		ElseIf cPaisLoc == "AUS"
			CCRAUS( @aRecnoCCR , cFilCCR, lIntEIC , @aRecCCREIC ) // Movido para LOCXFUNA
		ElseIf cPaisLoc == "COS"
			CCRCOS( @aRecnoCCR , cFilCCR, lIntEIC , @aRecCCREIC ) // Movido para LOCXFUNA
		EndIf
	EndIf
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³Se o arquivo estiver vazio faz a carga com todos os registros, senao apenas com ³
	//³aqueles da integracao com o SIGAEIC                                             ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	If Len(aRecnoCCR) > 0
		dbSelectArea("CCR")
		CCR->(dbSetOrder(1))
		For nI := 1 to Len(aRecnoCCR)
			RecLock("CCR",.T.)
			Replace CCR_FILIAL  With aRecnoCCR[nI][01]
			Replace CCR_CONCEP  With aRecnoCCR[nI][02]
			Replace CCR_DESCR   With aRecnoCCR[nI][03]
			Replace CCR_ALIQ 	With aRecnoCCR[nI][04]
			Replace CCR_PAIS 	With aRecnoCCR[nI][05]
			If cPaisLoc $ "AUS|CHI|COS|DOM|EQU|PAD|VEN" .and. Len(aRecnoCCR[nI]) > 5
				Replace CCR_IMP  	With aRecnoCCR[nI][06]
			EndIf
			If cPaisLoc $ "AUS|CHI|COS|CHI|COS|DOM|EQU|PAD|VEN" .and. Len(aRecnoCCR[nI]) > 6
				Replace CCR_CLIFOR  With aRecnoCCR[nI][07]
			EndIf
			If cPaisLoc $ "AUS|COS|DOM|PAD" .and. Len(aRecnoCCR[nI]) > 7
				Replace CCR_VALOR  With aRecnoCCR[nI][08]
			EndIf
			If cPaisLoc $ "COS|DOM|PAD" .and. Len(aRecnoCCR[nI]) > 8
				Replace CCR_APUR  With aRecnoCCR[nI][09]
			EndIf
			If cPaisLoc $ "COS|DOM|PAD" .and. Len(aRecnoCCR[nI]) > 9
				Replace CCR_GAPUR  With aRecnoCCR[nI][10]
			EndIf
			If cPaisLoc $ "COS|DOM|PAD" .and. Len(aRecnoCCR[nI]) > 10
				Replace CCR_REDUC  With aRecnoCCR[nI][11]
			EndIf
			MsUnLock()
		Next nI
	EndIf
	RestArea( aAreaOld )
Endif
Return(.T.)
