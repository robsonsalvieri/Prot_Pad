/*/
Function CriaSFC()

Local aAreaOld   := GetArea()                         // Area atual
Local aRecnoSFC  := {}                                // Dados dos TES
Local aRecSFCEIC := {}                                // Dados dos TES do SIGAEIC
Local nI         := 0                                 // Controle de loop
Local cFilSFC    := xFilial("SFC")                    // Filial do arquivo SFC
Local lIntEIC    := ( SuperGetMV("MV_EASY") == "S" ) // Controla se tem integracao com SIGAEIC
Local lAchouFil	 :=.T.
Local aAreaSFC	 := {}
Local cAuxPaisl  := "ANG|ARG|AUS|BOL|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"

If nModulo <> 23
	If cPaisLoc == "ARG"
		SFCARG( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "CHI"
		SFCCHI( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "EUA"
		SFCEUA( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "MEX"
		SFCMEX( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "PAR"
		SFCPAR( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "POR"
		SFCPOR( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "COL"
		SFCCOL( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "URU"
		SFCURU( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "VEN"
		SFCVEN( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "DOM"
		SFCDOM( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "PER"
		SFCPER( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "COS"
		SFCCOS( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "SAL"
		SFCSAL( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "GUA"
		SFCGUA( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "BOL"
		SFCBOL( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "PTG"
		SFCPTG( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "ANG"
		SFCANG( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "EQU"
		SFCEQU( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC )
	ElseIf cPaisLoc == "AUS"
		SFCAUS( @aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "RUS"
		SFCRUS(@aRecnoSFC , cFilSFC , lIntEIC , @aRecSFCEIC)
	EndIf

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³Se o arquivo estiver vazio faz a carga com todos os registros, senao apenas com ³
	//³aqueles da integracao com o SIGAEIC                                             ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ

	DbSelectArea("SFC")
	lAchouFil:=.T.
	aAreaSFC:=SFC->(GetArea())
	If !SFC->(DbSeek(xFilial("SFC")))
		lAchouFil:=.F.
	EndIf
    SFC->(RestArea(aAreaSFC))


	If SFC->(reccount()) > 0 .And. lAchouFil
		aRecnoSFC := {}
	EndIf

	For nI := 1 to Len(aRecSFCEIC)
		AADD(aRecnoSFC,aRecSFCEIC[nI])
	Next nI
	If Len(aRecnoSFC) > 0

		DbSelectArea("SFC")
		DbSetOrder(1)

		For nI := 1 to Len(aRecnoSFC)

			If !DbSeek(aRecnoSFC[nI][1]+aRecnoSFC[nI][02] + aRecnoSFC[nI][03])
				RecLock("SFC",.T.)
				Replace FC_FILIAL  With aRecnoSFC[nI][01]
				Replace FC_TES     With aRecnoSFC[nI][02]
				Replace FC_SEQ     With aRecnoSFC[nI][03]
				Replace FC_IMPOSTO With aRecnoSFC[nI][04]
				Replace FC_INCDUPL With aRecnoSFC[nI][05]
				Replace FC_INCNOTA With aRecnoSFC[nI][06]
				Replace FC_CREDITA With aRecnoSFC[nI][07]
				Replace FC_INCIMP  With aRecnoSFC[nI][08]
				Replace FC_BASE    With aRecnoSFC[nI][09]
				Replace FC_CALCULO With aRecnoSFC[nI][10]
				Replace FC_LIQUIDO With aRecnoSFC[nI][11]
				If cPaisLoc $ cAuxPaisL
					Replace FC_INTEIC  With aRecnoSFC[nI][12]
				Endif
				If Len(aRecnoSFC[nI]) >= 13
					Replace FC_AGRBASE  With aRecnoSFC[nI][13]
				Endif
				MsUnLock()
			EndIf
		Next nI
	EndIf
	RestArea( aAreaOld )
Endif

Return(.T.)


