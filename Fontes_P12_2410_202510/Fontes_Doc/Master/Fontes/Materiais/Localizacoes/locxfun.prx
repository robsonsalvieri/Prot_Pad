		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE INSCRIPT","NDE",cImp,"A","E","I"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE NO INSCR","NDE",cImp,"A","E","N"})
