/*/
Function TransfCons( cMovto )
Local lRet 		:= .T.
Local _cLocDest := SCN->CN_LOCDEST
Local aCM

If Empty(_cLocDest)
	_cLocDest := SuperGetMV("MV_PODER3")
EndIf
If Empty(_cLocDest)
	_cLocDest := SA1->A1_LOCCONS
EndIf
If cMovto == "I"
	DbSelectArea("SCN")
	RecLock("SD3",.T.)
	Replace D3_FILIAL       With xFilial("SD3")
	Replace D3_DOC          With SCN->CN_REMITO
	Replace D3_ITEM         With SCN->CN_ITEM
	Replace D3_COD          With SCN->CN_PRODUTO
	Replace D3_UM           With SCN->CN_UM
