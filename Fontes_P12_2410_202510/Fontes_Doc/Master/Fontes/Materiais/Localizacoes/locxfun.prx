	DbSelectArea("SFY")
	DbSetOrder(1)
	If Upper(SuperGetMV("MV_CTRLCOR",,"M"))=="M"
		cPeriodo:=Left(Dtos(dCorrData),6)
	Else
		cPeriodo:=Left(Dtos(dCorrData),4)+"00"
	Endif
	cCorrelat:=Padl(AllTrim(cCorrelat),TamSX3("FY_NUMCOR")[1],"0")
	If MsSeek(xFilial("SFY")+cPeriodo+cCorrelat)
		RecLock("SFY",.F.)
		DbDelete()
		lRet:=.T.
		MsUnlock()
	Else
