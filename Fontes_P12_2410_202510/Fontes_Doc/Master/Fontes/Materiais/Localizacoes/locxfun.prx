		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE INSCRIPT","NF ",cImp,"M","S","I"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE NO INSCR","NF ",cImp,"M","S","N"})
