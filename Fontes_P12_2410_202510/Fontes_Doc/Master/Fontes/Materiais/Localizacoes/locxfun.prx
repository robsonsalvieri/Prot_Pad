Function CRIASED()
	Local aAreaOld   := GetArea()                 // Area atual
	Local aRecnoSED  := {}                        // Dados das naturezas
	Local nI         := 0                         // Controle de loop

	If cPaisLoc == "PER"
		SEDPER( @aRecnoSED)
	EndIf
	If SED->(RecCount()) <= 0
		If Len(aRecnoSED) > 0
			DbSelectArea("SED")
			DbSetOrder(1)
			For nI := 1 to Len(aRecnoSED)
				If !dbSeek(aRecnoSED[nI][1]+aRecnoSED[nI][2])
					If RecLock("SED",.T.)
						Replace ED_FILIAL  With aRecnoSED[nI][01]
						Replace ED_CODIGO  With aRecnoSED[nI][02]
						Replace ED_DESCRIC With aRecnoSED[nI][03]
						MsUnLock()
					EndIf
				EndIf
			Next nI
		Endif
	Endif
	RestArea( aAreaOld )
Return()

