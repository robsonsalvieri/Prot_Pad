Function CriaSFF()
Local aAreaOld   := GetArea()                         // Area atual
Local aRecnoSFF  := {}                                // Dados dos arquivo SFF(impostos)
Local aRecSFFEIC := {}                                // Dados do SFF(impostos) do SIGAEIC
Local aAreaSFF   := {}
Local nI         := 0                                 // Controle de loop
Local cFilSFF    := xFilial("SFF")                    // Filial do arquivo SFF
Local lIntEIC    := ( SuperGetMV("MV_EASY") == "S" ) // Controla se tem integracao com SIGAEIC
Local lNotIMS	 :=	.F.                               // Controla se imposto IMS (Uruguai)
Local lEncontrou := .F.     						  // Controla se encontrou registro no SFF
Local cAuxPaisL  := ""                                // Auxiliar para controle de Pais com AtuSx

dbSelectArea("SFF")
If SFF->(RecCount()) == 0 .or. !SFF->(dbSeek(cFilSFF))

	If cPaisLoc == "ARG"
		SFFARG( @aRecnoSFF , cFilSFF , lIntEIC , @aRecSFFEIC )
	ElseIf cPaisLoc == "EUA"
		SFFEUA( @aRecnoSFF , cFilSFF , lIntEIC , @aRecSFFEIC )
	ElseIf cPaisLoc == "COL"
		SFFCOL( @aRecnoSFF , cFilSFF , lIntEIC , @aRecSFFEIC )
	ElseIf cPaisLoc == "CHI"
		SFFCHI( @aRecnoSFF , cFilSFF , lIntEIC , @aRecSFFEIC )
	ElseIf cPaisLoc == "PAR"
		SFFPAR( @aRecnoSFF , cFilSFF , lIntEIC , @aRecSFFEIC )
	ElseIf cPaisLoc == "URU"
		SFFURU( @aRecnoSFF , cFilSFF , lIntEIC , @aRecSFFEIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "PTG"
		SFFPTG( @aRecnoSFF , cFilSFF , lIntEIC , @aRecSFFEIC )
	ElseIf cPaisLoc == "VEN"
		SFFVEN( @aRecnoSFF , cFilSFF , lIntEIC , @aRecSFFEIC )
	ElseIf cPaisLoc == "BOL"
		SFFBOL()
	EndIf

	For nI := 1 to Len(aRecSFFEIC)
		AADD(aRecnoSFF,aRecSFFEIC[nI])
	Next nI

EndIf

If Len(aRecnoSFF) > 0
	DbSelectArea("SFF")
	DbSetOrder(5)
	If cPaisLoc == "URU"

		lNotIMS	:=	!DbSeek(cFilial+'IMS').And.!DbSeek(cFilial+'BA1')

	Endif
	For nI := 1 to Len(aRecnoSFF)
		lEncontrou  := DbSeek(aRecnoSFF[nI][1]+ aRecnoSFF[nI][21] + aRecnoSFF[nI][10] + aRecnoSFF[nI][11]+ aRecnoSFF[nI][23])
		If cPaisLoc == "PAR"
		   lEncontrou  := DbSeek(aRecnoSFF[nI][1]+ aRecnoSFF[nI][21])
		   //Valida pelos campos Imposto e Item
		   If lEncontrou
		      While !Eof() .And. aRecnoSFF[nI][1]+ aRecnoSFF[nI][21] == SFF->FF_FILIAL+SFF->FF_IMPOSTO
		         If SFF->FF_ITEM <> aRecnoSFF[nI][05]
		            lEncontrou  := .F.
		            DbSkip()
		            Loop
		         EndIf
		         lEncontrou  := .T.
		         Exit
		      End
		   EndIf
		EndIf
		If cPaisLoc == "PTG"
			aAreaSFF := SFF->(GetArea())
			SFF->(dbSetOrder(14))
			lEncontrou  := DbSeek(aRecnoSFF[nI][1]+aRecnoSFF[nI][21]+aRecnoSFF[nI][28]+aRecnoSFF[nI][29])
			RestArea(aAreaSFF)
		Endif
		If cPaisLoc == "COL"
			lEncontrou := .F.
		Endif
		If cPaisloc == "VEN"
			aAreaSFF := SFF->(GetArea())
			SFF->(DBSetOrder(14))//FF_FILIAL+FF_IMPOSTO+FF_REGIAO+FF_GRPPRD
			lEncontrou  := DBSeek(aRecnoSFF[nI][1] + aRecnoSFF[nI][21] + aRecnoSFF[nI][28] + aRecnoSFF[nI][29])
			RestArea(aAreaSFF)
		EndIf
		If lNotIMS .Or. !lEncontrou
			RecLock("SFF",.T.)
			Replace FF_FILIAL  With aRecnoSFF[nI][01]
			Replace FF_ALIQ    With aRecnoSFF[nI][02]
			Replace FF_NUM     With aRecnoSFF[nI][03]
			If cPaisLoc == "ARG"
				Replace FF_MESREF  With aRecnoSFF[nI][04]
			EndIf
			Replace FF_ITEM    With aRecnoSFF[nI][05]
			If cPaisLoc == "ARG"
				Replace FF_ATE     With aRecnoSFF[nI][06]
				Replace FF_ALQINSC With aRecnoSFF[nI][07]
			Endif
			Replace FF_CFO     With aRecnoSFF[nI][08]
			If cPaisLoc == "ARG"
				Replace FF_ALQNOIN With aRecnoSFF[nI][09]
			Endif
			Replace FF_CFO_C   With aRecnoSFF[nI][10]
			Replace FF_CFO_V   With aRecnoSFF[nI][11]
			If cPaisLoc $ "ARG|BOL|CHI|COL|EUA|PAR|PER|URU|VEN"
				Replace FF_IMPORTE With aRecnoSFF[nI][12]
			Endif
			If cPaisLoc $ "ANG|ARG|BOL|EQU|EUA|HAI|PAR|PTG|URU|VEN"
				Replace FF_CONCEPT With aRecnoSFF[nI][13]
			Endif
			If  cPaisLoc $ "ARG|COL|PER"
				Replace FF_FXDE    With aRecnoSFF[nI][14]
			EndIf
			If cPaisLoc == "ARG"
				Replace FF_DESDE   With aRecnoSFF[nI][15]
				Replace FF_FXATE   With aRecnoSFF[nI][16]
			EndIf
			If cPaisLoc $ "ARG|CHI|PAR"
				Replace FF_RETENC  With aRecnoSFF[nI][17]
			Endif
			If cPaisLoc $ "ARG|COL|PER|VEN"
				Replace FF_PERC    With aRecnoSFF[nI][18]
			EndIf
			If cPaisLoc == "ARG"
				Replace FF_EXCEDE  With aRecnoSFF[nI][19]
				Replace FF_ESCALA  With aRecnoSFF[nI][20]
			Endif
			Replace FF_IMPOSTO With aRecnoSFF[nI][21]
			Replace FF_SERIENF With aRecnoSFF[nI][22]
			Replace FF_TIPO    With aRecnoSFF[nI][23]
			Replace FF_ZONFIS  With aRecnoSFF[nI][24]
			If cPaisLoc == "COL"  .And. Len(aRecnoSFF[nI]) > 24
				Replace FF_COD_TAB With aRecnoSFF[nI][25]
			Endif

			cAuxPaisL := "ANG|ARG|AUS|BOL|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"

			If cPaisLoc $ cAuxPaisL .And. Len(aRecnoSFF[nI]) > 25
				Replace FF_TEC With aRecnoSFF[nI][26]
			Endif
			If cPaisLoc $ cAuxPaisL .And. Len(aRecnoSFF[nI]) > 26
				Replace FF_GRUPO With aRecnoSFF[nI][27]
			EndIf
			If cPaisLoc $ cAuxPaisL  .And. Len(aRecnoSFF[nI]) > 27
				Replace FF_REGIAO With aRecnoSFF[nI][28]
			Endif
			If cPaisLoc $ cAuxPaisL .And. Len(aRecnoSFF[nI]) > 27
				Replace FF_GRPPRD With aRecnoSFF[nI][29]
			Endif
			If cPaisloc == "URU" .And. Len(aRecnoSFF[nI]) > 29
				Replace FF_CONCIRP With aRecnoSFF[nI][30]
			Endif
			If cPaisloc == "URU" .And. Len(aRecnoSFF[nI]) > 30
				Replace FF_MOEDA With aRecnoSFF[nI][31]
			Endif
			If cPaisloc == "VEN" .And. Len(aRecnoSFF[nI]) > 29
				Replace FF_EXCEDE With aRecnoSFF[nI][30]
			Endif

			MsUnLock()
		EndIf
	Next nI
EndIf
RestArea( aAreaOld )
Return(.T.)

