			cPeriodo:=Left(Dtos(dCorrData),6)
		Else
			cPeriodo:=Left(Dtos(dCorrData),4)+"00"
		Endif
		cCorrelat:=Padl(AllTrim(cCorrelat),TamSX3("FY_NUMCOR")[1],"0")
		If !Empty(cCorrelat)
			#IFDEF TOP
				cQuery:="select count(*) as NUMCOR from "+RetSqlName("SFY")+" where FY_FILIAL='"+xFilial("SFY")+"'"
				cQuery+=" and FY_PERIODO='"+cPeriodo+"' and FY_NUMCOR='"+cCorrelat+"' and D_E_L_E_T_=' '"
