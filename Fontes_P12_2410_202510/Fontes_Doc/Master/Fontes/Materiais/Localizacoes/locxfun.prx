Static Function ARIMPIB4(aRecno,cTab,cProv)
Local cFilTab := ""
Local cTit	  := ""
Local cImp	  := ""

If cProv = "TU"     //Tucuman
	cTit := "PERC. I.BRUTOS TUCUMAN   "
	cImp := "IB4"
Elseif cProv = "CA" //Catamarca
	cTit := "PERC. I.BRUTOS CATAMARCA "
	cImp := "IB5"
Elseif cProv = "CB" //Chubut
	cTit := "PERC. I.BRUTOS CHUBUT    "
	cImp := "IB6"
Elseif cProv = "CH" //Chaco
	cTit := "PERC. I.BRUTOS CHACO     "
	cImp := "IB7"
Elseif cProv = "CO" //Cordoba
	cTit := "PERC. I.BRUTOS CORDOBA   "
	cImp := "IB8"
Elseif cProv = "ER" //Entre Rios
	cTit := "PERC. I.BRUTOS ENTRE RIOS"
	cImp := "IBA"
Elseif cProv = "FO" //Formosa
	cTit := "PERC. I.BRUTOS FORMOSA   "
	cImp := "IBB"
Elseif cProv = "JU" //Jujuy
	cTit := "PERC. I.BRUTOS JUJUY     "
	cImp := "IBC"
Elseif cProv = "LP" //La Pampa
	cTit := "PERC. I.BRUTOS LA PAMPA  "
	cImp := "IBD"
Elseif cProv = "LR" //La Rioja
	cTit := "PERC. I.BRUTOS LA RIOJA  "
	cImp := "IBE"
Elseif cProv = "ME" //Mendoza
	cTit := "PERC. I.BRUTOS MENDOZA   "
	cImp := "IBF"
Elseif cProv = "MI" //Misiones
	cTit := "PERC. I.BRUTOS MISIONES  "
	cImp := "IBG"
Elseif cProv = "NE" //Neuquen
	cTit := "PERC. I.BRUTOS NEUQUEN   "
	cImp := "IBH"
Elseif cProv = "SA" //Salta
	cTit := "PERC. I.BRUTOS SALTA     "
	cImp := "IBI"
Elseif cProv = "SC" //Santa Cruz
	cTit := "PERC. I.BRUTOS SANTA CRUZ"
	cImp := "IBJ"
Elseif cProv = "SF" //Santa Fe
	cTit := "PERC. I.BRUTOS SANTA FE  "
	cImp := "IBK"
Elseif cProv = "RN" //Rio Negro
	cTit := "PERC. I.BRUTOS RIO NEGRO "
	cImp := "IBL"
Elseif cProv = "SE" //Santiago del Estero
