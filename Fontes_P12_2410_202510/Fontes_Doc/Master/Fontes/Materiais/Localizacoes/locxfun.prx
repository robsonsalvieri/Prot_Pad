Function GetBook( aGetBook	, aImpostoL	, cTipoL	, nTaxaL	,;
				  aLivroF	, cModo		, lSoma		, lGlobal 	,;
                  dDtEmissa	, cSerieF2 , lUltimoItem	)

Local   aLivro   := {}
Local   cAuxPaisL := ""
Private aImposto := aClone( aImpostoL ) // Estas Variaveis DEVEM SER PRIVATES
Private aLivroL  := aClone( aLivroF ) // por causa do uso de macro
Private nTaxa    := nTaxaL // no execblock .

Default lSoma       := .T.
Default lGlobal     := .F.
Default cSerieF2    := ""  // Somente e utilizada quando emitido fatura global (Mexico)
Default lUltimoItem := .F. // Utilizado para arredondamento no Chile
Default dDtEmissa := IIf(Type("DDEMISSAO")!="U",DDEMISSAO,dDataBase)

aGetBook := {}
cAuxPaisL := "ANG|ARG|AUS|BOL|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"
If (Iif(cPaisLoc $ cAuxPaisL ,SF4->F4_GERALF<>"2",.F.)) .AND. !SF4->(EOF())	// Livro deve ser gerado ? ( "1"-Gera Livro, "2"-Nao Gera ... )
   If cModo == "S"
		aLivro := M460Livr( aImposto ,aLivroL ,nTaxa ,cTipoL ,If(lSoma,1,-1) ,dDtEmissa ,cSerieF2 ,lUltimoItem )
   Else
		aLivro := M100Livr( aImposto ,aLivroL ,nTaxa ,cTipoL ,If(lSoma,1,-1) ,dDtEmissa )
   EndIf
ElseIf lGlobal  //Se esta gerando Fatura Global(rotina LOJA480) deve gerar registro de Livro Fiscal(Loc. Mexico)
   aLivro := M460Livr( aImposto ,aLivroL ,nTaxa ,cTipoL ,If(lSoma,1,-1) ,dDtEmissa ,cSerieF2 )
Else
   aLivro := aClone( aLivroF )
EndIf

Return( aLivro )

