		AAdd(aRecno,{cFilTab,cTit,"S","                    ","NDC",cImp,"M","S","N"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE INSCRIPT","NDI",cImp,"M","S","I"})
