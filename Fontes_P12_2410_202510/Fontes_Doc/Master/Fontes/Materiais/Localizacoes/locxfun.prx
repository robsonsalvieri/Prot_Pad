		AAdd(aRecno,{cFilTab,  1.50,"000001","    ","  ","    ",  0.00,"   ",  0.00,"180","680",          300.00,"ALQUILERES                                                                                                                                            ",            0.00,"0198",            0.00,            0.00,  0.00,            0.00," ",cImp,"   ","*",cProv,"   ","                ","   "})
		AAdd(aRecno,{cFilTab,  1.50,"000001","    ","  ","    ",  0.00,"   ",  0.00,"185","685",          300.00,"SERVICIOS PUBLICOS                                                                                                                                    ",            0.00,"0198",            0.00,            0.00,  0.00,            0.00," ",cImp,"   ","*",cProv,"   ","                ","   "})
	Case cTab == "SFZ"
		cFilTab := xFilial("SFZ")
