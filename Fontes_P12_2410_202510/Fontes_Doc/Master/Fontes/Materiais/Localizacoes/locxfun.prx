Function LjValTroco( aMoedas )
Local lRet   := .T.
Local lErro  := .F.
Local nY
Local nDecs

For nY := 1 to Len(aMoedas)
	nDecs := MsDecimais(nY)
	If Round(aMoedas[nY][2],nDecs) > 0
		lErro := .T.
		Exit
	Endif
Next nY
lRet := IIf(lErro,MsgYesNo(STR0010),.T.)  //"Existe uma diferenca no troco. Continua?"
Return(lRet)
