Function LjCalcTrc( cRotina, nValRec, nVlrBruto, oFolder,;
					aMoedas, aTroco , aPosObj  , aDadosJur)
//                   Valores da Variavel cRotina                   //
//-----------------------------------------------------------------//
// FRONT  - Indica que a rotina esta sendo chamada do Front Office //
// BALCAO - Indica que a rotina esta sendo chamada da Venda Balcao //
// RAPIDA - Indica que a rotina esta sendo chamada da Venda Rapida //
/////////////////////////////////////////////////////////////////////
//                Descricao do Array aTroco                      //
//                Venda Rapida/Front Office                      //
//---------------------------------------------------------------//
// 01 - Valor que deve ser dado como troco                       //
// 02 - Troco efetivamente dado pelo usuario                     //
// 03 - Diferenca entre o troco e o que efetivamente foi dado    //
// 04 - Moeda                                                    //
///////////////////////////////////////////////////////////////////
//                 Descricao do Array aObjs                      //
//---------------------------------------------------------------//
// 01 - Objeto de display do nome da moeda                       //
// 02 - Nome da moeda                                            //
// 03 - Objeto de display do saldo do troco a ser dado           //
// 04 - Saldo do troco a ser dado                                //
// 05 - Objeto de Get do troco informado pelo usuario            //
// 06 - Troco informado pelo usuario                             //
// 07 - Moeda                                                    //
///////////////////////////////////////////////////////////////////
//                  Descricao do Array aMoedas                   //
//              Chamada da Venda Rapida/Front Office             //
//---------------------------------------------------------------//
// 01 - Descricao da moeda                                       //
// 02 - Simbolo da moeda                                         //
// 03 - Taxa da moeda                                            //
// 04 - Qtde. de casas decimais da moeda                         //
// 05 - Moeda                                                    //
///////////////////////////////////////////////////////////////////

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³                 ****** Descric„o do Array aMoedas *******              ³
//³                          Chamada da Venda Balcao                       ³
//ÃÄÄÄÄÄÄÄÄÄÄÄÄÂÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´
//³ Dimensoes  ³ Descric„o						 					       ³
//ÃÄÄÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´
//³ 	  1	   ³ Nome da Moeda		     		   						   ³
//³ 	  2	   ³ Saldo do Troco					 						   ³
//³ 	  3	   ³ Troco ( usuario vai informar )	  						   ³
//³ 	  4	   ³ Somatorio do troco convertido para a moeda correspondente ³
//³ 	  5	   ³ Contem o valor original do troco (backup)      		   ³
//³ 	  6	   ³ Moeda                                           		   ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
Local nTroco    := 0
Local nTmpTroco := 0
Local aObjs     := {}
Local aTrocoBkp := {}
Local oDlgTroco
Local oTraco
Local cTraco	:= Replicate("-",55)
Local oMensagem
Local cMensagem := ""
Local nX		:= 0
Local nI		:= 0
Local nLin		:= 001
Local nLinFim	:= 150
Local aTamCpos 	:= {}
Local nTam     	:= 0
Local aCabGrid  := {STR0006,STR0008,STR0007} // Moedas ### Saldo ### Troco
Local aTamGrid  := {}
Local cMoneda   := ""
DEFAULT aPosObj 	:= {}
DEFAULT aDadosJur	:= {0,0,0,0,0,0}

If Type("nMoedaCor")=="U"
   nMoedaCor  := 1
EndIf
If aMoedas == Nil  //Venda Rapida e Front
	aMoedas := {{SuperGetMV("MV_MOEDA"+AllTrim(Str(nMoedaCor))),;
                    SuperGetMV("MV_SIMB"+AllTrim(Str(nMoedaCor))),;
				    RecMoeda(dDataBase,nMoedaCor),;
				    MsDecimais(nMoedaCor),;
				    nMoedaCor}}
EndIf
If aTroco == Nil  //Venda Rapida e Front
   If nValRec > 0
	  nTroco := Round(nValRec - ((nVlrBruto-aDadosJur[6])+aDadosJur[4]+aDadosJur[1]),aMoedas[1][4])
   EndIf
   aTroco := {{nTroco,0,nTroco,nMoedaCor}}
EndIf
If cRotina == "FRONT"
	cMensagem := STR0023 //" Front Loja "
ElseIf cRotina == "RAPIDA"
	cMensagem := STR0024  //" SigaLoja - Venda Rapida "
ElseIf cRotina == "ASSISTIDA"
	cMensagem := STR0103  //" SigaLoja - Venda Assistida "
Else
	cMensagem := STR0025 //" SigaLoja - Venda Balcao "
EndIf
If (cRotina == "RAPIDA" .or. cRotina == "FRONT") .And. nTroco > 0
	AAdd(aObjs,{Nil,SuperGetMV("MV_MOEDA"+AllTrim(Str(nMoedaCor))),Nil,nTroco,Nil,0,nMoedaCor})
	For nI := 1 To MoedFin()
		If nI == nMoedaCor
			Loop
		EndIf
		If RecMoeda(dDataBase,nI) > 0 // So alimenta o array se a moeda tiver com a taxa preenchida para o dia
			cMoneda := SuperGetMV("MV_MOEDA"+Alltrim(STR(nI)))
			If !Empty(cMoneda)
				AAdd(aMoedas,{cMoneda, SuperGetMV("MV_SIMB"+AllTrim(Str(nI))),;
				RecMoeda(dDataBase,nI),MsDecimais(nI),nI})
				nTmpTroco:= Round(xMoeda(nTroco,nMoedaCor,nI,dDataBase,aMoedas[nI][4]+1,aMoedas[1][3],aMoedas[nI][3]),aMoedas[nI][4])
				AAdd(aObjs,{Nil,cMoneda,Nil,nTmpTroco,Nil,0,nI})
				AAdd(aTroco,{nTmpTroco,0,nTmpTroco,nI})
				nLinFim += 50
			Endif
		Endif
	Next nI
	//Copia do array de troco original. Caso o usuario cancele a rotina esse
	//array sera retornado para a rotina que esta "chamando" a funcao...
	aTrocoBkp := aClone(aTroco)
	DEFINE FONT oFntMda	NAME "Times New Roman" SIZE 07,11 BOLD
	DEFINE FONT oFntMsg NAME "Times New Roman" SIZE 11,40 BOLD
	DEFINE FONT oFntVal	NAME "Times New Roman" SIZE 10,30
	DEFINE FONT oFntGet	NAME "Times New Roman" SIZE 10,30 BOLD
	oDlgTroco:=MSDialog():New(10,05,nLinFim,550,STR0026,,,,,CLR_BLACK,CLR_WHITE,,,.T.) //"Calculo de Troco"
	For nI := 1 To Len(aMoedas)
		LjMontaTela(nI,@nLin,@aObjs,cTraco,@aTroco,aMoedas)
	Next nI
	nLin--
	// Area de Mensagens
	@ nLin,000 SAY oMensagem VAR cMensagem FONT oFntMsg PIXEL SIZE 282,20 COLOR CLR_BLACK,CLR_WHITE
	nLin += 10
	DEFINE SBUTTON FROM nLin,210 TYPE 1 ACTION oDlgTroco:End() ENABLE OF oDlgTroco
	DEFINE SBUTTON FROM nLin,240 TYPE 2 ACTION IIf(CancTroco(@aTroco,aTrocoBkp),(aTroco:=.F.,oDlgTroco:End()),.F.) ENABLE OF oDlgTroco
	oDlgTroco:Activate(,,,.T.,{||ValidTrc(aObjs,aTroco,cRotina)})
ElseIf (cRotina == "BALCAO" .Or. cRotina == "ASSISTIDA")
	AAdd(aMoedas,{SuperGetMV("MV_MOEDA"+AllTrim(Str(nMoedaCor))),0.00,0.00,0.00,0.00,nMoedaCor})
	For nX := 1 To MoedFin()
		If nX == nMoedaCor
			Loop
		EndIf
		If RecMoeda(dDataBase,nX) > 0
			nTam := Iif(TamSx3("L1_TROCO"+AllTrim(Str(nX)))[1]>nTam,;
			TamSx3("L1_TROCO"+AllTrim(Str(nX)))[1],nTam)
			cMoneda := SuperGetMV("MV_MOEDA"+Alltrim(STR(nX)))
			If !Empty(cMoneda)
				AAdd(aMoedas,{cMoneda,0.00,0.00,0.00,0.00,nX})
			Endif
		Endif
	Next nX
	cTitulo := aCabGrid[1]
	AAdd(aTamGrid,Iif(nTam > Len(Trim(cTitulo)),nTam*4.1,Len(Trim(cTitulo))*4.1))
	nTam    := TamSx3("L1_TROCO1")[1]
	cTitulo := aCabGrid[2]
	AAdd(aTamGrid,Iif(nTam > Len(Trim(cTitulo)),nTam*4.1,Len(Trim(cTitulo))*4.1))
	cTitulo := aCabGrid[3]
	AAdd(aTamGrid,Iif(nTam > Len(Trim(cTitulo)),nTam*4.1,Len(Trim(cTitulo))*4.1))
	If cRotina == "BALCAO"
		oLBBaixa := TwBrowse():New(087,007,000,000,,aCabGrid,aTamGrid,oFolder:aDialogs[3],,,,,,,,,,,,.F.,,.T.,,.F.,,,)
	Else
		oLBBaixa := TCBrowse():New( aPosObj[2,3]-aPosObj[2,1]-45, aPosObj[2,2], aPosObj[2,4]-aPosObj[2,2], 0,;
							 	    Nil, aCabGrid, aTamGrid, oFolder, Nil, Nil, Nil, Nil,,,,,,,,.F.,, .T.,,.T.,,.F. )
	EndIf
	oLBBaixa:lHScroll := .T.
	oLBBaixa:lVScroll := .T.
	If cRotina == "BALCAO"
		oLBBaixa:nHeight  := 088
		oLBBaixa:nWidth   := 200
	Else
		oLBBaixa:nHeight  := 075
	EndIf
	oLBBaixa:SetArray(aMoedas)
	oLBBaixa:bLine      := {|| {aMoedas[oLBBaixa:nAT][1],Transform(aMoedas[oLBBaixa:nAT][2],PesqPict("SL1","L1_TROCO1",18,oLBBaixa:nAT)) ,Transform(aMoedas[oLBBaixa:nAT][3],PesqPict("SL1","L1_TROCO1",18,oLBBaixa:nAT)) }}
	oLBBaixa:bLDblClick := {|| DCLkGrid(@aMoedas,@aTroco)}
EndIf
Return aTroco
/*
