/*/
Function CalcTroco( aMoedas, nPos, aTroco )
Local nDif 		 := 0    					// Diferenca do troco
Local nDiferenca := 0						// Armazena as diferencas entre valr cheio e somatoria do troco
Local nDecimais  := 0                       // Numero de casas decimais
Local lZeraTudo  := .F.						// Controla se ira' zerar todas as posicoes do array aMoedas
Local nY		 := 0						// Controle de loop
Local nVlrDigit  := 0						// Somatoria do troco ja digitada
Local nDecs 	 := 0						// Numero de casa decimais, auxiliar
Local nMvCent    := SuperGetMV("MV_CENT")   // Numero de casas decimais
Local nMvCentX   := 0                       // Numero de casas decimais

nDif := aMoedas[nPos][3] - aTroco[nPos]  // Armazena a diferenca do troco ( Do que esta sendo digitado com o que estava no grid )
aMoedas[nPos][4] += nDif
nVlrDigit := aMoedas[nPos][4]  // Recebe o somatorio do troco que o usuario ja digitou
For nY := 1 To Len(aMoedas)
    nDecs := MsDecimais(nY)
    aMoedas[nY][4] := Round(xMoeda(nVlrDigit,aMoedas[nPos][6],aMoedas[nY][6],dDataBase,nDecs+1),nDecs) // Converte o somatorio do troco digitado pelo usuario da moeda escolhida para a moeda correspondente
    nDecimais  := nMvCent
	If nY > 1
		nMvCentX  := SuperGetMV("MV_CENT"+Alltrim(Str(nY)))
		nDecimais := nMvCentX
	EndIf
    If aMoedas[nPos][3] <> 0
	    nDiferenca      := (aMoedas[nY][5] - aMoedas[nY][4])
		If nDiferenca <> 0
		    If Abs(nDiferenca) <= 1/(10**nDecimais)
		       aMoedas[nY][4]  := aMoedas[nY][5]
	        Endif
		Endif
	Else
		nDiferenca := (aMoedas[nY][4] - aMoedas[nY][3])
		If nDiferenca <> 0
		    If Abs(nDiferenca) <= 1/(10**nDecimais)
		       aMoedas[nY][4]  := aMoedas[nY][3]
	        Endif
		Endif
	Endif
    If aMoedas[nY][5] - aMoedas[nY][4] < 0  // Troco digitado e maior que o valor original
    	If aMoedas[nPos][4] - aMoedas[nPos][5] >= 1/(10**nDecimais)
    	    // 1/(10**nDecimais) -> Tolerancia de arredondamento
   			// Se a diferenca entre o troco e o saldo for maior que a tolerancia 1/(10**nDecimais)
			// entao exibe mensagem que troco e maior
			MsgStop(STR0009)                 // "O troco e maior que o saldo."
			aMoedas[nPos][4] -= nDif
			aMoedas[nPos][3]:= aTroco[nPos] // Volta o valor que estava no grid (backup)
			Return
		Endif
    Else
		aMoedas[nY][2] := aMoedas[nY][5] - aMoedas[nY][4]   // Recalcula o saldo ( Valor original - Somatorio do valor informado )
		If aMoedas[nY][2] > 0
			If aMoedas[nY][2] < 1/(10**nDecimais)
				lZeraTudo := .T.
			Endif
		Endif
	Endif
	aTroco[nY]:= aMoedas[nY][3] // Guarda o valor do troco digitado pelo usuario
Next nY
For nY := 1 To Len(aMoedas)                   // Se o saldo de qualquer moeda estiver
	If aMoedas[nY][2] < 1/(10**nDecimais)   // menor a tolerancia de decimais,
		lZeraTudo := .T.                     // entao atribui .T. para lZeraTudo
		Exit
	Endif
Next nY
If lZeraTudo
	For nY :=1 To Len(aMoedas)   // Vai zerar a coluna de saldos do grid
		aMoedas[nY][2] := 0
	Next nY
Endif
oLBBaixa:Refresh()
Return(.T.)
