*/
Function EstruTabela(cTabela,lContr)
Local cAliasDest := ""
Local cQuery     := ""
Local aStruTab   := {}
Local aStruTabRet:= {}
Local aArea      := GetArea()
Local aCamposCont:= {"D_E_L_E_T_","R_E_C_D_E_L_","R_E_C_N_O_"}
Local nPos       := 0
lOCAL nX         := 0
Default lContr   := .F.

DbSelectArea("SX2")
DbSetOrder(1)

If  dbSeek(Trim(cTabela))
	//Tabela existe no Dicionario
	DbSelectArea(cTabela)
	aStruTab:=dbStruct()
Else
	//Tabela Fisica - Nao existe no Dicionario
	If TCCANOPEN(cTabela)
		cQuery:="Select * FROM "+cTabela+" Where 1=2"
		cAliasDest := GetNextAlias()
		cQuery     := ChangeQuery(cQuery)
		DbUseArea(.T., 'TOPCONN', TcGenQry(,,cQuery), cAliasDest, .T., .F.)

		DbSelectArea(cAliasDest)
		aStruTab:=dbStruct()
		DbCloseArea()
	EndIf
EndIf

//Elimina campos de controle do array
If lContr
	For nX=1 to Len(aStruTab)
		 nPos:=Ascan(aCamposCont,{|X| AllTrim(X) ==  aStruTab[nX][1]})
		 If nPos==0
		 	aAdd(aStruTabRet,{aStruTab[nX][1],aStruTab[nX][2],aStruTab[nX][3],aStruTab[nX][4]})
		 EndIf
	Next nX
Else
	aStruTabRet:=AClone(aStruTab)
EndIf

RestArea(aArea)
Return aStruTabRet

