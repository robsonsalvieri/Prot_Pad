For nX := 1 to nOpcX
	AADD(aRotina,{"","",0,nOpcx})
Next nX
oObj  := MsGetDb():New(nT,nL,nB,nR,nOpcX,cLinhaOk,cTudoOk,cIniCpos,lDeleta,aAlter,nFreeze,lEmpty,nMax,cTrb,cFieldOk,lCondicional,lAppend)
