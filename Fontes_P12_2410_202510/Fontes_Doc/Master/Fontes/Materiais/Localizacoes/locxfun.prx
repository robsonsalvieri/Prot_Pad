Function CriaSFB()

Local aAreaOld   := GetArea()                 // Area atual
Local aRecnoSFB  := {}                        // Dados dos impostos
Local aRecSFBEIC := {}                        // Dados dos impostos do SIGAEIC
Local nI         := 0                         // Controle de loop
Local cFilSFB    := xFilial("SFB")            // Filial do arquivo SFB
Local cCpoLvro   := ""                        // Campo de indice para gravacao dos impostos
Local lIntEIC    := ( SuperGetMV("MV_EASY")="S" ) // Controla se tem integracao com SIGAEIC
Local lAchouFil	 :=.T.
Local aAreaSFB	 := {}
Local cAuxPaisL  := ""                        // Axiliar para verificar se o campo esta criando para o Pais no AtuSx

If nModulo <> 23
	If cPaisLoc == "ARG"
		SFBARG( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "CHI"
		SFBCHI( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
		lAddSFB:=.T.
	ElseIf cPaisLoc == "EUA"
		SFBEUA( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "MEX"
		SFBMEX( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "PAR"
		SFBPAR( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "POR"
		SFBPOR( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "COL"
		SFBCOL( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "URU"
		SFBURU( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "VEN"
		SFBVEN( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "DOM"
		SFBDOM( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "PER"
		SFBPER( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "COS"
		SFBCOS( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "SAL"
		SFBSAL( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "GUA"
		SFBGUA( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "BOL"
		SFBBOL( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "PTG"
		SFBPTG( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "ANG"
		SFBANG( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "EQU"
		SFBEQU( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	ElseIf cPaisLoc == "AUS"
		SFBAUS( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC ) // Movido para LOCXFUNA
	ElseIf cPaisLoc == "RUS"
		SFBRUS( @aRecnoSFB , cFilSFB , lIntEIC , @aRecSFBEIC )
	EndIf
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³Se o arquivo estiver vazio faz a carga com todos os registros, senao apenas com ³
	//³aqueles da integracao com o SIGAEIC                                             ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ

	DbSelectArea("SFB")
	lAchouFil:=.T.
	aAreaSFB:=SFB->(GetArea())
	If !SFB->(DbSeek(xFilial("SFB")))
		lAchouFil:=.F.
	EndIf
    SFB->(RestArea(aAreaSFB))

	If SFB->(RecCount()) > 0 .And. lAchouFil
		While !SFB->(Eof())
			If Empty(SFB->FB_CPOLVRO)
				If !Empty(SFB->FB_CPOVREI)
					cCpolvro := Right(Alltrim(SFB->FB_CPOVREI),1)
					RecLock("SFB",.F.)
					Replace FB_CPOLVRO With cCpolvro
					SFB->(MsUnLock())
				Endif
			Endif
			SFB->(DbSkip())
		End
		aRecnoSFB := {}
	EndIf

	For nI := 1 to Len(aRecSFBEIC)
		AADD(aRecnoSFB,aRecSFBEIC[nI])
	Next nI

	If Len(aRecnoSFB) > 0

		DbSelectArea("SFB")
		DbSetOrder(1)

		For nI := 1 to Len(aRecnoSFB)

			If !DbSeek(aRecnoSFB[nI][1]+aRecnoSFB[nI][2])
				RecLock("SFB",.T.)
				Replace FB_FILIAL  With aRecnoSFB[nI][01]
				Replace FB_CODIGO  With aRecnoSFB[nI][02]
				Replace FB_DESCR   With aRecnoSFB[nI][03]
				Replace FB_CPOLVRO With aRecnoSFB[nI][04]
				Replace FB_FORMENT With aRecnoSFB[nI][05]
				Replace FB_FORMSAI With aRecnoSFB[nI][06]
				Replace FB_COLBAS  With aRecnoSFB[nI][07]
				Replace FB_COLVAL  With aRecnoSFB[nI][08]
				Replace FB_ALIQ    With aRecnoSFB[nI][09]
				If Len(aRecnoSFB[nI])  > 09
					Replace FB_DIAVENC With aRecnoSFB[nI][10]
				Endif
				If Len(aRecnoSFB[nI])  > 10
					Replace FB_PERIODO With aRecnoSFB[nI][11]
				Endif
				If Len(aRecnoSFB[nI])  > 11
					Replace FB_JNS     With aRecnoSFB[nI][12]
				Endif
				cAuxPaisL := "ANG|ARG|AUS|BOL|BRA|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|POR|PTG|SAL|URU|VEN"
				If cPaisloc $ cAuxPaisL   .And. Len(aRecnoSFB[nI])  > 12
					Replace FB_TABELA  With aRecnoSFB[nI][13]
				Endif
				cAuxPaisL := "ANG|ARG|AUS|BOL|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"
				If cPaisloc $ cAuxPaisL .And. Len(aRecnoSFB[nI])  > 13
					Replace FB_INTEIC  With aRecnoSFB[nI][14]
				EndIf
				cAuxPaisL := "ANG|ARG|AUS|BOL|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"
				If cPaisloc $ cAuxPaisL .And. Len(aRecnoSFB[nI])  > 14
					Replace FB_NATUREZ  With aRecnoSFB[nI][15]
				EndIf
				If cPaisLoc $ "EQU|CHI"  .And. Len(aRecnoSFB[nI])  > 15
					Replace FB_CODRET  With aRecnoSFB[nI][16]
				Endif
				If cPaisLoc=="VEN" .And. Len(aRecnoSFB[nI])  > 16
					Replace FB_ALIQRD   With aRecnoSFB[nI][17]
				Endif
				If cPaisLoc=="VEN" .And. Len(aRecnoSFB[nI])  > 17
					Replace FB_ALIQAD   With aRecnoSFB[nI][18]
				Endif
				If cPaisLoc=="VEN" .And. Len(aRecnoSFB[nI])  > 18
					Replace FB_FORMA	With aRecnoSFB[nI][19]
				Endif
				If cPaisLoc=="VEN" .And. Len(aRecnoSFB[nI])  > 19
					Replace FB_MINRET	With aRecnoSFB[nI][20]
				Endif
				If cPaisLoc $ "COS|URU|PER" .And. Len(aRecnoSFB[nI])  > 19
					Replace FB_GERTIT	With aRecnoSFB[nI][21]
				Endif
				MsUnLock()
			EndIf
		Next nI
	Endif
Endif
RestArea( aAreaOld )

Return .T.

