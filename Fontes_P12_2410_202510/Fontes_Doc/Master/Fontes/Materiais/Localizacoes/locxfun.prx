Function CORR_DOC( dCorrData, cCorrelat )
Local aArea 	:= GetArea()
Local aRet		:= {}
Local cFil		:= ""
Local cPeriodo	:= ""
Local nPFY_FORN	:= 0
Local nPFY_CLIEF	:= 0

If CORR_USO()
	DbSelectArea("SFY")
	DbSetOrder(1)
	If Upper(SuperGetMV("MV_CTRLCOR",,"M"))=="M"
		cPeriodo:=Left(Dtos(dCorrData),6)
	Else
		cPeriodo:=Left(Dtos(dCorrData),4)+"00"
	Endif
	cFil:=xFilial("SFY")
	cCorrelat:=Padl(AllTrim(cCorrelat),TamSX3("FY_NUMCOR")[1],"0")
	MsSeek(cFil+cPeriodo+cCorrelat)
	nPFY_FORN := IIf(cPaisLoc == "CHI",1,0)
	nPFY_CLIEF:= IIf(cPaisLoc == "CHI",1,0)
	While FY_FILIAL==cFil .And. FY_PERIODO==cPeriodo .And. !Eof()
		Aadd(aRet,{FY_DOC,FY_SERIE,FY_ESPECIE,If(nPFY_FORN > 0,FY_FORNECE,If(nPFY_CLIEF > 0,FY_CLIEFOR,"")),FY_LOJA})
		DbSkip()
	End
Endif
RestArea(aArea)
Return(aRet)
