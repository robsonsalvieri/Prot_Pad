ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß*/
Function RcFatMex(lSigaLoja)
Local aArea			:= GetArea()
Local aTotal1		:= {}
Local aTotal2		:= {}
Local aTotal3		:= {}
Local aTotal4		:= {}
Local aTotal5		:= {}
Local aTotal6		:= {}
Local aBase1		:= {}
Local aBase2		:= {}
Local aBase3		:= {}
Local aBase4		:= {}
Local aBase5		:= {}
Local aBase6		:= {}
Local cSerie		:= SF2->F2_SERIE
Local cNumero		:= SF2->F2_DOC
Local cCliente		:= SF2->F2_CLIENTE
Local cLoja			:= SF2->F2_LOJA
Local cPrefix		:= SF2->F2_PREFIXO
Local cChaveSE1		:= xFilial("SE1")+cNumero+cPrefix
Local cChaveSE2		:= xFilial("SE2")+cCliente+cLoja+cNumero+cPrefix
Local lExistE1		:= cPaisLoc $ "ANG|COL|EQU|HAI|MEX|PER|PTG"
Local lExistE2		:= cPaisLoc $ "ANG|COL|EQU|HAI|MEX|PER|PTG"
Local lVerCpoE1		:= cPaisLoc $ "ANG|COL|EQU|HAI|MEX|PER|PTG"
Local lVerCpoE2		:= cPaisLoc $ "ANG|COL|EQU|HAI|MEX|PER|PTG"
Local nValImp1		:= SF2->F2_VALIMP1   // Busca valor generado en la facturacion
Local nValImp2		:= SF2->F2_VALIMP2   // Busca valor generado en la facturacion
Local nValImp3		:= SF2->F2_VALIMP3   // Busca valor generado en la facturacion
Local nValImp4		:= SF2->F2_VALIMP4   // Busca valor generado en la facturacion
Local nValImp5		:= SF2->F2_VALIMP5   // Busca valor generado en la facturacion
Local nValImp6		:= SF2->F2_VALIMP6   // Busca valor generado en la facturacion
Local nConta		:= 0
Local nVlrNcc   	:= 0 				 // Valor da NCC
Local lVerifica 	:= .T.              // Verificação para se caso seja Modulo Front ou Sigaloja verifique Mv_Crneg
Local aAlicuotas	:= {}

Default lSigaLoja 	:= .F.

//Somente efetua o processamento se os campos do E1 e E2 para lançamento dos impostos variaveis existirem.
If lExistE1 .And. lExistE2

	// No estoy tratando IVA con retencion. Tendra que prever.
	aTotal1 := Condicao(nValImp1,SF2->F2_COND) // Genera el IVA igual em todas as parcelas
	aTotal2 := Condicao(nValImp2,SF2->F2_COND) // Genera el IVA igual em todas as parcelas
	aTotal3 := Condicao(nValImp3,SF2->F2_COND) // Genera el IVA igual em todas as parcelas
	aTotal4 := Condicao(nValImp4,SF2->F2_COND) // Genera el IVA igual em todas as parcelas
	aTotal5 := Condicao(nValImp5,SF2->F2_COND) // Genera el IVA igual em todas as parcelas
	aTotal6 := Condicao(nValImp6,SF2->F2_COND) // Genera el IVA igual em todas as parcelas

	aBase1 := Condicao(SF2->F2_BASIMP1,SF2->F2_COND) // Genera el Base IVA igual em todas as parcelas
	aBase2 := Condicao(SF2->F2_BASIMP2,SF2->F2_COND) // Genera el Base IVA igual em todas as parcelas
	aBase3 := Condicao(SF2->F2_BASIMP3,SF2->F2_COND) // Genera el Base IVA igual em todas as parcelas
	aBase4 := Condicao(SF2->F2_BASIMP4,SF2->F2_COND) // Genera el Base IVA igual em todas as parcelas
	aBase5 := Condicao(SF2->F2_BASIMP5,SF2->F2_COND) // Genera el Base IVA igual em todas as parcelas
	aBase6 := Condicao(SF2->F2_BASIMP6,SF2->F2_COND) // Genera el Base IVA igual em todas as parcelas

	aAlicuotas := LxGetAlic("SF2", cPrefix, cNumero, cCliente, cLoja, SF2->F2_ESPECIE)

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³Como possui uma condicao de pagamento nao calcula imposto para NCC³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	If lSigaLoja
		SL1->(DbSetOrder(2)) //L1_FILIAL+L1_NUM
		If SL1->(DbSeek(xFilial("SL1")+ SF2->F2_SERIE + SF2->F2_DOC))
			nVlrNcc := SL1->L1_CREDITO
			If nVlrNcc > 0
				If Len(aTotal1) > 0
					aAdd(aTotal1,{dDatabase,0})
					aAdd(aBase1,{dDatabase,0})
				ElseIf Len(aTotal2) > 0
					aAdd(aTotal2,{dDatabase,0})
					aAdd(aBase2,{dDatabase,0})
				ElseIf Len(aTotal3) > 0
					aAdd(aTotal3,{dDatabase,0})
					aAdd(aBase3,{dDatabase,0})
				ElseIf Len(aTotal4) > 0
					aAdd(aTotal4,{dDatabase,0})
					aAdd(aBase4,{dDatabase,0})
				ElseIf Len(aTotal5) > 0
					aAdd(aTotal5,{dDatabase,0})
					aAdd(aBase5,{dDatabase,0})
				ElseIf Len(aTotal6) > 0
					aAdd(aTotal6,{dDatabase,0})
					aAdd(aBase6,{dDatabase,0})
				EndIf
			EndIf
		EndIf
	EndIf

	If SF2->F2_TIPO $ "DB" // Solmiente ejetuca se for "D" o "B"
		DBSelectArea("SE2")
	    cChaveSE2	:= xFilial("SE2")+cCliente+cLoja+cPrefix+cNumero //Projeto Chave Unica - Tiago Silva

		SE2->(DBSetOrder(6)) //E2_FILIAL+E2_FORNECE+E2_LOJA+E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO
		SE2->(DBSeek(xFilial("SE2")+cCliente+cLoja+cPrefix+cNumero))

		nConta := 1

		While !Eof() .And. E2_FILIAL+E2_FORNECE+E2_LOJA+E2_PREFIXO+E2_NUM == cChaveSE2
			If SF2->F2_ESPECIE <> E2_TIPO
				DbSkip()
				Loop
			Endif

			RecLock("SE2",.F.)

			//aTotal1 y aBase1
			If Len(aTotal1) > 0
				SE2->E2_VALIMP1 := aTotal1[nConta,2]
				If Len(aBase1) >= nConta
					SE2->E2_ALQIMP1 := LxValAlqIm(aAlicuotas, "1")
				EndIf
			Endif
			If lVerCpoE2 .And. Len(aBase1) >= nConta
				SE2->E2_BASIMP1 := aBase1[nConta,2]
			EndIf

			//aTotal2 y aBase2
			If Len(aTotal2) > 0
				SE2->E2_VALIMP2 := aTotal2[nConta,2]
				If Len(aBase2) >= nConta
					SE2->E2_ALQIMP2 := LxValAlqIm(aAlicuotas, "2")
				EndIf
			Endif
			If lVerCpoE2 .And. Len(aBase2) >= nConta
				SE2->E2_BASIMP2 := aBase2[nConta,2]
			EndIf

			//aTotal3 y aBase3
			If Len(aTotal3) > 0
				SE2->E2_VALIMP3 := aTotal3[nConta,2]
				If Len(aBase3) >= nConta
					SE2->E2_ALQIMP3 := LxValAlqIm(aAlicuotas, "3")
				EndIf
			Endif
			If lVerCpoE2 .And. Len(aBase3) >= nConta
				SE2->E2_BASIMP3 := aBase3[nConta,2]
			EndIf

			//aTotal4 y aBase4
			If Len(aTotal4) > 0
				SE2->E2_VALIMP4 := aTotal4[nConta,2]
				If Len(aBase4) >= nConta
					SE2->E2_ALQIMP4 := LxValAlqIm(aAlicuotas, "4")
				EndIf
			Endif
			If lVerCpoE2 .And. Len(aBase4) >= nConta
				SE2->E2_BASIMP4 := aBase4[nConta,2]
			EndIf

			//aTotal5 y aBase5
			If Len(aTotal5) > 0
				SE2->E2_VALIMP5 := aTotal5[nConta,2]
				If Len(aBase5) >= nConta
					SE2->E2_ALQIMP5 := LxValAlqIm(aAlicuotas, "5")
				EndIf
			Endif
			If lVerCpoE2 .And. Len(aBase5) >= nConta
				SE2->E2_BASIMP5 := aBase5[nConta,2]
			EndIf

			//aTotal6 y aBase6
			If Len(aTotal6) > 0
				SE2->E2_VALIMP6 := aTotal6[nConta,2]
				If Len(aBase6) >= nConta
					SE2->E2_ALQIMP6 := LxValAlqIm(aAlicuotas, "6")
				EndIf
			Endif
			If lVerCpoE2 .And. Len(aBase6) >= nConta
				SE2->E2_BASIMP6 := aBase6[nConta,2]
			EndIf

			MsUnLock()
			nConta ++
			DbSelectArea("SE2")
			DbSkip()
		End
	Else // Caso contrario ejecuta para situacion normal
		DbSelectArea("SE1")
	    cChaveSE1	:= xFilial("SE1")+cPrefix+cNumero

		SE1->(DbSetOrder(1)) //E1_FILIAL+E1_PREFIXO+E1_NUM+E1_PARCELA+E1_TIPO
		SE1->(DbSeek(xFilial("SE1")+cPrefix+cNumero))

		nConta = 1

		While !Eof() .And. E1_FILIAL+E1_PREFIXO+E1_NUM == cChaveSE1
			//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
			//³Incluida Condição caso seja Tipo = Ncc pula o registro e Next na tabela       ³
			//³Estava gerando erro quando continham 2 titulos iguais mais de tipos diferentes³
			//³um de tipo dinheiro e outro de tipo Ncc.                                      ³
			//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
			If nModulo == 12 .OR. nModulo == 23 .OR. nModulo == 72
				If SE1->E1_TIPO $ MV_CRNEG
					lVerifica := .F.
				EndIf
			EndIf

			If lVerifica // Somente Verifica se for Front ou Sigaloja
				RecLock("SE1",.F.)

				//aTotal1 y aBase1
				If Len(aTotal1) > 0
					SE1->E1_VALIMP1 := aTotal1[nConta,2]
					If Len(aBase1) >= nConta
						SE1->E1_ALQIMP1 := LxValAlqIm(aAlicuotas, "1")
					EndIf
				Endif
				If lVerCpoE1 .And. Len(aBase1) >= nConta
					SE1->E1_BASIMP1 := aBase1[nConta,2]
				EndIf

				//aTotal2 y aBase2
				If Len(aTotal2) > 0
					SE1->E1_VALIMP2 := aTotal2[nConta,2]
					If Len(aBase2) >= nConta
						SE1->E1_ALQIMP2 := LxValAlqIm(aAlicuotas, "2")
					EndIf
				Endif
				If lVerCpoE1 .And. Len(aBase2) >= nConta
					SE1->E1_BASIMP2 := aBase2[nConta,2]
				EndIf

				//aTotal3 y aBase3
				If Len(aTotal3) > 0
					SE1->E1_VALIMP3 := aTotal3[nConta,2]
					If Len(aBase3) >= nConta
						SE1->E1_ALQIMP3 := LxValAlqIm(aAlicuotas, "3")
					EndIf
				Endif
				If lVerCpoE1 .And. Len(aBase3) >= nConta
					SE1->E1_BASIMP3 := aBase3[nConta,2]
				EndIf

				//aTotal4 y aBase4
				If Len(aTotal4) > 0
					SE1->E1_VALIMP4 := aTotal4[nConta,2]
					If Len(aBase4) >= nConta
						SE1->E1_ALQIMP4 := LxValAlqIm(aAlicuotas, "4")
					EndIf
				Endif
				If lVerCpoE1 .And. Len(aBase4) >= nConta
					SE1->E1_BASIMP4 := aBase4[nConta,2]
				EndIf

				//aTotal5 y aBase5
				If Len(aTotal5) > 0
					SE1->E1_VALIMP5 := aTotal5[nConta,2]
					If Len(aBase5) >= nConta
						SE1->E1_ALQIMP5 := LxValAlqIm(aAlicuotas, "5")
					EndIf
				Endif
				If lVerCpoE1 .And. Len(aBase5) >= nConta
					SE1->E1_BASIMP5 := aBase5[nConta,2]
				EndIf

				//aTotal6 y aBase6
				If Len(aTotal6) > 0
					SE1->E1_VALIMP6 := aTotal6[nConta,2]
					If Len(aBase6) >= nConta
						SE1->E1_ALQIMP6 := LxValAlqIm(aAlicuotas, "6")
					EndIf
				Endif
				If lVerCpoE1 .And. Len(aBase6) >= nConta
					SE1->E1_BASIMP6 := aBase6[nConta,2]
				EndIf

				MsUnLock()
				nConta ++
			EndIf
			lVerifica := .T.
			SE1->(DbSelectArea("SE1"))
			SE1->(DbSkip())
		End
	EndIf
Endif
RestArea(aArea)
Return

/*ÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜ
±±ÉÍÍÍÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍ»±±
±±ºPrograma  ³PgComMex    ºAutor  ³Mary C. Hergert     º Data ³ 03/01/2007  º±±
±±ÌÍÍÍÍÍÍÍÍÍÍØÍÍÍÍÍÍÍÍÍÍÍÍÊÍÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÊÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍ¹±±
±±ºDesc.     ³Esta funcao foi criada com base no ponto de entrada generico  º±±
±±º          ³MT100AGR para ser implementada na localizacao MEXICO, nos fon-º±±
±±º          ³tes onde o ponto de entrada e chamado. Para o MEXICO, esta    º±±
±±º          ³funcionalidade ficara no padrao e nao mais atraves de ponto deº±±
±±º          ³entrada. Esta funcao grava o valor dos impostos nos titulos a º±±
±±º          ³pagar gerados pelo compras (ou a receber, quando D/B).        º±±
±±ÌÍÍÍÍÍÍÍÍÍÍØÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¹±±
±±ºUso       ³SIGAFAT e SIGAFIS.                                            º±±
±±ÈÍÍÍÍÍÍÍÍÍÍÏÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍ¼±±
ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß*/
Function PgComMex()
Local aArea			:= GetArea()
Local aTotal1		:= {}
Local aTotal2		:= {}
Local aTotal3		:= {}
Local aTotal4		:= {}
Local aTotal5		:= {}
Local aTotal6		:= {}
Local aBase1		:= {}
Local aBase2		:= {}
Local aBase3		:= {}
Local aBase4		:= {}
Local aBase5		:= {}
Local aBase6		:= {}
Local cSerie		:= SF1->F1_SERIE
Local cNumero		:= SF1->F1_DOC
Local cCliente		:= SF1->F1_FORNECE
Local cLoja			:= SF1->F1_LOJA
Local cPrefix		:= SF1->F1_PREFIXO
Local cChaveSE1		:= xFilial("SE1")+cNumero+cPrefix//cSerie
Local cChaveSE2		:= xFilial("SE2")+cCliente+cLoja+cNumero+cPrefix//cSerie
Local lExistE1		:= cPaisLoc $ "ANG|COL|EQU|HAI|MEX|PER|PTG"
Local lExistE2		:= cPaisLoc $ "ANG|COL|EQU|HAI|MEX|PER|PTG"
Local lVerCpoE1		:= cPaisLoc $ "ANG|COL|EQU|HAI|MEX|PER|PTG"
Local lVerCpoE2		:= cPaisLoc $ "ANG|COL|EQU|HAI|MEX|PER|PTG"
Local nValImp1		:= SF1->F1_VALIMP1   // Busca valor generado en la facturacion
Local nValImp2		:= SF1->F1_VALIMP2   // Busca valor generado en la facturacion
Local nValImp3		:= SF1->F1_VALIMP3   // Busca valor generado en la facturacion
Local nValImp4		:= SF1->F1_VALIMP4   // Busca valor generado en la facturacion
Local nValImp5		:= SF1->F1_VALIMP5   // Busca valor generado en la facturacion
Local nValImp6		:= SF1->F1_VALIMP6   // Busca valor generado en la facturacion
Local nConta		:= 0
Local aAlicuotas	:= {}

//³Somente efetua o processamento se os campos do E1 e E2 para lançamento dos impostos variaveis existirem.³
If lExistE1 .And. lExistE2

	// No estoy tratando IVA con retencion. Tendra que prever.
	aTotal1 := Condicao(nValImp1,SF1->F1_COND) // Genera el IVA igual em todas as parcelas
	aTotal2 := Condicao(nValImp2,SF1->F1_COND) // Genera el IVA igual em todas as parcelas
	aTotal3 := Condicao(nValImp3,SF1->F1_COND) // Genera el IVA igual em todas as parcelas
	aTotal4 := Condicao(nValImp4,SF1->F1_COND) // Genera el IVA igual em todas as parcelas
	aTotal5 := Condicao(nValImp5,SF1->F1_COND) // Genera el IVA igual em todas as parcelas
	aTotal6 := Condicao(nValImp6,SF1->F1_COND) // Genera el IVA igual em todas as parcelas

	aBase1 := Condicao(SF1->F1_BASIMP1,SF1->F1_COND) // Genera el Base IVA igual em todas as parcelas
	aBase2 := Condicao(SF1->F1_BASIMP2,SF1->F1_COND) // Genera el Base IVA igual em todas as parcelas
	aBase3 := Condicao(SF1->F1_BASIMP3,SF1->F1_COND) // Genera el Base IVA igual em todas as parcelas
	aBase4 := Condicao(SF1->F1_BASIMP4,SF1->F1_COND) // Genera el Base IVA igual em todas as parcelas
	aBase5 := Condicao(SF1->F1_BASIMP5,SF1->F1_COND) // Genera el Base IVA igual em todas as parcelas
	aBase6 := Condicao(SF1->F1_BASIMP6,SF1->F1_COND) // Genera el Base IVA igual em todas as parcelas

	aAlicuotas := LxGetAlic("SF1", cPrefix, cNumero, cCliente, cLoja, SF1->F1_ESPECIE)

	nValPor1 := 0

	If !(SF1->F1_TIPO $ "DB") // Solmiente ejetuca se no for "D" o "B"
		DbSelectArea("SE2")
	    cChaveSE1	:= xFilial("SE2")+cCliente+cLoja+cNumero+cPrefix

		SE2->(DBSetOrder(6)) //E2_FILIAL+E2_FORNECE+E2_LOJA+E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO
		SE2->(DbSeek(xFilial("SE2")+cCliente+cLoja+cPrefix+cNumero))

		nValPor1	:= 0
		nConta		:= 1
	    nValPor1	:= Round((SF1->F1_VALIMP1/SF1->F1_BASIMP1)*100,0)

		While !Eof() .And. E2_FILIAL+E2_FORNECE+E2_LOJA+E2_NUM+E2_PREFIXO == cChaveSE2
			If SF1->F1_ESPECIE <> E2_TIPO
				DbSkip()
				Loop
			Endif

			RecLock("SE2",.F.)

			//aTotal1 y aBase1
			If Len(aTotal1) >= nConta
				SE2->E2_VALIMP1 := aTotal1[nConta,2]
				If Len(aBase1) >= nConta
					SE2->E2_ALQIMP1 := LxValAlqIm(aAlicuotas, "1")
				EndIf
			Endif
			If lVerCpoE2 .And. Len(aBase1) >= nConta
				SE2->E2_BASIMP1 := aBase1[nConta,2]
			EndIf

			//aTotal2 y aBase2
			If Len(aTotal2) >= nConta
				SE2->E2_VALIMP2 := aTotal2[nConta,2]
				If Len(aBase2) >= nConta
					SE2->E2_ALQIMP2 := LxValAlqIm(aAlicuotas, "2")
				EndIf
			Endif
			If lVerCpoE2 .And. Len(aBase2) >= nConta
				SE2->E2_BASIMP2 := aBase2[nConta,2]
			EndIf

			//aTotal3 y aBase3
			If Len(aTotal3) >= nConta
				SE2->E2_VALIMP3 := aTotal3[nConta,2]
				If Len(aBase3) >= nConta
					SE2->E2_ALQIMP3 := LxValAlqIm(aAlicuotas, "3")
				EndIf
			Endif
			If lVerCpoE2 .And. Len(aBase3) >= nConta
				SE2->E2_BASIMP3 := aBase3[nConta,2]
