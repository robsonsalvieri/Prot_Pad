*/
Function aFielDetOk( cTipo )
Local cVar	  := AllTrim(ReadVar())
Local nPosCol := 0
Local nPosCpo := 0

If lDetalhe
	nPosCol  := Ascan (aHeader,{|x| Alltrim(x[2]) == Alltrim(Substr(cVar,4,Len(cVar) )) })
	If cTipo == "E" .And. (cVar	== "M->D1_COD" .Or. cVar	== "M->D1_VUNIT" .Or. cVar == "M->D1_QUANT";
		.Or. cVar == "M->D1_TOTAL"	.Or. cVar == "M->D1_TES" .Or. cVar == "M->D1_CFO" )
		If aCols[n][nPosCol]	<> &(cVar)
			lDetalhe	:=	.F.
		Endif
	ElseIf cTipo == "S" .And. (cVar	== "M->D2_COD" .Or. cVar	== "M->D2_PRCVEN" .Or. cVar == "M->D2_QUANT";
		.Or. cVar == "M->D2_TOTAL"	.Or. cVar == "M->D2_TES" .Or. cVar == "M->D2_CFO" )
		If aCols[n][nPosCol]	<> &(cVar)
			lDetalhe	:=	.F.
		Endif
	ElseIf cTipo == "L"
		If aCposLoja == NIL
			aCposLoja := {"M->L2_PRODUTO","M->L2_VLRITEM","M->L2_QUANT",;
			"M->L2_VRUNIT","M->L2_TES","M->L2_CF","M->L2_VALDESC","M->L2_DESC",;
			"M->L2_TABELA"}
		EndIf
		nPosCpo   := Ascan(aCposLoja,cVar)
		If nPosCpo > 0 .And. aCols[n][nPosCol]	<> &(cVar)
			lDetalhe   := .F.
		EndIf
	Endif
Endif
Return .T.
