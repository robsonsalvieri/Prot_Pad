	    	//Replace F3_OUTRAS	With aArraySF3[nPosTES][38]
	    	Replace F3_VALCONT	With F3_BASIMP1
	   		MsUnLock()
	   		If lMAF3EIC
	   			ExecBlock("MAF3EIC",.F.,.F.)
	   		EndIf
    	Next nI
	EndIf
EndIf
// Excluir do SF3 a NF de despacho com o codigo do Proveedor Original...
If nCaso == 2
     DbSelectArea("SF3")
     DbSetOrder(6)
     If DbSeek(xFilial("SF3")+ Subs(cDespacho,4,12)+"A  ")
     	While !Eof() .and. F3_FILIAL == xFilial("SF3") .And.;
     	 	F3_NFISCAL == Subs(cDespacho,4,12) .And. F3_SERIE == "A  "
     		RecLock("SF3",.F.)
     		DbDelete()
