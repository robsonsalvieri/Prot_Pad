/*/
Function NToB64( nNumero )
Local _digitos 	:= "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz+/"
Local _ret  	:= ""
Local _valbase	:= 1
Local _nPos  	:= 1
Local _interm 	:= 0

While nNumero >= (_valbase*64)
	_valbase:=_valbase * 64
	_nPos++
End
While _nPos > 0
	_interm  := NoRound((nNumero / _valBase),0)
	nNumero  := nNumero - (_interm*_valbase)
	_ret     += substr(_digitos,_interm+1,1)
	_valBase := ( _valbase / 64 )
	_nPos--
End
Return(_ret)
