If cHon <> "1"
	If CORR_USO() .And. GeraCorrel()
		If cHon =="2" .And. GeraCorrel() .And. !(cEspecDoc$"NCP")
	   		M->F1_NUMCOR:=CORR_PROX(M->F1_DTDIGIT)
			cCorrelat:=M->F1_NUMCOR
		EndIf

		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		//³Numero correlativo especifico para Nota de Credito³
		//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
		If cHon =="2" .And. GeraCorrel() .And. cEspecDoc$"NCP"
	   		M->F2_NUMCOR:=CORR_PROX(M->F2_DTDIGIT)
			cCorrelat:=M->F2_NUMCOR
