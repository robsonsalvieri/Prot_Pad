		AAdd(aRecno,{cFilTab,cTit,"S","                    ","NCC",cImp,"M","E","N"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE INSCRIPT","NCI",cImp,"M","E","I"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE NO INSCR","NCI",cImp,"M","E","N"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE INSCRIPT","NDE",cImp,"M","E","I"})
