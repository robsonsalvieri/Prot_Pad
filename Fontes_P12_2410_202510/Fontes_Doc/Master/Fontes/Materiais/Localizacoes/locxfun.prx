/*/
Function RetCF(aDadosPar,cFilUso)
Local aArea		:= GetArea()
Local aAreaSFP	:= SFP->(GetArea())
Local cSerie	:= ""
Local cEspecie	:= ""
Local cNFiscal	:= ""
Local cTpEspec	:= ""
Local cNIT		:= ""
Local aRet		:= {Space(TamSX3("FP_NUMAUT")[1]),Space(TamSX3("FP_CODCTR")[1]),CtoD("")}
Local aDados	:= {}
Local cFilSFP	:= xFilial("SFP")
Local lParCdCtr := ExistBlock("ParCdCtr")
Default cFilUso := cFilAnt
aDados		:= Aclone(aDadosPar)
cSerie		:= subStr(aDados[1],1,3)
cEspecie	:= aDados[2]
cNFiscal 	:= aDados[3]
Do Case
	Case Alltrim(cEspecie) == "NF" .OR. (cPaisLoc == "BOL" .AND. Alltrim(cEspecie) == "CF")
		cTpEspec := "1"
	Case Alltrim(cEspecie) == "NCI"
		cTpEspec := "2"
	Case Alltrim(cEspecie) == "NDI"
		cTpEspec := "3"
	Case Alltrim(cEspecie) == "NCC"
		cTpEspec := "4"
	Case Alltrim(cEspecie) == "NDC"
		cTpEspec := "5"
	Case Alltrim(cEspecie) == "NCE"
		cTpEspec := "6"
	Case Alltrim(cEspecie) == "NDE"
		cTpEspec := "7"
EndCase
DbSelectArea("SFP")
DbSetOrder(6)
DbSeek(xFilial("SFP")+cFilUso+cTpEspec+cSerie,.T.)
While !Eof() .And. SFP->(FP_FILIAL+FP_FILUSO+FP_ESPECIE+FP_SERIE) == cFilSFP+cFilUso+cTpEspec+cSerie
	If Val(cNFiscal) >= Val(SFP->FP_NUMINI) .And. Val(cNFiscal) <= Val(SFP->FP_NUMFIM)
		If cPaisLoc == "BOL" .and. SFP->(ColumnPos("FP_TPTRANS")) > 0 .and. SFP->FP_TPTRANS == "1"
			Exit
		EndIf
		aRet[1]	:= SFP->FP_NUMAUT	//Numero de Autorizacao
		aRet[2]	:= SFP->FP_CODCTR	//Codigo de Controle
		aRet[3]	:= SFP->FP_DTAVAL	//Data Limite de Emissao
		//Automatica
		If SFP->FP_LOTE == "2" .And. Len(aDados) > 3
			/*
			³Ponto de entrada para tratamento do NIT.  ³
			³Recebe como parametros: especie,NIT       ³
			³Retorno: nit tratado (C)                  ³
			*/
			If lParCdCtr
				cNIT := ExecBlock("ParCdCtr",.F.,.F.,{aDados[2],aDados[4]})
				If ValType(cNIT) <> "C"
					cNIT := aDados[4]
				Endif
				aDados[4] := cNIT
			EndIf
			AADD(aDados,SFP->FP_NUMAUT)		//Numero de Autorizacao
			AADD(aDados,SFP->FP_CHAVE)		//Llave de Dosificacion
			aRet[2]	:= GerCodCtr(aDados)	//Codigo de Controle
			Exit
		EndIf
	Endif
	DbSkip()
End
RestArea(aAreaSFP)
RestArea(aArea)
Return(aRet)
