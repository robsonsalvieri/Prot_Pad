		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE INSCRIPT","NCE",cImp,"A","S","I"})
		AAdd(aRecno,{cFilTab,cTit,"S","RESPONSABLE NO INSCR","NCE",cImp,"A","S","N"})
