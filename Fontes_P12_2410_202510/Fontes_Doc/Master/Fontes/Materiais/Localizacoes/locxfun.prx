Function A101ClassLinOK( nLinha )
Local cVar	  := AllTrim(ReadVar())
Local nPosCol := 0
Local lRet 	  := .T.

nPosCol  := Ascan (aHeader,{|x| Alltrim(x[2]) == Alltrim(Substr(cVar,4,Len(cVar) )) })
If (cVar	== "M->D1_COD" .Or. cVar	== "M->D1_QUANT" .Or. cVar == "M->D1_QTSEGUM" .Or. cVar == "M->D1_LOCAL";
	.Or. cVar == "M->D1_LOTECTL" .Or. cVar == "M->D1_NUMLOTE" .Or. cVar == "M->D1_DTVALID")
	//Nao permite modificar os campos acima, que sao provenientes do remito
	If n <= nLinha
		lRet := .F.
	EndIf
EndIf
Return (lRet)
