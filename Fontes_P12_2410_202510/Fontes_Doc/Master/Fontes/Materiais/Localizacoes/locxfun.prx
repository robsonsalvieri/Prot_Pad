Function VldRncCie( cTipRnc )
Local cPict := ""

If cTipRnc == "1"
	cPict := "@R XXX-9999999-9"
Else
	cPict := "@R 9-99-99999-9"
Endif
cPict := cPict + "%C"
Return cPict
