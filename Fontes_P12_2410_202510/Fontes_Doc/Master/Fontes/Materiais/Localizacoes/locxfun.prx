Function RoteiroCalc( nVlrTot, nDescLoj, cTipoTran )
Local nX 			:= 0
Local nC 			:= 0
Local nY 			:= 0
Local nA 			:= 0
Local nQuant 		:= 0
Local nPrcVen 		:= 0
Local nValorItem 	:= 0
Local nTotBaseImp 	:= 0
Local nPosCols 		:= 0
LocaL nPosRow 		:= 0
Local cProduto 		:= CriaVar("L2_PRODUTO")
Local cTES 			:= CriaVar("L2_TES")
Local cCampo
Local nDescProp  	:= 0   //Desconto proporcional ao item para o recalculo dos impostos variaveis
Local aUltImp 		:= {}
Local nCod      	:= Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_PRODUTO"  } )
Local nTes      	:= Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_TES"    } )
Local nCf       	:= Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_CF"     } )
Local nQtd      	:= Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_QUANT"  } )
LocaL nVunit    	:= Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_VRUNIT"  } )
Local nTotal    	:= Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_VLRITEM"  } )
Local nNit      	:= Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_NIT"    } )
Local nMoeda    	:= IIf(Type("nMoedaCor")#"U",nMoedaCor,1)
Local aColsBack 	:= aClone(aCols)
Local nArredIt		:= TAMSX3("L2_VRUNIT")[2]
Local lItem			:= (nArredIt > TAMSX3("L2_VLRITEM")[2])
Local nDecs
Local nDescAcum		:= 0
Local nArredImp
Local nAcrProp 		:= 0
Local nAcrAcum 		:= 0
Local nValTot  		:= 0

DEFAULT cTipoTran := "V"  //Venda

If Type("nTxJuros")=="U"
   nTxJuros  := 0
EndIf
If Type("nDecimais")=="U"
   nDecimais  := 2
EndIf
nDecs		:= If(lItem,nArredIt,nDecimais)
nArredImp   := nDecs
aImps		:=	{}
aImpVarSF2  := {}
//Calculo de Impostos pelo Roteiro Amarra‡Æo TesXImpostos.
If cCalcImpV == "S"
	If lItem
		//Se sao usadas mais decimais no valor unitario do que no total devo calcular os impostos pelo unitario
		// vezes a quantidade senao estaria perdendo decimais. Bruno REX 12/08/01
		For	nX:= 1 To Len(aCols)
			aCols[nX][nTotal]	:=	aCols[nX,nVunit] * aCols[nX,nQtd]
		Next
	Endif
	AEval(aCols,{|x,y| nValTot += If(!aCols[y][Len(aCols[y])],aCols[y][nTotal],0)})
	nMaxArray := Len(aCols)
	//Zera o Valor da Base de Calculo  o Valor do Imposto e a Aliquota antes de Recalcular os Impostos.
	For nX := 1 To nMaxArray
		//Carrega o Array aUltImp para identificar o ultimo item do aCols que esta contido o(s) Imposto(s) com Calculo P/Total
		If !aCols[nX][Len(aCols[nX])]
			SFC->(DbSeek(xFilial("SFC")+aCols[nX][nTes]))
			While SFC->FC_TES == aCols[nX][nTes] .And. !SFC->(EOF())
				If SFC->FC_CALCULO == "T"
					SFB->(DbSeek(xFilial("SFB")+SFC->FC_IMPOSTO))
					nAliquota := IIf(SFC->FC_BASE==0, SFB->FB_ALIQ, SFB->FB_ALIQ  *  (SFC->FC_BASE/100) )
					nPos	:=	Ascan(aUltImp,{|x| x[1] == SFC->FC_IMPOSTO .And. x[4]==nAliquota} )
					If nPos == 0
						aadd(aUltImp,{SFC->FC_IMPOSTO,nX,aCols[nX][nTotal],nAliquota })
					Else
						aUltImp[nPos][2]  := nX                   // Ultima Posicao do Imposto
						aUltImp[nPos][3]  += aCols[nX][nTotal]   //  Base de Calculo
					Endif
				Endif
				SFC->(DbSkip())
			End
		Endif
	Next nX
	nValTotIV  := 0
	nValBaseIV := 0
	aImpCusto  := {}
	aImpVarDup := {}
	//Obter o valor do Desconto para cada Item qdo impostos variaveis.
	aDescontos := Lj010DescIV()
	For nx := 1 to Len(aCols)
		If aCols[nx][Len(aCols[nx])]
			AAdd(aImps,{0,0,0,0,0,{}})
			Loop
		Else
			If nCod     >  0
			   cProduto := aCols[nx][nCod]
			   If ValType(cProduto)#"C"
			      cProduto  := Space(TamSX3("B1_COD")[1])
			   EndIf
			Endif
			If nTes     >  0  ;  cTes     := aCols[nx][nTes    ]  ;  Endif
			//Posiciono SF4.
			SF4->( DbSeek(xFilial("SF4")+cTes) )
			If nCf       >  0  ;  cCF      := aCols[nx][nCf       ]  ;  Endif
			If nQtd      >  0  ;  nQuant   := aCols[nx][nQtd      ]  ;  Endif
			If nVunit    >  0  ;  nPrcVen  := aCols[nx][nVunit    ]  ;  Endif
			If nNit      >  0  ;  cNitcc   := aCols[nx][nNit      ]  ;  Endif
			If nTotal   >  0
				nValorItem  := aCols[nx][nTotal]
				nTotBaseImp += aCols[nx][nTotal]
			Endif
			//Deduzir os Descontos sobre o Total do Item...
			If !nDescLoj == NIL .And. nVlrTot > 0
			   If nX == nMaxArray
			      nDescProp	 :=	nDescLoj - nDescAcum  //Desconto pela diferenca para o ultimo
               Else                                   //item para evitar problemas de arredondamento
			      nDescProp  := Round((nValorItem/nValTot) * nDescLoj,nDecs)
                  nDescAcum	 +=	nDescProp
			   Endif
			   nTotBaseImp -= nDescProp
			EndIf
			//Somar o Acrescimo ao Total do Item...
			If (nTxJuros > 0) .And. (nVlrTot > 0)
			   If nX == nMaxArray
			      nAcrProp	 :=	aDadosJur[1] - nAcrAcum    //Acrescimo pela diferenca para o ultimo
               Else                                        //item para evitar problemas de arredondamento
			      nAcrProp   := Round((nValorItem/nValTot) * aDadosJur[1],nDecs)
                  nAcrAcum	 +=	nAcrProp
			   Endif
			   nTotBaseImp += nAcrProp
			EndIf
			aImpVarSD2[1] := nQuant
			aImpVarSD2[2] := Round(nPrcVen + (nAcrProp/nQuant),nDecs)
			aImpVarSD2[3] := Round(nValorItem + nAcrProp,nDecs)
			If !lVendaRapida .And. cTipoTran == "V" .And. (M->L1_FRETE > 0 .Or. M->L1_SEGURO > 0 .Or. M->L1_DESPESA > 0)
				aImpVarSD2[4] := LjRatFrete(If(nX==1,.T.,.F.),nX)
			Else
				aImpVarSD2[4] := 0.00
			EndIf
			aImpVarSD2[5] := 0.00
			aImpVarSD2[6] := {}
			//O cadastro de Produtos deve estar posicionado...
			SB1->( DbSeek(xFilial("SB1")+cProduto) )
			//Posicionar o SF4 (Obrigat¢rio)...
			SF4->( DbSeek(xFilial("SF4")+cTes) )
			If SF4->(Found()) .And. nValorItem <> 0
				nPosRow := nX
				//Disparar rotina para executar o RdMake do calculo dos Impostos
				CalcTesxIp( "S", nTotBaseImp, nValorItem,cProduto,nposCols,nPosRow,"BALCAO",nDescProp,nDescLoj,,aUltImp)
				AAdd(aImps,aClone(aImpVarSD2))
			Else
				AAdd(aImps,{0,0,0,0,0,{}})
			Endif
		EndIf
	Next nX
	//Restaura o aCols
	aCols	:=	aClone(aColsBack)
	For nX := 1 To Len(aCols)
		For nY := 1 to Len(aHeader)
			cCampo:=AllTrim(aHeader[ny,2])
			If Substr(cCampo,4,6) == "VALIMP" .Or. Substr(cCampo,4,6) == "BASIMP" .Or. Substr(cCampo,4,6) == "ALQIMP"
				aCols[nX][nY] := 0        //Zerar aCols para atualizacao dos valores
			EndIf
		Next nY
		//Grava Base de calculo e Vlr do impostos obtidos atraves do Roteiro da Amarracao TesXImpostos no array aCols...
		For nC := 1 To Len( aImps[nX,6] )
			If aImps[nX,6,nC,3] <> 0.00 .And. !Empty( aImps[nX,6,nC,6] )
				If (nE := aScan( aImpVarSF2,{|x| x[1] == aImps[nX,6,nC,8]   } )) == 0
					AAdd( aImpVarSF2, { aImps[nX,6,nC,8], 0.00 , aImps[nX,6,nC,2]  } )
					nE := Len( aImpVarSF2 )
				EndIf
				aImpVarSF2[ nE,2 ] += aImps[nX,6,nC,4]
				If (nE := aScan( aImpVarSF2,{|x| x[1] == aImps[nX,6,nC,9]  } )) == 0
					AAdd( aImpVarSF2, { aImps[nX,6,nC,9], 0.00 , aImps[nX,6,nC,2]  } )
					nE := Len( aImpVarSF2 )
				EndIf
				aImpVarSF2[ nE,2 ] += aImps[nX,6,nC,3]
				//Acumula em "aImpVarDup" os Impostos que Incidem no Valor Total Base da Duplicata...                                ³
				If SubStr( aImps[nX,6,nC,5],1,1 ) == "1"
					AAdd(aImpVarDup,{aImps[nX,6,nC,1],aImps[nX,6,nC,4]})
				EndIf
				//Valor Base do Imposto...
				nColuna  := Ascan(aHeader,{|X| Trim(X[2]) ==  aImps[nX,6,nC,7] })
				If nColuna > 0
				   aCols[nx][nColuna] := aImps[nX,6,nC,3]
				EndIf
				//Valor do Imposto calculado...
				nColuna  := Ascan(aHeader,{|X| Trim(X[2]) == aImps[nX,6,nC,6] })
				If nColuna > 0
				   aCols[nx][nColuna] := aImps[nX,6,nC,4]
				EndIf
				//Gravar Aliquotas dos Impostos...
				nColuna  := Ascan(aHeader,{|X| Trim(X[2]) == "L2_ALQ"+Subs(aImps[nX,6,nC,7],7,4) })
				If nColuna > 0
					aCols[nx][nColuna] := aImps[nX,6,nC,2]
				EndIf
			EndIf
		Next nC
	Next nX
EndIf
//Acertar arredondamentos se calculou algum imposto
If Len(aImpVarSF2) > 0
	aRedo := {}
	For nX	:=	1	To Len(aImps)
		For nY	:=	1	To Len(aImps[nX][6])
			If aImps[nX][6][nY][4] <> 0
				nPosImp	:=	AScan(aRedo,{|x| x[1]==	aImps[nX][6][nY][8] .And. x[2]==	aImps[nX][6][nY][9]  })
				aImps[nX][6][nY][4]	:=	Round(NoRound(aImps[nX][6][nY][4],nDecs+1),nDecs)
				aImps[nX][6][nY][3]	:=	Round(NoRound(aImps[nX][6][nY][3],nDecs+1),nDecs)
				If nPosImp == 0
					AAdd(aRedo,{aImps[nX][6][nY][8], aImps[nX][6][nY][9], aImps[nX][6][nY][4],aImps[nX][6][nY][3]})
				Else
					aRedo[nPosImp][3]	+=	aImps[nX][6][nY][4]
					aRedo[nPosImp][4]	+=	aImps[nX][6][nY][3]
				Endif
			Endif
		Next nY
	Next nX
	For nX	:=	1	To Len(aRedo)
		//Acertar el redondeo en la base del impuesto
		nPosImp	:=	Ascan(aImpVarSF2,{|x| x[1]==aRedo[nX][1]})
		aImpVarSF2[nPosImp][2]	:=	Round(NoRound(aImpVarSF2[nPosImp][2],nDecs+1),nDecs)
		nDif	:=	aImpVarSF2[nPosImp][2] - aRedo[nX][3]
		lAchou	:=	.F.
		While !lAchou
			For nY	:=	1	To Len(aImps)
				nPosIt	:=	Ascan(aImps[nY][6],{|X| x[8] == aRedo[nX][1]  })
				If nPosIt > 0
					aImps[nY][6][nPosIt][4]	+=	nDif
					lAchou	:=	.T.
					nY	:=	Len(aImps)+1
				Endif
			Next nY
		End
		//Acertar el redondeo en el valor del impuesto
		nPosImp	:=	Ascan(aImpVarSF2,{|x| x[1]==aRedo[nX][2]})
		aImpVarSF2[nPosImp][2]	:=	Round(NoRound(aImpVarSF2[nPosImp][2],nDecs+1),nDecs)
		nDif	:=	aImpVarSF2[nPosImp][2] - aRedo[nX][4]
		lAchou	:=	.F.
		While !lAchou
			For nY	:=	1	To Len(aImps)
				nPosIt	:=	Ascan(aImps[nY][6],{|X| x[9] == aRedo[nX][2]  })
				If nPosIt > 0
					aImps[nY][6][nPosIt][3]	+=	nDif
					lAchou	:=	.T.
					nY	:=	Len(aImps)+1
				Endif
			Next nY
		End
	Next nX
EndIf
nValTotIV	:=	0
nValBaseIV	:=	0
For nX	:= 1 To Len(aImps)
	aPropImp	:=	TesImpInf(aCols[nX][nTes])
	For nY	:=	1	To Len(aImps[nX][6])
		If ( aPropImp[nY][3]=="1" )
			nValTotIV	+= aImps[nX][6][nY][4]
		ElseIf ( aPropImp[nY][3] == "2" )
			nValTotIV	-= aImps[nX][6][nY][4]
		Endif
	Next nY
Next nX
nValTotIV	:=	Round(NoRound(nValTotIV,nArredImp+1),nArredImp )
nValBaseIV	:=	Round(NoRound(nValTotIV,nArredImp+1),nArredImp )

Return aImps
/*
±±³Fun‡„o    ³ CalcTesXIp ³ Autor ³Jos‚ Lucas           ³ Data ³ 18/05/99 ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄ´±±
±±³Descri‡„o ³ C lculo dos Impostos pela Amarra‡Æo TesXImpostos, gravando ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´±±
±±³Sintaxe   ³ CalcTesXIp( cMovto,nTotBaseImp,nValorItem,cVenda )         ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´±±
±±³Parametros³ ExpC1 := Tipo de Movimento "E"-Entradas e "S"-Saidas       ³±±
±±³          ³ ExpN1 := Valor Total das Mercadorias da Factura            ³±±
±±³          ³ ExpN2 := Valor Total de cada Item da Factura               ³±±
±±³          ³ ExpN3 := Numero da posi‡„o da linha na GetDados            ³±±
±±³          ³ ExpC2 := Tipo da Venda "NORMAL" ou "BALCAO"                ³±±

*/
Function CalcTesXIp( cMovto	 , nTotBaseImp, nValorItem, cProduto ,;
					 nPosCols, nPosRow	  , cVenda 	  ,nDescontos,;
					 nDescTot, nMaxArray  , aUltItem )
Local cFileRdMk   := ""
Local aImposto    := {}
Local nTotBasImp  := 0
Local nTotVlrImp  := 0
Local nVlrImposto := 0
Local nVlrBase    := 0
Local nPosTotal   := 0
Local nPosDesc    := 0
Local nPosBasImp  := 0
Local nPosVlrImp  := 0
Local nOrderSFC   := 0
Local nRecnoSFC   := 0
Local cCpoVlrItem := ""
Local cCpoBaseIte := ""
Local cCpoVlrCab  := ""
Local cCpoBaseCab := ""
Local nC 		  := 0
Local nZ		  := 0
Local nMaxItem    := 0
Local nPosImp	  := 0
Local nTotMerc	  := 0
Local nTotBase	  := 0
Local nTotImp 	  := 0
LocaL nPosPrcVen  := Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_VRUNIT"  } )
LocaL nPosPrcTab  := Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_PRCTAB"  } )
LocaL nPosValDesc := Ascan(aHeader,{|X| Alltrim(X[2]) ==  "L2_VALDESC" } )
Local aBase
Local lCposHab    := .T.
Local lCalcTo     := .T.

Private aInfo := {}

If cMovto == "S"
	aBase	:=	{aImpVarSD2[1],aImpVarSD2[2],aImpVarSD2[3],aImpVarSD2[4],aImpVarSD2[5]}
Else
	aBase	:=	{aImpVarSD1[1],aImpVarSD1[2],aImpVarSD1[3],aImpVarSD1[4],aImpVarSD1[5]}
Endif

DEFAULT nDescontos:=	0
DEFAULT nDescTot  :=	0
DEFAULT nMaxArray := 0
DEFAULT aUltItem  := {}

nValFrete := IIf( Type("NVALFRETE")=="U", 0, nValFrete)
nValDesp  := IIf( Type("NVALDESP")=="U", 0, nValDesp)
cVenda := IIf( cVenda==NIL,"NORMAL", cVenda )
If cVenda <> "RAPIDA"
	For nC   := 1  To Len(aCols)
		If !aCols[nC][Len(aCols[nC])]
			nMaxArray++
		Endif
	Next nC
EndIf
SFC->( DbSetOrder( 1 ) )
SFB->( DbSetOrder( 1 ) )
If cMovto == "E"
	If Len( aImpVarSD1 ) == 5 ; AADD( aImpVarSD1, {} ) ; EndIf
Else
	If Len( aImpVarSD2 ) == 5 ; AADD( aImpVarSD2, {} ) ; EndIf
Endif
aImps   := If(Type("aImps") == "A",aClone(aImps),{})
//Correr o arquivo de Itens do Tes para disparar o Rdmake de Impostos.
SFC->( DbSeek( xFilial("SFC")+SF4->F4_CODIGO ) )
While SFC->( !Eof() .And. SFC->FC_FILIAL+SFC->FC_TES == xFilial("SFC")+SF4->F4_CODIGO )
	lCalcTo := .T.
	If SFB->( DbSeek( xFilial("SFB")+SFC->FC_IMPOSTO ) )
		If SFC->FC_CALCULO $ "T"
			//Calcular impostos baseando se nos Valores dos Itens...
			nPosImp  := Ascan(aUltItem,{|x| SFC->FC_IMPOSTO==x[1] } )
			nMaxItem := IIf(nPosImp==0,nMaxArray,aUltItem[nposImp][2])
			If  nPosRow == nMaxItem
				If nPosImp > 0
					Iif( cMovto=="E", aImpVarSD1[1]:=1, aImpVarSD2[2]:=1 )
					Iif( cMovto=="E", aImpVarSD1[2]:= aUltItem[nPosImp][3], aImpVarSD2[2] := aUltItem[nPosImp][3] )  //VALOR DO ITEM
					Iif( cMovto=="E", aImpVarSD1[3]:= aUltItem[nPosImp][3], aImpVarSD2[3] := aUltItem[nPosImp][3] )  //BASE DE CALCULO
					Iif( cMovto=="E", aImpVarSD1[4]:= nValFrete, aImpVarSD2[4]:=nValFrete )
					Iif( cMovto=="E", aImpVarSD1[5]:= nValDesp, aImpVarSD2[5]:=nValDesp )
				Else
					Iif( cMovto=="E", aImpVarSD1[1]:=1, aImpVarSD2[2]:=1 )
					Iif( cMovto=="E", aImpVarSD1[2]:=nTotBaseImp, aImpVarSD2[2]:=nTotBaseImp )
					Iif( cMovto=="E", aImpVarSD1[3]:=nTotBaseImp, aImpVarSD2[3]:=nTotBaseImp )
					Iif( cMovto=="E", aImpVarSD1[4]:=nValFrete, aImpVarSD2[4]:=nValFrete )
					Iif( cMovto=="E", aImpVarSD1[5]:=nValDesp, aImpVarSD2[5]:=nValDesp )
				Endif
			EndIf
		Endif
		//Preparar campos com base em cMovto e cVenda...
		If cMovto == "E"
			cFileRdMk   := AllTrim(SFB->FB_FORMENT)
			If cVenda == "BALCAO"  //Somente para o SigaLoja - Troca
				cCpoVlrItem := "L2_VALIMP"+SFB->FB_CPOLVRO
				cCpoBaseIte := "L2_BASIMP"+SFB->FB_CPOLVRO
				cCpoVlrCab  := "L1_VALIMP"+SFB->FB_CPOLVRO
				cCpoBaseCab := "L1_BASIMP"+SFB->FB_CPOLVRO
			Else
				cCpoVlrItem := "D1_VALIMP"+SFB->FB_CPOLVRO
				cCpoBaseIte := "D1_BASIMP"+SFB->FB_CPOLVRO
				cCpoVlrCab  := "F1_VALIMP"+SFB->FB_CPOLVRO
				cCpoBaseCab := "F1_BASIMP"+SFB->FB_CPOLVRO
			EndIf
		Else
			cFileRdMk := AllTrim(SFB->FB_FORMSAI)
			If cVenda == "BALCAO"  //Somente para o SigaLoja
				cCpoVlrItem := "L2_VALIMP"+SFB->FB_CPOLVRO
				cCpoBaseIte := "L2_BASIMP"+SFB->FB_CPOLVRO
				cCpoVlrCab  := "L1_VALIMP"+SFB->FB_CPOLVRO
				cCpoBaseCab := "L1_BASIMP"+SFB->FB_CPOLVRO
			Else
				cCpoVlrItem := "D2_VALIMP"+SFB->FB_CPOLVRO
				cCpoBaseIte := "D2_BASIMP"+SFB->FB_CPOLVRO
				cCpoVlrCab  := "F2_VALIMP"+SFB->FB_CPOLVRO
				cCpoBaseCab := "F2_BASIMP"+SFB->FB_CPOLVRO
			EndIf
		EndIf
		lCposHab  := nPosPrcTab > 0 .And. nPosPrcVen > 0 .And. nPosValDesc > 0
		If SFC->FC_LIQUIDO == "N" .And. lCposHab                           //Calcular os impostos pelo valor bruto
		   If aCols[nPosRow][nPosPrcTab] > aCols[nPosRow][nPosPrcVen]      //Contemplar as situacoes em que se da desconto no
			  aImpVarSD2[2] := aCols[nPosRow][nPosPrcTab]                  //item e o calculo dos impostos eh pelo valor bruto(FC_LIQUIDO = N)
			  aImpVarSD2[3] := aImpVarSD2[1] * aImpVarSD2[2]
			  nDescontos    := aCols[nPosRow][nPosValDesc]
		   EndIf
		EndIf
		//Preparar array de Impostos a ser tratados no RdMake do usuario.
		aImposto  := {}
		AAdd( aImposto, SFB->FB_CODIGO )
		AAdd( aImposto, SFB->FB_ALIQ )
		AAdd( aImposto, 0.00 )
		AAdd( aImposto, 0.00 )
		AAdd( aImposto, SFC->FC_INCDUPL + SFC->FC_INCNOTA + SFC->FC_CREDITA + SFC->FC_LIQUIDO )
		AAdd( aImposto, cCpoVlrItem )
		AAdd( aImposto, cCpoBaseIte )
		AAdd( aImposto, cCpoVlrCab )
		AAdd( aImposto, cCpoBaseCab )
		AAdd( aImposto, SFC->FC_INCIMP )
		AAdd( aImposto, 0.00 )
		AAdd( aImposto, 0.00 )
		AAdd( aImposto, 0.00 )
		AAdd( aImposto, 0.00 )
		AAdd( aImposto, SFC->FC_CALCULO )
		AAdd( aImposto, cProduto )		//troca de nome do campo pelo parametro recebido - Percy - 11/10/2000
		//AAdd( aImposto, SB1->B1_COD )
		AAdd( aImposto, SFB->FB_CPOLVRO )
		AAdd( aImposto, IIf(SFC->FC_CALCULO $ "T" .And. nPosRow == nMaxItem ,nDescTot,nDescontos ) )
		AADD( aImposto, {1,-1,0} )
		AADD( aImposto, SFC->FC_PROV )
		nRecnoSFC := SFC->( Recno() )
		nOrderSFC := SFC->( IndexOrd() )
		//Disparar o RdMake do Usuario com base em FB_FORMENT e FB_FORMSAI
		If FindFunction(cFileRdMk)
			SB1->(DbSetOrder(1))
			SB1->(DbSeek(xFilial("SB1")+cProduto))
			If cPaisLoc == "ARG" .And. FunName() != "MATA465N" .And. cFunName == "MATA465N"
				aSize(aInfo, 0)
				aInfo := {SFB->FB_CODIGO,  SFB->FB_CPOLVRO}
			EndIf
			If cMovto == "E"
				aImposto := ExecBlock( cFileRdMk,.F.,.F., { aImpVarSD1, aImposto,;
					nPosRow,"D1_VALIMP"+SFB->FB_CPOLVRO,nTotBaseImp},.t. )
				If aImposto <> Nil
					AAdd( aImpVarSD1[6], AClone( aImposto ) )
				EndIf
			Else
				aImposto := ExecBlock( cFileRdMk,.F.,.F., { aImpVarSD2, aImposto,;
					nPosRow,"D1_VALIMP"+SFB->FB_CPOLVRO,nTotBaseImp},.t. )
				If aImposto <> Nil
					AADD( aImpVarSD2[6], AClone( aImposto ) )
				EndIf
			EndIf
		EndIf
		SFC->( DbSetOrder(nOrderSFC) )
		SFC->( dbGoTo(nRecnoSFC) )
		//Ratear base de calculo e valor do imposto quando calculo for com base no Total das Mercadorias...
		If SFC->FC_CALCULO $ "T" .And. nPosRow == nMaxItem
			nTotMerc :=	0
			nTotImp  := 0
			nTotBase := 0
			aImposTmp := aClone(IIf(cMovto=="S",aImpVarSD2,aImpVarSD1))

			//Si es para la generación de NCC desde rutina automática, no borra las bases imponibles cuando no retiene.
			If cPaisLoc == "ARG" .And. FunName() != "MATA465N" .And. cFunName == "MATA465N"
				lCalcTo := .F.
			EndIf
			If Len(aImposTmp[6]) > 0 .And. lCalcTo
				If nMaxItem > Len(aImps) .and. Len(aImposTmp[6]) > 0
					If aImposTmp[6][Len(aImposTmp[6])][4] == 0
						aImposTmp[6][Len(aImposTmp[6])][3]   := 0
					EndIf
				EndIf
				For nZ := 1	To Len(aImps)
					If aImps[nZ] <> Nil .And. Len(aImps[nZ]) > 0
						//Para o Paraguai deve considerar apenas o codigo do imposto porque a aliquota
						//pode mudar dependendo do valor minimo para retencao
					    If cPaisLoc == "PAR"
						   nPosImp  := Ascan(aImps[nZ][6],{|x| x[1] == aImposTmp[6][Len(aImposTmp[6])][1]})
					    Else
                           nPosImp  := Ascan(aImps[nZ][6],{|x| x[1] == aImposTmp[6][Len(aImposTmp[6])][1] .And. x[2] == aImposTmp[6][Len(aImposTmp[6])][2] })
						EndIf
						If nPosImp > 0 .And. aImps[nZ][6][nPosImp][15]=="T"
							If aImposTmp[6][Len(aImposTmp[6])][4] <> 0
								nRat                     := (aImps[nZ][6][nPosImp][3]/aImposTmp[6][Len(aImposTmp[6])][3])
								aImps[nZ][6][nPosImp][4] := aImposTmp[6][Len(aImposTmp[6])][4] * nRat
								aImps[nZ][6][nPosImp][3] := aImposTmp[6][Len(aImposTmp[6])][3] * nRat
								nTotImp                  += aImps[nZ][6][nPosImp][4]
								nTotBase                 += aImps[nZ][6][nPosImp][3]
							Else
								aImps[nZ][6][nPosImp][4]           := 0
								aImps[nZ][6][nPosImp][3]           := 0
								aImposTmp[6][Len(aImposTmp[6])][3] := 0
							Endif
						Endif
					Endif
				Next nZ
				If Len(aImposTmp[6]) > 0
					If aImposTmp[6][Len(aImposTmp[6])][4] <> 0
						aImposTmp[6][Len(aImposTmp[6])][3]   -= nTotBase
						aImposTmp[6][Len(aImposTmp[6])][4]   -= nTotImp
					Else
						aImposTmp[6][Len(aImposTmp[6])][3]   :=	0
					Endif
				EndIf
				//Restaurar os valores originais
				aImposTmp[1]	:=	aBase[1]
				aImposTmp[2]	:=	aBase[2]
				aImposTmp[3]	:=	aBase[3]
				aImposTmp[4]	:=	aBase[4]
				aImposTmp[5]	:=	aBase[5]
				IIf(cMovto=="S",aImpVarSD2:=aClone(aImposTmp),aImpVarSD1:=aClone(aImposTmp))
			EndIf
		EndIf
	EndIf
	SFC->( DbSkip() )
End
Return
/*
±±³Fun‡Æo    ³ DefImposto ³ Autor ³Jos‚ Lucas           ³ Data ³ 19/11/98 ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄ´±±
±±³Descri‡Æo ³ Obter a caracteristica de um Determinado imposto que faz   ³±±
±±³          ³ parte do roteiro de c lculo ( Impostos Internacionais)...  ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´±±
±±³Sintaxe   ³ ExpA1 := DefImposto( ExpC1 )                               ³±±
±±³          ³ Onde: ExpC1 := C¢digo do TES    Ex:"001", "501"            ³±±
±±³          ³       ExpA1[n][1] := C¢digo do Imposto Ex: "IVA"           ³±±
±±³          ³       ExpA1[n][2] := Identifica em qual campos sera grav.  ³±±
±±³          ³                      o valor do imposto Ex: "VALIMP1"      ³±±
±±³          ³       ExpA1[n][3] := Define se o Imposto ser  somado ao    ³±±
±±³          ³                      total da Nota Fiscal. Ex: "S,N"       ³±±
±±³          ³       ExpA1[n][4] := Define se o Imposto ser  somado ao    ³±±
±±³          ³                      valor base das duplicatas. Ex: "S,N"  ³±±
±±³          ³       ExpA1[n][5] := Define se o Imposto ser  creditado ao ³±±
±±³          ³                      custo. Ex: "S,N"                      ³±±
±±³          ³       ExpA1[n][6] := Define se o Imposto ser  calculado    ³±±
±±³          ³                      com base no total dos itens ou no     ³±±
±±³          ³                      total das mercadorias. Ex: "S,N"      ³±±
±±³          ³       ExpA1[n][7] := Identifica em qual campo sera grav.   ³±±
±±³          ³                      a Base do  imposto Ex: "BASIMP1"      ³±±
±±³          ³       ExpA1[n][8] := Identifica em qual campo sera grav.   ³±±
±±³          ³                      a Aliquota do imposto Ex: "ALQIMP1"   ³±±
*/
Function DefImposto( cTes )
Local aImposto := {}

SFB->( DbSetOrder( 1 ) )
SFC->( DbSetOrder( 1 ) )
SFC->( DbSeek( xFilial("SFC")+cTes ) )
While SFC->(!Eof()) .and. SFC->FC_FILIAL+SFC->FC_TES == xFilial("SFC")+cTes
	If SFB->( DbSeek( xFilial("SFB")+SFC->FC_IMPOSTO ) )
		aLinha := Array(8)
		aLinha[1] := SFC->FC_IMPOSTO
		If cTes <= "500"
			aLinha[2] := "VALIMP"+SFB->FB_CPOLVRO
			aLinha[7] := "BASIMP"+SFB->FB_CPOLVRO
			aLinha[8] := "ALQIMP"+SFB->FB_CPOLVRO
		Else
			aLinha[2] := "VALIMP"+SFB->FB_CPOLVRO
			aLinha[7] := "BASIMP"+SFB->FB_CPOLVRO
			aLinha[8] := "ALQIMP"+SFB->FB_CPOLVRO
		EndIf
		aLinha[3] := SFC->FC_INCNOTA
		aLinha[4] := SFC->FC_INCDUPL
		aLinha[5] := SFC->FC_CREDITA
		aLinha[6] := SFC->FC_CALCULO
		AADD(aImposto,AClone(aLinha))
	EndIf
	SFC->( DbSkip() )
End

Return( aImposto )
/*/
±±³Fun‡Æo    ³A100TesXIp³ Autor ³ Jos‚ Lucas            ³ Data ³ 24/04/98 ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄ´±±
±±³Descri‡Æo ³ Rotina para inicializar array de impostos e executar o     ³±±
±±³          ³ arquivo em RdMake para calcular os impostos variaveis.     ³±±
±±ÃÄÄÄÄÄÄÄÄÄÄÅÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´±±
±±³Sintaxe   ³ a100TexXIp( aImpVarSD1,nTotBaseImp,nItemValTot, nPosCols)  ³±±
±±³          ³                                                            ³±±
±±³          ³ aImpVarSD1[1]      -> Quantidade                           ³±±
±±³          ³           [2]      -> Pre‡o Unit rio Venda                 ³±±
±±³          ³           [3]      -> Valor Total                          ³±±
±±³          ³           [4]      -> Valor de Frete/Despesas              ³±±
±±³          ³      (*)  [5]      -> {...}                                ³±±
±±³          ³ (*) Array de Retorno c/os Impostos Calculados /            ³±±
±±³          ³ nTotBaseImp	->Base Total do Iten                           |±±
±±³          ³ nItemValTot ->                                             ³±±
±±³          ³ cProduto    -> Codigo do Produto                           ³±±
±±³          ³ nPosCols		-> Posicao atual da linha do aCols.            ³±±
±±³          ³                                                            ³±±
±±|          ³     Campos de Grava‡Æo dos Valores no SD2-SF2              ³±±
±±³Uso       ³ MATA100                                                    ³±±

/*/
//³ aImpVarSD1[6][n][01] := C¢digo do Imposto                                                 ³
//³              [n][02] := Al¡quota do Imposto                                               ³
//³              [n][03] := Base de C lculo do Imposto                                        ³
//³              [n][04] := Valor Calculado do Imposto                                        ³
//³              [n][04] := Valor Calculado do Imposto                                        ³
//³              [n][05] := "___" = Pos.: 1-Inclui no Valor da Duplicata   (S/N)              ³
//³                                       2-Inclui no Total da Nota Fiscal (S/N)              ³
//³									    			3-Credita se do imposto para c lculo do Custo (SN)  ³
//³              [n][06] := Cpo.Grava‡Æo SD1 (Valor Imposto)                                  ³
//³              [n][07] :=                  (Base de C lculo)                                ³
//³              [n][08] := Cpo.Grava‡Æo SF1 (Valor Imposto)                                  ³
//³              [n][09] :=                  (Base de C lculo)                                ³
//³              [n][10] := C¢digo dos Impostos Incidentes no C lculo.                        ³
//³					 [n][11] := Valor do Frete Rateado.                                         ³
//³					 [n][12] := Valor Calculado do Imposto sobre Valor do Frete.                ³
//³					 [n][13] := Valor das Despesas Rateadas.                                    ³
//³					 [n][14] := Valor Calculado do Imposto sobre Valor das Despesas.            ³
//³					 [n][15] := ...
//³					 [n][16] := ...
//³					 [n][17] := ...
//³					 [n][18] := Forma de obter a Base do imposto por Item,Total Factura ou CFO. ³
Function A100TesXIp( aImpVarSD1, nTotBaseImp, nItemValTot, cProduto, nPosCols  , nDescontos , nDescTot, aUltItem )
Local cFileRdMk
Local aImposto	  := {}
Local nTotBasImp  := 0
Local nTotVlrImp  := 0
Local nVlrImposto := 0
Local nVlrBase    := 0
Local nPosTotal   := 0
Local nPosDesc    := 0
Local nPosBasImp  := 0
Local nPosVlrImp  := 0
Local nMaxArray   := 0
Local nC          := 0
Local nZ          := 0
Local nMaxItem	  := 0
Local nPosImp	  := 0
Local nTotMerc	  := 0
Local nTotBase	  := 0
Local nTotImp 	  := 0
Local lCpoLiquid := .F.
//Guardar os valores BASE do iTEM
Local aBase	:=	{aImpVarSD1[1],aImpVarSD1[2],aImpVarSD1[3],aImpVarSD1[4],aImpVarSD1[5]}

DEFAULT nDescontos:= 0
DEFAULT nDescTot  := 0

For nC := 1 To Len(aCols)
	If !aCols[nC][Len(aCols[nC])]
		nMaxArray++
	Endif
Next nC
SFC->( DbSetOrder( 1 ) )
SFB->( DbSetOrder( 1 ) )
If Len( aImpVarSD1 ) == 5
	AADD( aImpVarSD1, {} )
EndIf
lCpoLiquid := SFC->(ColumnPos("FC_LIQUIDO"))
SFC->( DbSeek( xFilial("SFC")+SF4->F4_CODIGO ) )
While SFC ->( !Eof() .And. FC_FILIAL+FC_TES == xFilial("SFC")+SF4->F4_CODIGO )
	SFB->( DbSeek( xFilial("SFB")+SFC->FC_IMPOSTO ) )
	//Calcular impostos baseando se nos Valores dos Itens...
	nPosImp	:=	Ascan(aUltItem,{|x| SFC->FC_IMPOSTO==x[1] } )
	nMaxItem	:=	IIf(nPosImp==0,nMaxArray,aUltItem[nposImp][2])
	If SFC->FC_CALCULO $ "T" .And. nPosCols == nMaxItem
		aImpVarSD1[1] := 1
		aImpVarSD1[2] := aUltItem[nPosImp][3] //VALOR DO ITEM
		aImpVarSD1[3] := aUltItem[nPosImp][3] //BASE DE CALCULO
	EndIf
	cFileRdMk := AllTrim(SFB->FB_FORMENT)
	aImposto  := {}
	AAdd( aImposto, SFB->FB_CODIGO )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, SFC->FC_INCDUPL + SFC->FC_INCNOTA + SFC->FC_CREDITA + If(lCpoLiquid, SFC->FC_LIQUIDO, "S") )
	AAdd( aImposto, "D1_VALIMP"+SFB->FB_CPOLVRO)
	AAdd( aImposto, "D1_BASIMP"+SFB->FB_CPOLVRO)
	AAdd( aImposto, "F1_VALIMP"+SFB->FB_CPOLVRO)
	AAdd( aImposto, "F1_BASIMP"+SFB->FB_CPOLVRO)
	AAdd( aImposto, SFC->FC_INCIMP )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, 0.00 )
	AAdd( aImposto, SFC->FC_CALCULO )
	AAdd( aImposto, cProduto)
	AAdd( aImposto, SFB->FB_CPOLVRO)
	AAdd( aImposto, IIf(SFC->FC_CALCULO $ "T".And.nPosCols == nMaxItem ,nDescTot,nDescontos ))
	AADD( aImposto, {1,-1,0} )
	AADD( aImposto, SFC->FC_PROV )
	If FindFunction(cFileRdMk)
		SB1->(DbSetOrder(1))
		SB1->(DbSeek(xFilial("SB1")+cProduto))
		aImposto := ExecBlock( cFileRdMk,.F.,.F., { aImpVarSD1, aImposto,;
			nPosCols,"D1_VALIMP"+SFB->FB_CPOLVRO,nTotBaseImp},.t. )
		AAdd( aImpVarSD1[6], AClone( aImposto ) )
		//	Else
		//Help(" ",1,"NO_IX",,cFileRdMk,1,30)
	EndIf
	//Ratear base de calculo e valor do imposto quando calculo for com base no Total das Mercadorias...
	If SFC->FC_CALCULO $ "T" .And. nPosCols == nMaxItem
		nTotBase	:=	0
		nTotImp	:=	0
		If nMaxItem > Len(aImps) .And. aImpVarSD1[6][Len(aImpVarSD1[6])][4] == 0
			aImpVarSD1[6][Len(aImpVarSD1[6])][3]	:= 0
		Endif
		For nZ := 1	To Len(aImps)
			If aImps[nZ] <> Nil .And. Len(aImps[nZ]) > 0
				nPosImp	:=	Ascan(aImps[nZ][6],{|x| x[1] == aImpVarSD1[6][Len(aImpVarSD1[6])][1] .And. x[2] == aImpVarSD1[6][Len(aImpVarSD1[6])][2] })
				If nPosImp > 0 .And. aImps[nZ][6][nPosImp][15]=="T"
					If aImpVarSD1[6][Len(aImpVarSD1[6])][4] <> 0
						nRat					        	 := (aImps[nZ][6][nPosImp][3]/aImpVarSD1[6][Len(aImpVarSD1[6])][3])
						aImps[nZ][6][nPosImp][4] := aImpVarSD1[6][Len(aImpVarSD1[6])][4] * nRat
						aImps[nZ][6][nPosImp][3] := aImpVarSD1[6][Len(aImpVarSD1[6])][3] * nRat
						nTotImp						    += aImps[nZ][6][nPosImp][4]
						nTotBase 					    += aImps[nZ][6][nPosImp][3]
					Else
						aImps[nZ][6][nPosImp][4]           := 0
						aImps[nZ][6][nPosImp][3]           := 0
						aImpVarSD1[6][Len(aImpVarSD1[6])][3] := 0
					Endif
				Endif
			Endif
		Next nZ
		//Se o imposto foi relamente calculado para este item...
		If aImpVarSD1[6][Len(aImpVarSD1[6])][4] <> 0
			aImpVarSD1[6][Len(aImpVarSD1[6])][3]		-= nTotBase
			aImpVarSD1[6][Len(aImpVarSD1[6])][4] 	-= nTotImp
			//Se nao foi, zero a base
		Else
			aImpVarSD1[6][Len(aImpVarSD1[6])][3]   :=	0
		Endif
		//Restaurar os valores originais
		aImpVarSD1[1]	:=	aBase[1]
		aImpVarSD1[2]	:=	aBase[2]
		aImpVarSD1[3]	:=	aBase[3]
		aImpVarSD1[4]	:=	aBase[4]
		aImpVarSD1[5]	:=	aBase[5]
	EndIf
	SFC->( DbSkip() )
End

Return
