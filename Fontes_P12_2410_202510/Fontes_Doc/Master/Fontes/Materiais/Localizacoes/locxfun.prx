*/
Function GrvTitLoc(nValImp,aParam)
	Local   aArea       := GetArea()
	Local   aDadosTit   := {}
	Local   aDadosSE2   := {}
	Local   nPosCpo     := 0
	Local   aErrObrig   := {}
	Local   aCpoPE      := {}
	Local   nK          := 0
	Local   aAchoTit    := {}
	Private lMsErroAuto := .F.
	Private cCadastro   := "Contas a pagar"
	Default aParam      := {}

	Begin Transaction
		If cPaisLoc $ "ANG|PER|EQU|VEN|DOM|BOL|COS"
			aDadosSE2 := AngTitApur(nValImp)

			If Len(aDadosSE2) > 0
				if ExistBlock("FS011CPO") // Ponto de entrada para adicionar campos ao Array que é enviado para a Rotina automática
					aCpoPE := ExecBlock("FS011CPO",.f.,.f.)
					If ValType(aCpoPE) = "A"
						For nK = 1 to Len(aCpoPE)
							aAdd(aDadosSE2,{aCpoPE[nK,1],aCpoPE[nK,2],nil})
						Next
					Endif
				Endif

				dbSelectArea("SX3")
				dbSetOrder(1)
				dbSeek("SE2")
				Do While !eof() .AND. X3_ARQUIVO = "SE2"
					if X3Obrigat(X3_CAMPO)
						nPosCpo := aScan(aDadosSE2 , {|x| AllTrim(x[1]) == AllTrim(X3_CAMPO) })
						if nPosCpo <= 0
							aAdd(aErrObrig,AllTrim(X3_CAMPO))
						Endif
					Endif
					dbSkip()
				Enddo

				MsExecAuto({|x,y,z| FINA050(x,y,z)},aDadosSE2,,3)
				If lMsErroAuto
					DisarmTransaction()
	       			MostraErro()
	    		Else
					For nK = 1 to Len(aDadosSE2)
						If !("ORIGEM" $ aDadosSE2[nK,1])
		    				aAdd(aAchoTit,aDadosSE2[nK,1])
						Endif
	    			Next nK

					dbSelectArea("SE2")
					AxAltera("SE2",SE2->(Recno()),4,aAchoTit)
					aDadosTit := {E2_PREFIXO,E2_NUM,E2_PARCELA,E2_TIPO,E2_FORNECE,E2_LOJA,xMoeda(E2_VALOR,1,2,dDataBase),E2_VALOR}
				Endif

			Endif
		EndIf
	End Transaction

	RestArea(aArea)
Return aDadosTit

Static Function AngTitApur(nValImp)
	Local aMvSigla := {}
	Local aMvPref  := {}
	local aMvNat   := {}
	Local cHist    := ""
	Local cMvPref  := ""
	Local cMvNat   := ""
	Local cMvUniao := ""
	Local aErros   := {}
	Local nErr     := 0
	Local cNumTit  := ""
	Local nTamTit  := TamSX3("E2_NUM")[1]
	Local cTipo    := "TX"+Space(TamSx3("E2_TIPO")[1]-2)
	Local aDados   := {}
	Local nPosPar  := 0
	Local dDatVenc := CTOD("  /  /  ")

	//Numero do imposto
	If cPaisLoc $ "VEN|DOM"
		nPosPar := 1
	ElseIf cPaisLoc $ "BOL|COS"
		If Type("nF032Apur") == "N"
			nPosPar := nF032Apur
		Else
			nPosPar := 1
		EndIf
	Else
		nPosPar := MV_PAR01
	EndIf

	//Data de vencimento do titulo
	If cPaisLoc == "EQU"
		dDatVenc := MV_PAR05
	ElseIf FunName() $ "FISA024|FISA028"
		dDatVenc := MV_PAR07
	Else
		dDatVenc := MV_PAR04
	EndIf

	//Parametro dos impostos
	If cPaisLoc == "ANG"
		aMvSigla := {"SEL","CON","EMP","IMP"}
		aMvPref  := {"MV_PFSELO" ,"MV_PFCONS" ,"MV_PFEMP"  ,"MV_PFIMP"  } // Prefixo dos titulos de apuração
		aMvNat   := {"MV_NATSELO","MV_NATCONS","MV_NAT0797","MV_NATIMP" } // Natureza dos titulos de apuração
	ElseIf cPaisLoc == "EQU"
		aMvSigla := {"IVA","ICE"}
		aMvPref  := {"MV_PFIVA" ,"MV_PFICE"  } // Prefixo dos titulos de apuração
		aMvNat   := {"MV_NATIVA","MV_NATICE" } // Natureza dos titulos de apuração
	ElseIf cPaisLoc == "VEN"
		aMvSigla := {"IVA"}
		aMvPref  := {"MV_PFIVA" } // Prefixo dos titulos de apuração
		aMvNat   := {"MV_NATIVA" } // Natureza dos titulos de apuração
	ElseIf cPaisloc == "DOM"
		aMvSigla := {"ISC"}
		aMvPref  := {"MV_PFISC"} // Prefixo dos titulos de apuração
		aMvNat   := {"MV_NATISC"} // Natureza dos titulos de apuração
	ElseIf cPaisloc == "BOL"
		aMvSigla := {"IVA","ITR","ITR"}
		aMvPref  := {"MV_PFIVA","MV_PFITR","MV_PFITR"} // Prefixo dos titulos de apuração
		aMvNat   := {"MV_NATIVA","MV_NATITR","MV_NATITR"} // Natureza dos titulos de apuração
	ElseIf cPaisLoc == "PER"
		aMvSigla := {"ISC","IGV"}
		aMvPref  := {"MV_PFISC" ,"MV_PFIGV"  } // Prefixo dos titulos de apuração
		aMvNat   := {"MV_NATISC","MV_NATIGV" } // Natureza dos titulos de apuração
	ElseIf cPaisLoc == "COS"
		aMvSigla := {"IVA","ISC"}
		aMvPref  := {"MV_PFIVA","MV_PFISC" } // Prefixo dos titulos de apuração
		aMvNat   := {"MV_NATIVA","MV_NATISC" } // Natureza dos titulos de apuração
	EndIf

	//Historico do titulo
	If cPaisLoc == "VEN"
		cHist := cImpDesc + ": "+DtoC(mv_par01)+" A "+Dtoc(mv_par02)
	ElseIf cPaisloc == "DOM"
		If FunName() == "FISA024"
			cHist := mv_par01+" IMP. TELECOM IST01"
		ElseIf FunName() == "FISA028"
			cHist := mv_par01+" IMP. SEGUROS DSS"
		Else
			cHist := AllTrim(Str(YEAR(MV_PAR01))) +"/"+ StrZero(MONTH(MV_PAR01),2) + " IMP. CONSUMO ISC02"
		EndIf
	ElseIf cPaisLoc $ "BOL|COS"
		cHist := "Apuração " + aMvSigla[nPosPar]
	Else
		cHist := cImpDesc+": "+DtoC(mv_par02)+" A "+Dtoc(mv_par03)
	EndIf

	cMvPref  := GetNewPar(aMvPref[nPosPar],"")
	cMvNat   := GetNewPar(aMvNat[nPosPar],"")
	cMvUniao := Padr(GetNewPar("MV_UNIAO","GOV"),TamSx3("A2_COD")[1])

	dbSelectArea("SA2")
	dbSetOrder(1)
	IF SA2->(!dbSeek(xFilial()+cMvUniao+"00"))
		Reclock("SA2",.T.)
		SA2->A2_FILIAL := xFilial("SA2")
		SA2->A2_COD    := cMvUniao
		SA2->A2_LOJA   := "00"
		SA2->A2_NOME   := "GOVERNO"
		SA2->A2_NREDUZ := "GOVERNO"
		SA2->A2_BAIRRO := "."
		SA2->A2_MUN    := "."
		If cPaisLoc == "ANG"
			SA2->A2_EST    := "11" //LUANDA, capital de ANGOLA
		ElseIf cPaisLoc == "PER"
			SA2->A2_EST    := "15" //LIMA, capital de PERU
		ElseIf cPaisLoc == "EQU"
			SA2->A2_EST    := "17" //LIMA, capital de PERU
		ElseIF cPaisLoc == "VEN"
			SA2->A2_EST    := "16" //MIRANDA, capital da Venezuela
		ElseIF cPaisLoc == "DOM"
			SA2->A2_EST    := "01" //Distrito Nacional
		ElseIf cPaisLoc == "BOL"
			SA2->A2_EST    := "LP" //La Paz
		ElseIf cPaisLoc == "COS"
			SA2->A2_EST    := "01" //San Jose
		EndIf
		SA2->A2_END    := "."
		MsUnlock()
	EndIF

	if Empty(cMvNat)
		aAdd(aErros,aMvNat[nPosPar])
	Endif

	if Empty(cMvPref)
		aAdd(aErros,aMvPref[nPosPar])
	Endif

	For nErr = 1 to Len(aErros)
		ApMsgStop(STR0144+" - "+aErros[nErr],aErros[nErr]) //"Parametro invalido ou não cadastrado"
	Next

	If Len(aErros) > 0
		DisarmTransaction()
		Return aDados
	Endif

	dbSelectArea("SED")
	IF SED->(!dbSeek(xFilial("SED")+cMvNat))
		RecLock("SED",.T.)
		SED->ED_FILIAL  := xFilial("SED")
		SED->ED_CODIGO  := cMvNat
		SED->ED_CALCIRF := "N"
		SED->ED_CALCISS := "N"
		If cPaisLoc $ "DOM|BOL|COS"
			SED->ED_DESCRIC := aMvSigla[nPosPar]
		Else
			SED->ED_DESCRIC := cImpDesc
		EndIf
		MsUnlock()
	EndIf

	Do While Empty(cNumTit) .OR. SE2->(dbSeek(xFilial("SE2")+cMvPref+cNumTit+Space(Len(SE2->E2_PARCELA))+cTipo+SA2->A2_COD+SA2->A2_LOJA))

		cNumTit := AllTrim(Tabela("53",aMvSigla[nPosPar],.F.))

		If Empty(cNumTit)
			cNumTit := StrZero(1,nTamTit)
		Else
			cNumTit := Soma1(cNumTit,nTamTit)
		Endif

		If RecLock("SX5",Val(cNumTit)=1)
			SX5->X5_FILIAL  := xFilial()
			SX5->X5_TABELA  := "53"
			SX5->X5_CHAVE   := aMvSigla[nPosPar]
			SX5->X5_DESCRI	:= cNumTit
			SX5->X5_DESCSPA := cNumTit
			SX5->X5_DESCENG := cNumTit
			MsUnLock()
		EndIf
	Enddo

	aAdd(aDados,{"E2_FILIAL" ,xFilial("SE2"),nil})
	aAdd(aDados,{"E2_NUM"    ,cNumTit,nil})
	aAdd(aDados,{"E2_PREFIXO",cMvPref,nil})
	aAdd(aDados,{"E2_TIPO"   ,cTipo,nil})
	aAdd(aDados,{"E2_NATUREZ",cMvNat,nil})
	aAdd(aDados,{"E2_FORNECE",SA2->A2_COD,nil})
	aAdd(aDados,{"E2_LOJA"   ,SA2->A2_LOJA,nil})
	aAdd(aDados,{"E2_NOMFOR" ,SA2->A2_NREDUZ,nil})
	aAdd(aDados,{"E2_MOEDA"  ,1,nil})
	aAdd(aDados,{"E2_VALOR"  ,nValImp,nil})
	aAdd(aDados,{"E2_SALDO"  ,nValImp,nil})
	aAdd(aDados,{"E2_VLCRUZ" ,nValImp,nil})
	aAdd(aDados,{"E2_HIST"   ,cHist,nil})
	aAdd(aDados,{"E2_EMISSAO",dDataBase,nil})
	aAdd(aDados,{"E2_VENCTO" ,dDatVenc,nil})
	aAdd(aDados,{"E2_VENCREA",DataValida(dDatVenc,.T.),nil})
	aAdd(aDados,{"E2_VENCORI",dDatVenc,nil})
	aAdd(aDados,{"E2_EMIS1"  ,dDataBase ,nil})
	aAdd(aDados,{"E2_ORIGEM" ,FunName(),nil})

Return aDados

Function SEDPER(aRecnoSED)
	aAdd(aRecnoSED, {xFilial("SED"),"0000000001","DETRACCION IGV"} )
