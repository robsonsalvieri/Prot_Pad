Function UpdIB4Arg(cProv)
Local aAreaOld   := GetArea()                         // Area atual
Local aRecno := {}
Local nI	 := 0
Local cAuxPaisL := ""

If SFB->(RecCount()) > 0
	aRecno := ARIMPIB4(aRecno,"SFB",cProv)
	For nI := 1 to Len(aRecno)
		dbSelectArea("SFB")
		dbSetOrder(1)
		If !DbSeek(aRecno[nI][1]+aRecno[nI][2])
			RecLock("SFB",.T.)
			Replace FB_FILIAL  With aRecno[nI][01]
			Replace FB_CODIGO  With aRecno[nI][02]
			Replace FB_DESCR   With aRecno[nI][03]
			Replace FB_CPOLVRO With aRecno[nI][04]
			Replace FB_FORMENT With aRecno[nI][05]
			Replace FB_FORMSAI With aRecno[nI][06]
			Replace FB_COLBAS  With aRecno[nI][07]
			Replace FB_COLVAL  With aRecno[nI][08]
			Replace FB_ALIQ    With aRecno[nI][09]
			Replace FB_DIAVENC With aRecno[nI][10]
			Replace FB_PERIODO With aRecno[nI][11]
			Replace FB_JNS     With aRecno[nI][12]
			cAuxPaisL := "ANG|ARG|AUS|BOL|BRA|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|POR|PTG|SAL|URU|VEN"
			If cPaisloc == cAuxPaisL
				Replace FB_TABELA  With aRecno[nI][13]
			Endif
			cAuxPaisL := "ANG|ARG|AUS|BOL|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"
			If cPaisloc == cAuxPaisL
				Replace FB_INTEIC  With aRecno[nI][14]
			EndIf
			MsUnLock()
		EndIf
	Next nI
EndIf
If SFF->(RecCount()) > 0
	aRecno := {}
	aRecno := ARIMPIB4(aRecno,"SFF",cProv)
	For nI := 1 to Len(aRecno)
		dbSelectArea("SFF")
		dbSetOrder(5) // FF_FILIAL+FF_IMPOSTO+FF_CFO_C+FF_ZONFIS
		If !(DbSeek(aRecno[nI][1]+ aRecno[nI][21] + aRecno[nI][10] + aRecno[nI][24]))
			RecLock("SFF",.T.)
			Replace FF_FILIAL  With aRecno[nI][01]
			Replace FF_ALIQ    With aRecno[nI][02]
			Replace FF_NUM     With aRecno[nI][03]
			If cPaisLoc == "ARG"
				Replace FF_MESREF  With aRecno[nI][04]
			EndIf
			Replace FF_ITEM    With aRecno[nI][05]
			If cPaisLoc == "ARG"
				Replace FF_ATE     With aRecno[nI][06]
				Replace FF_ALQINSC With aRecno[nI][07]
			Endif
			Replace FF_CFO     With aRecno[nI][08]
			If cPaisLoc == "ARG"
				Replace FF_ALQNOIN With aRecno[nI][09]
			Endif
			Replace FF_CFO_C   With aRecno[nI][10]
			Replace FF_CFO_V   With aRecno[nI][11]
			If cPaisLoc $ "ARG|BOL|CHI|COL|EUA|PAR|PER|URU|VEN"
				Replace FF_IMPORTE With aRecno[nI][12]
			Endif
			If cPaisLoc $ "ANG|ARG|BOL|EQU|EUA|HAI|PAR|PTG|URU|VEN"
				Replace FF_CONCEPT With aRecno[nI][13]
			Endif
			If cPaisLoc $ "ARG|COL|PER"
				Replace FF_FXDE    With aRecno[nI][14]
			EndIf
			If cPaisLoc == "ARG"
				Replace FF_DESDE   With aRecno[nI][15]
				Replace FF_FXATE   With aRecno[nI][16]
			EndIf
			If cPaisLoc $ "ARG|CHI|PAR"
				Replace FF_RETENC  With aRecno[nI][17]
			Endif
			If cPaisLoc $ "ARG|COL|PER|VEN"
				Replace FF_PERC    With aRecno[nI][18]
			EndIf
			If cPaisLoc == "ARG"
				Replace FF_EXCEDE  With aRecno[nI][19]
				Replace FF_ESCALA  With aRecno[nI][20]
			Endif
			Replace FF_IMPOSTO With aRecno[nI][21]
			Replace FF_SERIENF With aRecno[nI][22]
			Replace FF_TIPO    With aRecno[nI][23]
			Replace FF_ZONFIS  With aRecno[nI][24]
			If cPaisLoc == "COL"  .And. Len(aRecno[nI]) > 24
				Replace FF_COD_TAB With aRecno[nI][25]
			Endif
			cAuxPaisL := "ANG|ARG|AUS|BOL|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"
			If cPaisLoc $ cAuxPaisL .And. Len(aRecno[nI]) > 25
				Replace FF_TEC With aRecno[nI][26]
			Endif
			If cPaisLoc $ cAuxPaisL .And. Len(aRecno[nI]) > 26
				Replace FF_GRUPO With aRecno[nI][27]
			EndIf
			MsUnLock()
		EndIf
	Next nI
EndIf
If SFZ->(RecCount()) > 0
	aRecno := {}
	aRecno := ARIMPIB4(aRecno,"SFZ",cProv)
	For nI := 1 to Len(aRecno)
		dbSelectArea("SFZ")
		dbSetOrder(1) // FZ_FILIAL+FZ_TIPMOV+FZ_ESPECIE+FZ_TIPO+FZ_SERIE+FZ_IMPOSTO
		If !DbSeek(aRecno[nI][1]+aRecno[nI][8]+aRecno[nI][05]+aRecno[nI][09]+aRecno[nI][07]+aRecno[nI][06])
			RecLock("SFZ",.T.)
			Replace FZ_FILIAL  With aRecno[nI][01]
			Replace FZ_DESIMPO With aRecno[nI][02]
			Replace FZ_DESTACA With aRecno[nI][03]
			Replace FZ_DESTIPO With aRecno[nI][04]
			Replace FZ_ESPECIE With aRecno[nI][05]
			Replace FZ_IMPOSTO With aRecno[nI][06]
			Replace FZ_SERIE   With aRecno[nI][07]
			Replace FZ_TIPMOV  With aRecno[nI][08]
			Replace FZ_TIPO    With aRecno[nI][09]
			MsUnLock()
		EndIf
	Next nI
EndIf
If SF4->(RecCount()) > 0
	aRecno := {}
	aRecno := ARIMPIB4(aRecno,"SF4")
	For nI := 1 to Len(aRecno)
		dbSelectArea("SF4")
		dbSetOrder(1) // F4_FILIAL+F4_CODIGO
		If !DbSeek(aRecno[nI][1]+aRecno[nI][2])
			RecLock("SF4",.T.)
			Replace F4_FILIAL  With aRecno[nI][01]
			Replace F4_CODIGO  With aRecno[nI][02]
			Replace F4_TIPO    With aRecno[nI][03]
			Replace F4_IPI     With aRecno[nI][04]
			Replace F4_CREDIPI With aRecno[nI][05]
			Replace F4_DUPLIC  With aRecno[nI][06]
			Replace F4_ESTOQUE With aRecno[nI][07]
			Replace F4_CF      With aRecno[nI][08]
			Replace F4_TEXTO   With aRecno[nI][09]
			Replace F4_BASEICM With aRecno[nI][10]
			Replace F4_BASEIPI With aRecno[nI][11]
			Replace F4_PODER3  With aRecno[nI][12]
			Replace F4_CIAP    With aRecno[nI][13]
			Replace F4_LIVRO   With aRecno[nI][14]
			Replace F4_ATUTEC  With aRecno[nI][15]
			Replace F4_ATUATF  With aRecno[nI][16]
			Replace F4_TPIPI   With aRecno[nI][17]
			cAuxPaisL := "ANG|ARG|AUS|BOL|BRA|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"
			If cPaisLoc $ cAuxPaisL
				Replace F4_DESTACA   With aRecno[nI][18]
			Endif
			Replace F4_ICM     With aRecno[nI][19]
			Replace F4_CREDICM With aRecno[nI][20]
			Replace F4_LFICM   With aRecno[nI][21]
			Replace F4_GERALF  With aRecno[nI][23]
			If !Empty(aRecno[nI][24])
				Replace F4_TESDV   With aRecno[nI][24]
			EndIf
			Replace F4_QTDZERO With aRecno[nI][25]
			If Len(aRecno[nI]) > 25 .And. FieldPos("F4_TESNCC") > 0
				Replace F4_TESNCC   With aRecno[nI][26]
			EndIf
			MsUnLock()
		EndIf
	Next nI
EndIf
If SFC->(RecCount()) > 0
	aRecno := {}
	aRecno := ARIMPIB4(aRecno,"SFC")
	For nI := 1 to Len(aRecno)
		dbSelectArea("SFC")
		dbSetOrder(1) // FC_FILIAL+FC_TES+FC_SEQUEN
		If !DbSeek(aRecno[nI][1]+aRecno[nI][02] + aRecno[nI][03])
			RecLock("SFC",.T.)
			Replace FC_FILIAL  With aRecno[nI][01]
			Replace FC_TES     With aRecno[nI][02]
			Replace FC_SEQ     With aRecno[nI][03]
			Replace FC_IMPOSTO With aRecno[nI][04]
			Replace FC_INCDUPL With aRecno[nI][05]
			Replace FC_INCNOTA With aRecno[nI][06]
			Replace FC_CREDITA With aRecno[nI][07]
			Replace FC_INCIMP  With aRecno[nI][08]
			Replace FC_BASE    With aRecno[nI][09]
			Replace FC_CALCULO With aRecno[nI][10]
			Replace FC_LIQUIDO With aRecno[nI][11]
			If cPaisLoc $ "ANG|ARG|AUS|BOL|CHI|COL|COS|DOM|EQU|EUA|HAI|MEX|PAD|PAN|PAR|PER|POR|PTG|SAL|URU|VEN"
				Replace FC_INTEIC  With aRecno[nI][12]
			Endif
			If Len(aRecno[nI]) >= 13
				Replace FC_AGRBASE  With aRecno[nI][13]
			Endif
			MsUnLock()
		EndIf
	Next nI
EndIf
RestArea( aAreaOld )
Return Nil
