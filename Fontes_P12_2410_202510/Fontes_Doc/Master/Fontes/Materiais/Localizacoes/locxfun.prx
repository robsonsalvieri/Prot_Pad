*/
Function LocIntDCF( cDCFori, lApaga )
Local lRetorno  := .T.
Local aAreaAnt  := GetArea()

If SuperGetMV('MV_INTDL')<>'S'
	Return lRetorno
EndIf
Do Case
	Case cDCFori == 'SC9'
		If !Empty(SC9->C9_SERVIC)
			If !lApaga
				//Alterado para incluir validação chamando esta função pois nela ja existe o tratamento para WMS novo e Atual .
			    lRetorno :=  WmsAvalSC6("4")
			Else
				//Chamada da função Wmsundoped pois a mesma trata WMS novo e Atual .
				lRetorno := WmsUndoPed(SC9->(Recno()))
			EndIf
		EndIf
	Case cDCFori == "SD1"
		If !Empty(SD1->D1_SERVIC)
			If !lApaga
				If WmsChkDCF("SD1",,,SD1->D1_SERVIC,/*Status*/"3",,SD1->D1_DOC,SD1->D1_SERIE,SD1->D1_FORNECE,SD1->D1_LOJA,SD1->D1_LOCAL,SD1->D1_COD,,,SD1->D1_NUMSEQ)
					lRetorno := WmsAvalDCF("2")
				EndIf
			Else
				lRetorno := WmsDelDCF("1","SD1")
			EndIf
		EndIf
	Case cDCFori == "SD2"
		If !Empty(SD2->D2_SERVIC)
			If !lApaga
				If WmsChkDCF("SD2",,,SD2->D2_SERVIC,/*Status*/"3",,SD2->D2_DOC,SD2->D2_SERIE,SD2->D2_FORNECE,SD2->D2_LOJA,SD2->D2_LOCAL,SD2->D2_COD,,,SD2->D2_NUMSEQ)
					lRetorno := WmsAvalDCF("2")
				EndIf
			Else
				lRetorno := WmsDelDCF("1","SD1")
			EndIf
		EndIf
EndCase
RestArea(aAreaAnt)
Return lRetorno
