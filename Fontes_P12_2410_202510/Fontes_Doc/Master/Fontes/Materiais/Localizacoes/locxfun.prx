Function CORR_GRAVA( dCorrData, cCorrelat, cDoc , cSerie, cEspecie , cCliFor	 , cLoja, lMsg )
Local aArea 	:= GetArea()
Local lRet  	:= .F.
Local cPeriodo	:= ""
Local cNumCor	:= ""
Local cFilSFY := xFilial("SFY")
DEFAULT lMsg:=.T.
DEFAULT dCorrData:=dDataBase
DEFAULT cCorrelat:="0"
DEFAULT cDoc:=""
DEFAULT cSerie:=""
DEFAULT cEspecie:=""
DEFAULT cCliFor:=""
DEFAULT cLoja:=""

If CORR_USO()
	If Upper(SuperGetMV("MV_CTRLCOR",,"M"))=="M"
		cPeriodo:=Left(Dtos(dCorrData),6)
	Else
		cPeriodo:=Left(Dtos(dCorrData),4)+"00"
	Endif
	cNumCor:=Padl(AllTrim(cCorrelat),TamSX3("FY_NUMCOR")[1],"0")
	lRet:=CORR_VERIF(dCorrData,cNumCor,.F.) .and.  MayIUseCode( "SFY"+cFilSFY+cNumCor)
	While !lRet     //verifica se esta na memoria, sendo usado
		cNumCor:=CORR_PROX(dCorrData)
		lRet:= CORR_VERIF(dCorrData,cNumCor,.F.) .and.  MayIUseCode( "SFY"+cFilSFY+cNumCor)
	End
	If lRet .And. GeraCorrel()
		DbSelectArea("SFY")
		RecLock("SFY",.T.)
	    Replace FY_FILIAL With xFilial("SFY")
	    Replace FY_PERIODO With cPeriodo
	    Replace FY_NUMCOR With cNumCor
	    Replace FY_DOC With cDoc
	    //Replace FY_SERIE With cSerie,
	    Iif(!empty(cSerie),SerieNfId("SFY",1,"FY_SERIE",,,,cSerie),) //Projeto Chave Unica - Tiago Silva
	    Replace FY_ESPECIE With cEspecie
	    If cPaisloc == "CHI"
		    Replace FY_FORNECE With cCliFor
		    Replace FY_CLIEFOR With cCliFor
		Endif
	    Replace FY_LOJA With cLoja
	    MsUnlock()
	    FreeUsedCode()
	    If cNumCor<>cCorrelat .and. lMsg .And. cPaisLoc <> 'CHI'
	    	MsgAlert(cCorrelat+" - "+STR0096+cNumCor) //"Este Correlativo foi utilizado enquanto se executava esta operacao. O novo Correlativo e "
	    Endif
	ElseIf lRet
	    cNumCor := ""
	Else
		cNumCor := "ERR"
	Endif
Endif
RestArea(aArea)
Return(cNumCor)
