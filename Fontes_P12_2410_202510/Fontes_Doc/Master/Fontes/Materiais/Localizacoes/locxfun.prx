Function LocDocPre( aDocPre )
Local aArea := GetArea()
Local nI	:= 0
Local cFilSF3	:= xFilial("SF3")

DbSelectArea("SF3")
DbSetOrder(4)
For nI := 1 to Len(aDocPre)
	dbSeek(cFilSF3+aDocPre[nI,1])
	While ! EOF() .And. SF3->(F3_CLIEFOR+F3_LOJA+F3_NFISCAL+F3_SERIE) == aDocPre[nI,1]
		If SF3->F3_ESPECIE == aDocPre[nI,2]
			RecLock("SF3",.F.)
			SF3->F3_DOCPRE := aDocPre[nI,3]
			MsUnLock()
		EndIf
		DbSkip()
	End
Next nI
RestArea(aArea)
Return
