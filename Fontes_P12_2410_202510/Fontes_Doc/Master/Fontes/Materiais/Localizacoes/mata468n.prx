				Posicione("SF2",1,xFilial('SF2')+(cRem)->D2_DOC+(cRem)->D2_SERIE+(cRem)->D2_CLIENTE+(cRem)->D2_LOJA+(cRem)->D2_FORMUL,'F2_MOEDA'))
				nPrcBrut:= nPrcVen
			Else
