	nTotMerc    := 0.00
	nTotDesc    := 0.00
	nTotDesBo   := 0.00
	nTotInden 	:= 0.00
	nVarDesc    := 0.00
	nFrete      := 0.00
	nSeguro     := 0.00
	nDespesa    := 0.00
	nDesNTrb    := 0.00
	nTara       := 0.00
	nVlrTot    	:= 0.00
	nVlrDsc    	:= 0.00
	nVlrTotal  	:= 0.00
	nVlrDscTot 	:= 0.00
	aFrete		:=	{}
	nl460PBRUTO	:=	0
	nl460PLIQUI	:=	0
	aLivro      := {}
	aUltItem    := {}
	aImpVarSF2  := {}
