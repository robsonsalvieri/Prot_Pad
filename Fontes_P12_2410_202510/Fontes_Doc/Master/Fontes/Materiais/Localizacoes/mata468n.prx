	If lPedidos .And. lMVLogMov .And. SC6->(MsSeek(cFilSC6+SC9->(C9_PEDIDO+C9_ITEM+C9_PRODUTO)))
		If SF4->(MsSeek(cFilSF4+SC6->C6_TES)) .And. SF4->F4_ESTOQUE == 'S'
			lLogMov := LogMov('SC9',.F.,.T.,SC9->C9_PRODUTO,SC9->C9_LOCAL,.T.)
		EndIf
	EndIf

	//Verifica novamente os empenhos para garantir
	If (!lPedidos .and. lFatDiverg) .Or. (!lLogMov .AND. lPedSemAd)

		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		//³Verifico se irei pegar os dados para comparar do SC5 ou do SD2³
		//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
		aQuebra	:=	Eval(bQuebra,lPedOk)

		If !lPedOk
