				If SuperGetMV("MV_ARREFAT")== "S"     // Arredonda Valores
					If cPaisLoc == "CHI"
						nDesconto	:=xmoeda(nDesconto,SC5->C5_MOEDA, nMoedSel,ddatabase,MsDecimais(nMoedSel)+1, nTaxaMoeda)
