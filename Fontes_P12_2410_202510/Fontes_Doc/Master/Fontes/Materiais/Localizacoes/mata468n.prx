Function A468ChgRel(nPos,aNFs,oLbx,aFacs,nPosNfs,aTitles)
Local oDlg			:= Nil
Local nLenFacs		:= Len(aFacs[1])
Local lRet			:= .T.

Private cSerOri		:= ""
Private cNFRef		:= ""
Private cUUIDRel	:= ""
Private cEspecie	:= ""	// Consulta a SF3

If nPos > 0

	cSerOri  := aFacs[nPos][nLenFacs-3]
	cNFRef   := aFacs[nPos][nLenFacs-2]
	cUUIDRel := aFacs[nPos][nLenFacs-1]
	cEspecie := PadR(MVNOTAFIS,GetSX3Cache("F2_ESPECIE", "X3_TAMANHO"))

	DEFINE MSDIALOG oDlg TITLE OemToAnsi(STR0269) FROM 180,180 TO 350,700 PIXEL //"Relación con CFDI a Sustituir"
	@ 010,010 SAY OemToAnsi(STR0051) OF oDlg PIXEL //"Serie"
	@ 008,035 MSGET cSerOri PICTURE "@!" OF oDlg PIXEL VALID A468ValRel(cSerOri,cNFRef,@cUUIDRel,1) F3 "DOCSUS"
	@ 025,010 SAY OemToAnsi(STR0050) OF oDlg PIXEL //"Factura"
	@ 023,035 MSGET cNFRef PICTURE "@!" OF oDlg PIXEL VALID A468ValRel(cSerOri,cNFRef,@cUUIDRel,1)
	@ 040,010 SAY OemToAnsi(STR0270) OF oDlg PIXEL //"UUID"
	@ 038,035 MSGET cUUIDRel PICTURE "@!" OF oDlg PIXEL WHEN .F.
	DEFINE SBUTTON FROM 060,200 TYPE 1 ENABLE OF oDlg ACTION (lRet := A468ValRel(cSerOri, cNFRef, cUUIDRel,2), IIf(lRet,oDlg:End(),))
	DEFINE SBUTTON FROM 060,230 TYPE 2 ENABLE OF oDlg ACTION (lRet := .F., oDlg:End())
	ACTIVATE MSDIALOG oDlg CENTERED

	If lRet
		aFacs[nPos][nLenFacs-3] := cSerOri
		aFacs[nPos][nLenFacs-2] := cNFRef
		aFacs[nPos][nLenFacs-1] := cUUIDRel
	EndIf

EndIf

Return Nil

/*/{Protheus.doc} A468ValRel
	Valida Serie/Folio NF relacionada en SF3, como factura cancelada y CFDI pendiente de timbrar
	@type    Static Function
	@author  ARodriguez
	@since   21/02/2022
	@version 1.0
	@param   cSerOri, c, Serie NF
	@param   cNFRef, c, Folio NF
	@param   cUUIDRel, c, Folio fiscal
	@param   nVal, n, Opción de validación; 2 para finalizar
	@return  lRet, l, ok?
/*/
Static Function A468ValRel(cSerOri,cNFRef,cUUIDRel,nVal)
Local cAliasQry	:= GetNextAlias()
Local cDtCanc	:= ""
Local cUUIDs	:= ""
Local lRet		:= .T.

If !Empty(cSerOri) .And. !Empty(cNFRef)
	BeginSQL Alias cAliasQry
		SELECT F3_DTCANC, F3_CNATREC, F3_STATUS
			FROM %Table:SF3% SF3
			WHERE F3_FILIAL = (%exp:XFILIAL("SF3")%)
			AND SF3.F3_SERIE = %Exp:cSerOri%
			AND SF3.F3_NFISCAL = %Exp:cNFRef%
			AND SF3.F3_TIPOMOV = 'V'
			AND SF3.F3_ESPECIE = %Exp:MVNOTAFIS%
			AND SF3.%NotDel%
	EndSQL

	cUUIDRel := Space(Len(cUUIDRel))

	If (cAliasQry)->(Eof())
		Aviso(OemToAnsi(STR0009),OemToAnsi(STR0271),{OemToAnsi(STR0176)}) //"La factura no existe."
		lRet := .F.

	Else
		Do While (cAliasQry)->(!Eof())
			If !Empty((cAliasQry)->F3_CNATREC) .And. (cAliasQry)->F3_STATUS $ 'S|A'
				cDtCanc := (cAliasQry)->F3_DTCANC
				cUUIDRel := (cAliasQry)->F3_CNATREC
				Exit
			EndIf
			(cAliasQry)->(dbSkip())
		Enddo

		(cAliasQry)->(dbCloseArea())

		If Empty(cUUIDRel)
			Aviso(OemToAnsi(STR0009),OemToAnsi(STR0272),{OemToAnsi(STR0176)}) //"La factura no está cancelada o ya se obtuvo acuse de la cancelación."
			lRet := .F.

		Else
			// Query de facturas emitidas a partir de la fecha de cancelación del CFDI relacionado
			BeginSQL Alias cAliasQry
				SELECT R_E_C_N_O_
					FROM %Table:SF2% SF2
					WHERE F2_FILIAL = (%exp:XFILIAL("SF2")%)
					AND SF2.F2_TIPODOC = '01'
					AND SF2.F2_ESPECIE = %Exp:MVNOTAFIS%
					AND SF2.F2_RELSAT = '04'
					AND SF2.F2_EMISSAO >= %Exp:cDtCanc%
					AND SF2.%NotDel%
			EndSQL

			Do While (cAliasQry)->(!Eof())
				SF2->(dbGoto((cAliasQry)->R_E_C_N_O_))
				cUUIDs := SF2->F2_UUIDREL
				If !Empty(cUUIDs) .And. cUUIDRel $ cUUIDs
					Aviso(OemToAnsi(STR0009),OemToAnsi(STR0273)+SF2->(F2_SERIE+"/"+F2_DOC),{OemToAnsi(STR0176)}) //"El folio fiscal ya está relacionado a la factura "
					lRet := .F.
					Exit
				EndIf
				(cAliasQry)->(dbSkip())
			Enddo

			(cAliasQry)->(dbCloseArea())

		EndIf

	EndIf

ElseIf Empty(cSerOri) .And. Empty(cNFRef)
	cUUIDRel := Space(Len(cUUIDRel))

ElseIf nVal == 2
	Aviso(OemToAnsi(STR0009),OemToAnsi(STR0274),{OemToAnsi(STR0176)}) //"Debe definir serie y número de factura o dejar ambos campos en blanco."
	lRet := .F.

EndIf

Return lRet

/*/{Protheus.doc} fVALNUMPV
	función para actualizar los números repetidos
	@author adrian.perez
	@since 09/11/2023
	@param cNumero , carácter,  número a asignar de la SX5
	@param cSerie, carácter,  serie usadda en la factura
	@param aNotaRes, arreglo,  arreglo donde se guarda los números actualizados
	@param nTamNf , número,  tamaño del número de la factura
	@return nulo
	/*/
Function fVALNUMPV(cNumero,cSerie,aNotaRes,nTamNf)

	Local cGetPV:=""
	Local nTamCamp:=GetSx3Cache('X5_CHAVE', 'X3_TAMANHO')
	DEFAULT cNumero:=""
	DEFAULT cSerie:=""
	DEFAULT aNotaRes:={}
	DEFAULT nTamNf:=0

	cGetPV:= POSICIONE("CFH",1, xFilial("CFH")+cPontoVend,"CFH_IDPV")
	IF len(aNotaRes)>0
		ASort(aNotaRes, , , {|x,y|x > y})
	ENDIF

	cNumero:=	fSX5NUMPV(PADR(ALLTRIM( cSerie )+cGetPV,nTamCamp),cNumero)

	IF Ascan(aNotaRes,cNumero+cSerie ) <> 0
		cNumero:=Soma1(Substr(aNotaRes[1],1,LEN(cNumero)),nTamNf)
	ENDIF

Return nil

/*/{Protheus.doc} fSX5NUMPV
	función para personalizar el filtrado de l SX5
	@author adrian.perez
	@since 08/11/2023
	@param cFiltro, , carácter,  filtro perzonalidado
	@param cNumero , carácter,  número a asignar de la SX5
	@return cNumero , carácter,  número a asignar de la SX5
	/*/
Function fSX5NUMPV(cFiltro,cNumero)

	Local aSX5 :=FWGetSX5("01")
	Local nPos:=0

	DEFAULT cFiltro:=""
	DEFAULT cNumero:=""

		IF len(aSx5)>0
			nPos:=Ascan(aSX5,{|x| x[3] ==cFiltro })
			IF nPos<>0
				cNumero := LocConvNota(aSX5[nPos][4],Len(Alltrim(aSX5[nPos][4])))
			EndIf
		ENDIF

Return cNumero

/*/{Protheus.doc} ChecErro
	Verifica el error generado dentro de MarkBrow()
	@type  Static Function
	@author ARodriguez
	@since 13/05/2024
	@version 1.0
	@param e, objet, ErrorBlock
	@param lPedidos, lógico, pedidos (.T.) / remisiones (.F.)
	@return Nil
	/*/
Static Function ChecErro(e,lPedidos)
	
	Default lPedidos := .F.

	If ("variable does not exist" $ e:Description .Or. "invalid field name in Alias" $ e:Description) .And. IsInCallStack("EXECUTEFILTER")
		Help(" ",1,"HELP",,STR0297+IIf(lPedidos,STR0089,STR0088),1,0,,,,,,{STR0298+IIf(lPedidos,STR0088,STR0089)})//"Existe al menos un filtro activo para "#"Remitos"/"Pedidos"##"Debe desactivarlo(s) antes de facturar "#"Pedidos"/"Remitos"
	Else
		AutoGRLog(e:ERRORSTACK)	// Crea archivo log a partir del atributo de Error que contiene la pila de llamadas
		MostraErro()			// Visualiza log del error a partir del archivo creado
	EndIf

	/* Para efectos de cobertura (IxC), es importante que el RETURN de la función esté en la misma línea que el BREAK,
		ya que, en líneas separadas, al ejecutarse el Break, nunca llega a Return */

	BREAK; Return


/*/{Protheus.doc} fSeries
	Se guarda el ultimo número de cada serie
	@author adrian.perez
	@since 21/10/2024
	@param cNUM, Carácter, número de la serie
	@param aSerLast,array, arreglo donde se guarda el ultimo número de cada serie
	@param cSerie, Carácter, serie
	@param nPos, numérico, posición de la serie dentro del arreglo aSerLast
	@param lNoFisrt, boleano, determina si no es la primera que se asigna un número a la serie
	@return Nil
	/*/
Function fSeries(cNUM,aSerLast,cSerie,nPos,lNoFisrt)

DEFAULT cNUM:=""
DEFAULT aSerLast:={}
DEFAULT cSerie:=""
DEFAULT nPos:=0
DEFAULT lNoFisrt:=.F.

	If nPos==0
		AAdd(aSerLast,{cSerie,cNUM})
	Else
		If  aSerLast[nPos,2]!=cNUM .AND. lNoFisrt
			aSerLast[nPos,2]:=cNUM
		EndIF
	EndIF
	
return nil



/*/{Protheus.doc} fChecaRem
Verifica si el remito a facturar tuvo algún cambio en los campos
--  'D2_QUANT'    //Cantidad   
--  'D2_QTDEFAT'  //Cantidad ya Facturada  
--  'D2_QTDAFAT'  //Cantidad a facturar     
--  'D2_QTDEDEV'  //Cantidad Devuelta    

@type  Function
@author adrian.perez
@since 12/08/2025
@version 1.0
@param
nRecno, Numérico, Número de registro en SD2
nRecTrb, Numérico, Número de registro seleccionado de la tabla TRB
aRecsTRB, Array, Arreglo que contiene los elementos seleccionados de la tabla TRB
aRecs, Array, Arreglo que contiene el número de registro de la tabla SD2 seleccionados

@return nActReg, Numérico, Indica el número de cambios encontrados
/*/

function fChecaRem(nRecno,nRecTrb,aRecsTRB,aRecs)

Local  nCampo	:=1
Local  nCantidad:=0
Local  nCantAFact:=0
Local  nCantDev	:=0
Local  nCantYFact:=0
Local  cQuery	:=""
Local  oExec 	:=Nil
Local  aArea := GetArea()
Local  nActReg:=0
Local  cMar:=""
Local  nCanSel:=0
Local  nPos:=0

DEFAULT nRecno:=0
DEFAULT nRecTrb:=0
DEFAULT aRecsTRB:={}
DEFAULT aRecs:={}



    cQuery+= " SELECT D2_QUANT,D2_QTDAFAT,D2_QTDEFAT,D2_QTDEDEV,D2_OK,D2_DOC "         
    cQuery+="  FROM "+ RetSqlName("SD2") +"  "
    cQuery+= " WHERE R_E_C_N_O_= ? "
    cQuery+= " AND   D_E_L_E_T_=? "
	cQuery := ChangeQuery(cQuery)
			
    oExec := FwExecStatement():New(cQuery)

    oExec:SetNumeric(nCampo++, nRecno) 
    oExec:SetString(nCampo++, ' ')
    
	nCantidad:=oExec:ExecScalar('D2_QUANT')		//Cantidad   
	nCantYFact:=oExec:ExecScalar('D2_QTDEFAT') //Cantidad ya Facturada  
	nCantAFact:=oExec:ExecScalar('D2_QTDAFAT')  //Cantidad a facturar     
	nCantDev:=oExec:ExecScalar('D2_QTDEDEV')   //Cantidad Devuelta        
	
	nCantidad-=nCantYFact+nCantDev   
	
	IF nCantidad<=0 
		
		TRB->(MsGoTo(nRecTrb))
		cMar:= TRB->D2_OK
		Reclock('TRB',.F.)
			
			TRB->D2_OK:="  "
			TRB->D2_QTDEFAT   := nCantYFact	//Cantidad ya Facturada  
			TRB->D2_QTDAFAT   := nCantAFact //Cantidad a facturar    
			TRB->D2_QTDEDEV   := nCantDev  //Cantidad Devuelta   
		
		TRB->(MsUnlock())

		nPos	:=	Ascan(aRecsTRB,nRecTrb)
		IF nPos>0
			Adel(aRecsTRB,nPos)
			aSize(aRecsTRB,Len(aRecsTRB)-1)
			nPos:=0
		ENDIF

		DbSelectArea("SD2")
		MsGoTo(nRecno)
		If SD2->D2_OK == cMar
			RecLock("SD2",.F.)
			 SD2->D2_OK:= "  "
			SD2->(MsUnLock())
		ENDIF 
		
		nPos	:=	Ascan(aRecs,nRecno)
		IF nPos>0
			Adel(aRecs,nPos)
			aSize(aRecs,Len(aRecs)-1)
			nPos:=0
		ENDIF

		nActReg++
	ENDIF
	
	If LEN(aAuxSelPar)>0
		nPos:=ascan(aAuxSelPar, {|x| x[1]==nRecno })
		IF nPos>0
			nCanSel:=aAuxSelPar[nPos][2]
			nPos:=0
		Endif
	Endif

	IF ((nCantidad>0) .AND. (nCanSel==0)) .OR. (nCanSel>nCantidad)
	  //si existio una devolución parcial O facturación parcial
		TRB->(MsGoTo(nRecTrb))
		cMar:= TRB->D2_OK
		IF TRB->D2_QTDAFAT <> nCantidad
			Reclock('TRB',.F.)
				
				TRB->D2_QTDEFAT   := nCantYFact	//Cantidad ya Facturada  
				TRB->D2_QTDAFAT   := nCantidad //Cantidad a facturar  
				TRB->D2_QTDEDEV   := nCantDev //Cantidad Devuelta   

			TRB->(MsUnlock())

			DbSelectArea("SD2")
			MsGoTo(nRecno)
			If SD2->D2_OK == cMar
				RecLock("SD2",.F.)
				SD2->D2_QTDAFAT:=  nCantidad 
				SD2->(MsUnLock())
			ENDIF 

			nActReg++
		ENDIF
		
	ENDIF

	oExec:Destroy()
    
	RestArea(aArea)
	FreeObj(oExec)
	oExec := nil 
	aSize(aArea,0)
	
return nActReg

/*/{Protheus.doc} RatDescIt
Verifica si hay diferencia entre el valor C5_DESCONT vs el valor asígnado a cada ítem a facturar.
@type function
@version 12.1.2410
@author luis.samaniego
@since 8/18/2025
@param nDesc_Tot, numeric, Valor de SC5->C5_DESCONT.
@param nDesc_It, numeric, Total descuento por ítem.
@param nC5Descont, numeric, Descuento del ítem.
@param nQtdIt, numeric, Cantidad del ítem (C9_QTDLIB).
/*/
Static Function RatDescIt(nDesc_Tot, nDesc_It, nC5Descont, nQtdIt)
Local nValDif := 0

Default nDesc_Tot := 0
Default nDesc_It := 0
Default nC5Descont := 0
Default nQtdIt := 0

	nValDif := (nDesc_Tot - nDesc_It)

	If nValDif > 0
		nC5Descont += nValDif / nQtdIt
	ElseIf nValDif < 0
		nC5Descont -= (nValDif * -1) / nQtdIt
	EndIf

Return
