If cPaisLoc == "ARG" .And. SC6->(FieldPos("C6_TPVENT"))>0 .And. SC6->(FieldPos("C6_FECDSE"))>0 .And. SC6->(FieldPos("C6_FECHSE"))>0
	cTpVent := SC6->C6_TPVENT
	dDtIniS	:= SC6->C6_FECDSE
	dDtFinS	:= SC6->C6_FECHSE
