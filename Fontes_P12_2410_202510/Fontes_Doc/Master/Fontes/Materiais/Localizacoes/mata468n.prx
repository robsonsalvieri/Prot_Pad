aRemTots :=	{}
aRatDesp :=	{}

For nNFs	:=	1	To Len(aNFs)
	If cPaisLoc $ "MEX|PER"
		aRegFIE := {}
		lRegFIE := {}
		RegFR3(aNFs[nNFs])
	EndIf
	If (lLancPad10 .or. lLancPad20) .And. aParams[24] == 1
		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
