Static Function PegaNum(cSerie)

Local cNumero
Local cFilSX5
Local aRet	  := {Nil,Nil}
Local lRetNum := .T.

DbSelectArea("SX5")
DbSetOrder(1)
If lSeqEspecie
   lRetNum  := MsSeek( xFilial("SX5")+"AC"+"RFN"+cSerie,.F. )
Else
	If cPaisLoc == "PAR" .And. FindFunction("CHX5FILPAR") 
		cFilSX5 := CHX5FILPAR(cSerie) 
		lRetNum  := MsSeek(cFilSX5+"01"+cSerie,.F. ) 
	Else 
		lRetNum  := MsSeek( xFilial("SX5")+"01"+cSerie,.F. )
	Endif
EndIf
If lRetNum
	cNumero := LocConvNota(X5Descri(),TamSX3('F2_DOC')[1])
	aRet	:=	{cSerie,cNumero}
Else
    //"Atencao"###'Nao foi achada numeracao para a serie'
	Aviso(STR0009,STR0169+cSerie+'"',{"OK"})
Endif

Return aRet


