Static Function ProrratPDV(aItems, nValor)
	Local nItem		:= 0
	Local nPorc		:= 0
	Local aProrrat	:= {}

	For nItem := 1 To Len(aItems)
		If cPaisLoc == "PER"
			nPorc := nVlrAdiant/nValMerc
		Else
			nPorc := aItems[nItem][2]/nValor
		EndIf
		AADD(aProrrat,nPorc)
	Next
Return aProrrat

