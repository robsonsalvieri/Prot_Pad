Static Function SetSD2SF2()
Local aArea		:=	GetArea()
Local aAreaSF2
Local nX	:=	0 ,cChave:=""

Default aSF2	:=	{{"",""}}
aSD2	:=	{}

DbSelectArea("SD2")
For nX:=1	To fCount()
	AAdd(aSD2,{FieldName(nX),FieldGet(nX)})
Next
Aadd(aSD2,{'RECNO',SD2->(Recno())})

cChave	:=	xFilial('SF2')+SD2->D2_DOC+SD2->D2_SERIE+SD2->D2_CLIENTE+SD2->D2_LOJA+SD2->D2_FORMUL
If aSF2[1][1] <> cChave
	aAreaSF2	:=	SF2->(GetArea())
	DbSelectArea("SF2")
	DbSetOrder(1)
	MsSeek(cChave)
	aSF2	:=	{{cChave,""}}
	For nX:=1	To fCount()
		AAdd(aSF2,{FieldName(nX),FieldGet(nX)})
	Next
	Aadd(aSF2,{'RECNO',SF2->(Recno())})
	RestArea(aAreaSF2)
Endif
RestArea(aArea)
Return

