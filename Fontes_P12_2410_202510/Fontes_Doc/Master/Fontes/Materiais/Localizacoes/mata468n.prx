			 	nPrcBruto :=xmoeda(nPrcBruto,GetSF2('F2_MOEDA'), nMoedSel,ddatabase,aTamSx3Prc[2],nTaxamoeda)
		 	EndIf
		EndIf
	Else
		lNoPrUnit   := Empty(GetSD2('D2_PRCVEN'))
		nPrcBruto   := Iif(GetSD2('D2_PRUNIT')>0,GetSD2('D2_PRUNIT') ,GetSD2('D2_PRCVEN'))
