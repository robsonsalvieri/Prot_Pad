/*/
Function A370Pos(oModel)
Local lRet 	:= .T.
Local nOpcA	:= oModel:getOperation()

If nOpcA == MODEL_OPERATION_INSERT .OR. nOpcA == MODEL_OPERATION_UPDATE 
	lRet := A370tOk(nOpcA)
ElseIf nOpcA == MODEL_OPERATION_DELETE
	lRet := A370Qualy()
	If lRet .AND. Existblock("MT370DEL")
		lRet := Execblock("MT370DEL",.F.,.F.,{nOpcA})
	Endif	 
EndIf

Return lRet

