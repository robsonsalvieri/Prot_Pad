Function A370tOk(nOpc)

Local lMt370Atu := ExistBlock("MT370TOK")
Local lRet := .T.         

lRet := A370ProCli(M->A7_PRODUTO)	

If lMt370Atu
	lRet := ExecBlock("MT370TOK",.F.,.F.,{nOpc})
	lRet := IIf(ValType(lRet) <> "L",.T.,lRet)
Endif

Return lRet
