Function PmsUpDat()
Local cText:= STR0001 //"Compatibilizando o arquivo"
Local aArea:= GetArea()

dbSelectArea("AE1")
dbGotop()
UpdSet01(LastRec())

While !Eof()
	UpdInc01(cText+" AE1",.T.)
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Grava os campos                                            ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	Reclock("AE1",.F.)
	Replace AE1_PRIORI With 500
	MsUnlock()
	dbSkip()
EndDo

dbSelectArea("AF2")
dbGotop()
UpdSet01(LastRec())

While !Eof()
	UpdInc01(cText+" AF2",.T.)
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Grava os campos                                            ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	Reclock("AF2",.F.)
	Replace AF2_PRIORI With 500
	MsUnlock()
	dbSkip()
EndDo

dbSelectArea("AF9")
dbGotop()
UpdSet01(LastRec())

While !Eof()
	UpdInc01(cText+" AF9",.T.)
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Grava os campos                                            ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	Reclock("AF9",.F.)
	Replace AF9_PRIORI With 500
	MsUnlock()
	dbSkip()
EndDo


RestArea(aArea)
Return