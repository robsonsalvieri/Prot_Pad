Function Mata490(xAuto,nOpcAuto,nPosArotina)
//Ŀ
// Define Variaveis                                             
//
Local aCores 		:= {}
Local lPanelFin 	:= If (FindFunction("IsPanelFin"),IsPanelFin(),.F.)

Local lSeek			:= If((nOpcAuto == 4 .OR. nOpcAuto == 5),.F.,.T.)  //Indica se a funo dever efetuar o posicionamento automatico com base nos dados enviados no parametro aAuto.

Private aAC 		:= { OemToAnsi(STR0001),OemToAnsi(STR0002) },;  //"Abandona"###"Confirma"
aCRA				:= { OemToAnsi(STR0003),OemToAnsi(STR0004),OemToAnsi(STR0005)}  //"Confirma"###"Redigita"###"Abandona"
PRIVATE cCadastro 	:= OemToAnsi(STR0006)  //"Atualizao das Comisses"
Private lTab 		:= .F.

PRIVATE aRotina 	:= MenuDef()
PRIVATE aAuto 		:= {}
PRIVATE l490Auto 	:= xAuto <> NIL

Static lIntegDef  	:= .F.

DEFAULT nPosArotina := 0

lIntegDef := FWHasEAI("MATA490",.T.,,.T.)

PswOrder(1)
If PswSeek( __cUserID, .T. )
   cPermis := subs(PswRet()[2,5],115,1) // verifica as permisses do usurio para Mbrowse
EndIf
//Ŀ
// Ponto de entrada                                             
//
If ExistBlock("MT490BRW")
	ExecBlock("MT490BRW",.F.,.F.)
EndIf

//Ŀ
// Endereca a funcao de BROWSE                                  
//
If ( Type("l490Auto") <> "U" .And. l490Auto )
	aAuto := xAuto
	MBrowseAuto( nOpcAuto, aClone( aAuto ), "SE3", lSeek )
Else
	If nPosArotina > 0 // Sera executada uma opcao diretamento de aRotina, sem passar pela mBrowse
		dbSelectArea("SE3")
		bBlock := &( "{ |a,b,c,d,e| " + aRotina[ nPosArotina,2 ] + "(a,b,c,d,e) }" )
		Eval( bBlock, Alias(), (Alias())->(Recno()),nPosArotina)
	Else 
		mBrowse( 6, 1,22,75,"SE3",,"E3_DATA",,,,A490Legend())
	Endif	
EndIf
Return

