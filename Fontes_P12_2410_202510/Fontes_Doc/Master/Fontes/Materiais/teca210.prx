/*/{Protheus.doc} UsaCTRMan

@description Verifica se possui integração com field service
@author Augusto Albuquerque
@since  13/01/2022
/*/
//--------------------------------------------------------------------------------
Static Function UsaCTRMan()
Local cAliasAAH	:= ""
Local cSql 		:= ""

If cUsaCtrMan == '0'

	cSql += " SELECT 1 REC FROM " + RetSqlName( "AAH" ) + " AAH "
	cSql += " WHERE "
	cSql += " AAH.AAH_FILIAL = '" + xFilial("AAH") + "' AND "
	cSql += " AAH.D_E_L_E_T_ = ' ' "

	cSql := ChangeQuery(cSql)
	cAliasAAH := GetNextAlias()
	dbUseArea( .T., "TOPCONN", TCGENQRY(,,cSql),cAliasAAH, .F., .T.)

	If (cAliasAAH)->(EOF())
		cUsaCtrMan := '2'
	Else
		cUsaCtrMan := '1'
	EndIf
	(cAliasAAH)->(DbCloseArea())
EndIf

Return (cUsaCtrMan == '1')
