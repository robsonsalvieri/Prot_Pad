/*/{Protheus.doc} AT210VerPV

@description Exclui a tabela auxiliar do pedido de venda e volta a data do ultimo pedido
@author Augusto Albuquerque
@since  13/01/2022
/*/
//--------------------------------------------------------------------------------
Function AT210VerPV()
Local aArea		:= GetArea()
Local cAliasAAW	:= ""
Local lRet		:= .T. 

If UsaCTRMan()
	If TECAAWHiPV()
		dbSelectArea("AAW")
		AAW->(dbSetOrder(2))
		If AAW->(dbSeek(xFilial("AAW")+SC5->C5_NUM+DToS(SC5->C5_EMISSAO) ))
			cAliasAAW := GetNextAlias()
			BeginSQL Alias cAliasAAW
			SELECT
				AAW.AAW_DTEMIS
			FROM %Table:AAW% AAW
			WHERE
				AAW.AAW_FILIAL = %Exp:xFilial("AAW")%
				AND AAW.%NotDel% 
				AND AAW.AAW_CONTRA = %Exp:AAW->AAW_CONTRA%
				AND AAW.AAW_DTEMIS > %Exp:DToS(SC5->C5_EMISSAO)%
				ORDER BY AAW_DTEMIS DESC
			EndSQL

			If !(cAliasAAW)->(EOF())
				lRet := .F.
				Help(NIL, NIL, "AT210VerPV", NIL, STR0008 + DToC(SToD((cAliasAAW)->AAW_DTEMIS)) + STR0009 + DToC(SC5->C5_EMISSAO) + ")." , 1, 0, NIL, NIL, NIL, NIL, NIL, {STR0010}) // "Existe pedido de venda com data de emissão superior(" ## ") ao que esta excluindo no momento(" ## "Para realizar a exclusão, por favor realize a exclusão dos outros pedidos de vendo com data mais recentes."
			EndIf
			(cAliasAAW)->(DbCloseArea())
		EndIf
	EndIf
EndIf

RestArea(aArea)
Return lRet

