@author    PLS TEAM
@version   1.xx
@since     27/09/2018
/*/
//------------------------------------------------------------------------------------------
Function PLS470Ret()
Local cMarca	:= oMark:Mark()
Local nPos		:= 0

_aMarks := {}

dbSelectArea(cAliasTRB)
(cAliasTRB)->(DbGoTop())

While !(cAliasTRB)->(Eof())
	nPos++
	If oMark:IsMark(cMarca)
		aAdd(_aMarks,{.T.,nPos})
	EndIf
	(cAliasTRB)->(dbSkip())
EndDo

CloseBrowse()

Return()


