	oMark:Activate()				// Ativa o MarkBrowse
	
	If !Empty(_aMarks)
		For nFor := 1 To Len(_aMarks)
			aAdd(aRet,{_aMarks[nFor][01],_aMarks[nFor][02]})
		Next nFor
	EndIf
Else
	Help( ,, 'HELP',,"Registros não encontrado !!!", 1, 0)
EndIf

If _oBaixa <> Nil
