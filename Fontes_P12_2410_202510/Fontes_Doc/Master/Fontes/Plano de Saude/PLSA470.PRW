LOCAL   aAuxFld := {}
LOCAL   cCampos := GetNewPar("MV_PLSFBAU","BAU_CODIGO,BAU_NOME,BAU_CONREG,BAU_TIPPRE")
LOCAL   nInd

PRIVATE cTitulo  := STR0124
PRIVATE oDlg
PRIVATE oBrowse                              
PRIVATE lCheck   := .T.
PRIVATE oCheck
PRIVATE aTela
PRIVATE cAno		:= ""
PRIVATE cMes        := ""
PRIVATE dVencto     := cTod("")
PRIVATE cFiltro     := ""
PRIVATE dDatMvIni   := cTod("")
PRIVATE dDatMvFin   := cTod("")
PRIVATE dDataPagto  := cTod("")
PRIVATE cLocDigIn   := ""
PRIVATE cLocDigFi   := ""
PRIVATE cCodPegIn   := ""
PRIVATE cCodPegFi   := ""
PRIVATE cNumMovIn   := ""
PRIVATE cNumMovFi   := ""
