//-------------------------------------------------------------------------------------
Static Function MenuDef()
	Local aRotina := {}

	aRotina := { { oemtoansi(STR0009),"AxPesqui", 0 , 1,,.F.},; // "Pesquisar"
				 { oemtoansi(STR0008),"AxCadVis", 0 , 2},; // "Visualizar"
				 { oemtoansi(STR0003),"AxCadInc", 0 , 3},; //"Incluir"
				 { oemtoansi(STR0004),"AxCadAlt", 0 , 4},; //"Alterar"
				 { oemtoansi(STR0005),"AxCadDel", 0 , 5}}  //"Excluir"
Return aRotina

