Function CN250DEL()
Local lRet   := .T.
Local cArqTrb:= GetNextAlias()
Local cQuery := ""

cQuery := "SELECT COUNT(*) QUANT FROM "+RetSQLName("CNQ")+" CNQ "
cQuery += "WHERE CNQ.CNQ_FILIAL = '"+xFilial("CNQ")+"' AND "
cQuery += "CNQ.CNQ_TPDESC = '"+ CNP->CNP_CODIGO +"' AND "
cQuery += "CNQ.D_E_L_E_T_ = ' ' "

cQuery := ChangeQuery( cQuery )                                          

dbUseArea( .T., "TopConn", TCGenQry(,,cQuery), cArqTrb, .T., .T. )

lRet := ((cArqTrb)->QUANT = 0)

(cArqTrb)->( dbCloseArea() )

If !lRet
	Aviso("CNTA250",OemToAnsi(STR0002),{"OK"})//"Item em uso, nao pode ser deletado"
EndIf

return lRet

