																@oGet		,;
																nOpcX		 ;
															  ),;
												Pn230Aponta( oGet , nOpcX , .T. ) ;
											 ),;
											 NIL;
							 );
						}

		IF !( lPona240 )
			bAponMarc	:= { || IF(oGet:TudoOk(),(;
