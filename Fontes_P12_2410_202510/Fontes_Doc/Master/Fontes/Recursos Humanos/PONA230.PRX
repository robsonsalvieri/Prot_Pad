	If lPort1510 .and. !lPona240 .and. nOpcX == 4
		aAdd(aButtons, { 'DEVOLNF' , { || Eval(bRecRFE) } , OemToAnsi(STR0176) } )		// 'Aponta Marcacoes... - <F11>'
		SetKey( K_ALT_R , bRecRFE )
