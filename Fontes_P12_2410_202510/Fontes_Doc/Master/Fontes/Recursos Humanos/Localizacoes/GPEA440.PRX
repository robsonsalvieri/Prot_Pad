	cCpoAtual := SubStr( cCampoAtu, 4, 10 )
	
	If aCols[n][nPosTpMov] == "05" .And. ( Len( aRCPColsAnt ) >= Len( aCols ) )
		If cCpoAtual $ "RCP_SALDII#RCP_SALMES#RCP_SALDIA"
			nPosCpoAtu    := GdFieldPos( cCpoAtual, aHeaderRCP )
