			oSectLan:Cell("REMOTROS"):SetValue(aVerba[5,1])	//REMUNERACAO - OUTROS BONOS (F)
			oSectLan:Cell("SDSRNUM"):SetValue(aVerba[6,2])		//SUELDO DSR - NUMERO DE DOMINGOS
