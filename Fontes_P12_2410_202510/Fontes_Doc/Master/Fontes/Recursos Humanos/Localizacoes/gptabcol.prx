Static Function Tab045( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local nVerProg		:= 0
Local nVerLoc		:= 0

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S048','Tipo Contrato XML','01','Codigo','Tipo Contrato','C',1,0,'@!','NaoVazio()','','001','1','S','',} )
aAdd( aRCBItens, { '','S048','Tipo Contrato XML','02','Descrip','DescripciСn','C',25,0,'@!','NaoVazio()','','001','2','S','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])

If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIf

nVerProg := Val(cVersao)
nVerLoc := Val(cCliVersao)

If nVerProg > nVerLoc
	fGravaTab( aRCBHeader, aRCBItens )
EndIf

Return( NIL )

