/*/{Protheus.doc} fVerSuspD()
Função que verifica suspensão disciplinar e as trata como faltas injustificadas
@author	    Allyson Mesashi
@since		05/11/2021
@version	P12
/*/
Static Function fVerSuspD( fOP, dInicio, aPerVenc, lTemPerFer )

Local aArea		:= GetArea()
Local cAliasTit	:= GetNextAlias()
Local cQrySt	:= ""

If __oStTit == Nil
	__oStTit := FWPreparedStatement():New()
	cQrySt := "SELECT TIT_FILIAL,TIT_MAT,TIT_DATA,TIT_QTDDIA"
	cQrySt += "FROM " + RetSqlName('TIT') + " TIT "
	cQrySt += "WHERE TIT.TIT_FILIAL = ? AND "
	cQrySt += 		"TIT.TIT_MAT = ? AND "
	cQrySt += 		"TIT.TIT_TIPO = '1' AND "
	cQrySt += 		"TIT.TIT_AFASTA = '1' AND "
	cQrySt += 		"TIT.TIT_DATA >= ? AND "
	cQrySt += 		"TIT.D_E_L_E_T_ = ' '"
	cQrySt := ChangeQuery(cQrySt)
	__oStTit:SetQuery(cQrySt)
EndIf

__oStTit:SetString(1, SRA->RA_FILIAL)
__oStTit:SetString(2, SRA->RA_MAT)
__oStTit:SetString(3, dToS(dInicio))

cQrySt := __oStTit:getFixQuery()

dbUseArea(.T.,"TOPCONN",TcGenQry(,,cQrySt),cAliasTit,.T.,.T.)

While (cAliasTit)->( !EoF() )
	fAcumFaltas(fOP, cDatai, cDataF, @aPerVenc, AnoMes(sToD( (cAliasTit)->TIT_DATA )), aCodFol[54,1], "1", Nil, "", dFimPer, sToD( (cAliasTit)->TIT_DATA ), dDataI, dDataF, AnoMes(sToD( (cAliasTit)->TIT_DATA )), lTemPerFer, .T., (cAliasTit)->TIT_QTDDIA, AnoMes(sToD( (cAliasTit)->TIT_DATA )))
	(cAliasTit)->( dbSkip() )
End

(cAliasTit)->(dbCloseArea())

RestArea(aArea)

Return

/*/{Protheus.doc} fPerPago
/Verifica se o ultimo periodo esta como pago
@author fernando luis guilherme
@since 10/03/2023
/*/
Static Function fPerPago()

Local aAreaSRF	:= SRF->(GetArea())
Local aAreaSRH	:= SRH->(GetArea())
Local lRet		:= .F.
Local cResiduo	:= 0

If !(cTipoRot == "4" .And. cCompl == "S")
	dbSelectArea("SRF")
	SRF->(dbSetOrder(1))
	If SRF->(dbSeek(SRA->RA_FILIAL+SRA->RA_MAT))
		While !SRF->(Eof()) .And. SRF->(RF_FILIAL+RF_MAT) == SRA->RA_FILIAL+SRA->RA_MAT
			If SRF->RF_STATUS == "3"
				lRet := .T.
				Exit
			Elseif SRF->RF_STATUS == "1"
				dbSelectArea("SRH")
				SRH->(dbSetOrder(1))
				If SRH->(dbSeek(SRA->RA_FILIAL+SRA->RA_MAT+DTOS(SRF->RF_DATABAS)))
					While !SRH->(Eof()) .And. SRH->(RH_FILIAL+RH_MAT)+DTOS(SRH->RH_DATABAS) == SRA->RA_FILIAL+SRA->RA_MAT+DTOS(SRF->RF_DATABAS)
						cResiduo += SRH->RH_DFERIAS
						SRH->(dbSKIP())
					Enddo
					If cResiduo == SRF->RF_DIASDIR
						lRet := .T.
						Exit
					ENDIF
				Endif
			EndIf
			SRF->(dbSkip())
		EndDo
	EndIf
EndIf

RestArea(aAreaSRF)
RestArea(aAreaSRH)

Return lRet

/*/{Protheus.doc} fFerCalc
/Verifica houve calculo de férias no mesmo mês e se calculou medias corretamente
@author fernando luis guilherme
@since 10/03/2023
/*/
Static Function fFerCalc()

Local aAreaSRH	:= SRH->(GetArea())
Local lRet		:= .F.

dbSelectArea("SRH")
SRH->(dbSetOrder(1))
If SRH->(dbSeek(SRA->RA_FILIAL+SRA->RA_MAT))
	While !SRH->(Eof()) .And. SRH->(RH_FILIAL+RH_MAT) == SRA->(RA_FILIAL+RA_MAT)
		If ( CPERIANT == AnoMes(SRH->RH_DATAINI) .OR. CPERIANT == AnoMes(SRH->RH_DATAFIM) ) .And. fTemHMed(SRH->RH_DATAINI,,SRH->RH_PERIODO,cNpagAnt,cRot)
			lRet := .T.
			Exit
		EndIf
		SRH->(dbSKIP())
	Enddo
EndIf

RestArea(aAreaSRH)

Return lRet
