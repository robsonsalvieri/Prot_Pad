/*/{Protheus.doc} fHistMed
/Grava tabelas de histórico de médias
@author flavio.scorrea
@since 23/10/2019
/*/
Function fHistMed( dDtRef, cDis, cPerMed, cSemMed, cRotMed, aSalMed, cPerSRH )
Local aArea		:= GetArea()
Local cId		:= ""

DEFAULT dDtRef  := DDATAREF
DEFAULT cDis	:= "2" //1=Sim;2=Nao
DEFAULT aSalMed	:= {}
DEFAULT cPerMed	:= CPERIODO
DEFAULT cSemMed	:= CSEMANA
DEFAULT cRotMed	:= CROT
DEFAULT cPerSRH	:= ""

DEFAULT lTabHist := AliasInDic("RJK") .And. AliasInDic("SRP") 

If !lTabHist .or. ( Empty(cPerMed) .and. Empty(cSemMed) .and. Empty(cRotMed) )
	Return ""
EndIf

If cPaisLoc != "BRA"
	If Empty(cSemMed) .And. !Empty(cNumPag)
		cSemMed	:= cNumPag
	EndIf
	If Empty(cRotMed)
		If !Empty(C__ROTEIRO)
			cRotMed	:= C__ROTEIRO
		Else
			cRotMed	:= Space( GetSx3Cache( "RCH_ROTEIR" , "X3_TAMANHO" ) )
		EndIf
	EndIf
EndIf

//RJK_FILIAL+RJK_PROCES+RJK_MAT+RJK_PERIOD+RJK_SEMANA+RJK_ROTEIR+DTOS(RJK_DTREF)+RJK_DISSI
cId := fGrvRJK( SRA->RA_FILIAL, SRA->RA_PROCES, SRA->RA_MAT, cPerMed, cSemMed, cRotMed, dDtRef, cDis, aSalMed, cPerSRH )
If !Empty(cId) .And. ChkFile("SRP")
	fGrvSRP(cId, cPerMed, cSemMed, cRotMed, dDtRef, cDis, aSalMed, cPerSRH)
EndIf

RestArea(aArea)
Return cId

/*/{Protheus.doc} fGrvSRP
/Grava tabela SRP de histórico de médias
@author flavio.scorrea
@since 23/10/2019
/*/
Static Function fGrvSRP(cId, cPerMed, cSemMed, cRotMed, dDtRef, cDis, aSalMed, cPerSRH)

Local aArea	    := GetArea()
Local aTipoAux  := {}
Local lRet	    := .T.
Local nPos      := 0
Local oBulk     := Nil

DEFAULT cSRP        := RetSQLName("SRP")
DEFAULT nTamRPData	:= TamSX3("RP_DATARQ")[1]
DEFAULT lUsaBulk    := ( nTamRPData == 6 .And. FwLibVersion() >= "20201009" .And. FindClass( "FwBulk" ) )
DEFAULT cPerSRH     := ""
DEFAULT lTemRJKTP   := RJK->(ColumnPos( "RJK_TIPO" )) > 0

If Type("cTBLXMED") == "U" .Or. Select(cTBLXMED) <= 0 
	Return .T.
EndIf

If lUsaBulk
    oBulk := FwBulk():New( cSRP )
    oBulk:SetFields({	{"RP_FILIAL"	},;
                        {"RP_MAT"		},;
                        {"RP_TIPO"	    },;
                        {"RP_DATARQ"	},;
                        {"RP_PD"	    },;
                        {"RP_HORAS" 	},;
                        {"RP_VALOR"	    },;
                        {"RP_VALATU"	},;
                        {"RP_TIPO1"		},;
                        {"RP_ADIC"	    },;
                        {"RP_DATPGT"	},;
                        {"RP_MESMEDI"	},;
                        {"RP_HEXTRA"	},;
                        {"RP_ID"		}})
EndIf

(cTBLXMED)->(dbGoTop())
While !((cTBLXMED)->(Eof()))
    If lUsaBulk
        oBulk:AddData({	(cTBLXMED)->RP_FILIAL   ,;
                        (cTBLXMED)->RP_MAT		,;
                        (cTBLXMED)->RP_TIPO     ,;
                        (cTBLXMED)->RP_DATARQ	,;
                        (cTBLXMED)->RP_PD		,;
                        (cTBLXMED)->RP_HORAS	,;
                        (cTBLXMED)->RP_VALOR	,;
                        (cTBLXMED)->RP_VALATU	,;
                        (cTBLXMED)->RP_TIPO1	,;
                        (cTBLXMED)->RP_ADIC		,;
                        (cTBLXMED)->RP_DATPGT	,;
                        (cTBLXMED)->RP_MESMEDI	,;
                        (cTBLXMED)->RP_HEXTRA	,;
                        cId						})
    Else
        RecLock("SRP",.T.)
            SRP->RP_FILIAL 	:= (cTBLXMED)->RP_FILIAL
            SRP->RP_MAT		:= (cTBLXMED)->RP_MAT
            SRP->RP_TIPO	:= (cTBLXMED)->RP_TIPO
            SRP->RP_DATARQ	:= (cTBLXMED)->RP_DATARQ
            SRP->RP_PD		:= (cTBLXMED)->RP_PD
            SRP->RP_HORAS	:= (cTBLXMED)->RP_HORAS
            SRP->RP_VALOR	:= (cTBLXMED)->RP_VALOR
            SRP->RP_VALATU	:= (cTBLXMED)->RP_VALATU
            SRP->RP_TIPO1	:= (cTBLXMED)->RP_TIPO1
            SRP->RP_ADIC	:= (cTBLXMED)->RP_ADIC
            SRP->RP_DATPGT	:= (cTBLXMED)->RP_DATPGT
            SRP->RP_MESMEDI	:= (cTBLXMED)->RP_MESMEDI
            SRP->RP_HEXTRA	:= (cTBLXMED)->RP_HEXTRA
            SRP->RP_ID		:= cId
        SRP->(MsUnlock())
    EndIf

	If lTemRJKTP .and. cRotMed $ "RES/PRV"
		aAdd(aTipoAux, (cTBLXMED)->RP_TIPO)
	EndIf

	(cTBLXMED)->(dbSkip())
EndDo

If lUsaBulk
    oBulk:Close()
    oBulk:Destroy()
    oBulk := nil
EndIf

//Grava as demais linhas na RJK quando provisão ou rescisão
For nPos := 1 to Len(aTipoAux)
	fGrvRJK( SRA->RA_FILIAL, SRA->RA_PROCES, SRA->RA_MAT, cPerMed, cSemMed, cRotMed, dDtRef, cDis, aSalMed, cPerSRH, aTipoAux[nPos], cId, .T. )
Next nPos

RestArea(aArea)

Return lRet

/*/{Protheus.doc} fGrvRJK
