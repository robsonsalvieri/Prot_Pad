Static Function SubMeses(dData,nMenos)
Local dDtMenos
Local nMes := Month(dData)
Local nAno := Year(dData)
Local n 	:= 0 

For n = 1 To nMenos
	nMes := nMes - 1
	If nMes = 0
		nMes := 12
		nAno -= 1
	EndIf
Next
dDtMenos := Ctod("01/"+StrZero(nMes,2)+"/"+StrZero(nAno,4) , "DDMMYY" )
Return(dDtMenos)

