								//-- Aviso Previo
								If Empty(cTipoRot) .or. ;											// Demonstrativo de Medias
								   (cTipoRot == "4" .and. MesAno(dDt5) > cMesFol ) 					// Calculo de medias s/ aviso na rescisao
