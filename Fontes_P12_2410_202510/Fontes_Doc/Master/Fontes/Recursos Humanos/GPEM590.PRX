		aCmp :=	{}
		aAdd (aCmp, {"TIPOBEN"	,"C", 	001,	0}) //Beneficiario (1-Pessoa fisica; 2-Pessoa juridica)
		aAdd (aCmp, {"CODPAIS"	,"C", 	003,	0}) //Codigo de pais
		aAdd (aCmp, {"NIF"		,"C", 	030,	0}) //Numero de identificação fiscal - NIF
