	cCodProc	:=	If( aTransf[nX,07] <= dDataRef, aTransf[nX,15], cCodProc)
	cDescProc	:=	LEFT(fDesc("RCJ",cCodProc,"RCJ_DESCRI",,SRA->RA_FILIAL),25)
Next	

Return
