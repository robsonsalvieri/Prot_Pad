Function ADescPd(cCampo)
	Local cVar
	Local aArea := GetArea()
	Local lRet  := .F.
	Local nLoop
	Local oModel
	Local oView
	
	cVar := &(ReadVar())
	dbSelectArea("SRV")
	nOrdemV := IndexOrd()
	dbSetOrder(1)
	
	If dbSeek( xFilial("SRV") + cVar)
		lRet := .T.
		If FunName() == "GPEA120"
			oModel 	:= FWModelActive()
			oModel:GetModel( 'GPEA120_SRD' ):SetValue("RD_DESCPD",SRV->RV_DESC)
			oView := FWViewActive()
			oView:Refresh()			
		Else
