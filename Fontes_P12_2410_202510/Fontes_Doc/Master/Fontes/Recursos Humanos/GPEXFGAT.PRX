Function fBuscaCC(dDataRef, cCodCC, cDescCC, uData, lNoRept , lOrigem  ) 
Local aTransf	:= {}
Local nX		:= 0 

fTransfCc( aTransf , uData , lNoRept , lOrigem )   

cCodCC 	:=	SRA->RA_CC
cDescCC	:=  LEFT(fDesc("CTT",SRA->RA_CC,"CTT_DESC01",,SRA->RA_FILIAL),25)

