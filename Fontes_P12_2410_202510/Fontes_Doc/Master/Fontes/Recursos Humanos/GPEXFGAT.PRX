	cMoeda:= StrZero(  Val(cMoeda), 2 )
EndIf
	
dbSelectArea("CTP")
                 
nTaxaConver:= POSICIONE( "CTP", 1, xFilial("CTP")+ StrZero(Year(dDataConv),4) + StrZero(Month(dDataConv), 2) + StrZero(Day(dDataConv), 2) + cMoeda, "CTP_TAXA" )

Return (nTaxaConver)          

