Function HMSToTime( nHours , nMinuts , nSeconds )

Local cTime

DEFAULT nHours		:= 0
DEFAULT nMinuts		:= 0
DEFAULT nSeconds	:= 0

cTime := AllTrim( Str( nHours ) )
cTime := StrZero( Val( cTime ) , Max( Len( cTime ) , 2 ) )
cTime += ":"
cTime += StrZero( Val( AllTrim( Str( nMinuts  ) ) ) , 2 )
cTime += ":"
cTime += StrZero( Val( AllTrim( Str( nSeconds ) ) ) , 2 )

Return( cTime )

