*/
Function fAprovador(cCodAva, cCodAdo)

Local aArea			:= GetArea()
Local aAvaliador	:= {}

DbSelectArea("RDA")
DbSetOrder(1)

If DbSeek( xFilial("RDA", RD6->RD6_FILIAl) + cCodAva + cCodAdo )
	While !EoF() .And. RDA->RDA_CODAVA + RDA->RDA_CODADO == cCodAva + cCodAdo
		aAdd(aAvaliador, RDA->RDA_CODDOR)
		DbSkip()		
	EndDo
EndIf

RestArea(aArea)

Return (aAvaliador)

