*/
Function RdbSxbFilt()

Local aArea		:= GetArea()
Local aAreaRD0  := {}
Local cFilt		:= "@#.T.@#"   
Local cFunName	:= FunName()
Local cCodAva	:= "" 	
Local cCodAdo	:= ""
Local cSolic	:= ""
Local oModel	:= Nil

If cFunName $ "APDA250|APDA260"

	cCodAva := oGdRdb:aCols[1][1]
	cCodAdo := oGdRdb:aCols[1][2] 
	cFilt := "@#aScan( fAprovador('"+cCodAva+"','"+cCodAdo+"'), RD0->RD0_CODIGO) > 0@#"
ElseIf cFunName == "FINA666"
	aAreaRD0	:= RD0->(FwGetArea())
	oModel := FWModelActive() //Modelo de dados ativo.
	If oModel:GetModel("FW3MASTER") <> Nil
		cSolic := Alltrim(oModel:GetValue( "FW3MASTER", "FW3_USER" ))
	

		If RD0->(DbSeek( xFilial("RD0") + cSolic ))
			cFilt := "@#(RD0->RD0_CODIGO == '"+cSolic+"' .OR. RD0->RD0_APROPC == '"+cSolic+"' .OR. RD0->RD0_APSUBS == '"+cSolic+"')@#"
		EndIf
	EndIf
	FWRestArea(aAreaRD0)
EndIf

RestArea(aArea)

Return( cFilt )

