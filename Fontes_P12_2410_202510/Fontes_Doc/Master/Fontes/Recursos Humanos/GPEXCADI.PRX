			"1290", ;	//	PERICULOSIDADE 13.SA	
			"1291", ;	//	PERICULOSIDADE 13.SA	
			"1300", ;	//	PERICULOSIDADE FERIA	
			"1301", ;	//	PERICULOSIDADE FERIA	
			"1302", ;	//	PERICULOSIDADE FERIA	
			"1303", ;	//	PERICULOSIDADE FERIA	
			"1316", ;	//	PERICULOSIDADE ABONO
			"1317", ;	//	PERICULOSIDADE ABONO
			"1318", ;	//	PERICULOSIDADE ABONO
			"1319", ;	//	PERICULOSIDADE ABONO
			"1340", ;	//	PERICULOSIDADE MATER
			"1353", ;	//  PRORROGACAO - PERICU	
			"1440", ;	//	PERIC 13º MATER     	
			"1441", ;	//	PERIC 13º S VERBA MA	
			"1643", ;	//	PERIC 13 SAL MAT 1P 	
			"1644", ;	//	PER 13 S. VBS MAT 1P	
			"1680", ;	//	PERIC FER INDENIZ   	
			"1681", ;	//	PERIC FER PROPORC   	
			"1682", ;	//	PERIC FER S/ AVIS PR	
			"1683", ;	//	PERIC S/ AVISO PREV 	
			"1684", ;	//	PERIC 13 NIDENIZADO 	
			"1685", ;	//	PERIC 13 S/ AVISO PR	
			"1686", ;	//	PERIC MED FER INDENI	
			"1687", ;	//	PERIC S/ MED FER PRO	
			"1688", ;	//	PERIC S/ MED AV. PRE	
			"1689", ;	//	PERIC MED AVISO PREV	
			"1690", ;	//	PERIC MED 13 INDENIZ	
			"1691" ;	//	PERIC MED 13 S/ AV.P	
			}

    EndIf

	//Deletar verba do APD
	For nX := 1 To Len(aVerbas)
		If ( nPos := aScan(aPd,{|x| x[1] == FGetCodFol(aVerbas[nX]) } )) > 0 

			//Verifica se Incorpora Salario e abate valor
			If RetValSRV( aPd[nPos][01], SRA->RA_FILIAL, "RV_INCORP" ) == "S"
				IncorpSal(aPd[nPos][05], , .T.)
			EndIf
		
			aDel(aPd, nPos)	
			aSize(aPd,Len(aPd)-1)
		EndIf
	Next nX
Return 

/*/{Protheus.doc} fCalAdcFun
Cálcula adicionais por função
@author Leandro Drumond
@since 23/10/2023
@version P12
@return Nil
/*/
Function fCalAdcFun(cTpCalc, dDataRef, nSalario)

Local aArea 	:= GetArea()
Local aTotADI   := {}
Local aAlocFunc := {}
Local aDataAux  := {}
Local aDetAux   := {}
Local cFilRUJ
Local cValAux
Local cUF
Local cPDAux 	:= ""
Local cAboAux   := ""
Local cAboAuxMS := ""
Local cCodCCT   := ""
Local cCCusto   := ""
Local cCodFunc	:= ""
Local cSeqAux   := " "
Local dDataAux
Local nBaseAdi  := 0
Local nFaltas   := 0 
Local nAtraso   := 0
Local nPos 		:= 0
Local nX 		:= 0
Local nValFixo
Local nValAdi
Local nValAdiMS
Local nValABO 
Local nValABOMS
Local nValAux
Local nPercAdt  := SRA->RA_PERCADT
Local nRefAux 	:= DiasTrab
Local nRefMS 	:= 0
Local nBaseAux  := 0
Local nAvosAux  := 0
Local nTotAdi 	:= 0
Local nDiasAloc := 0
Local nTotDias  := 0
Local nDiasAF	:= 0
Local lTemGS    := .F.
Local lMudou    := .T.
Local lLctoDia  := .F.
Local lPropDia
Local lSkipCCT

Static lIntGS

DEFAULT lUsaRUJ		:= cPaisLoc == "BRA" .and. AliasInDic("RUJ") 
DEFAULT lNewRUJ		:= lUsaRUJ .and. RUJ->(ColumnPos( "RUJ_EVEATR" )) > 0
DEFAULT lRUJCCT 	:= lUsaRUJ .and. RUJ->(ColumnPos( "RUJ_CODCCT" )) > 0
DEFAULT lUsaABO 	:= lUsaRUJ .and. RUJ->(ColumnPos( "RUJ_PDABO" )) > 0
DEFAULT lIntGS  	:= GetMvRH("MV_TECXRH", NIL, .F. ) .and. FindFunction("fAlocFunGS")
DEFAULT __lMemCalc	:= cPaisLoc == "BRA" .And. fMemCalc() // Memória de Cálculo

DEFAULT cTpCalc := cTipoRot
DEFAULT dDataRef:= dDataAte
DEFAULT nSalario:= SALARIO

If lUsaRUJ

	RCE->(DbSetOrder(1))
	If !Empty(SRA->RA_SINDICA)
		RCE->(DbSeek(xFilial("RCE",SRA->RA_FILIAL) + SRA->RA_SINDICA))
		cCodCCT := RCE->RCE_CCT
	EndIf

	If cTpCalc == 'PFER' .or. cTpCalc == "P132"
		dDataDe := CtoD('01/'+StrZero(Month(dDataRef))+ '/' +StrZero(Year(dDataRef)) , "DDMMYY")
		dDataAte:= dDataRef
	EndIf

	aDataAux := {dDataDe, dDataAte}
	
	If lIntGS
		nTotDias  := 30 //Para o GS o Divisor sempre será 30
		cPeriodo  := AnoMes(dDataRef)
		lTemGS 	  := .T.
		aAlocFunc := fAlocFunGS(cPeriodo)
		If(__lMemCalc .and. !Empty(aAlocFunc),fAddMemLog("Alocação do funcionário retornada pelo GS",1,2),Nil)
	EndIf

	If Empty(aAlocFunc) 
		aAdd(aAlocFunc, {dDataDe, dDataAte, SRA->RA_CODFUNC, cCodCCT, 0, SRA->RA_CC})
		lTemGS    := .F. //Se não foi enviado nada pelo GS
	EndIf

	cFilRUJ := xFilial("RUJ", SRA->RA_FILIAL)
	
	DbSelectArea("RUJ")

	For nX := 1 to Len(aAlocFunc)

		dDataDe   := aAlocFunc[nX,1]
		dDataRef  := aAlocFunc[nX,2]
		cCodFunc  := aAlocFunc[nX,3]
		cCodCCT   := aAlocFunc[nX,4]
		cCCusto   := If(Len(aAlocFunc[nX]) >= 6, aAlocFunc[nX,6], SRA->RA_CC )
		aTotADI   := {}
		nDiasAF   := 0
		lMudou    := .T.
		lLctoDia  := .F.
		cSeqAux   := " "

		If lTemGS
			If(__lMemCalc,fAddMemLog("Data Inicio: " + DtoC(dDataDe) + " - Data Fim: " + DtoC(dDataRef) + " - Função: " + cCodFunc + " - CCT: " + cCodCCT,1,2),Nil)
			aDetAux := aClone(aDetAfas)
			FDiasAfast(@nDiasAF, DiasTrab, dDataRef, , , , , , .F.,dDataDe,dDataRef)
			aDetAfas := aClone(aDetAux) //Retorna conteúdo original
		EndIf

		Begin Sequence
		
			If RUJ->(DbSeek(cFilRUJ + cCodFunc))
				While RUJ->(!Eof() .and. RUJ_FILIAL + RUJ_FUNCAO == cFilRUJ + cCodFunc )
					
					//Posição 5 enviada pelo GS é o total de dias alocados
					nDiasAloc := If(lTemGS .And. Len(aAlocFunc[nX]) > 4, aAlocFunc[nX, 5], (dDataRef - dDataDe) + 1)
					
					If AnoMes(RUJ->RUJ_DTINI) > AnoMes(dDataRef) .or. (!Empty(RUJ->RUJ_DTFIM) .and. AnoMes(RUJ->RUJ_DTFIM) < AnoMes(dDataRef) )
						RUJ->(DbSkip())
						Loop 
					EndIf

					If lRUJCCT .and. !Empty(RUJ->RUJ_CODCCT)
						lSkipCCT := .T.
						If !Empty(cCodCCT)
							lSkipCCT := cCodCCT <> RUJ->RUJ_CODCCT
						EndIf
						If lSkipCCT
							RUJ->(DbSkip())
							Loop 
						EndIf
					EndIf

					nFaltas   := 0 
					nAtraso   := 0

					If lNewRUJ 
						//Valida carga horário minima diária
						If RUJ->RUJ_HRMIN > 0 .and. RUJ->RUJ_HRMIN > SRA->RA_HRSDIA 
							RUJ->(DbSkip())
							Loop
						EndIf

						//Valida quantidade máxima de faltas e atrasos
						If !Empty(RUJ->RUJ_EVEFAL) .or. !Empty(RUJ->RUJ_EVEATR)

							dDataAux := dDataDe
							
							fGetAtrFal(RUJ->RUJ_EVEFAL,RUJ->RUJ_EVEATR,dDataAux,dDataRef,@nFaltas,@nAtraso)

							If(__lMemCalc .and. lTemGS .and. lMudou,fAddMemLog("Faltas no Período: " + cValToChar(nFaltas),1,2),Nil)

							lMudou := .F.
							
							If ( nFaltas > 0 .and. nFaltas >RUJ->RUJ_QTFALT ) .Or. ;
							( nAtraso > 0 .and. nAtraso >RUJ->RUJ_QTATRA )

								RUJ->(DbSkip())
								Loop
							EndIf
						EndIf
					EndIf					
					
					cPDAux    := ""
					cAboAux   := ""
					cAboAuxMS := ""
					nValAdi   := 0
					nValAdiMS := 0
					nValABO	  := 0 
					nValABOMS := 0
					nRefAbo   := 0
					nRefAboMS := 0
					
					If cTpCalc == "1"
						cPdAux := RUJ->RUJ_PDFOL
					ElseIf cTpCalc == "2"
						cPdAux := RUJ->RUJ_PDADI
						If Len(aPercAdi) > 0
							aEval(aPercAdi, {|x| If(x[1] <= DIASTB, nPercAdt := x[2], Nil )})
						EndIf
					ElseIf cTpCalc == "3" .or. cTpCalc == "PFER" 
						cPdAux := RUJ->RUJ_PDFER
					ElseIf cTpCalc == "4"
						cPdAux := RUJ->RUJ_PDRESC
					ElseIf cTpCalc == "5"
						cPdAux := RUJ->RUJ_PD131
					ElseIf cTpCalc == "6" .or. cTpCalc == "P132" 
						cPdAux := RUJ->RUJ_PD132
					EndIf

					//Se a verba para pagamento não foi informada, desconsidera
					//Na rescisão deve calcular os adicionais de férias e 13º
					If Empty(cPdAux) .and. ( cTpCalc <> "4" .or. Empty(RUJ->RUJ_PDFER + RUJ->RUJ_PD132) )
						RUJ->(DbSkip())
						Loop
					EndIf
					
					If RUJ->RUJ_TPCALC $ "1*3" //Percentual ou Percentual + Valor FIxo
						
						nBaseAdi := 0
						nValFixo := If(RUJ->RUJ_TPCALC == "3", RUJ->RUJ_VLFIXO, 0) //Valor Fixo
						nValFixo := If(cTpCalc=="2",(nValFixo/100)*nPercAdt,If(cTpCalc=="5",nValFixo*nPercentua,nValFixo))

						If RUJ->RUJ_BSCALC $ "1*2" //Salário
							nBaseAdi += nSalario
						ElseIf RUJ->RUJ_BSCALC == "3" //Piso Sindicato
							If RCE->(DbSeek(xFilial("RCE", SRA->RA_FILIAL) + SRA->RA_SINDICA))
								nValAux 	:= RCE->RCE_PISO
			
								cValAux := fBuscaRG4("RCE_PISO",dDataRef,.T.)
								
								If !Empty(cValAux)
									nValAux := val(cValAux)
								EndIf
								nBaseAdi += nValAux
							EndIf
						ElseIf RUJ->RUJ_BSCALC == "4" //Salário Minimo
							nBaseAdi += VAL_SALMIN
						ElseIf RUJ->RUJ_BSCALC == "5" //Salário Minimo Estadual
							If RCE->(DbSeek(xFilial("RCE", SRA->RA_FILIAL) + SRA->RA_SINDICA))
								cValAux := fBuscaRG4("RCE_UF",dDataRef,.T.)
								cUf := RCE->RCE_UF
								If !Empty(cValAux)
									cUf := Alltrim(cValAux)
								EndIf
							
								//Buscar salario minimo estadual
								nValAux := Sal_MinEst(AnoMes(dDataRef),cUf)
							
								If nValAux <= 0
									FinalCalc()
									Break
								EndIf
								nBaseAdi += nValAux
							EndIf
						EndIf

						nBaseAdi := If(cTpCalc=="2",(nBaseAdi/100)*nPercAdt,If(cTpCalc=="5",nBaseAdi*nPercentua,nBaseAdi))

						If RUJ->RUJ_BSCALC == "2" //Salário + Verbas
							If !Empty(RUJ->RUJ_PDBASE)
								aEval( aPd ,{ |X|  If(X[1] $ RUJ->RUJ_PDBASE .and. x[9] <> "D", nBaseAdi += x[5],Nil)})
							EndIf
						EndIf
					Else 
						nValFixo := If(cTpCalc=="2",(RUJ->RUJ_VLFIXO/100)*nPercAdt,If(cTpCalc=="5",RUJ->RUJ_VLFIXO*nPercentua,RUJ->RUJ_VLFIXO))
					EndIf

					If nBaseAdi > 0 .or. nValFixo > 0

						If lTemGS
							If ( lLctoDia := ( RetValSRV(cPdAux , SRA->RA_FILIAL , "RV_LCTODIA" ) != "S" ) )
								cSeqAux := "0"
								aEval(aPd, {|x| If(x[1] == cPdAux .and. x[9] <> "D", cSeqAux := Soma1(cSeqAux), NIl)})
								cSeqAux := If(cSeqAux == "0", " ", cSeqAux)
							EndIf
							If !(cTpCalc $ "3*5*6") 
								If RUJ->RUJ_PROPOR == "1" .And. nDiasAF > 0
									nDiasAloc -= nDiasAF
								EndIf 
								If P_DFALADF .and. nFaltas > 0
									nDiasAloc -= nFaltas
								EndIf								
							EndIf
							If nBaseAdi > 0
								nBaseAdi := ( nBaseAdi / nTotDias ) * nDiasAloc
							EndIf 
							If nValFixo > 0 
								nValFixo := ( nValFixo / nTotDias ) * nDiasAloc
							EndIf
							nRefAux := nDiasAloc						
						EndIf

						If cTpCalc == "3" //Férias
							If lUsaABO .and. !Empty(RUJ->RUJ_PDABO)
								cAboAux := RUJ->RUJ_PDABO
								cAboAuxMs := fCarPdMS(cAboAux)
							EndIf

							cPdAuxMS := fCarPdMS(cPdAux)

							If Empty(cAboAux)
								If !Empty(cPdAuxMS)							
									nRefMS	  := ( nDiasFMS + nDiasFMS1 + nDiasAbMs + nDiasAbMs1 )
									nRefAux  := ( nDiasFM + nDiasAbM )
								Else 
									nRefAux  := ( nDiasFM + nDiasFMS + nDiasFMS1 + nDiasAbM + nDiasAbMs + nDiasAbMs1 )
								EndIf
							Else 
								If !Empty(cPdAuxMS)							
									nRefMS	  := ( nDiasFMS + nDiasFMS1 )
									nRefAux  := ( nDiasFM )
								Else 
									nRefAux  := ( nDiasFM + nDiasFMS + nDiasFMS1 )
								EndIf
								If Empty(cAboAuxMs)
									nRefAbo := nDiasAbM + nDiasAbMs + nDiasAbMs1
								Else 
									nRefAbo := nDiasAbM
									nRefAboMS := nDiasAbMs + nDiasAbMs1
								EndIf
							EndIf

							nBaseAdi := If( nBaseAdi > 0,( nBaseAdi / 30 ),0)

							If nBaseAdi > 0 .or. nValFixo > 0
								nValAdi   := ( ( nBaseAdi * nRefAux ) / 100 ) * RUJ->RUJ_PERC
								nValAdi   += ( nValFixo / 30 ) * nRefAux

								nValAdiMS := ( ( nBaseAdi * nRefMS ) / 100 ) * RUJ->RUJ_PERC
								nValAdiMS += ( nValFixo / 30 ) * nRefMS

								nValABO   := ( ( nBaseAdi * nRefAbo ) / 100 ) * RUJ->RUJ_PERC
								nValABO   += ( nValFixo / 30 ) * nRefAbo

								nValABOMS := ( ( nBaseAdi * nRefAboMS ) / 100 ) * RUJ->RUJ_PERC
								nValABOMS += ( nValFixo / 30 ) * nRefAboMS
							EndIf		
						ElseIf cTpCalc == "4" //Rescisão
							
							If nBaseAdi + nValFixo > 0
								lPropDia := RUJ->RUJ_PROPOR == "1"
								
								If !Empty(cPdAux)
									nValAdi := If( nBaseAdi > 0, ( nBaseAdi / 100 ) * RUJ->RUJ_PERC, 0)
									nValAdi += nValFixo 
									fMatriz(cPdAux,nValAdi,nRefAux,,cCCusto,'V',,,,,.T.,cSeqAux,,,,,,,If(lLctoDia,dDataDe,Nil),lPropDia)
								EndIf 
								If !Empty(RUJ->RUJ_PDFER) .and. RUJ->RUJ_PDFER <> cPdAux//Férias
									nRefAux := M->RG_DFERVEN + M->RG_DFERPRO + M->RG_DFERAVI
									nBaseAux:= If( nBaseAdi > 0, ( nBaseAdi / 30 ) * nRefAux, 0)
									nValAdi := If( nBaseAux > 0,( nBaseAux / 100 ) * RUJ->RUJ_PERC,0)
									nValAdi += If(nValFixo > 0, ( nValFixo / 30 ) * nRefAux, 0)
									fMatriz(RUJ->RUJ_PDFER,nValAdi,nRefAux,,cCCusto,'V',,,,,.T.,cSeqAux,,,,,,,If(lLctoDia,dDataDe,Nil),lPropDia)
								EndIf 
								If !Empty(RUJ->RUJ_PD132) .and. !( RUJ->RUJ_PD132 $ cPdAux + "/" + RUJ->RUJ_PDFER )//13º salário
									nRefAux 	:= 0
									dDataAux	:= CtoD('01/01/'+StrZero(Year(dDataDem)) , "DDMMYY")
									fAvos13(@nRefAux,dDataDem,@nAvosFal13,nFalAtuD,,dDataDem)
									fRetAfas(dDataAux,dDataDem,"13",@nAvosAux,,,,,,,,.T.,nFalAtuD)
									nRefAux	:= nRefAux - ( nAvosAux - nAvosMat)
									nBaseAux:= If( nBaseAdi > 0, ( nBaseAdi / 12 ) * nRefAux, 0)
									nValAdi := If( nBaseAux > 0,( nBaseAux / 100 ) * RUJ->RUJ_PERC,0)
									nValAdi += If(nValFixo > 0, ( nValFixo / 12 ) * nRefAux, 0)
									fMatriz(RUJ->RUJ_PD132,nValAdi,nRefAux,,cCCusto,'V',,,,,.T.,cSeqAux,,,,,,,If(lLctoDia,dDataDe,Nil),lPropDia)
								EndIf
							EndIf
						ElseIf cTpCalc $ "5*6" //13º salário
							nRefAux := nAvosAux := 0
							fAvos13(@nRefAux,If(cTpCalc == "5", aPergunte[6,3], If(P_CCOMP13 = 'S' .And. lCalcFol .And. Month(dDataRef) == 12, dDataRef, aPergunte[2,3])),nAvosFal13,,"1")
							dDataAux := Max( SRA->RA_ADMISSA, Ctod("01/01/"+SubStr(DtoS(dDataRef),1,4), "DDMMYY") )
							fRetAfas(dDataAux,IIf( cTpCalc == "5",aPergunte[6,3],aPergunte[2,3] ),"13",@nAvosAux)

							nRefAux		:= nRefAux - ( nAvosAux - nAvosMat)
							nBaseAdi	:= If( nBaseAdi > 0,( nBaseAdi / 12 ) * nRefAux,0)
							nValAdi 	:= If( nBaseAdi > 0, ( nBaseAdi / 100 ) * RUJ->RUJ_PERC, 0)
							nValAdi 	+= If(nValFixo > 0, nValFixo / 12 * nRefAux, 0)
						Else
							nValAdi 	:= If( nBaseAdi > 0, ( nBaseAdi / 100 ) * RUJ->RUJ_PERC, 0)
							nValAdi 	+= nValFixo 
						EndIf 
					EndIf

					If cTpCalc == "PFER" .or. cTpCalc == "P132"
						If lRUJCCT
							If ( nPos := aScan(aTotADI, {|x| x[1] == cPdAux }) ) > 0
								aTotADI[nPos,2] := nValAdi 
							Else 
								aAdd(aTotADI,{cPdAux,nValAdi})
							EndIf
						Else 
							nTotAdi += nValAdi
						EndIf
					ElseIf nValAdi > 0 .and. cTpCalc <> "4"
						lPropDia := !(cTpCalc $ "3*5*6") .And. RUJ->RUJ_PROPOR == "1" .and. !lTemGS //Se for férias e 13º ou Alocação do GS, já foi proporcionalizado
						fMatriz(cPdAux,nValAdi,nRefAux,,cCCusto,'V',,,,,.T.,cSeqAux,,,,,,,If(lLctoDia,dDataDe,Nil),lPropDia)
						If nValAdiMS > 0 
							fMatriz(cPdAuxMS,nValAdiMS,nRefMS,,cCCusto,'V',,,,,.T.,cSeqAux,,,,,,,If(lLctoDia,dDataDe,Nil),lPropDia)
						EndIf
						If nValABO > 0
							fMatriz(cAboAux,nValABO,nRefAbo,,cCCusto,'V',,,,,.T.,cSeqAux,,,,,,,If(lLctoDia,dDataDe,Nil),lPropDia)						
						EndIf
						If nValABOMS > 0
							fMatriz(cAboAuxMS,nValABOMS,nRefAboMS,,cCCusto,'V',,,,,.T.,cSeqAux,,,,,,,If(lLctoDia,dDataDe,Nil),lPropDia)
						EndIf
					EndIf

					RUJ->(DbSkip())
				EndDo		
			EndIf

			If lRUJCCT .and. ( cTpCalc == "PFER" .or. cTpCalc == "P132" )
				For nPos := 1 to Len(aTotADI)
					nTotAdi += aTotADI[nPos,2]
				Next nPos
			EndIf
			
		End Sequence
	Next nX

	dDataDe  := aDataAux[1]
	dDataAte := aDataAux[2]
EndIf

RestArea(aArea)

Return nTotAdi

/*/{Protheus.doc} fCarPdMS
Busca verba de férias mês seguinte
@author Leandro Drumond
@since 30/10/2023
@version P12
@return Nil
/*/
Static Function fCarPdMS(cPdMes)
Local aArea 	:= GetArea()
Local cRet		:= ""

SRV->(DbSetOrder(7)) //RV_FILIAL + RV_CODMSEG

If SRV->(DbSeek(xFilial("SRV",SRA->RA_FILIAL) + cPdMes))
	cRet := SRV->RV_COD
EndIf

RestArea(aArea)
Return cRet

/*/{Protheus.doc} fCalSalSub
Cálcula Salário Substituição
@author Leandro Drumond
@since 07/05/2024
@version P12
@return Nil
/*/
Function fCalSalSub()

Local aArea 	:= GetArea()
Local aVerbas   := {}
Local cVerba    := ""
Local cSeq      := "1"
Local nSalSub   := 0
Local dDtSubIni := CtoD("")
Local dDtSubFim := Ctod("")
Local nDiasSub  := 0
Local nPercAdt  := SRA->RA_PERCADT
Local nRecSRA
Local nSalDia
Local nFaltas	:= 0
Local nDiasCalc := If(cPaisLoc == "BRA" .and. P_LDCALCSS, nDiasC, nDiasP)

DEFAULT lUsaRUE	:= cPaisLoc == "BRA" .and. AliasInDic("RUE") 

If lUsaRUE

	DbSelectArea("RUE")
	DbSetOrder(1)

	If DbSeek(SRA->RA_FILIAL + SRA->RA_MAT)

		While RUE->(!Eof() .and. RUE_FILIAL + RUE_MAT == SRA->RA_FILIAL + SRA->RA_MAT)
			If RUE->RUE_DATDE <= dDataAte .and. ( Empty(RUE->RUE_DATATE) .or. RUE->RUE_DATATE >= dDataDe )
				nSalSub := 0
				cVerba  := If(cTipoRot == "1", RUE->RUE_VERBA, If(cTipoRot == "2", RUE->RUE_PDADI, RUE->RUE_PDRES))

				If !Empty(cVerba)
					nSalDia := Round(SALARIO / nDiasCalc,2)
					If !Empty(RUE->RUE_FILSUB) .and. !Empty(RUE->RUE_MATSUB)
						nRecSRA := SRA->(Recno())
						If SRA->(DbSeek(RUE->RUE_FILSUB + RUE->RUE_MATSUB))
							nSalSub := Round(If(SRA->RA_CATFUNC == "H", SRA->RA_SALARIO * SRA->RA_HRSDIA, SRA->RA_SALARIO / nDiasCalc),2)
							nSalSub := If(nSalSub > nSalDia, Round(nSalSub - nSalDia,2), 0)
						EndIf
						SRA->(DbGoTo(nRecSRA))
					ElseIf !Empty(RUE->RUE_SALARI)
						nSalSub := Round(RUE->RUE_SALARI / nDiasCalc,2)
						nSalSub := If(nSalSub > nSalDia, Round(nSalSub - nSalDia,2), 0)
					EndIf
					If nSalSub > 0
						dDtSubIni := Max(dDataDe, RUE->RUE_DATDE)
						dDtSubFim := If(Empty(RUE->RUE_DATATE), dDataAte, Min(RUE->RUE_DATATE,dDataAte))
						dDtSubFim := If(cTipoRot=="4",Min(dDtSubFim,dDataDem),dDtSubFim)
						nDiasSub  := Min(( dDtSubFim - dDtSubIni ) + 1, nDiasCalc)

						If !Empty(RUE->RUE_EVEFAL)
						
							fGetAtrFal(RUE->RUE_EVEFAL,"",dDtSubIni,dDtSubFim,@nFaltas)

							nDiasSub -= nFaltas 
						EndIf
					
						If nDiasSub > 0 
							nSalSub := nSalSub * nDiasSub
							cSeq := "1"
							If cTipoRot == "2" .and. Len(aPercAdi) > 0
								aEval(aPercAdi, {|x| If(x[1] <= DIASTB, nPercAdt := x[2], Nil )})
							EndIf
							aEval(aVerbas,{|x| If(x[1] == cVerba, cSeq := Soma1(x[2]),Nil)})
							
							nSalSub := If(cTipoRot=="2",(nSalSub/100)*nPercAdt,nSalSub)
							
							If nSalSub > 0
								fMatriz(cVerba,nSalSub,nDiasSub,,,'V',,,,,,cSeq)
								aAdd(aVerbas,{cVerba,cSeq})
							EndIf
						EndIf
					EndIf
				EndIf
			EndIf

			RUE->(DbSkip())
		EndDo 
	EndIf 
EndIf

RestArea(aArea)

Return Nil 

/*/{Protheus.doc} fGetAtrFal
Obtem quantidade de faltas e atrasos de acordo com as verbas informadas
@author Leandro Drumond
@since 09/05/2024
@version P12
@return Nil
/*/
Static Function fGetAtrFal(cFalta, cAtraso, dDataIni, dDataFim, nFaltas, nAtraso)

Local cAliasAux := GetNextAlias()
Local cQryPON   := ""

Static oTabRUJ
Static cEmpRUJ := ""

nFaltas := 0
nAtraso := 0

If oTabRUJ == Nil .OR. cEmpRUJ != cEmpAnt
	oTabRUJ := FWPreparedStatement():New()

	cEmpRUJ := cEmpAnt

	cQryPON := "SELECT PC_PD, PC_PDI, PC_QUANTC, PC_QUANTI, PC_DATA "
	cQryPON += "FROM " + RetSqlName("SPC") + " SPC "
	cQryPON += "WHERE "
	cQryPON += "SPC.PC_FILIAL = ? "
	cQryPON += "AND SPC.PC_MAT = ? "
	cQryPON += "AND SPC.PC_DATA >= ? "
	cQryPON += "AND SPC.PC_DATA <= ? "
	cQryPON += "AND SPC.D_E_L_E_T_ = ' ' "
	cQryPON += "UNION "
	cQryPON += "SELECT PH_PD, PH_PDI, PH_QUANTC, PH_QUANTI, PH_DATA "
	cQryPON += "FROM " + RetSqlName("SPH") + " SPH "
	cQryPON += "WHERE "
	cQryPON += "SPH.PH_FILIAL = ? "
	cQryPON += "AND SPH.PH_MAT = ? "
	cQryPON += "AND SPH.PH_DATA >= ? "
	cQryPON += "AND SPH.PH_DATA <= ? "
	cQryPON += "AND SPH.D_E_L_E_T_ = ' ' "

	cQryPON := ChangeQuery(cQryPON)

	oTabRUJ:SetQuery(cQryPON)
EndIf

// DEFINIÇÃO DOS PARÂMETROS.
oTabRUJ:SetString(1, SRA->RA_FILIAL)
oTabRUJ:SetString(2, SRA->RA_MAT)
oTabRUJ:SetString(3, DtoS(dDataIni))
oTabRUJ:SetString(4, DtoS(dDataFim))
oTabRUJ:SetString(5, SRA->RA_MAT)
oTabRUJ:SetString(6, SRA->RA_MAT)
oTabRUJ:SetString(7, DtoS(dDataIni))
oTabRUJ:SetString(8, DtoS(dDataFim))

// RESTAURA A QUERY COM OS PARÂMETROS INFORMADOS.
cQryPON := oTabRUJ:getFixQuery()

dbUseArea(.T., "TOPCONN", TcGenQry(,, cQryPON), cAliasAux, .T., .T.)

While (cAliasAux)->(!Eof())
	nFaltas += If(!Empty((cAliasAux)->PC_PDI) .and.(cAliasAux)->PC_PDI $ cFalta, 1 ,0)
	nFaltas += If(Empty((cAliasAux)->PC_PDI) .and.(cAliasAux)->PC_PD $ cFalta, 1 ,0)

	nAtraso += If(!Empty((cAliasAux)->PC_PDI) .and.(cAliasAux)->PC_PDI $ cAtraso, (cAliasAux)->PC_QUANTI ,0)
	nAtraso += If(Empty((cAliasAux)->PC_PDI) .and.(cAliasAux)->PC_PD $ cAtraso, (cAliasAux)->PC_QUANTC ,0)

	(cAliasAux)->(DbSkip())
EndDo

(cAliasAux)->(dbCloseArea())

Return Nil

/*/{Protheus.doc} GpCrgAdiGS
Carrega configuração de adicionais de periculosidade/insalubridade
@author Leandro Drumond
@since 09/06/2025
@version P12
@parametros "1" para periculosidade/ "2" para insalubridade
@return array
/*/
Function GpCrgPerIns(cTipo)
Local aRet  		:= {}
Local aAloc 		:= {}
Local cBsCal        := ""
Local cGrau  		:= ""
Local nPerc 		:= 0
Local nX 			:= 0
Local lTemADI       := .F.

Static lUsaGS
Static aDescBIns	  := {}
Static aDescBPer      := {}

DEFAULT lUsaGS        := GetMvRH("MV_TECXRH", NIL, .F. ) .and. FindFunction("fAlocPerInsGS")
DEFAULT __lMemCalc 	  := cPaisLoc == "BRA" .And. fMemCalc() // Memória de Cálculo

If lUsaGS 
	aAloc := fAlocPerInsGS(cRot,cPeriodo)
	/*	Retorno
		Tipo de Adicional - "1" para periculosidade e "2" para insalubridade
		Data inicial da alocação
		Data final da alocação
		Base de cálculo do adicional - retornar o código conforme opções disponíveis no cadastro de sindicato
		Percentual do adicional 
		Grau de insalubridade - apenas quando o tipo de adicional for "2"
		Divisor (nDiasC)
		Centro de Custos
	*/
EndIf

If __lMemCalc .and. !Empty(aAloc) .and. Empty(aDescBIns)
	aDescBIns := StrTokArr2( fboxInsal(), ";")
	aDescBPer := StrTokArr2( fboxPeri(), ";")
EndIf

For nX := 1 to Len(aAloc)
	If aAloc[nX,1] == cTipo
		If(__lMemCalc .and. Empty(aRet),fAddMemLog("Alocação enviada pelo GS para " + If(cTipo == "1", "Periculosidade", "Insalubridade"),1,2),Nil)

		aAdd(aRet, {aAloc[nX,5] > 0, aAloc[nX,2], aAloc[nX,3] , aAloc[nX,4] , aAloc[nX,5], aAloc[nX,6], 0, 0, If(Len(aAloc[nX])>6,aAloc[nX,7],0),If(Len(aAloc[nX])>7,aAloc[nX,8],SRA->RA_CC)})

		If(__lMemCalc,fAddMemLog("Data Inicial da Alocação : " + DtoC(aAloc[nX,2]),1,2),Nil)
		If(__lMemCalc,fAddMemLog("Data Final da Alocação   : " + DtoC(aAloc[nX,3]),1,2),Nil)
		If(__lMemCalc,fAddMemLog("Bs. Cálculo do Adicional : " + If(cTipo=="1", fDescBsAdi(aDescBPer,aAloc[nX,4]), fDescBsAdi(aDescBIns,aAloc[nX,4])),1,2),Nil)
		If(__lMemCalc,fAddMemLog("Percentual do Adicional  : " + cValToChar(aAloc[nX,5]),1,2),Nil)
		If(__lMemCalc .and. cTipo == "2",fAddMemLog("Grau de Insalubridade    : " + If(aAloc[nX,6]=="2", "Mínima", If(aAloc[nX,6]=="3", "Média", "Máxima")),1,2),Nil)
		If(__lMemCalc .and. Len(aAloc[nX])>6,fAddMemLog("Divisor : " + cValToChar(aAloc[nX,7]),1,2),Nil)
		If(__lMemCalc .and. Len(aAloc[nX])>7,fAddMemLog("Centro de Custos : " + aAloc[nX,8],1,2),Nil)
	EndIf
Next nX 

If Empty(aRet)
	If cTipo == "1"
		lTemAdi := If(SRA->RA_ADCPERI == "2", .T., lDissidio .and. !Empty(GPRETSR9( "SRA", dDataAte, "RA_PERICUL" )))
	Else 
		lTemAdi := If(SRA->RA_ADCINS $ "2*3*4", .T., lDissidio .and. AllTrim(gpRetSR9( "SRA", dDataAte, "RA_ADCINS" )) $ "2*3*4")
	EndIf
	If lTemAdi
		If cTipo == "1"
			cBsCal := fBuscaRG4("RCE_BCALPE",dDataAte,.T.,.T.)
			nPerc  := RCE->RCE_PERPE
			cGrau  := ""
		Else 
			cBsCal := fBuscaRG4("RCE_BCALIN",dDataAte,.T.,.T.)
			cGrau  := If(!lDissidio, SRA->RA_ADCINS, If(SRA->RA_ADCINS == "1", AllTrim(gpRetSR9( "SRA", dDataAte, "RA_ADCINS" )), SRA->RA_ADCINS ))
			nPerc  := If(cGrau == "2", RCE->RCE_PINSMI, If(cGrau == "3", RCE->RCE_PINSME, RCE->RCE_PINSMA))			
		EndIf
		aAdd(aRet, {.T.,dDataDe, dDataAte, cBsCal, nPerc, cGrau, 0, 0, 0, SRA->RA_CC})
	Else 
		aAdd(aRet, {.F.,dDataDe, dDataAte, "", 0, "", 0, 0, 0, SRA->RA_CC} )
	EndIf
EndIf

Return aRet

/*/{Protheus.doc} fDescBsAdi
Carrega descrição da base do adicional
@author Leandro Drumond
@since 10/06/2025
@version P12
@return string
/*/
Static Function fDescBsAdi(aDesc,cTipo)
Local cRet     := ""
Local nPos 	   := 0

If ( nPos := aScan(aDesc, {|x| SubStr(x,1,1) == cTipo}) ) > 0
	cRet := SubStr(aDesc[nPos],3)
EndIf

Return cRet

/*/{Protheus.doc} GpCrgAdiGS
Chama a função do GS para alterar/resetar os campos de periculosidade e insalubirdade no cadastro funcionário
@author Bruno Costa
@since 10/07/2025
@version P12
@parametros lAlt = .T. chama a função para alterar os campos / .F. chama a função para resetar
@return Nil
/*/
Function fCpoGsInsPer(lAlt)

Static lCpoGs
Static cRotGS

DEFAULT lCpoGs := GetMvRH("MV_TECXRH", NIL, .F. ) .And. FindFunction("TecCheckAd") .And. FindFunction("TecResetAd")
DEFAULT cRotGS := If(lCpoGs, fGetCalcRot("1")+"/"+fGetCalcRot("2")+"/"+fGetCalcRot("3")+"/"+fGetCalcRot("4")+"/"+fGetCalcRot("5")+"/"+fGetCalcRot("6")+"/"+fGetCalcRot("N"), "")

If lCpoGs .And. cRot $ cRotGS
	If(lAlt, TecCheckAd(), TecResetAd())
EndIf

Return 
