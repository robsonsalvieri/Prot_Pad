	Local nPosPDV		:= 0
	Local nPosSB		:= 0 
	Local nLimAnu		:= 0  
	Local nLimBie		:= 0  
	Local nLimTri		:= 0  
	Local nLimQua		:= 0  
	Local nLimQui		:= 0
	Local nBsAuxAts		:= 0 
	Local l1Per			:= .F.
	Local lSaldoSal		:= .T.
	Local lPerAdiBas	:= P_RESADISB .And. cTipoRot == "4" //Calcula adicional sobre Salario Base na rescisao
	Local lIncorp080	:= !(IsInCallStack("GPER080") .And. Type("lSalInco") <> "U" .And. lSalInco .And. (RCE->RCE_BASCAT == "2") )
	Local lDifFerRes    := (FwIsInCallStack("GPEM040") .Or. FwIsInCallStack("CALRESC")) .And. cTipoRot == "3"
	Local lAdicInteg	:= .F.
	Local lDiasPer		:= (GetMVRH("MV_DIASPER",,"1") == "1")
	Local lProvisao 	:= IsInCallStack("GPEM070")
	Local nDiasFer		:= 0 
	Local nDiasTrab		:= 0 
