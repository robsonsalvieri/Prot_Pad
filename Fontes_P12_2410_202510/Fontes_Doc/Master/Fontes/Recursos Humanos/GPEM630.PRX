/*/{Protheus.doc} GetPerAPO
Obtem período de apontamento atual
@author Leandro Drumond
@since 07/03/2023
@version 1.0
/*/
Static Function GetPerAPO(dPerIni, dPerFim, dData)
Local cPerAponta := GetPaponta( SRA->RA_FILIAL )

If PerCompleto( @cPerAponta )
	dPerIni := Stod( Left(	cPerAponta , 08 ) )
	dPerFim := Stod( Right(	cPerAponta , 08 ) )
Else
	nDiaIni := Val( Left(	cPerAponta , 02 ) )
	nDiaFim := Val( Right(	cPerAponta , 02 ) )

	If Day(dData) > nDiaIni
		dPerIni := StoD(AnoMes((dData))+StrZero(nDiaIni,2))
		dPerFim := StoD(AnoMes(MonthSum(dData,If(nDiaFim < nDiaIni,1,0)))+StrZero(nDiaFim,2))
	Else
		dPerIni := StoD(AnoMes(MonthSub(dData,1))+StrZero(nDiaIni,2))
		dPerFim := StoD(AnoMes((dData))+StrZero(nDiaFim,2))
	EndIf
EndIf

Return Nil

