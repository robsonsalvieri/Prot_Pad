Local aTabRES		:= {}
Local aGpm630Log	:= {}
Local aOfusca		:= If(FindFunction('ChkOfusca'), ChkOfusca(), { .T., .F. }) //[2]Ofuscamento
Local aFldRel		:= Iif( aOfusca[2], FwProtectedDataUtil():UsrNoAccessFieldsInList( {"RA_NOME"} ), {} )
Local nX			:= 0
Private lOfusca		:= Len(aFldRel) > 0
Private aLogFile	:= {}
Private aLogTitle	:= {}
Private aFuncProc	:= {}
Private aLogConsig	:= {}

Begin Sequence

	If cTipCalc == "3"
		If !lBlind
			Proc2BarGauge({|lEnd| Gp630Efetiva()},,,, .T. , .T. , .F. , .F. )
		Else
			Gp630Efetiva()
		EndIf
		Break
	EndIf

	If cTipCalc == "4"
		If !lBlind
			Proc2BarGauge({|lEnd| Gp630Exclui()},,,, .T. , .T. , .F. , .F. )
		Else
			Gp630Exclui()
		EndIf
		Break
	EndIf

	If cPaisLoc == "VEN"
		fRetTab(@aTabRES,"S005",,,dDataBase)
		If Empty(aTabRES)
			FMSGFORM( {33} , , 1 , , .F. , .F.)	//# "Tabela de Tipos de Rescisao sem dados."
			Return .F.
		EndIf
	ElseIf cPaisLoc == "ANG"
		fRetTab(@aTabRES,"S015",,,dDataBase)
		If Empty(aTabRES)
			FMSGFORM( {33} , , 1 , , .F. , .F.)	//# "Tabela de Tipos de Rescisao sem dados."
			Return .F.
		EndIf
	Elseif cPaisLoc == "EQU"
		fRetTab(@aTabRES,"S018",,,dDataBase)
		If Empty(aTabRES)
			FMSGFORM( {33} , , 1 , , .F. , .F.)	//# "Tabela de Tipos de Rescisao sem dados."
			Return .F.
		EndIf
	EndIf

	If cModFol == "2"
		If !lBlind
			Proc2BarGauge({|lEnd| Gpem022Processa()},,,, .T. , .T. , .F. , .F. )
		Else
			Gpem022Processa()
		EndIf
	Else
		If !lBlind
			Proc2BarGauge({|lEnd| Gpem630Processa()},,,, .T. , .T. , .F. , .F. )
		Else
			Gpem630Processa()
		EndIf
	EndIf

End Sequence

/*
ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
³ Geracao do Log de Ocorrencias                              ³
ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ*/
If lGPEM040B
	aAdd(aGpm040Log, aLogFile)
	aAdd(aGpm040Log, aFuncProc)
	aAdd(aGpm040Log, aLogConsig)
Else
	aAdd(aGpm630Log, aLogFile)
	aAdd(aGpm630Log, aFuncProc)
	aAdd(aGpm630Log, aLogConsig)
	aLogFile := {}
	For nX := 1 to Len(aGpm630Log)
		If Len(aGpm630Log[nX]) > 0
			If nX == 1
				aAdd( aLogTitle, STR0045 )	//"Foram Encontradas as Seguintes Inconsistencias no Calculo das Rescisoes:"
			ElseIf nX == 2 
				aAdd( aLogTitle, STR0140 )	//"Funcionários processados:"
			Else
				//Atenção, o(s) funcionário(s) abaixo possuí(em) empréstimo(s) que serão liquidados parcialmente ou NÃO serão liquidados,
				//pois o saldo devedor do(s) mesmo(s) ultrapassa o valor do teto estabelecido pelo mnemônico P_PERCONS. Verifique os Lançamentos Futuros.
				aAdd( aLogTitle, STR0133 + CRLF + STR0134 )
			EndIf
			aAdd( aLogFile, aGpm630Log[nX] )
		EndIf
	Next nX

	fMakeLog( aLogFile, aLogTitle, NIL, NIL, FunName(), STR0046, , , , .F. ) //"Log de Ocorrências do Calculo da Rescisao"

EndIf

Return .T.

/*
ÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜ
±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
±±ÉÍÍÍÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍÍÍËÍÍÍÍÍÍÑÍÍÍÍÍÍÍÍÍÍÍÍÍ»±±
