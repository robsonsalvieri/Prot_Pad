			SetMnemonicos(NIL,NIL,.T.)

			cTipCalc	:= aParam[01]
			cProcAux	:= aParam[02]
			cRotAux		:= aParam[03]
			cPerAux		:= aParam[04]
			cSemAux		:= aParam[05]
			dDataIni	:= aParam[06]
			dDataFim	:= aParam[07]
			cSabDom		:= aParam[08]
			cMesAtu		:= aParam[09]
			cTipoRes	:= aParam[10]
			dDtAviso	:= aParam[11]
