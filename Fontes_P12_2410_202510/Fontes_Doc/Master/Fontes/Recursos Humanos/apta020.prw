Function Aptx020Del ()

Local uRet 
Local cAlias 	:= "RE3"  
Local nReg		:= 0

dbSelectArea("RE3")
dbSetOrder(1)
nReg			:= ( cAlias )->( Recno() )
                      
If (ChkDelRegs("RE3"))
	RecLock("RE3",.F.)
	uRet := AxDeleta( cAlias , nReg , 5 , NIL , NIL , NIL , NIL , NIL , .T. )
	MSUnlock()
Endif

Return Nil

