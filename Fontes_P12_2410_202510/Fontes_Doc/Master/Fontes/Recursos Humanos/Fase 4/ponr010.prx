	TurDe		:= IF( !lTerminal , mv_par10, SRA->RA_TNOTRAB)			//Turno De
	TurAte		:= IF( !lTerminal , mv_par11, SRA->RA_TNOTRAB)			//Turno Ate
	MatDe		:= IF( !lTerminal , mv_par12, cMatTerminal)				//Matricula De
	MatAte		:= IF( !lTerminal , mv_par13, cMatTerminal)				//Matricula Ate
	NomDe		:= IF( !lTerminal , mv_par14, SRA->RA_NOME)				//Nome De
	NomAte		:= IF( !lTerminal , mv_par15, SRA->RA_NOME)				//Nome Ate
	cSit		:= IF( !lTerminal , mv_par16, fSituacao( NIL , .F. ))	//Situacao
	cCat		:= IF( !lTerminal , mv_par17, fCategoria( NIL , .F. ))	//Categoria
	nImpHrs		:= IF( !lTerminal , mv_par18, 3 )						//Imprimir horas Calculadas/Inform/Ambas/NA
	nImpAut		:= IF( !lTerminal , mv_par19, 1 )						//Demonstrar horas Autoriz/Nao Autorizadas
	nCopias		:= IF( !lTerminal , If(mv_par20>0,mv_par20,1),1)		//N£mero de C¢pias
	lSemMarc	:= IF( !lTerminal , (mv_par21==1)	, .F. )				//Imprime para Funcion rios sem Marca‡oes
	cMenPad1	:= IF( !lTerminal , mv_par22, "" )						//Mensagem padr„o anterior a Assinatura
	cMenPad2	:= IF( !lTerminal , mv_par23, "" )						//Mens. padr„o anterior a Assinatura(Cont.)
	lSexagenal	:= IF( !lTerminal , (mv_par24==1), .T.  )				//Horas em  (Sexagenal/Centesimal)
    lImpRes		:= IF( !lTerminal , (mv_par25==1), .F.	)				//Imprime eventos a partir do resultado ?		
	lImpTroca   := IF( !lTerminal , (mv_par26==1), .F.	)				//Imprime Descricao Troca de Turnos ou o Atual 
	lImpExcecao := IF( !lTerminal , (mv_par27==1), .F.	)				//Imprime Descricao da Excecao no Lugar da do Afastamento 
