Static Function fSP2Cpos()
Local aCpos			:= {}
Local aNotCpos		:= {'P2_FILIAL','P2_ID', 'P2_TPEXCE'}
Local aSP2Campos	:= FWSX3Util():GetAllFields("SP2" , .T.)
Local nI			:= 0
Local nLenSP2		:= Len(aSP2Campos)

For nI := 1 To nLenSP2
	If X3USO(GetSx3Cache(aSP2Campos[nI],"X3_USADO")) .And. cNivel >= GetSx3Cache(aSP2Campos[nI],"X3_NIVEL") .And. Empty(Ascan(aNotCpos, {|x| x == AllTrim(aSP2Campos[nI])}))
		aAdd(aCpos, aSP2Campos[nI])
	EndIf
Next

Return aCpos

