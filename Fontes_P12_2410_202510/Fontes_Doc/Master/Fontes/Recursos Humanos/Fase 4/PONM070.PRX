Static Function fDiaArray( dData )

Local nIndice

Do Case
	Case ( Dow(dData) == 1 )
		nIndice := 20
	Case ( Dow(dData) == 2 )
		nIndice := 14	
	Case ( Dow(dData) == 3 )
		nIndice := 15	
	Case ( Dow(dData) == 4 )
		nIndice := 16	
	Case ( Dow(dData) == 5 )
		nIndice := 17	
	Case ( Dow(dData) == 6 )
		nIndice := 18	
	Case ( Dow(dData) == 7 )
		nIndice := 19	
EndCase

Return nIndice

