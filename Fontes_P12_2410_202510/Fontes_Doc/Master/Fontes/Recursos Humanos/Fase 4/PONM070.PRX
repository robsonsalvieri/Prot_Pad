						aCodAut[ nCodAut , 07 ] := SP9->P9_BHNATE
						aCodAut[ nCodAut , 08 ] := SP9->P9_BHPERC
						aCodAut[ nCodAut , 09 ] := IF( ( nBHDe + nBHAte ) > 0 .and. SP9->P9_BHAGRU == "S" , "S" , "N" )
						aCodAut[ nCodAut , 10 ] := SP9->P9_BHVAL
