Static Function Pn280LoadCalend()
Local lRet		 	:= .T.
Local aAreaDtMin 	:= GetArea()
Local cFilter    	:= ""
Local cAliasQuery 	:= ""
Local cFil          := SRA->RA_FILIAL
Local cMat			:= SRA->RA_MAT

//Busca a menor data existente na SP8 para mostrar todas as datas
#IFDEF TOP
	Local lExInAs400 := ExeInAs400()

	cAliasQuery := GetNextAlias()
		
	cFilter := " P8_FILIAL = '"+cFil+"'"
	cFilter += " AND P8_MAT = '"+cMat+"'"

	If !lExInAs400
		cFilter    += " AND D_E_L_E_T_ = ' ' "
	Else
		cFilter    += " AND @DELETED@ = ' ' "
	EndIf

	cFilter := "%"+cFilter+"%"
		
	BeginSql alias cAliasQuery
		column MINDTA as Date
		%NoParser%
		SELECT 
			MIN(cAliasQuery.P8_DATAAPO) MINDTA
		FROM 
			%Table:SP8% cAliasQuery
		WHERE 
			%Exp:cFilter%
	EndSql
							
	If !Empty((cAliasQuery)->(MINDTA)) .and. (cAliasQuery)->(MINDTA) < dPerIni
		dPerIni := (cAliasQuery)->(MINDTA)
	EndIf
				
	(cAliasQuery)->(dbCloseArea())
				
#ELSE

	Local cAlias 		:= "SP8"
	Local nOrderSP8     := 0
			
	cFilter := cFil
	cFilter += cMat
				
	nOrderSP8 := (cAlias)->(dbSetOrder(RetOrder("P8_FILIAL+P8_MAT+DTOS(P8_DATAAPO)+DTOS(P8_DATA)+P8_HORA")))
			
	dbSelectArea(cAlias)
	dbSetOrder(nOrderSP8)
				
	(cAlias)->(dbSeek( cFilter, .F. ))
				
	If Found() .and. (cAliasQuery)->(P8_DATAAPO) < dPerIni
		dPerIni := (cAliasQuery)->(P8_DATAAPO)
	EndIf
	
#ENDIF
			
RestArea( aAreaDtMin )
oPeriodo:dDataIni := dPerIni

/*
ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
³ Carrega o Calendario do Periodo Para o Funcionario           ³
ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ*/
IF SRA->( !CriaCalend(	dPerIni		,;	//01 -> Periodo Inicial
						dPerFim		,;	//02 -> Periodo Final
						RA_TNOTRAB	,;	//03 -> Turno de Trabalho
						RA_SEQTURN	,;	//04 -> Sequencia de Turno
						@aTabPadrao	,;	//05 -> Tabela de Horario Padrao
						@aTabCalend	,;	//06 -> Calendario de Marcacoes
						RA_FILIAL	,;	//07 -> Filial 
						RA_MAT		,;	//08 -> Matricula
						RA_CC		,;	//09 -> Centro de Custo
				   		NIL			,;	//10 -> Array com as Trocas de Turno
						NIL			,;	//11 -> Array com Todas as Excecoes do Periodo
						NIL			,;	//12 -> Se executa Query para a Montagem da Tabela Padrao
						.T.			;	//13 -> Se executa a funcao se sincronismo do calendario
					  );
		 )
	oCalendError:ShowErrors()
    lRet:=  .F.
EndIF        

Return ( lRet )
                                 


