	cMensagem	:= OemToAnsi(STR0085)		//"Carregando Abonos..." 
	
	bBlocMsg	:={|| nCnt:=P280AboCols(aHeader	,@aArrayMain	,@aRecno	,aFields	,@aVirtual	,nOpcX)}
	MsAguarde(bBlocMsg,cTitulo,cMensagem,.F.)


	aArrayClone := aClone(aArrayMain)     	
	    
