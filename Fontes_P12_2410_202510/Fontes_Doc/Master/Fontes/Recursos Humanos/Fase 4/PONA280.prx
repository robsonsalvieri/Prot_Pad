Static Function fChange(bBloco) 
Local lRet	:= .T.

Local lMuda	:= .F.
Local lTrata:= .F.
Local lList	:= .T.
Local xRet  

If bBloco<>Nil
   xRet:=Eval(bBloco)
   If  ValType(xRet)=="L"
		lMuda	:= xRet
		lTrata	:= .F.
		lList	:= .T.
   Else
       lMuda	:=xRet[1]
       lTrata	:=xRet[2]
       lList	:=xRet[3]
       
   Endif
Endif   

If cEstou=='1'
   If lMuda 
      fMuda(1)
   Endif  
   Eval(bTrataMarc,lTrata)
   If lList
      Eval(bApoList)
   Endif
ElseIf cEstou=='2'
  	   If lMuda
  	      fMuda(2)
  	   Endif
  	   Eval(bTrataApo,lTrata)
  	   If lList 
  	      Eval(bMarcList)
  	   Endif   
Else
   	If lMuda
   	   fMuda(3)
   	Endif
   	Eval(bTrataAbo,lTrata)
   	Eval(bAboList)
Endif

Return lRet                  

