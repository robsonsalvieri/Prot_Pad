		    P280AboGet()					//-- Reinicializa aCols Static Abonos
            Eval(bTrataApo,.F.)          	//-- Atualiza aApoCols baseado no ApoMain
            fMuda(2,DESCARREGA)  			//-- Salva as Informacoes dos Apontamentos em Matriz Auxiliar
            fMuda(1) 
		Endif
		nOpcao := 1 
		lRet:=.T. 
		If lSai
