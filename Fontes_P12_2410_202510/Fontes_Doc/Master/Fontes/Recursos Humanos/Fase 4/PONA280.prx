                                                                      IF( oGet:TudoOk() ,;
																		 (lGRAVA_EM_ARQUIVO:=.F.,;                                                                                                                                                                                   
																		  pn210Grava('SPK' ,nOpcx,aElem   ,aSVTREE[G_APO,G_HEADER],@aSVTREE[G_APO,G_ACOLS],@aSVTREE[G_APO,G_N],Nil     ,cEvento,cCCSPC,cTpMarca,cPrefix,IF( lPona190 , "SPH" , "SPC" ),lGRAVA_EM_ARQUIVO,cDeptoSPC, cPostoSPC, cFunSPC, cProcesSPC, cRotSPC, cPerSPC, cNrPagSPC),;
																		  Eval(bTrataAbo,.T.),;
																		  fMuda(3,DESCARREGA),;
																		  nOpcA := 1 , RestKeys(aKeys,.T.),;
																		  oDlg1:End() ) , ;
																		  ( nOpcA := 0 , .F. ) );
				   }
//-- Antes de Fechar o dialogo alterar setar nOpca para 3 senao realizamos a pergunta de abandono 2 vezes
bSet24 	:= { ||fVarGet(@o3Get, @aHeader,@aCols,@n, DESCARREGA,3),If( fSai280({{aSVTREE[G_ABO,G_ACOLS],aCols}}),( nOpca := 3, RestKeys(aKeys,.T.), oDlg1:End(),.T.),(RestKeys(aKeys,.T.),.F.)) }



//-- Posiciona no Get de Abonos e realiza as tarefas necessarias
Eval(o3Get:oBrowse:bGotFocus)

ACTIVATE MSDIALOG oDlg1 ON  INIT (EnchoiceBar (oDlg1,bSet15, bSet24))  CENTERED 
//-- Confirmou Abonos Digitados
If nOpcA==1                   
   lRet:=.T.
Else
    //-- Aborta operacao e Recupera as informacoes dos Abonos 
    fMuda(3)
    lRet:=.F.
Endif   
                    
//-- Desprograma Teclas de Botoes do Abono
fProgTeclas('3',.F.)                     

RETURN lRet
         

