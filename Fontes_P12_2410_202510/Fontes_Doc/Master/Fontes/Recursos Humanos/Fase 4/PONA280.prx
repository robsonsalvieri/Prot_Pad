//---------------------variaveis
Local bAfterCols	:= {|| }
Local aNoFields 	:= {cPrefix+"MAT",cPrefix+"NOME"}	
Local bSeekWhile	:= {|| }
Local nOrdcAlias	:= RetOrdem( cAlias , (cAlias)->(cPrefix+"FILIAL"+cPrefix+"MAT"+cPrefix+"PD"+Dtos(ctod(cPrefix+"DATA"))+cPrefix+"TPMARCA"+cPrefix+"CC"+cPrefix+"DEPTO"+cPrefix+"POSTO"+cPrefix+"CODFUNC"    ) )
local nCont	:= 0
