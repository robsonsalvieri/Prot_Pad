/*/{Protheus.doc} fMigrRR1
	Efectua la migraciСn de los registros de MSA_CONTROL_MARCAC a la tabla RR1.
	@type  Function
	@author oscar.lopez
	@since 08/08/2025
	@version 1.0
	@return lOk, lСgico, Indica si los registros que no existen en MSA_CONTROL_MARCAC
							son incluidos correctamente en tabla RR1
	@example
	lMigra := fMigrRR1()
	/*/
Function fMigrRR1()

	Local aArea			:= GetArea()
	Local cAliasMSA		:= ""
	Local lMigra		:= .T.
	Local lOk			:= .T.
	Local lExtFilMsa	:= .F.
	Local lTabInfMSA	:= .T.
	Local nRegCont		:= 0
	Local nRegProc		:= 0
	Local oTabMSA		:= Nil

	If !TCCanOpen("MSA_CONTROL_MARCAC") .Or. !(TCSQLExec("SELECT * FROM MSA_CONTROL_MARCAC") >= 0)
		lTabInfMSA := .F.
	EndIf

	If lTabInfMSA
		lExtFilMsa := fVldMSAFil()

		cQuery	:= " SELECT * FROM MSA_CONTROL_MARCAC MSA "
		oTabMSA	:= FwExecStatement():New(cQuery)
		cAliasMSA := oTabMSA:OpenAlias()

		RR1->( dbSetOrder(6) ) // STR(RR1_VALCON,16)+RR1_CODREL+STR(RR1_CODNSR,9)
		If (cAliasMSA)->( !Eof() ) .And. RR1->( MsSeek( Str( (cAliasMSA)->VAL_CONTROL_MARCAC, 16 ) ) )
			lMigra	:= .F.
		EndIf

		If lMigra
			While (cAliasMSA)->( !Eof() )
				If !RR1->( MsSeek( Str( (cAliasMSA)->VAL_CONTROL_MARCAC, 16 ) ) )
					nRegCont++
					If RR1->( RecLock("RR1", .T.) )
						nRegProc++
						If lExtFilMsa
							RR1->RR1_FILIAL	:= (cAliasMSA)->COD_FIL_MARC
						EndIf
						RR1->RR1_VALCON	:= (cAliasMSA)->VAL_CONTROL_MARCAC
						RR1->RR1_CODREL	:= fVldNull( (cAliasMSA)->COD_RELOGIO_EXT_CHAVE )
						RR1->RR1_CODFUN	:= fVldNull( (cAliasMSA)->COD_FUNC_MSA )
						RR1->RR1_LOGIP	:= AllTrim( Str( (cAliasMSA)->LOG_IMPORT_PRODUT ) )
						RR1->RR1_CODNSR	:= (cAliasMSA)->COD_NSR
						RR1->RR1_CODPIS	:= fVldNull( (cAliasMSA)->COD_PIS_MSA )
						RR1->RR1_DATMAR	:= (cAliasMSA)->DAT_MARCAC_ACES
						RR1->RR1_NUMMAR	:= (cAliasMSA)->NUM_HORAR_MARCAC_ACES
						RR1->RR1_CODREP	:= (cAliasMSA)->COD_REP
						RR1->RR1_CODUNI	:= fVldNull( (cAliasMSA)->COD_UNID_EXT_CHAVE )
						RR1->RR1_CODUSU	:= fVldNull( (cAliasMSA)->COD_USUAR_EXT_CHAVE )
						RR1->RR1_FUSO	:= fVldNull( (cAliasMSA)->COD_FUSO )
						RR1->RR1_CODCPF	:= fVldNull( (cAliasMSA)->COD_CPF_FUNC )
						RR1->RR1_VRSLAY	:= fVldNull( (cAliasMSA)->NUM_VERS_LAYOUT_AFD, 0 )
						RR1->RR1_INSCR	:= fVldNull( (cAliasMSA)->COD_INSCR_EMP )
						RR1->RR1_CCTREP	:= fVldNull( (cAliasMSA)->COD_CCT )
						RR1->( MsUnlock() )
						TcSqlExec("DELETE FROM MSA_CONTROL_MARCAC WHERE VAL_CONTROL_MARCAC = '" + cValToChar( (cAliasMSA)->VAL_CONTROL_MARCAC ) + "'")
					EndIf
				EndIf
				(cAliasMSA)->( dbSkip() )
			EndDo

			// Verifica que la cantidad de registros de MSA_CONTROL_MARCAC que no existan en tabla RR1 hayan sido incluidos.
			lOk := (nRegCont == nRegProc)

			// Elimina la tabla MSA_CONTROL_MARCAC si fueron procesados todos los registros que no existian en tabla RR1
			If lOk
				TCSQLExec("DROP TABLE MSA_CONTROL_MARCAC")
			EndIf
		EndIf

		(cAliasMSA)->( dbCloseArea() )

		FWFreeObj(oTabMSA)
	EndIf

	RestArea(aArea)

Return lOk	

/*/{Protheus.doc} fVldNull
	Valida si valor es nulo (NULL), para asignar un valor por defecto.
	@type  Static Function
	@author oscar.lopez
	@since 08/08/2025
	@version 1.0
	@param xContCampo, variable, Contenido del campo que deberА ser validado
	@param xValDef, variable, Valor por defecto si el campo contiene valor nulo (NULL)
	@return xRet, variable, Retora el valor a grabar despues de validar que no sea nulo (NULL)
	@example
		nVal := fVldNull( (cAliasMSA)->NUM_VERS_LAYOUT_AFD, 0 )
/*/
Static Function fVldNull(xContCampo, xValDef)

	Local xRet

	Default xContCampo	:= ""
	Default xValDef		:= ""

	xRet := xValDef

	If AllTrim( xContCampo ) != "NULL"
		xRet := xContCampo
	EndIf

Return xRet
