Static Function DbfGetData( cDbfAlias , nFields )

Local cTxt := ""

Local cFieldType
Local nField
Local uData

For nField := 1 To nFields
	uData		:= ( cDbfAlias )->( FieldGet( nField ) )
	cFieldType	:= ValType( uData )
	IF ( cFieldType == "C" )
		cTxt += uData
	ElseIF ( cFieldType == "N" )
		cTxt += StrZero( uData , 5 , 2 )
	ElseIF ( cFieldType == "D" )
		cTxt += Dtos( uData )
	EndIF
Next nField

Return( cTxt )

