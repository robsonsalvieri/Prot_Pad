	ACTIVATE MSDIALOG oDlgGrid CENTERED ON INIT EnchoiceBar(oDlgGrid, bOK ,bFecha, NIL, NIL)

	If nOpcB == 1
		aArrayFil		:= {}
		//Adiciona filiais selecionadas
