	aAdd(aCombAhgora, OemToAnsi(STR0535)) //Ponto obrigatório
	aAdd(aCombAhgora, OemToAnsi(STR0534)) //Ponto livre
	cCampoAlt := OemToAnsi(STR0541) 	  //Ponto do funcionário
