					aRCPCols[nX][ny] := cCodPat
				EndIf
			ElseIf Trim(aHeaderRCP[ny][2]) == "RCP_TIPCOT"
				aRCPCols[nX][ny] := GetMemVar( "RA_TIPCOT" )
			EndIf
		EndIf
		If cPaisLoc == "PAR"
			If Trim(aHeaderRCP[ny][2]) == "RCP_CIC"
				aRCPCols[nX][ny] := GetMemVar( "RA_CIC" )
			ElseIf Trim(aHeaderRCP[ny][2]) == "RCP_CODFUN"
