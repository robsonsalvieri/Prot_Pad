			ElseIF	 Trim(aHeaderRCP[ny][2]) == "RCP_NITARL"
				aRCPCols[nX][ny] := GetMemVar( "RA_NITARL" )
			ElseIF	 Trim(aHeaderRCP[ny][2]) == "RCP_NITAFP"
