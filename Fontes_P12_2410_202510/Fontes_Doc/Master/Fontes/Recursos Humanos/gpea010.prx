				If !Empty(aRCPCols[nPos,nPosDtImss] )
					If lGeraLog
						lLog := .T.
						If aTotRegs[1]== 0
							cLog := STR0059 									// "Registro ja foi enviado ao Seguro Social"
							Aadd(aTitle,cLog)
