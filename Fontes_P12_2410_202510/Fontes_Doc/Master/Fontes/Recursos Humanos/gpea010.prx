		cAliasTRB		:= GetNextAlias()
		aSM0     		:= FWLoadSM0(.T.,,.T.)

		Aadd(aStru, {"OK"		, "C", 2						, 0})
		Aadd(aStru, {"FILIAL"	, "C", FwGetTamFilial			, 0})
		Aadd(aStru, {"NOME"  	, "C", 100						, 0})
		Aadd(aStru, {"CNPJ"  	, "C", TamSx3("CTT_CEI")[1]  	, 0})
