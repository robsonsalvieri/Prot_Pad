	@ aObjSize[1][1] -10 , 001 SCROLLBOX oScroll HORIZONTAL VERTICAL SIZE aObjSize[1][1]+380, aObjSize[1][1] +400 PIXEL OF oDlg
	@ aObjSize[1][1] -7, aObjSize[1][1] SAY oSay1 PROMPT OemToAnsi(STR0538) SIZE 215, 035 OF oScroll COLORS 0, 16777215 PIXEL
	@ aObjSize[1][1] +21, aObjSize[1][1] SAY oSay1 PROMPT cCampoAlt SIZE aObjSize[1][1] + 40, aObjSize[1][1] - 5 OF oScroll COLORS 0, 16777215 PIXEL
	@ aObjSize[1][1] +18, aObjSize[1][1] +55 MSCOMBOBOX oComboBox VAR cComboBox ITEMS aCombAhgora SIZE aObjSize[1][1] + 40, aObjSize[1][1] - 2 OF oScroll COLORS 0, 16777215 PIXEL WHEN ( If(nOpc != 2, .T., .F.) )
	@ aObjSize[1][1] +42, aObjSize[1][1] SAY  OemToAnsi(STR0542) SIZE aObjSize[1][1] + 40, aObjSize[1][1] - 5 OF oScroll COLORS 0, 16777215 PIXEL //"Data da elegibilidade"
	@ aObjSize[1][1] +39, aObjSize[1][1] +55  MSGET oPerIni VAR dDtEleg  SIZE 50,10  OF oScroll COLORS 0, 16777215 PIXEL WHEN ( If(nOpc != 2, .T., .F.) ) VALID ( GpAhgVldDt(dDtEleg) ) HASBUTTON
	@ aObjSize[1][1] +58, aObjSize[1][1] +100 SAY  cConteSR9  SIZE aObjSize[1][1] + 120, aObjSize[1][1] +120 OF oScroll COLORS 0, 16777215 PIXEL

ACTIVATE MSDIALOG oDlg ON INIT EnchoiceBar( oDlg , bSet15 , bSet24 ) CENTERED

If nOpc <> 2 .And. SR9->(dbSeek(SRA->RA_FILIAL + SRA->RA_MAT + cCampoSR9 + Dtos(dDtEleg)))
	fDadosAhgora(nOpc, "P",, Rtrim(SR9->R9_DESC))
EndIf

RestKeys(aKeys, .T.)
RestArea(aArea)

Return

/*/{Protheus.doc} GpAhgVldDt
Função para validar a data de elegibilidade para o ponto Ahgora
@author Bruno Costa
@since 12/08/2024
/*/
Static Function GpAhgVldDt(dDtVld)

If Empty(dDtVld)
	Help( " ", 1, OemToAnsi(STR0536),, OemToAnsi(STR0537), 1, 0 ) //"AHGORA" ### "A data de elegibilidade deve ser preenchida!"
	Return .F.
ElseIf M->RA_ADMISSA > dDtVld
	Help( " ", 1, OemToAnsi(STR0536),, OemToAnsi(STR0543), 1, 0 ) //"AHGORA" ### "A data de elegibilidade não pode ser anterior a data de admissão do funcionário."
	Return .F.
ElseIf dDtVld > Date()
	Help( " ", 1, OemToAnsi(STR0536),, OemToAnsi(STR0544), 1, 0 ) //"AHGORA" ### "A data de elegibilidade não pode ser posterior à data atual."
	Return .F.
EndIf

Return .T.


/*/{Protheus.doc} fAltAhgora
Verifica se os campos de envio da API Ahgora foram alterados
@author Bruno Costa
@since 27/08/2024
/*/
Static Function fAltAhgora(dDtSR7)
Local lAlterou  := .F.
Local bCond     := {|x|( SRA->RA_NOME <> GetMemVar( "RA_NOME" ) ) .Or. ;
( SRA->RA_NOMECMP <> GetMemVar( "RA_NOMECMP") ) .Or. ;
( SRA->RA_NSOCIAL <> GetMemVar( "RA_NSOCIAL") ) .Or. ;
( SRA->RA_NASC <> GetMemVar( "RA_NASC") ) .Or. ;
( SRA->RA_CIC <> GetMemVar( "RA_CIC") )  .Or. ;
( SRA->RA_SEXO <> GetMemVar( "RA_SEXO") ) .Or. ;
( SRA->RA_RG <> GetMemVar( "RA_RG") ) .Or. ;
( SRA->RA_EMAIL <> GetMemVar( "RA_EMAIL") ) .Or. ;
( SRA->RA_TNOTRAB <> GetMemVar( "RA_TNOTRAB") ) .Or. ;
( SRA->RA_CATFUNC <> GetMemVar( "RA_CATFUNC") ) .Or. ;
( SRA->RA_TELEFON <> GetMemVar( "RA_TELEFON") ) .Or. ;
( SRA->RA_ADMISSA <> GetMemVar( "RA_ADMISSA") ) .Or. ;
( SRA->RA_DEMISSA <> GetMemVar( "RA_DEMISSA") ) .Or. ;
( SRA->RA_HRSMES <> GetMemVar( "RA_HRSMES") ) .Or. ;
( SRA->RA_SALARIO <> GetMemVar( "RA_SALARIO") ) .Or. ;
( SRA->RA_PIS <> GetMemVar( "RA_PIS") )	.Or. ;
( SRA->RA_NUMCP <> GetMemVar( "RA_NUMCP") ) .Or. ;
( SRA->RA_SINDICA <> GetMemVar( "RA_SINDICA") )}

If Eval( bCond )
	lAlterou := .T.
EndIf

If SRA->RA_CODFUNC != GetMemVar( "RA_CODFUNC")
	lAlterou := .T.
	dDtSR7   := GetMemVar( "RA_DATAALT")
EndIf

Return (lAlterou)

/*/{Protheus.doc} fDadosAhgora
Envia dados para gravação na RUM - Tabela de integração com Ahgora
@author Bruno Costa
@since 27/08/2024
/*/
Function fDadosAhgora(nOpc, cPonto, dDTurno, cTpPW)
Local aArea		  := GetArea()
Local aCodInterno := {}
Local cFilAhgora  := SRA->RA_FILIAL
Local cChave      := cEmpAnt + "|" + cFilAhgora + "|" + SRA->RA_MAT
Local cVldPonto   := SuperGetMv("MV_INTEGPW",, "1")
Local dDtSR7 	  := CTOD("//")

DEFAULT cPonto  := ""
DEFAULT cTpPW   := ""
DEFAULT nOpc    := 0
DEFAULT dDTurno := cToD("//")

If !ChkFile("RUM")
	Return
EndIf

If cVldPonto <> "1" .And. cTpPW <> "1"
	If !GPEAhGora():fAhgoraPW(cVldPonto, cTpPW)
		Return
	EndIf
EndIf

If nOpc == 3 .Or. nOpc == 7
	aAdd(aCodInterno, {cFilAhgora, SRA->RA_MAT, cEmpAnt+cFilAhgora+SRA->RA_MAT, Nil, Nil, "I" })
ElseIf Empty(cPonto)
	If nOpc == 4 .And. fAltAhgora(@dDtSR7)
		aAdd(aCodInterno, {cFilAhgora, SRA->RA_MAT, cEmpAnt+cFilAhgora+SRA->RA_MAT, Nil, Nil, "A", dDtSR7 })
	ElseIf nOpc == 5
		aAdd(aCodInterno, {cFilAhgora, SRA->RA_MAT, cEmpAnt+cFilAhgora+SRA->RA_MAT, Nil, Nil, "A" })
	EndIf
ElseIf !Empty(dDTurno)
	aAdd(aCodInterno, {cFilAhgora, SRA->RA_MAT, cEmpAnt+cFilAhgora+SRA->RA_MAT, Nil, Nil, "A", Nil, dDTurno })
Else
	aAdd(aCodInterno, {cFilAhgora, SRA->RA_MAT, cEmpAnt+cFilAhgora+SRA->RA_MAT, Nil, Nil, "A" })
EndIf

GPEAhGora():gravaRUM("I", "SRA", cChave, aCodInterno, "1")

RestArea(aArea)

Return

