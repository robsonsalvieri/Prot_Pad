Function GP10OrgxRSP()
Local lRet 	:= .F.
Local lIntRSP	:=  GetMv("MV_IORGRSP", NIL, "0") == "1"
Local nA		:= 0

	If lIntRSP
		For nA := 1 to 10
			If Upper( AllTrim( ProcName( nA ) ) ) $ "RSPM001/RS001CNV"
