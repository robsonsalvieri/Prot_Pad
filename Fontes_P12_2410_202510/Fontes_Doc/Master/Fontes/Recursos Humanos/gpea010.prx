/*/{Protheus.doc} fVldMatEsp
Função que valida se existem caracteres especiais no campo RA_MAT
@author martins.marcio
@since 28/05/2024
/*/
Function fVldMatEsp(cRA_MAT)
	Local lRet 		:= .T.
	Local nW		:= 0
	Local cHelp		:= ""
	Local cChr01	:= ""

	DEFAULT cRA_MAT := M->RA_MAT

	If cPaisLoc == 'BRA'
		 cRA_MAT := AllTrim(cRA_MAT)
		 For nW := 1 To Len(cRA_MAT)
		 	// Permite apenas letras e números
			cChr01 := SUBSTR(cRA_MAT,nW,1)
			If !(Empty(cChr01) .Or. ISALPHA(cChr01) .Or. ISDIGIT(cChr01))
				// "No preenchimento do campo Matricula(RA_MAT) não devem ser usados caracteres especiais: [ ] "
				cHelp := OemtoAnsi(STR0533) + "[" + cChr01 + "]"
				Help("",1,"ATENCAO", , cHelp ,1,0)
				lRet := .F.
				EXIT
			EndIf
		Next nW
	EndIf

Return lRet

