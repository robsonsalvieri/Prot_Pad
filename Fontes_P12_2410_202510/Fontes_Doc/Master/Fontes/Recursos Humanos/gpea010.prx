Function RaTpMoeda()
Local cOpcBox:=''
Local I := 0

	For I = 1 To 5
 		cMOEDA  := GetMv("MV_MOEDA"+STR(I,1))
   		If Empty(cMOEDA)
     		Loop
       	Else
