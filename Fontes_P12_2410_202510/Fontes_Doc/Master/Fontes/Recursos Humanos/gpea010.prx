			ElseIf Trim(aHeaderRCP[ny][2]) == "RCP_NPGMOV"
				aRCPCols[nX][ny] := GetMemVar( "RA_PAGADM" )
			ElseIf Trim(aHeaderRCP[ny][2]) == "RCP_RG"
				aRCPCols[nX][ny] := GetMemVar( "RA_RG" )
			ElseIf Trim(aHeaderRCP[ny][2]) == "RCP_CARGO"
