Local cCampoAlt    := ""
Local cCampoSR9	   := "AHGORA"+Space(4)
Local dDtEleg	   := If(nOpc == 3 .Or. nOpc == 7, SRA->RA_ADMISSA, Date())
Local dDHisSR9     := CtoD("")
Local bSet15	   := {|| IIf(!Empty(dDtEleg), IIf(nOpc != 2, (fGravaSr9(cCampoSR9, If(cComboBox == OemToAnsi(STR0534), "2", "1"), "", dDtEleg, .T. ), oDlg:End()), oDlg:End()), Help( " ", 1, OemToAnsi(STR0536),, OemToAnsi(STR0537), 1, 0 ))}
