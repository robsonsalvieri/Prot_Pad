	ElseIf dData1 >= dData2
		lRet:= .F.
		Help( ,, 'Help',, cTitloCmp2 +" "+OemToAnsi(STR0275), 1, 0 )//" e invalida, pois e menor ou igual a sua emissao."
	EndIf

Return lRet


