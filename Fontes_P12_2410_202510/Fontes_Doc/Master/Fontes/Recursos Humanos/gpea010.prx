/*{Protheus.doc} somaAfasDn
Função que retorna o total de afastamentos por doença ou acidente do trabalho no período
@author  martins.marcio
@since   12/12/2024
@version 1
/*/
Static Function somaAfasDn(dDataIni, dDataFim)
	Local aArea		:= GetArea()
	Local nDiasAfas	:= 0
	Local cQuery	:= ""
	Local oStatement := Nil
	Local cQrAlias	:= ""

	Default dDataIni := SRA->RA_ADMISSA
	Default dDataFim := YearSum(SRA->RA_ADMISSA, 5)

	cQuery += "SELECT SR8.R8_TIPOAFA, SR8.R8_DATAINI,SR8.R8_DATAFIM, SR8.R8_DURACAO, RCM.RCM_TPEFD, RCM.RCM_CODSEF"

	cQuery += "		FROM "+ RetSqlName("SR8") + " SR8"
	cQuery += "		LEFT JOIN "+ RetSqlName("RCM") + " RCM ON RCM.RCM_TIPO = SR8.R8_TIPOAFA"

	cQuery += "		WHERE SR8.R8_FILIAL = '" 	+ xFilial("SR8", SRA->RA_FILIAL) + "'"
	cQuery += "		AND   RCM.RCM_FILIAL = '" 	+ xFilial("RCM", SRA->RA_FILIAL) + "'"
	cQuery += "		AND   SR8.R8_MAT = ? "
	cQuery += "		AND   SR8.R8_DURACAO > 15 "
	cQuery += "		AND   SR8.R8_DATAINI BETWEEN ? AND ? "
	cQuery += "		AND   ( RCM.RCM_TPEFD IN (?, ?) OR SUBSTRING(RCM.RCM_CODSEF, 1, 1) IN (?, ?) ) "
	cQuery += "		AND   SR8.D_E_L_E_T_ = ' '	"
	cQuery += "		AND   RCM.D_E_L_E_T_ = ' '	"

	cQuery := ChangeQuery(cQuery)
	oStatement := FwExecStatement():New(cQuery)
	oStatement:SetString(1, SRA->RA_MAT )
	oStatement:SetString(2, DTOS(dDataIni) )
	oStatement:SetString(3, DTOS(dDataFim) )
	oStatement:SetString(4, '01' )
	oStatement:SetString(5, '03' )
	oStatement:SetString(6, 'P' )
	oStatement:SetString(7, 'O' )

	// Executa a query e retorna o alias criado
    cQrAlias := oStatement:OpenAlias()

	(cQrAlias)->(DBGoTop())

	While (cQrAlias)->(!Eof())
		nDiasAfas += (cQrAlias)->R8_DURACAO
		(cQrAlias)->(DBSkip())
	EndDo
	(cQrAlias)->(DbCloseArea())

	RestArea(aArea)

Return nDiasAfas

/*{Protheus.doc} existRB6
Função que retorna o registro existe na tabela RB6
@author  martins.marcio
@since   16/07/2025
@version 1
/*/
Function existRB6(cChvRB6, nIndRB6)
	Local lRet    := .F.
	Local nTamChv := 0

	Default cChvRB6 := ""
	Default nIndRB6 := 1 //RB6_FILIAL+RB6_TABELA+RB6_NIVEL+RB6_FAIXA

	If !Empty(cChvRB6)
		If Type("lGp010Auto") == "U" .Or. !lGp010Auto
			lRet := EXISTCPO("RB6", cChvRB6)
		Else
			nTamChv := Len(cChvRB6)
			DbSelectArea("RB6")
			RB6->(DbSetOrder(nIndRB6))
			If RB6->(DbSeek(xFilial("RB6") + cChvRB6))
				If RB6->(ColumnPos("RB6_MSBLQL")) > 0
					While !RB6->(Eof() ) .And. RB6->RB6_FILIAL == xFilial("RB6") .And. Left(RB6->RB6_TABELA+RB6->RB6_NIVEL+RB6->RB6_FAIXA, nTamChv) == cChvRB6
						If RB6->RB6_MSBLQL <> "1"
							lRet := .T.
							EXIT
						EndIf
						RB6->(DbSkip())
					EndDo
				Else
					lRet := .T.
				EndIf
			EndIf
		EndIf
	EndIf

Return lRet
