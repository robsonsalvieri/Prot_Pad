/*{Protheus.doc} fUpdSR9Ahgora
Processa a atualização da SR9
@author  Leandro Drumond
@since   11/09/2024
@version 1
*/
Static Function fUpdSR9Ahgora(oSay, oMeter2, oMeter3, oSayProc2, oSayProc3, lPonto)

	Local aArea 		:= GetArea()
	Local aFuncProc     := {}
	Local cCampoSR9		:= "AHGORA"
	Local cFilQry		:= ""
	Local cTpPonto		:= If(lPonto, "1", "2")
	Local cAliasSRA		:= GetNextAlias()
	Local dDataAtu      := CtoD("")
	Local nContFil		:= 0
	Local nTotFil       := Len(aArrayFil)
	Local nTotFunc      := 100
	Local nFunc 		:= 0

	oMeter2:SetTotal( nTotFil ) //Número de filiais

	For nContFil := 1 To nTotFil
		cFilQry += aArrayFil[nContFil]
		aAdd(aFuncProc,{})
	Next nContFil

	cFilQry := "%(" + fSqlIn(cFilQry, FwGetTamFilial) + ")%"

	BeginSql Alias cAliasSRA
		SELECT RA_FILIAL, RA_MAT, RA_CATFUNC
		FROM %table:SRA% SRA
		WHERE	SRA.RA_FILIAL IN %exp:cFilQry%
			AND SRA.RA_MAT >= %exp:cMatDe%
			AND SRA.RA_MAT <= %exp:cMatAte%
			AND SRA.RA_CODFUNC >= %exp:cFuncDe%
			AND SRA.RA_CODFUNC <= %exp:cFuncAte%
			AND SRA.RA_DEMISSA = %exp:''%
			AND SRA.%notDel%
		ORDER BY RA_FILIAL, RA_MAT
	EndSql

	While (cAliasSRA)->(!Eof())
		If (cAliasSRA)->RA_CATFUNC $ cCategs
			nContFil := aScan(aArrayFil, {|x| x == (cAliasSRA)->RA_FILIAL })
			aAdd(aFuncProc[nContFil], (cAliasSRA)->(RA_FILIAL+RA_MAT))
		EndIf
		(cAliasSRA)->(dbSkip())
	EndDo

	(cAliasSRA)->( dbCloseArea() )

	//Processamento
	For nContFil := 1 to nTotFil

		oMeter2:Set(nContFil)
		oSayProc2:SetText( STR0568 + cValToChar(nContFil) + "/" + cValToChar(nTotFil) )//"Filiais processadas: "

		nTotFunc := Len(aFuncProc[nContFil])

		If nTotFunc > 0
			oMeter3:SetTotal( nTotFunc ) //Número de Funcionários
			For nFunc := 1 to nTotFunc
				oMeter3:Set(nFunc)
				oSayProc3:SetText( STR0569 + cValToChar(nFunc) + "/" + cValToChar(nTotFunc) )//"Funcionários processados: "

				If SRA->(DbSeek(aFuncProc[nContFil][nFunc]))
					dDataAtu := If(lAdmissao .or. SRA->RA_ADMISSA > dDataEleg, SRA->RA_ADMISSA, dDataEleg)

					fGravaSr9(cCampoSR9, cTpPonto,"", dDataAtu, .T. )
					fDadosAhgora(3, "P",, cTpPonto)
				EndIf
			Next nFunc
		EndIf
	Next nContFil

	MsgInfo(STR0570, STR0245)//"Processamento Finalizado."##"Atenção"
	lInExec := .F.
	lConcl  := .T.

	If ValType(oMeter2) <> "U"
		oSay:setText(Space(20)+STR0570)//"Processamento Finalizado."
		oMeter2:Free()
		oMeter3:Free()
		oSayProc2:setText("")
		oSayProc3:setText("")
	EndIf

	RestArea(aArea)

Return

