					EndIf
					If IsMemVar("RA_TJRNDA") .And. (GetMemVar("RA_TJRNDA") # GdFieldGet("RA_TJRNDA", nLin, .F. ,aSraHeader,aSvSraCols) )
						cTipoMov := "23"
					EndIf
	                If IsMemVar("RA_CODFUNC" ) .And. (GetMemVar("RA_CODFUNC") # GdFieldGet("RA_CODFUNC", nLin, .F. ,aSraHeader,aSvSraCols) )
