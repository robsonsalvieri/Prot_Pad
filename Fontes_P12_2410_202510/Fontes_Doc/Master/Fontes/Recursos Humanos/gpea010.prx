Local oScroll 	   := NIL
Local oDlg    	   := NIL
Local oSay1   	   := NIL
Local oPerIni      := NIL
Local oComboBox    := NIL

aAdvSize	 := MsAdvSize()
aInfoAdvSize := { aAdvSize[1], aAdvSize[2], aAdvSize[3], aAdvSize[4], 0, 0 }
aAdd(aObjCoords, {000, 000, .T., .T.})
aObjSize     := MsObjSize( aInfoAdvSize , aObjCoords )

If !(nOpc == 3 .Or. nOpc == 7)
	cConteSR9 := gpRetSR9("SRA", Date(), "AHGORA", @dDHisSR9)
	If !Empty(cConteSR9) .And. !Empty(dDHisSR9)
		aAdd(aCombAhgora, If(nOpc != 2, If(Alltrim(cConteSR9) == "1", OemToAnsi(STR0534), OemToAnsi(STR0535)), If(Alltrim(cConteSR9) == "1", OemToAnsi(STR0535), OemToAnsi(STR0534))))
		cCampoAlt := If(nOpc != 2, OemToAnsi(STR0546), OemToAnsi(STR0541)) //"Alterar Para"
		cConteSR9 := If(Alltrim(cConteSR9) == "1", OemToAnsi(STR0539) + DtoC(dDHisSR9), OemToAnsi(STR0540) + DtoC(dDHisSR9)) //"Histórico: Ponto obrigatório desde" ### Histórico: Ponto livre desde
		dDtEleg   := If(nOpc == 2, dDHisSR9, dDtEleg)
	Else
		aAdd(aCombAhgora, OemToAnsi(STR0535)) //Ponto obrigatório
		aAdd(aCombAhgora, OemToAnsi(STR0534)) //Ponto livre
		cCampoAlt := OemToAnsi(STR0541)       //Ponto do funcionário
	EndIf
