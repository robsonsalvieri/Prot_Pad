	Local aDicFch			:= {}
	Local cCPF				:= ""
	Local cCatEFD			:= ""
	Local cMatric			:= ""
	Local cCodigo
	Local bSraGsp			:= {||.T.}
	Local nPosicao := 0
	Local bFina673Obr		:= {|| If( FindFunction('Fina673Obr'), Fina673Obr("SRA"),.T.) }
	Local lAutoErro 		:= .F.
