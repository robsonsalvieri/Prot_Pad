   			//oNewBtn := TBtnBmp():NewBar( cRes,cRes,,,cDesc, bAction, .T., oBar,,,cToolt)//cria botao
		Next
	EndIf
EndIf

If lPnaBar
	aBar40 	:= ExecBlock( "PNA040BAR", .F., .F., {aButtons} )
