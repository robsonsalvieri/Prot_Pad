Function fAltAtTno(aMarcacoes,aCols1)
local nLenaCols		:= 0
Local nI			:= 0
Local nY			:= 0
Local nPosaMarc		:= 0
Local dData			:= cToD("  /  /  ")


DEFAULT aMarcacoes	:= {}
DEFAULT aCols1		:= {}


If Empty(aMarcacoes) .Or. Empty(aCols1)
	Return	
EndIf

nLenaCols := Len(aCols1)

For nI := 1 To nLenaCols
	dData := aCols1[nI,2]
	For nY := 3 To nColMarc
		nPosaMarc := aScan(aMarcacoes, {|x| x[AMARC_DATA] == dData .And. x[AMARC_HORA] == Val(aCols1[nI,nY,1])})
		If nPosaMarc > 0 .And. aCols1[nI,nY,20] <> aMarcacoes[nPosaMarc,AMARC_TURNO]
			aCols1[nI,nY,20] := aMarcacoes[nPosaMarc,AMARC_TURNO]
		EndIf
	Next
Next

Return
