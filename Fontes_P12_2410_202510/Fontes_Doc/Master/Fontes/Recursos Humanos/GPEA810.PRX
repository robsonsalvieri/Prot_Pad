			EndIf

            If lEnd810GRV .And. lRet
                //Integracao com o TAF
                lReinEFD := fInt2298(aInfoTaf, @aErros,"S2298")

                If !lMiddleware
	                If ( !lReinEFD ) .And. MsgYesNo(OemToAnsi( STR0076 )) //Visualizar o Log?
