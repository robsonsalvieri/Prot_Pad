	Local aInfoTaf		:= {}
	Local cTrabVincu 	:= fCatTrabEFD("TCV") //"101|102|103|104|105|106|111|301|302|303|304|306|307|309" //Trabalhador com vinculo
	Local aErros		:= {}
	Local cCpf			:= ""
	Local lReinEFD		:= .T.
	local aTitle		:= {}
	local cCpf1			:= ""
	Local lRet			:= .T.
    Local dDcgIni		:=  SuperGetMV("MV_DTCGINI",nil,CTOD(" / / ") )

	Local aInfoC	 := {}
	Local cChaveMid	 := ""
	Local cStatMid   := ""
	Local lAdmPubl	:= .F.
	Local cBkpFil		:= cFilAnt
	Local cNrInsc		:= ""
	Local cTpInsc		:= ""

	Private aTpAlt		:= {.F.,.F.,.F.}
	Private cStatus		:= ""
	Private lEnd810GRV	:= .T.
	Private lS2200		:= .F.
	Private c810CodUnic := ""
	Private lExclRes	:= (SuperGetMv("MV_REINRES",, .F.) == .T.)
	Private lReinteg 	:= SRG->(ColumnPos("RG_REINTEG")) > 0


	// Inicializa statics
	Rst810Stc()

	// Reserva registros das tabelas SRA, SRF e SRG
	dbSelectArea("SRA")
	If !SoftLock("SRA")
		Return( NIL )
	EndIf

	aAdd( aSftLck, { "SRA", Recno() } )

		If cVersGPE >= "9.0.00"
			aTpRei := {	OemToAnsi(STR0033),;
							OemToAnsi(STR0034),;
							OemToAnsi(STR0066),;
							OemToAnsi(STR0067),;
							OemToAnsi(STR0068),;
