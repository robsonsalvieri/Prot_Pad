	Local aSvKeys		:= GetKeys()
	Local aOpcCad		:= {}
	Local aSftLck		:= {}
	Local bSet15   := { || If( GpVldTpRein() .And. fReintOk() .and. Gp810ValPE(), ( nOpcA := 1 , oDlg:End() ), ) }
    Local bSet24    := { ||  nOpcA := 2 , oDlg:End() }
