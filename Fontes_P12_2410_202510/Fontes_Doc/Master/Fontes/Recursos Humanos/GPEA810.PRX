	If nOpcA == 1
	    c810CodUnic:= ALLTRIM(SRA->RA_CODUNIC)
		If (lIntTaf .Or. lMiddleware)  .AND. SRA->RA_CATEFD $ cTrabVincu .And. !lS2200
			//Carrega informacoes para Taf
			aInfoTaf := {SRA->RA_FILIAL,;
							SRA->RA_MAT,;
							SRA->RA_CIC,;
							cTpRei,;
							cNroProc,;
							cNroLeiAn,;
							dDtEfRet,;
							dDtEfRein}
			If( lReinEFD ) .And. lRet
