STATIC Function fOpcReint()

	lOpcReint	:= ( nRntOpc == 1 )

	If lOpcReint
		ocNewMat:Disable()
		cNewMat		:= Space( nTamMat )
		ocCpyCad:Disable()
		cCpyCad		:= "4"
	Else
		ocNewMat:Enable()
		ocCpyCad:Enable()
	EndIf

	ocNewMat:Refresh()
	ocCpyCad:Refresh()

	nOldOpc := nRntOpc

Return

