	Local aOrd		:= {}
	Local aParam	:= {}
	
	aParam := {	"P"			, ; // Tipo R para relatorio P para processo
				"PNM040"	, ;	// Pergunte do relatorio, caso nao use passar ParamDef
				""			, ; // Alias para o relatório
				aOrd		, ; // Array de ordens para o relatório
				""			}
Return aParam
