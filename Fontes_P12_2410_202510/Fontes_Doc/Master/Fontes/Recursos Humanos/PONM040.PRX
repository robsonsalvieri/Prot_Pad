Function PONM040
	
	Local aArea					:= GetArea()
	Local cPerg					:= "PNM040"
	Local cUrl					:= ""
	Local cSvFilAnt				:= cFilAnt
	Local nOp					:= 0
	Local lExib2Plan			:= FwLibVersion() >= "20240520"
	Local lExec2Plan			:= FwIsInCallStack("WFLAUNCHER") .or. FwIsInCallStack("FWBOSCHDEXECUTE")
	
	Private lAbortPrint			:= .F.
	Private cCadastro   		:= OemToAnsi(STR0001 )	//"Integração com a Folha de Pagamento"
	Private lSegFaltas			:= SuperGetMv("MV_SEGFALT",NIL,.F.)
	Private aCodFol				:= {}
	Private lSchedule  			:= IsBlind()
	
	DEFAULT lPnm040CposBlock	:= ExistBlock("PNM040CPOS") 
	DEFAULT lPN040ALTL			:= ExistBlock("PNM040ALTL")
	
	SetMnemonicos(xFilial("RCA"),NIL,.T.)
	P_SEGFALT := If( Type("P_SEGFALT") == "U", .F., P_SEGFALT)
	
	If !lSegFaltas
		If !IsBlind()
			cUrl := "https://tdn.totvs.com/pages/viewpage.action?pageId=847569991"
			nOp := Aviso(STR0006,STR0055 + CRLF + CRLF + CRLF + STR0056 + cUrl,{STR0057,STR0058}) //"A partir de 01/06/2024 o parâmetro MV_SEGFALT obrigatoriamente deverá ser ativado para realizar a Integração com a Folha." ### "Consulte a documentação em: "
			
			If nOp == 2
				shellExecute("Open",cUrl, "", "", 1)
			EndIf
		EndIf
		
		Return
	EndIf
	
	If !Fp_CodFol(@aCodFol, xFilial('SRV'))
		Return(.F.)
	EndIf
	
	If ValidArqPon()
		
		// Integração com a Folha de Pagamento
		// Este programa ira efetuar a Geraçäo dos Resultados na Folha de Pagamento.
		TNewProcess():New("PONM040", STR0001, {|oSelf| Pnm040Processa(cPerg, oSelf, lExec2Plan)}, STR0002, cPerg, NIL, NIL, NIL, NIL, !lSchedule, .T., lExib2Plan)
		
	EndIf
	
	cFilAnt := cSvFilAnt
	
	RestArea( aArea )
	
Return( NIL )

