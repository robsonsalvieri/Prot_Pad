    If !Empty(cMens)
    	cMens += ", "
    EndIf
	cMens += OemToAnsi(STR0032)
EndIf

If Empty(cRot)
    If !Empty(cMens)
    	cMens += ", "
    EndIf
	cMens += OemToAnsi(STR0033)
EndIf

If !Empty(cMens)
