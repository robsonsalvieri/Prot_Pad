Local aOfusca			:= If(FindFunction('ChkOfusca'), ChkOfusca(), {.T. , .F.}) //[1] Acesso; [2]Ofusca
Local bSet15			:= { ||nopcA:=1,IF(oGet:TudoOk(),(Pn300GetaCols(@aCols,@aArrayMain,aNotFields,,,nOpcX,.T.),oDlg:End()),(nOpcA:=0,.T.))}
Local bSet24			:= { ||oDlg:End()  }
