		oFolders:bSetOption := { |nNewFolder,lValidFolder| (nSvOpcTree	:= nOpcTree, nOpcTree	:= 3 ), ( lFolderOk := FolderSetOption( nNewFolder , oFolders:nOption , aObjFolder , NIL , NIL , lValidFolder ) ) }

