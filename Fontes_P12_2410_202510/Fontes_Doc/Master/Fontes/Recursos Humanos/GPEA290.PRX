Static Function Gpea290Valid( cSelect , cCampo , nPosTree , lShowMsgInfo, lGravaFor )

Local aVldMne           := {}

Local cAux1				:= ""
Local cAux2				:= ""
Local cExecute			:= ""
Local cMsgInfo			:= ""

Local lValid			:= .T.
Local lVldMne			:= .T.

Local nContMne          := 0
Local nPosAux
Local nLoop
Local nLoops
Local nRc3Result
Local nRc3Form01
Local nRc3Form02

DEFAULT nPosTree		:= Val( oTree:GetCargo() )
DEFAULT cSelect 		:= Space( GetSx3Cache( "RC3_RESULT" , "X3_TAMANHO" ) )
DEFAULT lShowMsgInfo	:= .T.
DEFAULT lGravaFor		:= .T.

CursorWait()

	Begin Sequence
	
		/*/
		здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		ЁSeto o Mnemonico de Retorno ( Se esse ainda nao tiver sido "SeЁ
		Ёtado"														   Ё
		юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
		SetMnemonicos( cFilRCA , @__aMnemonicos , .T. , GetMemVar( "RC2_RETURN" ) , .T. )	//__aMnemonicos eh Private em Gpea290 aa Partir de Gpea290Mnt()
 	
		/*/
		здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		Ё Verifica se a Expressao digitada esta OK								 Ё
		юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
		If !( lValid := ValidRot( NIL , @cSelect , GetSx3Cache( "RC3_RESULT" , "X3_TAMANHO" ) ) )
			Break
		EndIf
	
		/*/
		здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		Ё Se Estiver Vazio Abandona porque esta OK								 Ё
		юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
		If Empty( cSelect )
			Break
		EndIf
	
		/*/
		здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		Ё BEGIN SET VAR															 Ё
		юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
			If lGravaFor
				/*/
				здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
				Ё Seto as variaveis nao declaradas aqui apenas para validar a Expressao. Ё
				Ё Na Montagem das Funcoes de Formula as variaveis nao Criadas como  MnemoЁ
				Ё nicos Serao Todas Declaradas como Locais, ja os Mnemonicos serao iniciaЁ
				Ё lizados conforme campo RCA_ACUMUL										 Ё
				юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
				nRc3Result	:= GdFieldPos( "RC3_RESULT" , aRc3Header )
				nRc3Form01	:= GdFieldPos( "RC3_FORM01" , aRc3Header )
				nRc3Form02	:= GdFieldPos( "RC3_FORM02" , aRc3Header )
				nLoops		:= Len( aRc3Cols )
				For nLoop := 1 To nLoops
					If nRc3Result > 0
						Gpea290VarInit( AllTrim( aRc3Cols[ nLoop , nRc3Result ] ) )
					EndIf
					If nRc3Form01 > 0
						Gpea290VarInit( AllTrim( aRc3Cols[ nLoop , nRc3Form01 ] ) )
					EndIf
					If nRc3Form02 > 0
						Gpea290VarInit( AllTrim( aRc3Cols[ nLoop , nRc3Form02 ] ) )
					EndIf
				Next nLoop
			EndIf
	
			/*/
			здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
			Ё Verifica a String Passada como Parametro								 Ё
			юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
			Gpea290VarInit( cSelect, @aVldMne, @lVldMne )
            If !lVldMne
				cMsgHelp := STR0186//'NЦo И permitido a utilizaГЦo de variАvel iniciada com o prefixo "M_" que nЦo seja um mnemТnico cadastrado na rotina GPEA300. VariАvel(is) incorretas: '
                For nContMne := 1 To Len(aVldMne)
                    cMsgHelp += CRLF + aVldMne[nContMne]
                Next nContMne
				Help( " ", 1, "ERR_FORM", Nil, cMsgHelp, 1, 0, Nil, Nil, Nil, Nil, Nil, { OemToAnsi(STR0187+"https://tdn.totvs.com/pages/releaseview.action?pageId=22480468") } )//"Efetue a declaraГЦo da variАvel seguindo a notaГЦo hЗngara, para ser possivel identificar o tipo da variАvel. Mais informaГУes: "
                lValid := .F.
                Break
            EndIf
		/*/
		здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		Ё END SET VAR															 Ё
		юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/

		/*/
		здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		Ё Inicia a Validacao													 Ё
		юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
		If ( nPosTree == 1 ) //Arquivos/Campos
		
			If !( lValid := ( nPosAux := At( "->" , cSelect ) ) > 0 )
				cMsgInfo := STR0055	//"Os Campos de Arquivos Dever estar prefixados com o Alias"
				Break
			EndIf
			
			cAux1	:= SubStr( SubStr( cSelect , 1 ,  nPosAux - 1  ) , -3 )
			If !( lValid := ( CheckExecForm( { || dbSelectArea( cAux1 ) , ( Select( cAux1 ) > 0 ) } , lShowMsgInfo ) ) )
				cMsgInfo := STR0108	//"O Arquivo Selecionado nao Consta no Banco de Dados"
				Break
			EndIf

			cAux2	:= SubStr( cSelect , nPosAux + 2 , Len( cSelect ) )
			If !( lValid := CheckExecForm( cSelect , lShowMsgInfo ) )
				Break
			EndIf

			If !( lValid := !( ( cAux1 )->( FieldPos( cAux2 ) ) == 0 ) )
				cMsgInfo := STR0056	//"O campo Selecionado nao Consta no Dicionario/Base de Dados"
				Break
			EndIf
		
		EndIf
		
		If ( nPosTree == 3 )	//Mnemonicos
			cAux := Upper( AllTrim( cSelect ) )
			If !( lValid := !( aScan( aObjects[ nPosTree , 01 ]:aArray , { |x| Upper( AllTrim( x[1] ) ) == cAux } ) == 0 ) )
				cMsgInfo := STR0064	//"Nao Existe o Mnemonico Informado na Expressao"
				Break
			EndIf
		EndIf

		If (;
					( nPosTree == 4 );	//Funcoes
					.or.;
					( nPosTree == 6 );	//Constantes
				)
			cExecute := Upper( AllTrim( cSelect ) )
			If ( "EXECUTE_FUN_" $ cExecute )
			 	cExecute := ExecFunRmv( cExecute )
				If !( lValid := CheckExecForm( cExecute , lShowMsgInfo ) )
					Break
				EndIf
			ElseIf !( lValid := ChkNoExecute( cSelect , lShowMsgInfo ) )
				Break
			EndIf
		EndIf
		
		If ( nPosTree == 5 )	//Formulas
			cAux := Upper( AllTrim( cSelect ) )
			If !( lValid := !( aScan( aObjects[ nPosTree , 01 ]:aArray , { |x| Upper( AllTrim( x[1] ) ) == cAux } ) == 0 ) )
				cMsgInfo := STR0065	//"Nao Existe a Formula Informada na Expressao"
				Break
			EndIf
		EndIf
	
		/*/
		здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		Ё Verifica o Retorno da CriaLocal()        							 	 Ё
		юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
		If !( lValid := CriaLocalOk() )
			Break
		EndIf
	
	End Sequence
	
CursorArrow()

If (;
		!( lValid );
		.and.;
		( lShowMsgInfo );
		.and.;
		!Empty( cMsgInfo );
	)
	MsgInfo( OemToAnsi( cMsgInfo ) , STR0066 )	//"Aviso de Inconsistencia"
EndIf		

Return( lValid )

