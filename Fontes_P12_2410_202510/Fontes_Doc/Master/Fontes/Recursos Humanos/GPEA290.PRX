Static Function Gpea290Sort( aCols , aHeader , cCpo , lDescending )

Local nPosSort	:= 0

DEFAULT aCols		:= {}
DEFAULT aHeader		:= {}
DEFAULT cCpo		:= "RC3_SEQFOR"
DEFAULT lDescending	:= .F.

nPosSort := GdFieldPos ( cCpo , aHeader )

If !( lDescending )
	aSort( aCols , NIL , NIL , { |x,y| x[ nPosSort ] < y[ nPosSort ] } )
Else
	aSort( aCols , NIL , NIL , { |x,y| x[ nPosSort ] > y[ nPosSort ] } )
EndIf

Return( NIL )

