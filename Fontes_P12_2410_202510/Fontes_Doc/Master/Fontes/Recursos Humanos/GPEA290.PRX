Static Function Gpea290VarInit( cExpr, aVldMne, lVldMne )

Local aParse
Local aGetLocal

Local cVar

Local nLoop
Local nLoops

Default aVldMne     := {}
Default lVldMne     := .T.

Begin Sequence

	/*/
	здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	Ё Abandona, caso esteja vazio           								 Ё
	юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
	If Empty( cExpr )
		Break
	EndIf
	cExpr := Upper( StrTran( cExpr , " " , "" ) )
	
	/*/
	здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	Ё Remove EXECUTE_FUN_ da Expressao										 Ё
	юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
	cExpr := ExecFunRmv( cExpr )

	/*/
	здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	Ё Carrego as Locais que Serao Inicializadas pela CriaLocal				 Ё
	юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
	If ( "CRIALOCAL" $ cExpr )

		ChkCriaLocal( cExpr )
		aGetLocal := GetLocal()

		If !Empty( aGetLocal )
			aGetLocal := aClone( aGetLocal[ 2 ] )
			/*/
			здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
			Ё Inicializo para uso na Validacao     									 Ё
			юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
			nLoops	:= Len( aGetLocal )
			For nLoop := 1 To nLoops
				cVar := aGetLocal[ nLoop , 1 ]
				If Empty( cVar )
					Loop
				EndIf
				/*/
				здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
				Ё Verifico se a Variavel eh um Mnemonico e Seto-o						 Ё
				юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
				If !SetMnemonicos( cFilRCA , @__aMnemonicos , .T. , cVar , .T. )	//__aMnemonicos eh Private em Gpea290 aa Partir de Gpea290Mnt()
					/*/
					здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
					Ё Caso Contrario Variavel nao esta Definica e Seto-a					 Ё
					юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
					If ChkAddVar( cVar )   
						If Empty( aGetLocal[ nLoop , 4 ] )
							_SetOwnerPrvt( cVar , GetValType( aGetLocal[ nLoop , 2 ] ) )
						Else
							_SetOwnerPrvt( cVar , aGetLocal[ nLoop , 4 ] )
						EndIf
					EndIf
				EndIf
			Next nLoop

		EndIf
	
	Else
		
		/*/
		здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		Ё Efetuo Parse Para Obter as variaveis									 Ё
		юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
		aParse	:= ParseVar( cExpr )
		nLoops	:= Len( aParse )
		For nLoop := 1 To nLoops
			cVar := AllTrim( aParse[ nLoop ] )
			If Empty( cVar )
				Loop
			EndIf
			If (;
					( aScan( __aParseVar , { |x| x == cVar } ) == 0 );
					.and.;
					ChkAddVar( cVar );
				)	
				aAdd( __aParseVar , cVar )
			EndIf
		Next nLoop
	
		/*/
		здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		Ё Inicializo para uso na Validacao     									 Ё
		юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
		nLoops	:= Len( __aParseVar )
		For nLoop := 1 To nLoops
			cVar := __aParseVar[ nLoop ]
			If Empty( cVar )
				Loop
			EndIf
			/*/
			здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
			Ё Verifico se a Variavel eh um Mnemonico e Seto-o						 Ё
			юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
			If !SetMnemonicos( cFilRCA , @__aMnemonicos , .T. , cVar , .T. )	//__aMnemonicos eh Private em Gpea290 aa Partir de Gpea290Mnt()
				If SubStr(cVar, 1, 2) == "M_"
                    If AScan( aVldMne, { |x| x == cVar } ) == 0
                        AAdd( aVldMne, cVar )
                    EndIf
                    lVldMne := .F.
                /*/
				здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
				Ё Caso Contrario Variavel nao esta Definica e Seto-a					 Ё
				юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды/*/
				ElseIf ChkAddVar( cVar )
					_SetOwnerPrvt( cVar , GetValType( SubStr( cVar , 1 , 1 ) ) )
				EndIf
			EndIf
		Next nLoop

	EndIf
	
End Sequence

Return( NIL )

