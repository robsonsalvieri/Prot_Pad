Function LoadHotkeys() 
	Local nI 	:= 0
	Local cNum	:= ""  
	
	aAdd(aHotkeys, {"A", OP_ANO})
	aAdd(aHotkeys, {"AFA", ME_ANO_FIM_ACU})
	aAdd(aHotkeys, {"AIA", ME_ANO_INI_ACU})   
	
	aAdd(aHotkeys, {"B", OP_BUSCA})
	aAdd(aHotkeys, {"BAE", OP_BUSCA_ANO_EMPRESA})	
	aAdd(aHotkeys, {"BPA", ME_BASE_PENSION})   
	aAdd(aHotkeys, {"BP", OP_BUSCA_PROCESO})
	aAdd(aHotkeys, {"BS", OP_BUSCA_SUCURSAL})
	aAdd(aHotkeys, {"CF", OP_CONVIERTE_FECHA})  
	
	For nI := 0 To 20 
		cNum := StrZero( nI , 2)
		aAdd(aHotkeys, {".C" + cNum, "C" + ME_AUXILIAR + cNum }) 	
	Next 
	
	For nI := 0 To 20
		cNum := StrZero( nI , 2)
		aAdd(aHotkeys, {".D" + cNum, "D" + ME_AUXILIAR + cNum }) 	
	Next 

	aAdd(aHotkeys, {"DS", OP_DIA_SEMANA})		
