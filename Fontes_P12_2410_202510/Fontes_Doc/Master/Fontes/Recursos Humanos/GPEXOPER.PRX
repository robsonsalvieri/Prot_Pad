Next nElem

// Variavel do Tipo Caracter
For nElem := 0 to 20
	aAdd(aMnemo, { '',"C"+ME_AUXILIAR + StrZero(nElem,2), OemToAnsi(STR0056),'R','','','','',''} ) // 'Variável de acumulação tipo caracter'
Next nElem

// Variavel do Tipo Numerico
For nElem := 0 to 30
	aAdd(aMnemo, { '',"N"+ME_AUXILIAR + StrZero(nElem,2), OemToAnsi(STR0057),'R','','','','',''} ) // 'Variável de acumulação tipo númerico'
Next nElem

// Variavel do Tipo Data
For nElem := 0 to 20
	aAdd(aMnemo, { '',"D"+ME_AUXILIAR + StrZero(nElem,2), OemToAnsi(STR0058),'R','','','','',''} ) // 'Variável de acumulação tipo data'
Next nElem

// Variavel do Tipo Logico
For nElem := 0 to 20
	aAdd(aMnemo, { '',"L"+ME_AUXILIAR + StrZero(nElem,2), OemToAnsi(STR0059),'R','','','','',''} ) // 'Variável de acumulação tipo lógico'
Next nElem

// Variavel de Tabela de Usuário
For nElem := 1 to 20
	aAdd(aMnemo, { '',"TABLA_U" + StrZero(nElem,3), OemToAnsi(STR0084),'R','','','','',''} ) // 'Variável de tabela auxiliar de usuário
Next nElem

Return( NIL )

