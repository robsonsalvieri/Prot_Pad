	aAdd(aHotkeys, {"H>=", OP_HASTA_MAYOR_IGUAL})
	aAdd(aHotkeys, {"H<=", OP_HASTA_MENOR_IGUAL})
	 
	For nI := 0 To 20
		cNum := StrZero( nI , 2)
		aAdd(aHotkeys, {".L" + cNum, "L" + ME_AUXILIAR + cNum }) 	
	Next nI
			
	aAdd(aHotkeys, {"MES", OP_MES})			
	aAdd(aHotkeys, {"M", OP_MUEVE})	 
	aAdd(aHotkeys, {"NA", ME_NO_APLICADO})	 
	
	aAdd(aHotkeys, {"*", OP_MULTIPLICA})	  
	
	For nI := 0 To 30
		cNum := StrZero( nI , 2)
		aAdd(aHotkeys, {".N" + cNum, "N"+ ME_AUXILIAR + cNum }) 	
	Next nI
			
	aAdd(aHotkeys, {"PRT", OP_PRIMER_REG_TRAY})			
	aAdd(aHotkeys, {"R", OP_REDONDEA})			
	aAdd(aHotkeys, {"MOD", OP_RESIDUO})			
	aAdd(aHotkeys, {"-", OP_RESTA})			
	aAdd(aHotkeys, {"S", OP_SALTA})	
	aAdd(aHotkeys, {"SP", ME_SIN_PROGRAMAR})
	aAdd(aHotkeys, {"ST", OP_SIG_TRAY})			
	aAdd(aHotkeys, {"SV", OP_STRTOVAL})		
	aAdd(aHotkeys, {"SPI", ME_SUMA_NPGINI})		
	aAdd(aHotkeys, {"SPF", ME_SUMA_NPGFIM})			
	aAdd(aHotkeys, {"STN", ME_SUMA_TIPO_NOM})			
	aAdd(aHotkeys, {"<>", OP_SI_DIFERENTE})			
	aAdd(aHotkeys, {"=", OP_SI_IGUAL})			
	aAdd(aHotkeys, {">", OP_SI_MAYOR})			
	aAdd(aHotkeys, {"<", OP_SI_MENOR})					
	aAdd(aHotkeys, {"+", OP_SUMA})			
	aAdd(aHotkeys, {"+D", OP_SUMA_DIAS})			
	aAdd(aHotkeys, {"+IH", OP_SUMA_IMPHIS })			
	aAdd(aHotkeys, {"+I", OP_SUMA_IMPORTES })			
	aAdd(aHotkeys, {"+M", OP_SUMA_MOVLI })			
	aAdd(aHotkeys, {"+U", OP_SUMA_UNIDADES })			
