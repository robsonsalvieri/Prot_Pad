	aAdd(aHotkeys, {"/", OP_DIVIDE})			
	aAdd(aHotkeys, {"E", OP_ETIQUETA}) 
	
	For nI := 0 To 99 
		cNum := StrZero( nI , 2)
		aAdd(aHotkeys, {".E" + cNum, ME_ELEMENTO + cNum }) 	
	Next 
				
	aAdd(aHotkeys, {"X", OP_EXPONENTE})			
	aAdd(aHotkeys, {"G", OP_GRABA})	 
	aAdd(aHotkeys, {"GM", ME_GEN_MOV})	 
	aAdd(aHotkeys, {"H", ME_HOY})				
