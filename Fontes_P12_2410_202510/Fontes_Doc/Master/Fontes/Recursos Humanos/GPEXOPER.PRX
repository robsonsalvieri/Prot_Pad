Function GpexOper( aMnemo )
Local nElem := 0     

DEFAULT aMnemo := {}

//*** Operadores
aAdd(aMnemo, { '',OP_SUMA,				OemToAnsi(STR0001),'P','','','','',''} ) // 'Operação matemática de Soma'
aAdd(aMnemo, { '',OP_RESTA,				OemToAnsi(STR0002),'P','','','','',''} ) // 'Operação matemática de Subtração'
aAdd(aMnemo, { '',OP_MULTIPLICA,		OemToAnsi(STR0003),'P','','','','',''} ) // 'Operação matemática de multiplicação'
aAdd(aMnemo, { '',OP_DIVIDE,			OemToAnsi(STR0004),'P','','','','',''} ) // 'Operação matemática de divisão'
aAdd(aMnemo, { '',OP_TRUNCA,			OemToAnsi(STR0005),'P','','','','',''} ) // 'Trunca o valor informado'
aAdd(aMnemo, { '',OP_REDONDEA,			OemToAnsi(STR0006),'P','','','','',''} ) // 'Arredondar os decimais'
aAdd(aMnemo, { '',OP_SI_IGUAL,			OemToAnsi(STR0007),'P','','','','',''} ) // 'Realiza a comparação entre dois valores'
aAdd(aMnemo, { '',OP_SI_MAYOR,			OemToAnsi(STR0008),'P','','','','',''} ) // 'Compara se o conteúdo do primeiro valor e maior que o segundo'
aAdd(aMnemo, { '',OP_SI_MENOR,			OemToAnsi(STR0009),'P','','','','',''} ) // 'Compara se o conteúdo do primeiro valor e menor que o segundo'
aAdd(aMnemo, { '',OP_SI_DIFERENTE,		OemToAnsi(STR0010),'P','','','','',''} ) // 'Verifica se os dois valores são diferentes'
aAdd(aMnemo, { '',OP_SALTA,		 		OemToAnsi(STR0011),'P','','','','',''} ) // 'Envia o cursor do proc. do roteiro de calc. p/ uma ETIQUETA'
aAdd(aMnemo, { '',OP_SUMA_DIAS,			OemToAnsi(STR0012),'P','','','','',''} ) // 'Soma os dias de incidências da mesma verba para os lanc. dia'
aAdd(aMnemo, { '',OP_RESIDUO,			OemToAnsi(STR0013),'P','','','','',''} ) // 'Resto de uma operação de divisão'
aAdd(aMnemo, { '',OP_BUSCA,				OemToAnsi(STR0014),'P','','','','',''} ) // 'Busca na Definição de Tabelas a tabela informada'
aAdd(aMnemo, { '',OP_HASTA_MAYOR,		OemToAnsi(STR0015),'P','','','','',''} ) // 'Realiza comparação até que o vlr do cpo seja > a 2aExpressão'
aAdd(aMnemo, { '',OP_HASTA_MENOR,		OemToAnsi(STR0016),'P','','','','',''} ) // 'Realiza comparação até que o vlr do cpo seja < a 2aExpressão'
aAdd(aMnemo, { '',OP_HASTA_IGUAL,		OemToAnsi(STR0017),'P','','','','',''} ) // 'Realiza comparação até que o vlr do cpo seja = a 2aExpressão'
aAdd(aMnemo, { '',OP_GRABA,				OemToAnsi(STR0018),'P','','','','',''} ) // 'Gravação de inf. nas tabel. SRA,SRK,RG4,RCP,RG1,SRD e SRF'
aAdd(aMnemo, { '',OP_EXPONENTE,			OemToAnsi(STR0019),'P','','','','',''} ) // 'Eleva a potência de um determinado número'
aAdd(aMnemo, { '',OP_MUEVE,				OemToAnsi(STR0020),'P','','','','',''} ) // 'Atribui um valor a uma determinada variável'
aAdd(aMnemo, { '',OP_MUEVE_STR,			OemToAnsi(STR0094),'P','','','','',''} ) // 'Atribui o valor de um campo texto a uma determinada variável'
aAdd(aMnemo, { '',OP_ETIQUETA,			OemToAnsi(STR0021),'P','','','','',''} ) // 'Utilizado para a criação de uma etiqueta'
aAdd(aMnemo, { '',OP_SUMA_IMPORTES,		OemToAnsi(STR0022),'P','','','','',''} )	// 'Sumariza todos os valores da verba calculada'
aAdd(aMnemo, { '',OP_SUMA_UNIDADES,		OemToAnsi(STR0023),'P','','','','',''} ) // 'Sumariza todos as unidades da verba calculada'
aAdd(aMnemo, { '',OP_CONVIERTE_FECHA,	OemToAnsi(STR0024),'P','','','','',''} ) // 'Converte uma Data Juliana em Data Gregoriana ou vice-versa.' 
aAdd(aMnemo, { '',OP_SUMA_IMPHIS,		OemToAnsi(STR0025),'P','','','','',''} ) // 'Sumarizar valores da verba calculada do processo informado'
aAdd(aMnemo, { '',OP_SUMA_UNIHIS,		OemToAnsi(STR0026),'P','','','','',''} ) // 'Sumarizar unidades da verba calculada do processo informado'
aAdd(aMnemo, { '',OP_TRACE_OFF,			OemToAnsi(STR0027),'P','','','','',''} ) // 'Desativa o processo de debug nas fórmulas'
aAdd(aMnemo, { '',OP_TRACE_ON,			OemToAnsi(STR0028),'P','','','','',''} ) // 'Ativa o processo de debug nas fórmulas'
aAdd(aMnemo, { '',OP_UNIDADES_EMPRESA,	OemToAnsi(STR0029),'P','','','','',''} ) // 'Sumariza em unidades a verba da tabela de acumuladores'
aAdd(aMnemo, { '',OP_IMPORTE_EMPRESA,	OemToAnsi(STR0030),'P','','','','',''} ) // 'Sumariza em valores a verba da tabela de acumuladores'
aAdd(aMnemo, { '',OP_DIAS_DERECHO,		OemToAnsi(STR0031),'P','','','','',''} ) // 'Num. dias de direito que faltam do func. sobre uma verba'
aAdd(aMnemo, { '',OP_DIAS_HABILES,		OemToAnsi(STR0032),'P','','','','',''} ) // 'Dias úteis de um determinado intervalo de datas'
aAdd(aMnemo, { '',OP_DIAS_HAB_PROG,		OemToAnsi(STR0033),'P','','','','',''} ) // 'D. úteis de um intervalo de datas da verba que se esta calc.'
aAdd(aMnemo, { '',OP_PRIMER_REG_TRAY,	OemToAnsi(STR0034),'P','','','','',''} ) // 'Le o primeiro registro da trajetoria laboral'
aAdd(aMnemo, { '',OP_ULTIMO_REG_TRAY,	OemToAnsi(STR0035),'P','','','','',''} ) // 'Le o ultimo registro da trajetoria laboral'
aAdd(aMnemo, { '',OP_LEE_TRAY,			OemToAnsi(STR0036),'P','','','','',''} ) // 'Retorna os reg. da traj. Laboral de um intervalo de datas'
aAdd(aMnemo, { '',OP_SIG_TRAY,			OemToAnsi(STR0037),'P','','','','',''} ) // 'Avança para o proximo regostro da trajetoria laboral'
aAdd(aMnemo, { '',OP_BUSCA_PROCESO,		OemToAnsi(STR0038),'P','','','','',''} ) // 'Utilizar o processo informado na busca de informações'
aAdd(aMnemo, { '',OP_BUSCA_ANO_EMPRESA,	OemToAnsi(STR0040),'P','','','','',''} ) // 'Ano que será utilizado para busca dos acumulados dos funcionários'
aAdd(aMnemo, { '',OP_BUSCA_SUCURSAL,	OemToAnsi(STR0064),'P','','','','',''} ) // 'Filiais que deseja considerar na soma dos acumuladores. Utilize "|" para separar as filiais'
aAdd(aMnemo, { '',OP_PAGA_DIA_DER,		OemToAnsi(STR0041),'P','','','','',''} ) // 'Retorna o numero de dias de direito a pagar'
aAdd(aMnemo, { '',OP_ANO,				OemToAnsi(STR0042),'P','','','','',''} ) // 'Retorna o ano de uma determinada data'
aAdd(aMnemo, { '',OP_DIA,				OemToAnsi(STR0043),'P','','','','',''} ) // 'Retorna o dia de uma determinada data'
aAdd(aMnemo, { '',OP_MES,				OemToAnsi(STR0044),'P','','','','',''} ) // 'Retorna o mes de uma determinada data'
aAdd(aMnemo, { '',OP_DIA_SEMANA,		OemToAnsi(STR0045),'P','','','','',''} ) // 'Retorna o dia da semana de uma determinada data'
aAdd(aMnemo, { '',OP_IMPORTE_TOTAL,		OemToAnsi(STR0062),'P','','','','',''} ) // 'Sumariza em Valores a verba da tabela de acumuladores das filiais'
aAdd(aMnemo, { '',OP_IE_TOTAL_EMP,		OemToAnsi(STR0072),'P','','','','',''} ) // 'Totaliza o Número de Funcionários que cumprem Critério de Acumulação'
aAdd(aMnemo, { '',OP_IE_TOTAL_CON,		OemToAnsi(STR0073),'P','','','','',''} ) // 'Totaliza o Valor da Verba que esta sendo calculada dos Funcionários que cumprem Critério de Acumulação'
aAdd(aMnemo, { '',OP_UNIDADES_TOTAL,	OemToAnsi(STR0063),'P','','','','',''} ) // 'Sumariza em Unidades a verba da tabela de acumuladores das filiais'
aAdd(aMnemo, { '',OP_STRTOVAL,			OemToAnsi(STR0065),'P','','','','',''} ) // 'Converte string para valor'
aAdd(aMnemo, { '',OP_VALTOSTR,			OemToAnsi(STR0066),'P','','','','',''} ) // 'Converte valor para string'
aAdd(aMnemo, { '',OP_SUMA_MOVLI,		OemToAnsi(STR0070),'P','','','','',''} ) // 'Soma das Verbas já geradas nos movimentos'
aAdd(aMnemo, { '',OP_HORAS_HABILES,		OemToAnsi(STR0075),'P','','','','',''} ) // 'Horas trabalhadas em um determinado intervalo de datas'
aAdd(aMnemo, { '',OP_GET_MONEDA,		OemToAnsi(STR0076),'P','','','','',''} ) // 'Busca fator de converaso de moedas'
aAdd(aMnemo, { '',OP_DIAS_FERIADO,		OemToAnsi(STR0077),'P','','','','',''} ) // 'Dias de Feriados de um determinado intervalo de datas'
aAdd(aMnemo, { '',OP_TIPO_DIA,			OemToAnsi(STR0078),'P','','','','',''} ) // 'Retorna o Tipo da Data Informada - S-Trabalhado, C-Compensado, D-Descanso, N-Não Trabalhado'
aAdd(aMnemo, { '',OP_PRIMER_REG_DEP,	OemToAnsi(STR0079),'P','','','','',''} ) // 'Ler o primeiro registro de Dependentes'
aAdd(aMnemo, { '',OP_ULTIMO_REG_DEP,	OemToAnsi(STR0080),'P','','','','',''} ) // 'Ler o ultimo registro de Dependentes'
aAdd(aMnemo, { '',OP_LEE_DEPENDIENTE,	OemToAnsi(STR0081),'P','','','','',''} ) // 'Retornar os registros de Dependentes'
aAdd(aMnemo, { '',OP_SIG_DEPENDIENTE,	OemToAnsi(STR0082),'P','','','','',''} ) // 'Avançar para o proximo registro de Dependentes'
aAdd(aMnemo, { '',OP_FILTRO_ACUM,		OemToAnsi(STR0085),'P','','','','',''} ) // 'Filtro Acumuladores'
aAdd(aMnemo, { '',OP_FILTRO_MOVLI,		OemToAnsi(STR0086),'P','','','','',''} ) // 'Filtro Movimentos'
aAdd(aMnemo, { '',OP_HASTA_MAYOR_IGUAL, OemToAnsi(STR0088),'P','','','','',''} ) // 'Realiza comparação até que o vlr do cpo seja >= a 2aExpressão'
aAdd(aMnemo, { '',OP_HASTA_MENOR_IGUAL, OemToAnsi(STR0089),'P','','','','',''} ) // 'Realiza comparação até que o vlr do cpo seja <= a 2aExpressão'
aAdd(aMnemo, { '',OP_BUSCA_CRITERIO, 	OemToAnsi(STR0090),'P','','','','',''} ) // 'Busca a Caixa (Arquivo Acumulador) correspondente ao mes passado no Operando 2'
aAdd(aMnemo, { '',OP_AUS_CONTINUO, 		OemToAnsi(STR0091),'P','','','','',''} ) //
aAdd(aMnemo, { '',OP_GEN_MOV	  , 	OemToAnsi(STR0092),'P','','','','',''} ) //
aAdd(aMnemo, { '',OP_DIAS_360	  , 	OemToAnsi(STR0095),'P','','','','',''} ) // 'Devuelve la cantidad de días entre dos fechas basándose en un año de 360 días.'  
aAdd(aMnemo, { '',OP_DIAS_AUSENCIA, 	OemToAnsi(STR0099),'P','','','','',''} ) // 'Devuelve el número de días entre dos fechas para un concepto de ausencia.'
aAdd(aMnemo, { '',OP_SUMA_IMPMOV,		OemToAnsi(STR0100),'P','','','','',''} ) // 'Suma valores del concepto calculado de otros periodos (lee movimiento de SRC).'
aAdd(aMnemo, { '',OP_SUMA_UNIMOV,		OemToAnsi(STR0101),'P','','','','',''} ) // 'Suma unidades del concepto calculado de otros periodos (lee movimiento de SRC).'
aAdd(aMnemo, { '',OP_SUMA_GAN_ACU,		OemToAnsi(STR0103),'P','','','','',''} ) // 'Obtiene el valor acumulado del mes (lee concepto de RT3).'
aAdd(aMnemo, { '',OP_LEE_AST_FIJO,		OemToAnsi(STR0104),'P','','','','',''} ) // 'Retornar los regisros de asientos fijos'
aAdd(aMnemo, { '',OP_SIG_AST_FIJO,		OemToAnsi(STR0105),'P','','','','',''} ) // 'Avanzar hacia el proximo registro de asientos fijos'
aAdd(aMnemo, { '',OP_PRIMER_REG_AF,		OemToAnsi(STR0106),'P','','','','',''} ) // 'Lee el primer registro de asientos fijos'
aAdd(aMnemo, { '',OP_ULTIMO_REG_AF,		OemToAnsi(STR0107),'P','','','','',''} ) // 'Lee el ultimo registro de asientos fijos'
aAdd(aMnemo, { '',OP_FILTRO_AF,			OemToAnsi(STR0108),'P','','','','',''} ) // 'Filtro Asientos Fijos'

//*** Mnemonicos Reservados
aAdd(aMnemo, { '',ME_HOY,			 OemToAnsi(STR0046),'R','','','','',''} ) // 'Retorna a data do Sistema em formato Juliana'
aAdd(aMnemo, { '',ME_IMPORTE,		 OemToAnsi(STR0047),'R','','','','',''} ) // 'Valor a ser gravado nas tabelas de Movimentos' 
aAdd(aMnemo, { '',ME_UNIDADES,		 OemToAnsi(STR0048),'R','','','','',''} ) // 'Horas/dias a serem gravados nas tabelas de Movimentos'
aAdd(aMnemo, { '',ME_NO_APLICADO,	 OemToAnsi(STR0049),'R','','','','',''} ) // 'Valor não aplicado a ser gravado nas tabelas de Movimentos'
aAdd(aMnemo, { '',ME_SUMA_NPGINI,	 OemToAnsi(STR0067),'R','','','','',''} ) // 'Numero Pagamento Inicial para Operador SUMA_IMPHIS y SUMA_UNIHIS'
aAdd(aMnemo, { '',ME_SUMA_NPGFIM,	 OemToAnsi(STR0068),'R','','','','',''} ) // 'Numero Pagamento Final para Operador SUMA_IMPHIS y SUMA_UNIHIS'
aAdd(aMnemo, { '',ME_SUMA_TIPO_NOM,	 OemToAnsi(STR0069),'R','','','','',''} ) // 'Tipo de Nomina para Operador SUMA_IMPHIS y SUMA_UNIHIS'

aAdd(aMnemo, { '',ME_DIAS_TRAY_SIG,	 OemToAnsi(STR0050),'R','','','','',''} ) // 'Retorna o numero de dias do Movimento da Trajetoria Laboral'
aAdd(aMnemo, { '',ME_GEN_MOV,		 OemToAnsi(STR0051),'R','','','','',''} ) // 'Indica se deve gerar um novo registro na Trajetoria Laboral'
aAdd(aMnemo, { '',ME_TODOS_LOS_DIAS, OemToAnsi(STR0052),'R','','','','',''} ) // 'Não considerar os dias prog.p/apurar dias direito faltantes'
aAdd(aMnemo, { '',ME_SIN_PROGRAMAR,  OemToAnsi(STR0053),'R','','','','',''} ) // 'Considerar os dias prog.p/apurar dias direito faltantes'
aAdd(aMnemo, { '',ME_DIAS_PAGADOS,   OemToAnsi(STR0102),'R','','','','',''} ) // 'Retorna los dias pagados del periodo indicado'
aAdd(aMnemo, { '',ME_VACIO,          OemToAnsi(STR0054),'R','','','','',''} ) // 'Retorna data vazia'
aAdd(aMnemo, { '',ME_ANO_INI_ACU,    OemToAnsi(STR0060),'R','','','','',''} ) // 'Ano Inicio para Generacao de Acumuladores'
aAdd(aMnemo, { '',ME_ANO_FIM_ACU,    OemToAnsi(STR0061),'R','','','','',''} ) // 'Ano Fim para Generacao de Acumuladores'
aAdd(aMnemo, { '',ME_BASE_PENSION,	 OemToAnsi(STR0071),'R','','','','',''} ) // 'Indica soma das verbas de Beneficiários'
aAdd(aMnemo, { '',ME_TP_SUMA_MOVLI,	 OemToAnsi(STR0074),'R','','','','',''} ) // 'indica o tipo de calculo sera realizado Horas/Dia ou Valor'
aAdd(aMnemo, { '',ME_RET_TABLA,	 	 OemToAnsi(STR0083),'R','','','','',''} ) // 'Retorno o conteudo referente a linha/coluna da tabela selecionada'
aAdd(aMnemo, { '',ME_TP_FILTRO,		 OemToAnsi(STR0087),'R','','','','',''} ) // 'Tipo de Filtro que será usado em conjunto com FILTRO_ACUM ou FILTRO_MOVLI'
aAdd(aMnemo, { '',ME_ENTIDAD,		 OemToAnsi(STR0093),'R','','','','',''} ) // 'Entidad
aAdd(aMnemo, { '',ME_DIAS_ANTIG,	 OemToAnsi(STR0096),'R','','','','',''} ) // 'Obtiene el número de días de dos fechas basándose en un año de 360 días.'
aAdd(aMnemo, { '',ME_MESES_ANTIG,	 OemToAnsi(STR0097),'R','','','','',''} ) // 'Obtiene el número de meses de dos fechas basándose en un año de 360 días.'
aAdd(aMnemo, { '',ME_ANIOS_ANTIG,	 OemToAnsi(STR0098),'R','','','','',''} ) // 'Obtiene el número de años de dos fechas basándose en un año de 360 días.'

For nElem := 1 to 99
