Function fChangeHk()
	Local cVarChange	:= ReadVar()
    Local cHotkey		:= ""                     
    Local nPoskey		:= 0  

    If Type("aHotkeys") == "U"
		_SetOwnerPrvt( "aHotkeys", {} )    
    EndIf
    
	If( Len(aHotkeys) == 0)
		aHotkeys := LoadHotkeys()
	EndIf 
			
	If( Replace(cVarChange,"M->","") $ "RC3_FORM01/RC3_OPERA1/RC3_FORM02/RC3_RESULT" )
		cHotkey := Upper(AllTrim(&cVarChange))
		nPoskey := aScan( aHotkeys, {|x| x[1] == cHotkey})	
		If (nPoskey > 0)
			&cVarChange := aHotkeys[nPoskey,2]		
		EndIf
	EndIf
Return .T.
