		aRBUSvCols	:= aClone( aRBUCols )           
	
		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//ЁInicializa o Ultimo Valor do RBU_POSTO  				   	     Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		IF (;
				(;
					Empty( RBUPostoGet() );
					.or.;
					Empty( StrTran( RBUPostoGet() , "0" , "" ) );
				);
				.and.;
				!Empty( aRBURecnos );
			)	
			RBUPostoSet( RBUPostoInit() )
		Else
			RBUPostoSet( GdFieldGet( "RBU_POSTO" , 1 , .F. , aRBUHeader , aRBUCols ) )	
		EndIF
		
		//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Define os Campos que nao se alteraram somente visualizacao   Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
