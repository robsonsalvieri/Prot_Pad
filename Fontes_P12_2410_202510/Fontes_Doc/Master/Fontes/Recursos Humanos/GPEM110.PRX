If lOk 
	cTipoCont := MV_PAR01
	If cTipoCont == 1
		If cPaisLoc == "BRA"
			Pergunte("GPE110D",.T. )
		Else
			Pergunte("GPE110B",.T. )
		EndIf
	ElseIf cTipoCont == 2
		Pergunte("GPE110C",.T. )
	ElseIf cTipoCont == 3
		If cPaisLoc == "BRA"
