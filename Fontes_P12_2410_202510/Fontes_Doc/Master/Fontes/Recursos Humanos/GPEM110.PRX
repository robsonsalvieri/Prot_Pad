	DEFAULT nTamRZItem	:= Alltrim(str(TamSx3( "RA_ITEM     " )[1]))
	DEFAULT nTamRZClas	:= Alltrim(str(TamSx3( "RA_CLVL     " )[1]))
Endif

If TCCanOpen(cTableTmp)
	TCDelFile(cTableTmp)
EndIf
	
