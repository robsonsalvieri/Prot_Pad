Static Function ReportPrint(oReport)

Static lIxbConTes := NIL

Local oSection1 := oReport:Section(1) 
Local oSection2 := oReport:Section(1):Section(1)  
Local oSection3 := oReport:Section(1):Section(1):Section(1)  
Local nOrdem    := oReport:Section(1):GetOrder() 

Local cSelectD1 := ''
Local cWhereD1  := ''
Local cSelectD2 := ''
Local cWhereD2  := ''
Local cSelectD3 := ''
Local cWhereD3  := ''
Local cProdAnt  := ''
Local cLocalAnt := ''
Local cNumSeqTr := '' 
Local cWhereB1A := ' ' 
Local cWhereB1B := ' ' 
Local cWhereB1C := ' ' 
Local cWhereB1D := ' ' 

Local cQueryB1A := ' ' 
Local cQueryB1B := ' ' 
Local cQueryB1C := ' ' 
Local cQueryB1D := ' ' 

Local cUnion    := '%%'

Local aDadosTran:= {}

Local lFirst    := .T. 
Local lTransEnd := .T.
Local lContinua := .F.
Local lImpSMov  := .F.

Local aSalAtu   := {}
Local cPicB2Qt2 := PesqPictQt("B2_QTSEGUM" ,18)
Local cMvCQ		:= GetMvNNR('MV_CQ','98')

Local nTotRegs  := 0
Local aProdsMNT := {}
Local nX        := 0

// Indica se esta listando relatorio do almox. de processo
Local lLocProc  := mv_par08 == GetMvNNR('MV_LOCPROC','99')
// Indica se deve imprimir movimento invertido (almox. de processo)
Local lInverteMov :=.F.
Local lPriApropri :=.T.
// Verifica se existe ponto de entrada
Local lTesNEst := .F.
// Codigo do produto importado - NAO DEVE SER LISTADO
Local cProdImp := GetMV("MV_PRODIMP")
// Codigo do Produto utilizado no SIGAMNT
Local cProdMNT := GetMv("MV_PRODMNT")
// Codigo do Produto que indica terceiros utilizado no SIGAMNT
Local cProdTER := GetMv("MV_PRODTER")
// Variaveis utilizadas para processamento de Filiais
Local aFilsCalc := MatFilCalc( mv_par16 == 1 )
Local cFilBack  := cFilAnt
Local nForFilial:= 0
Local lWmsNew	:= SuperGetMv("MV_WMSNEW",.F.,.F.)
