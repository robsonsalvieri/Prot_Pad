Static Function MR978ImpS1(aSalAtu,cAliasTop,lQuery,lCusUnif,oSection1,oSection2,oReport)

Local aArea     := GetArea()
Local nCusMed   := 0
Local i         := 0
Local aSalAlmox := {}
Local cSeek     := ""
Local cCampo    := ""
Local aRegraCP  := {}
Local nPartes	:= 0

Default lQuery    := .F.
Default cAliasTop := "SB1"

If ExistBlock("MA330CP")
	aRegraCP:=ExecBlock("MA330CP",.F.,.F.)
	If ValType(aRegraCP) # "A"
		aRegraCP:={}
	EndIf
EndIf
If Len(aRegraCP) >= 1
	nPartes := Len(aRegraCP) + 1
EndIf
//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Calcula o Saldo Inicial do Produto             ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
If lCusUnif
	aArea:=GetArea()
	aSalAtu  := {0, 0, 0, 0, 0, 0, 0, 0}
	dbSelectArea("SB2")
	dbSetOrder(1)
	dbSeek(cSeek:=xFilial("SB2") + If(lQuery,(cAliasTOP)->PRODUTO,SB1->B1_COD))
	While !Eof() .And. B2_FILIAL+B2_COD == cSeek
		aSalAlmox := CalcEstCP(If(lQuery,(cAliasTOP)->PRODUTO,SB1->B1_COD),SB2->B2_LOCAL,mv_par05,,,nPartes)
		For i:=1 to Len(aSalAtu)
			aSalAtu[i] += aSalAlmox[i]
		Next i
		dbSkip()
	End
	RestArea(aArea)
Else
	aSalAtu := CalcEstCP(If(lQuery,(cAliasTOP)->PRODUTO,SB1->B1_COD),mv_par08,mv_par05,,,nPartes)
EndIf

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Calcula o Custo Medio do Produto               ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
SB2->(dbSetOrder(1))
SB2->(dbSeek(xFilial("SB2") + If(lQuery,(cAliasTOP)->PRODUTO,SB1->B1_COD)))
If AsalAtu[1] > 0
	nCusmed := aSalAtu[mv_par10+7]/aSalAtu[1]
ElseIf AsalAtu[1] == 0 .and. AsalAtu[mv_par10+7] == 0
	nCusMed := 0
Else
	cCampo  := Eval(bBloco,"B2_CM",mv_par10)
	IF  SB2->(Fieldpos(cCampo))  <> 0
		nCusmed := &(Eval(bBloco,"SB2->B2_CM",mv_par10))
	Else
		nCusmed := &("SB2->B2_CM"+Iif(valtype(mv_par10)='C',mv_par10,cvaltochar(mv_par10)))
	EndIf
EndIf


oSection1:Init()
oSection2:Init()

oSection1:Cell("nCusMed"):SetValue(nCusMed)
oSection1:Cell("nQtdSal"):SetValue(aSalAtu[1])
oSection1:Cell("nVlrSal"):SetValue(aSalAtu[mv_par10+7])			

oSection1:Cell("cProduto"):SetValue((cAliasTop)->PRODUTO)			
oSection1:Cell("cTipo"   ):SetValue((cAliasTop)->TIPO	)
dbSelectArea("SB2")
dbSeek(xFilial("SB2")+(cAliasTop)->PRODUTO+If(lCusUnif,"",mv_par08))

oSection1:PrintLine()
oSection2:PrintLine()

RestArea(aArea)
RETURN
