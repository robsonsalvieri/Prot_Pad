/*/
Function A150Quant(nQuant)

	Local aArea		:= GetArea()
	Local oModel    := FwModelActive()
	Local lNFC      := A150GetPGC() .And. ValType(oModel) == 'O' .And. oModel:GetId() == 'NFCA020' // Verifico se a origem é Legado ou NFC
	Local nPosItem	:= iif( !lNFC, aScan(aHeader,{|x|x[2] = "C8_ITEM"}), 0 )
	Local lRet 		:= .T.
	Local lAuto     := Type("l150Auto")=="L"

	if !lNFC
		If lAuto .And. IsInCallStack("PUTQUOTE") .And. Type("aAutoCab") == "A"
			DbSelectArea("SC8")
			DbSetOrder(1)
			MsSeek(xFilial("SC8")+aAutoCab[ProcH("C8_NUM"),2]+aAutoCab[ProcH("C8_FORNECE"),2]+aAutoCab[ProcH("C8_LOJA"),2]+aCols[n][nPosItem])

			If QtdComp(nQuant)<> QtdComp(SC8->C8_QUANT)
				Help(" ",1,"A150001")
				lRet := .F.
			EndIf

		EndIf
	else
		lRet := NF020QtDisp()
	endif
	RestArea(aArea)
Return lRet

