/*/
Function A150Total(nTotal)

	Local aArea		:= GetArea()
	Local oModel    := FwModelActive()
	Local lNFC      := A150GetPGC() .And. ValType(oModel) == 'O' .And. oModel:GetId() == 'NFCA020' // Verifico se a origem é Legado ou NFC
	Local nPosTES	:= iif( !lNFC, aScan(aHeader,{|x|x[2] = "C8_TES"}), 0 )
	Local nPosProv  := iif( !lNFC, Ascan(aHeader,{|x|x[2] = "C8_PROVENT"}), 0 )
	Local nPQtSegUm := iif( !lNFC, Ascan(aHeader,{|x|x[2] = "C8_QTSEGUM"}), 0 )
	Local nPPreco	:= iif( !lNFC, Ascan(aHeader,{|x|x[2] = "C8_PRECO"}), 0 )
	Local nPQuant	:= iif( !lNFC, Ascan(aHeader,{|x|x[2] = "C8_QUANT"}), 0 )
	Local nQuant	:= iif( !lNFC, aCols[n][aScan(aHeader,{|x|x[2] = Iif(_lPGC,"C8_QTDISP","C8_QUANT")})], 0 )
	Local nPreco	:= iif( !lNFC, aCols[n][GDFieldPos("C8_PRECO")], 0 )
	Local lRet 		:= .T.
	Local nDif		:= NoRound(nQuant*nPreco,2)-nTotal
	Local lCalc2Um  := SuperGetMV( "MV_CALC2UM", .F., .T. )

	if !lNFC
		If nDif < 0
			nDif := -(nDif)
		EndIf

		If lCalc2Um
			If nPQtSegUm > 0
				If aCols[n][nPQtSegUm] > 0 .And. nDif > 0.49
					aCols[n][nPPreco] := nTotal / aCols[n][nPQuant]
					If MaFisFound( "IT", n )
						MaFisAlt( "IT_PRCUNI", aCols[n][nPPreco], n )
					EndIf
					nDif := Abs( NoRound( aCols[n][nPQuant] * aCols[n][nPPreco], 2 ) - nTotal )
				EndIf
			EndIf 
		EndIf

		If nDif > 0.09
			Help(" ",1,"A12003")
			lRet := .F.
		EndIf

		If lRet
			MaFisAlt('IT_TES',aCols[n][nPosTES],n)
			If nPosProv > 0 .And. VerProEnIt(aCols[n][nPosProv],aCols[n][nPosTES],.T.,.F.)
				MaFisAlt('IT_PROVENT',aCols[n][nPosProv],n)
			EndIf
		EndIf
		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		//³ Verifica e Calcula o Desconto em cascata           ³
		//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
		If lRet 
			MaFisRef("IT_VALMERC","MT150",M->C8_TOTAL)
		Endif 

		if !l150Auto
			A150AtuRef("NF_FRETE"   ,a150Var[FRETE],.T.)
		Endif

		IF lAtuDesp
			A150AtuRef("NF_DESPESA" ,a150Var[VALDESP],.T.)
		Endif

		if ( l150Auto .and. Type("aAutoItens") == "A" .and. len(aAutoItens) > 0 .and. len(aCols) == len(aAutoItens) ) .or. !l150Auto
			A150AtuRef("NF_SEGURO"  ,a150Var[SEGURO],.T.)
		endif

		If lAtudesc
			A150AtuRef("NF_DESCONTO",a150Var[VALDESC],.T.)
		Endif
		
		Eval(bCalcDesc)
	endif
	RestArea(aArea)

Return lRet

