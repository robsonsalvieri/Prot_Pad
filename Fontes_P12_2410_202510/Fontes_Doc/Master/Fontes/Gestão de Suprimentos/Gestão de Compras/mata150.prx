Static Function A150FisToaCols(nItem)

	Local nX		:= 0
	Local nY		:= 0
	Local cValid	:= ""
	Local nPosRef	:= 0
	Local nItemIni	:= If(nItem==Nil,1,nItem)
	Local nItemAte	:= If(nItem==Nil,Len(aCols),nItem)

