Function A150DescCnd(cCondicao,oDescCond,cDescCond,oGetDados)

	dbSelectArea("SE4")
	dbSetOrder(1)
	If MsSeek(xFilial()+cCondicao)
		cDescCond := SE4->E4_DESCRI
		If oDescCond != Nil
			oDescCond:Refresh()
		EndIf
	ElseIf Empty(cCondicao)
		cDescCond := "        "
	EndIf

	If oGetDados <> Nil
		oGetDados:oBrowse:Refresh()
	EndIf

Return .T.


