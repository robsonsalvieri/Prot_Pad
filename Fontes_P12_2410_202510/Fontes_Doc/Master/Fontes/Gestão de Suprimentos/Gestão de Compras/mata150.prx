			a150Var[VALSOL] := MaFisRet(,"NF_VALSOL")
			a150Var[VALCOMP]:= MaFisRet(,"NF_VALCMP")
			a150Var[SEGURO] := MaFisRet(,"NF_SEGURO")
			a150Var[VALDESP]:= MaFisRet(,"NF_DESPESA")
