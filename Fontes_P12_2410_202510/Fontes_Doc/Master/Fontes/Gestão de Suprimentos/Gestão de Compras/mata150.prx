Static Function A150Refresh(a150Var)

	Local aArea	:= GetArea()
	Local nPFrete := 0
	
	If !l150Auto .OR. (l150Inclui .and. !l150Propost)
			a150Var[VALMERC]:= MaFisRet(,"NF_VALMERC")
			a150Var[VALDESC]:= MaFisRet(,"NF_DESCONTO")
