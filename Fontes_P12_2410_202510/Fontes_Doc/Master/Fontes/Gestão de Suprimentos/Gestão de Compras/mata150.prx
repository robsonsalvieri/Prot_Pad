Function A150Trigger(cCampo)

	Local aArea 	:= GetArea()
	Local lRet		:= .T.
	Local nX		:= 0
	Local nPosCpo	:= 0
	Local oModel    := FwModelActive()
	Local lNFC      := A150GetPGC() .And. ValType(oModel) == 'O' .And. oModel:GetId() == 'NFCA020' // Verifico se a origem é Legado ou NFC

	if !lNFC
		nPosCpo	:= aScan(aHeader,{|x| x[2]=cCampo})
		If ( nPosCpo > 0 )
			For nX := 1 to Len(aHeader)
				M->&(AllTrim(aHeader[nX][2])) := aCols[n][nX]
			Next
			If &(aHeader[nPosCpo][6])
				A150FieldOk()
			Else
				lRet := .F.
			EndIf
		Else
			lRet := .F.
		EndIf

		If "C8_TOTAL" $ cCampo
			A150ArrTot()
		EndIf
	endif

	RestArea(aArea)

Return lRet

