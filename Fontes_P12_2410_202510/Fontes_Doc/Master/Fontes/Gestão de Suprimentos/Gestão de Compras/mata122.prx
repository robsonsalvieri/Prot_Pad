aAdd(aRotina,{STR0004,"A120Pedido", 0, 4, 6, Nil }) //"Alterar"
aAdd(aRotina,{STR0005,"A120Pedido", 0, 5, 7, Nil }) //"Excluir"
aAdd(aRotina,{STR0006,"A120Copia" , 0, 9, 0, Nil }) //"Copia"
aAdd(aRotina,{STR0007,"A120Impri" , 0, 2, 0, Nil }) //"Imprimir"
