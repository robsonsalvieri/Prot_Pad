Function Mata122(aCabec,aItens,nOpc)
PRIVATE aRotina := MenuDef()
Default  aCabec := {}
Default  aItens := {}
Default  nOpc := 0

If empty(aCabec)
         Mata120(2)
Else
         Mata120(2,aCabec,aItens,nOpc)
Endif

Return Nil

