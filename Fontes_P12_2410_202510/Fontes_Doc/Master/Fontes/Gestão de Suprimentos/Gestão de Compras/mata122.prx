Static Function MenuDef()     

PRIVATE aRotina	:= {}

aAdd(aRotina,{STR0001,"PesqBrw"   , 0, 1, 0, .F. }) //"Pesquisar"
aAdd(aRotina,{STR0002,"A120Pedido", 0, 2, 0, Nil }) //"Visualizar"
