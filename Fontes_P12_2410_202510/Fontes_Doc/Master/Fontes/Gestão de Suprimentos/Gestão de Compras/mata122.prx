aAdd(aRotina,{STR0009,"MsDocument", 0, 4, 0, Nil }) //"Conhecimento"

If ExistBlock("MT122BRW")
	ExecBlock("MT122BRW",.F.,.F.)
EndIf

Return(aRotina)  
