Method New() Class LJCLogIntegra
	
	//Executa o metodo construtor da classe pai.
	::ALog()
	
	::cNomeArq 	:= NOMEARQUIVO 
	::cPath		:= PATH
		
	If GetRemoteType() == REMOTE_LINUX
		::cPath := StrTran(::cPath, "/", "\")	
	EndIf
		
Return Self

