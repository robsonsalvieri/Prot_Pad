#INCLUDE "TOTVS.CH"
#INCLUDE "tlpp-core.th"
#INCLUDE "tlpp-rest.th"

namespace totvs.protheus.retail.rmi.monitoramento.api.pshMonitorDetIncidentes

/*/{Protheus.doc} getDetIncidentes
API para disponibilizar os detalhes dos incidentes
@author  Eduardo Sales
@since   06/06/2024
@version 1.0
@exemple: http://localhost:8090/REST/api/retail/v1/monitoramento/getdetalhesincidentes?uuid=12345
/*/
@get("/api/retail/v1/monitoramento/getdetalhesincidentes") 
Function getDetIncidentes()

Local oIncidentes := Nil as Object

oIncidentes := pshMonitorDetIncidentesObj():New(oRest)
oRest:setKeyHeaderResponse('Content-Type','application/json; charset=utf-8')
oRest:setResponse( oIncidentes:GetJsonReturn() )

Return
