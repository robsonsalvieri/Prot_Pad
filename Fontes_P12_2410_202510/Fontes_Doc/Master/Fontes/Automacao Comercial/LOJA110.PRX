	Private lAtuB0Fil	:= ExistBlock("ATUB0FIL")	// Gera no SB0 registros para todas as filiais
EndIf

If SuperGetMv("MV_LJOFFLN", Nil, .F.) 
	Lj110Inclui(cAlias, nReg, nOpc)
Else   
	A010INCLUI(cAlias, nReg, nOpc) 
EndIf

