Static Function Destrutor(oObj, lClean)
    
	Default lClean := .F.

	If oObj <> Nil
		//Libera o objeto
		FreeObj(oObj)
		//Inicializa o objeto
		oObj := Nil
		
		If lClean
			//Libera recurso de memoria
			//Obs: este comando so precisa ser executado uma unica vez apos a execucao do processo
			DelClassIntf()
		EndIf
	EndIf

Return Nil
