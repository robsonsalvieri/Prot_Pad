Function Epson_A(cFunction, aParams, cDelimit, cFimStr)
    
    Local oRet		:= Nil							//Retorno da dll
	Local cBuffer 	:= ""                       	//Retorno da funcao
	Local cParam1	:= ""							//Variavel auxiliar
	Local cParam2	:= ""                         	//Variavel auxiliar
	Local cParam3	:= ""							//Variavel auxiliar
	Local cParam4	:= ""							//Variavel auxiliar
	Local cParam5	:= ""							//Variavel auxiliar
	Local cParam6	:= ""							//Variavel auxiliar
	Local cParam7	:= ""							//Variavel auxiliar
	Local cParam8	:= ""							//Variavel auxiliar
	Local cParam9	:= ""							//Variavel auxiliar
	Local cParam10	:= ""							//Variavel auxiliar
	Local cParam11	:= ""							//Variavel auxiliar
	Local cParam12	:= ""							//Variavel auxiliar
	Local cParam13  := ""							//Variavel auxiliar
	Local cParam14	:= ""							//Variavel auxiliar
	Local cParam15	:= ""							//Variavel auxiliar
	Local cParam16	:= ""							//Variavel auxiliar
	Local cParam17	:= ""							//Variavel auxiliar
	Local cParam18	:= ""							//Variavel auxiliar
	Local cParam19  := ""							//Variavel auxiliar
	Local cParam20	:= ""							//Variavel auxiliar
	Local cParam21	:= ""							//Variavel auxiliar
	Local cParam22	:= ""							//Variavel auxiliar
	Local cParam23	:= ""							//Variavel auxiliar
	Local cParam24	:= ""							//Variavel auxiliar
	Local cParam25  := ""							//Variavel auxiliar
	Local cParam26	:= ""							//Variavel auxiliar
	Local cParam27	:= ""							//Variavel auxiliar
	Local nParam1 	:= 0							//Variavel auxiliar
	Local nParam2 	:= 0							//Variavel auxiliar
	Local nParam3 	:= 0							//Variavel auxiliar
	Local nParam4 	:= 0 							//Variavel auxiliar
	Local nParam5 	:= 0							//Variavel auxiliar
	Local nParam6 	:= 0							//Variavel auxiliar
	
	//Executa o comando selecionado
	Do Case
	
		Case cFunction == Upper("EPSON_Obter_Codigo_Nacional_ECF")
			cParam1 := DoEncode(aParams[3])	
			cParam2 := DoEncode(aParams[4])	
			oRet 	:= EP_OBCODNA(cParam1, cParam2)
			cBuffer := AllTrim(Str(oRet)) + cDelimit + DoDecode(cParam1) + cDelimit + DoDecode(cParam2) + cFimStr

		Case cFunction == Upper("EPSON_Obter_Versao_SWBasicoEX")
			cParam1 := DoEncode(aParams[3])	
			cParam2 := DoEncode(aParams[4])	
			cParam3 := DoEncode(aParams[5])	
			oRet 	:= EP_OBVERSB(cParam1, cParam2, cParam3)
			cBuffer := AllTrim(Str(oRet)) + cDelimit + DoDecode(cParam1) + cDelimit + DoDecode(cParam2) + cDelimit + DoDecode(cParam3) + cFimStr
									
		Case cFunction == Upper("EPSON_Config_Mensagem_Aplicacao")
			cParam1 := DoEncode(aParams[3])	
			cParam2 := DoEncode(aParams[4])	
			oRet 	:= EP_CNGMAPL(cParam1, cParam2)
			cBuffer := AllTrim(Str(oRet)) + cFimStr
			 					
		Case cFunction == Upper("EPSON_Config_Mensagem_Aplicacao_Auto")
			cParam1 := DoEncode(aParams[3])	
			cParam2 := DoEncode(aParams[4])	
			oRet 	:= EP_CNGMAPA(cParam1, cParam2)
			cBuffer := AllTrim(Str(oRet)) + cFimStr
							
		Case cFunction == Upper("EPSON_Fiscal_Vender_ItemEX")
			cParam1 := DoEncode(aParams[3])
			cParam2 := DoEncode(aParams[4])
			cParam3 := DoEncode(aParams[5])
			nParam1 := Val(aParams[6])
			cParam4 := DoEncode(aParams[7])
			cParam5 := DoEncode(aParams[8])
			nParam2 := Val(aParams[9])
			cParam6 := DoEncode(aParams[10])		
			nParam3 := Val(aParams[11])
			nParam4 := Val(aParams[12])
			oRet 	:= EP_VNDITEX(cParam1,cParam2,cParam3,nParam1,cParam4,cParam5,nParam2,cParam6,nParam3,nParam4)
			cBuffer := AllTrim(Str(oRet)) + cFimStr
							
		Case cFunction == Upper("EPSON_Config_Dados_PAFECF")
			cParam1 := DoEncode(aParams[3])
			cParam2 := DoEncode(aParams[4])
			cParam3 := DoEncode(aParams[5])
			cParam4 := DoEncode(aParams[6])
			cParam5 := DoEncode(aParams[7])
			cParam6 := DoEncode(aParams[8])
			cParam7 := DoEncode(aParams[9])
			cParam8 := DoEncode(aParams[10])  
			oRet 	:= EP_CNGDPAF(cParam1, cParam2, cParam3, cParam4, cParam5, cParam6, cParam7, cParam8)
			cBuffer := AllTrim(Str(oRet)) + cFimStr
							
		Case cFunction == Upper("EPSON_Config_Habilita_EAD")
			nParam1 := Val(aParams[3])
			oRet 	:= EP_CNGHEAD(nParam1)
			cBuffer := AllTrim(Str(oRet)) + cFimStr
			

		Case cFunction == Upper("EPSON_Config_Forma_PagamentoEX")		
			nParam1 := Val(aParams[3])
			cParam1 := DoEncode(aParams[4])			
			oRet := EP_CNFPAGX(nParam1,cParam1)
			cBuffer := AllTrim(Str(oRet)) + cFimStr 	
			
		Case cFunction == Upper("EPSON_Obter_Arquivo_Binario_MF")		
			cParam1 := DoEncode(aParams[3])			
			oRet := EP_ARQMFBI(cParam1)
			cBuffer := AllTrim(Str(oRet)) + cFimStr 	
			
		Case cFunction == Upper("EPSON_Obter_Arquivo_Binario_MFD")		
			cParam1 := DoEncode(aParams[3])			
			oRet := EP_ARQMFDB(cParam1)
			cBuffer := AllTrim(Str(oRet)) + cFimStr

