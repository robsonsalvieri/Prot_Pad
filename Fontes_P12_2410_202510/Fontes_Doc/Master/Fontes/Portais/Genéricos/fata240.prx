/*/
Function MenuDesc(cAlias)
Local cTexto	:= ""
Local cIdioma 	:= ""

Default cAlias 	:= "AI8"
cIdioma := Upper( Left( FWRetIdiom(), 2 ) )
Do Case
	Case cIdioma == 'ES'
		cTexto:=(cAlias)->AI8_TEXTOS
	Case cIdioma == 'EN'
		cTexto:=(cAlias)->AI8_TEXTOE
	Case cIdioma == 'RU' .And. lCpoRUS
		cTexto:=(cAlias)->AI8_TEXTOR
	Otherwise
		cTexto:=(cAlias)->AI8_TEXTO
EndCase

Return cTexto

