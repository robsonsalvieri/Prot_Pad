/*/
Static Function FT240Tree(nOpcao,oTree,aTree)

Local aArea  	:= GetArea()
Local nOpcA 	:= 0
Local nPos		:= 0
Local cPai      := ""
Local lContinua := .T.

PRIVATE cCodigo := oTree:GetCargo()
PRIVATE nOrdem  := 0
PRIVATE cOkFunc := Nil
PRIVATE cDelFunc:= Nil
PRIVATE aRotina := {}

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³Verifica a Operacao selecionada                                         ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
Do Case
	Case nOpcao == 1
		aRotina := {{"","",0,3}}
		aEval(aTree, {|x| if(x[3] == cCodigo, nOrdem++, nOrdem)} )
		nOrdem++
		nOpcA := FWExecView("",'FATA250', MODEL_OPERATION_INSERT,, { || .T. } )
		If nOpcA == 0
			nOpca := 1
		Else
			nOpca := 2
		EndIf

		If nOpcA == 1
			oTree:TreeSeek(cCodigo)
			oTree:addItem(MenuDesc(),AI8->AI8_CODMNU,"FOLDER5","FOLDER6",,,2)
			oTree:TreeSeek(AI8->AI8_CODMNU)
			aadd(aTree,{MenuDesc(),AI8->AI8_CODMNU,cCodigo, nOrdem } )
		EndIf
	Case nOpcao == 2
		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		//³Posiciona no item selecionado                                           ³
		//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
		dbSelectArea("AI8")
		dbSetOrder(1)
		If MsSeek(xFilial("AI8")+AI9->AI9_PORTAL+cCodigo)
			cCodigo := AI8->AI8_CODPAI
			aRotina := {{"","",0,4}}

			nOpcA := FWExecView("",'FATA250', MODEL_OPERATION_UPDATE,, { || .T. } )
			If nOpcA == 0
				nOpca := 1
			Else
				nOpca := 2
			EndIf

			If nOpcA == 1
				oTree:ChangePrompt(MenuDesc())
				if (nPos := aScan(aTree,{|x| x[2] == AI8->AI8_CODMNU})) > 0
					aTree[nPos][1] := MenuDesc()
					aTree[nPos][4] := AI8->AI8_ORDEM
				endif
			EndIf
		EndIf
	Case nOpcao == 3
		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		//³Verifica se este item pode ser deletado                                 ³
		//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
		dbSelectArea("AI8")
		dbSetOrder(2)
		If !MsSeek(xFilial("AI8")+AI9->AI9_PORTAL+cCodigo)
			//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
			//³Posiciona no item selecionado                                           ³
			//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
			dbSelectArea("AI8")
			dbSetOrder(1)
			If MsSeek(xFilial("AI8")+AI9->AI9_PORTAL+cCodigo)
				aRotina := {{"","",0,5}}

				nOpcA := FWExecView("",'FATA250', MODEL_OPERATION_DELETE,, { || .T. } )
				If nOpcA == 0
					nOpca := 2
				Else
					nOpca := 1
				EndIf

			EndIf

			If nOpcA == 2
				dbSelectArea("AI8")
				dbSetOrder(1)
				If !MsSeek(xFilial("AI8")+AI9->AI9_PORTAL+cCodigo)
					oTree:DelItem()

					if (nPos := aScan(aTree,{|x| x[2] == cCodigo})) > 0
						cPai   := aTree[nPos][3]
						nOrdem := aTree[nPos][4]

						aDel(aTree,nPos)
						aSize(aTree,Len(aTree)-1)

					 	while lContinua
							if nPos < len(aTree) .AND. aTree[nPos][3] == cPai
								aTree[nPos][4] := nOrdem
								nPos++
								nOrdem++
							else
								lContinua := .F.
							endif
						enddo
					endif
				EndIf
			EndIf
		Else
			Help(" ",1,"NODELETA")
		EndIf
EndCase
RestArea(aArea)
Return

