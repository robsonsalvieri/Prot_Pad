/*/
Function FT240Menu()

Local aArea     := GetArea()
Local aSize     := MsAdvSize(.F.)
Local aObjects  := {{ 100, 100, .T., .T. },{ 10, 10, .T., .F. }}
Local aInfo     := { aSize[ 1 ], aSize[ 2 ], aSize[ 3 ], aSize[ 4 ], 5, 5 }
Local cPortal   := PadR(PortalDesc(),Len(MenuDesc()))
Local cCodigo   := AI9->AI9_PORTAL
Local aTree     := {}
Local nOrdem    := 0
Local nPosicao	:= 0

Local cCadastro := STR0002 //"Menu dos Portais"
Local oTree
Local oDlg

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³Montagem da Interface                                                   ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
aPosObj := MsObjSize( aInfo, aObjects,.T.)
DEFINE MSDIALOG oDlg TITLE cCadastro From aSize[7],0 To aSize[6],aSize[5] of oMainWnd PIXEL
oTree := DbTree():New(aPosObj[1,1],aPosObj[1,2],aPosObj[1,3],aPosObj[1,4],oDlg,,,.T.)
oTree:addItem(cPortal,Space(Len(AI8->AI8_CODMNU)),"FOLDER5","FOLDER6",,,2)

dbSelectArea("AI8")

dbSetOrder(3)

MsSeek(xFilial("AI8")+cCodigo)

While !Eof() .And. xFilial("AI8") == AI8->AI8_FILIAL .And.;
	AI8->AI8_PORTAL == cCodigo

	oTree:TreeSeek(AI8->AI8_CODPAI)
	oTree:addItem(MenuDesc(),AI8->AI8_CODMNU,"FOLDER5","FOLDER6",,,2)

	nOrdem := AI8->AI8_ORDEM

	aadd(aTree,{ MenuDesc(), AI8->AI8_CODMNU, AI8->AI8_CODPAI,  nOrdem} )

	dbSelectArea("AI8")
	dbSkip()
EndDo
DBENDTREE oTree
nPosicao := 0

//Botão OK
nPosicao += 30
DEFINE SBUTTON FROM aPosObj[2,1],aPosObj[2,4]-nPosicao TYPE 01 ENABLE OF oDlg ACTION ( FT240GrvOrdem(cCodigo, aTree, oDlg) )

//Botão Cancelar
nPosicao += 30
DEFINE SBUTTON FROM aPosObj[2,1],aPosObj[2,4]-nPosicao TYPE 02 ENABLE OF oDlg ACTION ( oDlg:End() )

//Botão Excluir
nPosicao += 30
DEFINE SBUTTON FROM aPosObj[2,1],aPosObj[2,4]-nPosicao TYPE 03 ENABLE OF oDlg ACTION ( FT240Tree(3,oTree,aTree) )

//Botão Editar
nPosicao += 30
DEFINE SBUTTON FROM aPosObj[2,1],aPosObj[2,4]-nPosicao TYPE 11 ENABLE OF oDlg ACTION ( FT240Tree(2,oTree,aTree) )

//Botão Incluir
nPosicao += 30
DEFINE SBUTTON FROM aPosObj[2,1],aPosObj[2,4]-nPosicao TYPE 04 ENABLE OF oDlg ACTION ( FT240Tree(1,oTree,aTree) )

oButton := tButton():New(aPosObj[2,1],005,STR0005,oDlg,{ || FT240ORD(oTree, aTree, cPortal, 1) },42,13,,,,.T.)
oButton := tButton():New(aPosObj[2,1],055,STR0006,oDlg,{ || FT240ORD(oTree, aTree, cPortal, 2) },42,13,,,,.T.)

ACTIVATE MSDIALOG oDlg

RestArea(aArea)
Return(.T.)

static function FT240ORD(oTree, aTree, cPortal, nOpcao)
	local cCodigo   := oTree:GetCargo()
	local nPosOrig  := aScan(aTree,{|x| x[2] == cCodigo})
	local nOrdem    := 0
	local lContinua := .F.
	local nBusca    := 0
	local nPosAux   := 0
	local nItem		 := 0
	local aItens	 := aClone(aTree)
   local nQtde     := 0

	if nPosOrig > 0
		nOrdem := aItens[nPosOrig][4]

		if nOpcao == 2 //Abaixo
			cPai := aItens[nPosOrig][3]
			aEval(aItens, {|x| if(x[3] == cPai, nQtde++, nQtde)} )
			if nOrdem < nQtde .and. nOrdem <> 0
				lContinua := .T.
				nBusca    := nOrdem + 1
				nPosAux   := nPosOrig + 1
			endif

		else //Acima
			if nOrdem > 1
				lContinua := .T.
				nBusca    := nOrdem - 1
				nPosAux   := nPosOrig - 1
			endif
		endif

		if lContinua .and. nBusca <> 0 .and. nPosAux > 0
			if aItens[nPosOrig][3] == aItens[nPosAux][3]

				aItens[nPosOrig][4] := nBusca
				aItens[nPosAux][4]  := nOrdem

				aSort(aItens,,,{|x,y| x[3]+str(x[4]) < y[3]+str(y[4])})

				aTree  := {}
				nOrdem := 0
				oTree:Reset()

				oTree:addItem(cPortal,Space(TamSX3("AI8_CODMNU")[1]),"FOLDER5","FOLDER6",,,2)

				For nItem := 1 to len(aItens)
					oTree:TreeSeek(aItens[nItem][3])
					oTree:addItem(aItens[nItem][1],aItens[nItem][2],"FOLDER5","FOLDER6",,,2)
					aadd(aTree,{aItens[nItem][1],aItens[nItem][2],aItens[nItem][3], aItens[nItem][4] } )
				next nItem

			endif
      endif
	endif

	oTree:TreeSeek(cCodigo)

return nil

static function FT240GrvOrdem(cPortal, aTree, oDlg)
	local aArea := AI8->( GetArea() )
	local nItem := 0

	dbSelectArea("AI8")
	dbSetOrder(1)

	for nItem := 1 to len(aTree)

		If MsSeek(xFilial("AI8")+cPortal+aTree[nItem][2])

			RecLock("AI8",.F.)
			AI8->AI8_ORDEM := aTree[nItem][4]
			AI8->( MsUnlock() )

		endif

	next nItem

	RestArea(aArea)

	oDlg:End()

return .T.

