Function PortalDesc()
Local cTexto	:= ""
Local cIdioma	:= ""

cIdioma := Upper( Left( FWRetIdiom(), 2 ) )
Do Case
	Case cIdioma == 'ES'
		cTexto:=AI9->AI9_DESC_S
	Case cIdioma == 'EN'
		cTexto:=AI9->AI9_DESC_E
	Case cIdioma == 'RU' .And. lCpoRUS
		cTexto:=AI9->AI9_DESC_R
	Otherwise
		cTexto:=AI9->AI9_DESCRI
EndCase

Return cTexto
