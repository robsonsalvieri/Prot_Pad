			{STR0394,"SpedCancRGS"	,0,2,0 ,NIL},; //"Cancelamento
			{STR0113,"SpedFiltro"	 ,0,3,0 ,NIL},; //"Filtro"
			{STR0006,"SpedCCePar()"    ,0,3,0 ,NIL}}//"Parâmetros"

		EndIf

	EndIf

