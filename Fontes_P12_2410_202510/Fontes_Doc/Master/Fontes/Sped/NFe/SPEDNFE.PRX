				If ValType(aRotAdic) == "A"
					aEval(aRotAdic,{|x| aAdd(aRotina,x)})
				EndIf
			EndIf
		ElseIf cRotina == "4"
			aRotina := {{STR0004,"PesqBrw"       ,0,1,0,.F.},; //"Pesquisar"
			{STR0005,"SpedNFeCfg"    ,0,3,0 ,NIL},; //"Wiz.Config."
			{STR0008,"SpedRGSRemessa",0,3,0 ,NIL},; //"Transmissao"
