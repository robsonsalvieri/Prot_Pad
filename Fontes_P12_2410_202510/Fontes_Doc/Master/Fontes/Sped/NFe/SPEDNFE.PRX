/*/{Protheus.doc} SpedContAutoPar
Função que parametriza o Protheus para a Contingencia Automatica

@author Sergio S. Fuzinaka
@since 25.04.2017
@version 12.1.017

@param	Null

/*/
//------------------------------------------------------------------------
Function SpedContAutoPar()
	Local aPerg  	:= {}
	Local aRet		:= {}
	Local aParam 	:= {"",""}
	Local cModel	:= "55"

	Local aCombo1	:= {}	// Habilitado: 0-Nao ou 1-Sim
	Local aCombo2	:= {}	// Modalidade: 5-EPEC

	Local cCombo1	:= ""
	Local cCombo2	:= ""

	Local cIdEnt 	:= ""
	Local lUsaColab	:= .F.
	Local cParCAPar	:= SM0->M0_CODIGO+SM0->M0_CODFIL+"AUTOCONT"

	Local lAdminNfe  := GetNewPar("MV_ADMNFE",.T.)
	local cMsgCont	 := ""

// Obtem o Modelo do Documento
	If FunName() $ "SPEDCTE,TMSA200,TMSA500,TMSAE70,TMSA050"
		cModel := "57"
	Else
		cModel := "55"
	Endif

// Habilitado
	AADD(aCombo1,"0-Nao")
	AADD(aCombo1,"1-Sim")

// Modalidade
	AADD(aCombo2,"5-EPEC")
	AADD(aCombo2,"8-Contingência SVC-AN")
	AADD(aCombo2,"9-Contingência SVC-RS")

	if cModel == "55"
		lUsaColab := UsaColaboracao("1")
	else
		lUsaColab := UsaColaboracao("2")
	endif

	if lUsaColab

		Aviso(STR0647, STR0831,{STR0114},3) //STR0647 = "SPED", STR0831 =  "Implementacao nao realizada para TOTVS Colaboracao !!!"

	ElseIf cModel == "57"

		Aviso(STR0647, STR0832,{STR0114},3)//STR0647 = "SPED", STR0832 =  "Implementacao nao realizada para o modelo 57-CT-e !!!"

	ElseIf cModel == "55"

		if !lAdminNfe .Or. ( lAdminNfe .And. PswAdmin( /*cUser*/, /*cPsw*/,RetCodUsr()) == 0 )

			If IsReady()

				// Obtem o codigo da entidade
				cIdEnt		:= GetIdEnt()

				// Título do Parambox
				cCadastro	:= " Contingência Automática - Entidade: " + cIdEnt + STR0788 + cVersaoTSS //STR0788 = " - TSS: "

				// Consulta a configuração da Contingencia Automatica
				If (getCfgAutoCont( "0", cIdEnt, cModel, , , @aRet, ,.T. ) )
					cCombo1 := aRet[1] + IIf(aRet[1] == "0","-Nao","-Sim")
					cCombo2 := getDescMod(aRet[2])
				Else
					cCombo1 := "0-Nao"
					cCombo2 := "5-EPEC"
				Endif

				aadd(aPerg,{2,"Habilita",cCombo1,aCombo1,120,".T.",.T.,".T."}) // Habilita: 0-Nao ou 1-Sim
				aadd(aPerg,{2,"Modalidade",cCombo2,aCombo2,120,".T.",.T.,".T."}) // Modalidade: 5-EPEC

				If ParamBox(aPerg,"TOTVS Web Service",aParam,,,,,,,cParCAPar,.F.,.F.)

					// Atualiza a configuração da Contingencia Automatica
					cMsgCont := ""
					If !getCfgAutoCont( "1", cIdEnt, cModel, Left(aParam[1],1), Left(aParam[2],1), , @cMsgCont, @aGlbCache[1]  )
						Aviso(STR0647,"Falha na gravacao das configuracoes. " + CHR(13) + CHR(10) + cMsgCont,{STR0114},3)//STR0647 = "SPED"
					Endif

				Endif

			Else

				Aviso(STR0647,STR0021,{STR0114},3) //"Execute o módulo de configuração do serviço, antes de utilizar esta opção!!!", STR0647 = "SPED"

			endif

		Else

			Help( "", 1, "SEMPERM" )

		Endif

	Else

		Aviso(STR0647, STR0833 + cModel + "] !!!",{STR0114},3) // STR0647 = "SPED", STR0833 =  "Implementacao nao realizada para o modelo ["

	Endif

Return

static function getDescMod(cModalidade)

	local cDescModalidade :=  ""

	do case
	case(cModalidade == "5")
		cDescModalidade := "5-EPEC"
	case(cModalidade == "8")
		cDescModalidade := "8-Contingência SVC-AN"
	case(cModalidade == "9")
		cDescModalidade := "9-Contingência SVC-RS"
	endcase

return cDescModalidade

static function BtmCaution(oTBitmap1, lEnable)

	if( oTBitmap1 <> nil )
		oTBitmap1:lVisible := lEnable
		oTBitmap1:refresh()
	endif

return .T.

