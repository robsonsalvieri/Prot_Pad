Function SpedNFeCan(cCodEmp,cCodFil,cIdEnt,cAmbiente,cModalidade,cVersao,aInfnota,lCTe)

	Local cUrl
	local lUsaColab 	:= .F.
	Local lSdoc
	local cModelo		:= "55"
	local lMDfe		:= .F.
	Default lCTe := .F.
	Default aInfnota	:= {}

	if( right( GetWebJob(), 1 ) == "_" ) .and. !empty(cCodEmp)  .and. !empty(cCodFil)
		RpcSetType(3)
		PREPARE ENVIRONMENT EMPRESA cCodEmp FILIAL cCodFil MODULO "FAT" TABLES "SF1","SF2","SD1","SD2","SB1","SB5","SF3","SF4","DT6","CKOCOL","CKP","CKQ"
	endif
	cUrl	:= Padr( GetNewPar("MV_SPEDURL",""), 250 )
	lSdoc   := TamSx3("F2_SERIE")[1] == 14

	If lSdoc
		GetFiltroF3(@aInfnota,.T.,aInfnota[4],aInfnota[5])
	EndIF

	lUsaColab :=  UsaColaboracao( IIF(lCte,"2","1") )
	cModelo   := 	IIf(lCte,"57",IIf(lMDfe,"58","55"))
	If lCTe
		JobNfeCan(cIdEnt,cAmbiente,cModalidade,cVersao,aInfnota,lCTE)
	Else
		MsgRun(STR0130,STR0073,{|| JobNfeCan(cIdEnt,cAmbiente,cModalidade,cVersao,aInfnota)}) //"Aguarde"###"Processando Transmissão NF-e"
	EndIf

Return

Static Function JobNFeCan(cIdEnt,cAmbiente,cModalidade,cVersao,aInfnota,lCTE)

	Local aArea			:= GetArea()
	Local aNotas		:= {}
	Local aNFeCol		:= {}
	Local aInfXml 		:= {}
	local aNFAtu		:= {}
	Local cLabelEven	:= ""
	Local cAliasSF3		:= "SF3"
	Local cWhere		:= ""
	Local cURL			:= PadR(GetNewPar("MV_SPEDURL","http://"),250)
	Local cModelo		:= "55"
	Local cXjust		:= ""
	Local cRet			:= ""
	Local cTpEvento		:= "110111"
	Local cErro			:= ""
	Local cAviso		:= ""
	Local cIdEven		:= ""
	Local cSerie		:= ""
	Local cNumero		:= ""
	Local cMod			:= ""
	Local dDataCanc		:= Date()
	Local lNfeCancEven	:= GetNewPar("MV_NFECAEV",.F.)	//-- Cancelamento de NF-e por Evento
	Local lXjust		:= If(AllTrim(GetNewPar("MV_INFXJUS","")) == "S" .Or. UPPER(AllTrim(GetNewPar("MV_INFXJUS",""))) == "NFE",.T.,.F.)
	Local lCTeXjust		:= SuperGetMv("MV_CTEXJUS", .F., .F.)
	Local lEndFis  		:= GetNewPar("MV_SPEDEND",.F.)
	Local cUFEnt   		:= IIF(!lEndFis,SM0->M0_ESTCOB,SM0->M0_ESTENT)
	Local lCancela		:= .F.
	Local lInutiliza	:= .F.
	Local lAviso		:= .F.
	Local lGera			:= .F.
	Local nCancExt 		:= GetNewPar("MV_CANCEXT",30)
	Local nX			:= 0
	Local nY			:= 0
	local nZ			:= 0
	Local oWs
	Local lCancOk		:= .F.
	Local lArqGer		:= .F.
	Local lUsaColab		:= .F.
	Local lTrfNfeCanc 	:= ExistBlock('TRFNFECANC')
	local lNFeCan       := .T.
	local lFisCannfe    := ExistBlock("FISCANNFE")
	local lNfeCanc01    := ExistBlock("NFECANC01")
	Local aParCanc      := {}
	Local nParCanc
	local cUFCode
	Default lCTE		:= .F.

	cUFEnt := UPPER(cUFEnt)

	//-- Cancelamento Extemporâneo
	If Empty(nCancExt) .OR. nCancExt == 0
		nCancExt := 30
	EndIf

	lUsaColab := UsaColaboracao( IIF(lCte,"2","1") )
	cAmbiente   := SubStr(cAmbiente,1,1)
	cModalidade := SubStr(cModalidade,1,1)

	If lCte
		cModelo := "57"
	EndIf

	dbSelectArea("SF3")
	dbSetOrder(5)

	dDataCanc	:= Iif(Empty(dtos(dDataBase-nCancExt)), dDataBase-30, dDataBase-nCancExt)
	cWhere		:= "%((SubString(SF3.F3_CFO,1,1) < '5' AND SF3.F3_FORMUL='S') OR (SubString(SF3.F3_CFO,1,1) >= '5'))"
	cRet		:= RetCodDene()
	cWhere		+= " AND ( (SF3.F3_ESPECIE <> 'SPED' AND SF3.F3_CODRSEF NOT IN ( " + cRet + ",'101','102','151','155'"

	If len(alltrim(SM0->M0_CGC)) == 14
		If (alltrim(upper(cUFEnt)) $ "MG|BA|MT")
			cWhere += " ))"
		Else
			cWhere += ",'220'))"
		EndIf
	Else
		if alltrim(upper(cUFEnt)) $ "MG|BA|MT"
			cWhere += ",'217','215'))"
		Else
			cWhere += ",'220','217','215'))"
		EndIf
	EndIf

	cWhere += " OR (SF3.F3_ESPECIE = 'SPED' AND SF3.F3_CODRSEF NOT IN ('101', '102', '110', '205', '155' "

	if len(alltrim(SM0->M0_CGC)) == 11 .and. alltrim(upper(cUFEnt)) $ "MT"
		cWhere += ",'217','215'"
	endif

	cWhere += " )))%"

	cAliasSF3 := GetNextAlias()

	BeginSql Alias cAliasSF3

		COLUMN F3_ENTRADA AS DATE
		COLUMN F3_DTCANC AS DATE
		SELECT	F3_FILIAL,F3_ENTRADA,F3_NFELETR,F3_CFO,F3_FORMUL,F3_NFISCAL,F3_SERIE,F3_CLIEFOR,F3_LOJA,F3_ESPECIE,F3_DTCANC,F3_CODRSEF
		FROM %Table:SF3% SF3
		WHERE
		SF3.F3_FILIAL = %xFilial:SF3% AND
		SF3.F3_ENTRADA >= %Exp:dDataCanc% AND
		SF3.F3_DTCANC  >= %Exp:dDataCanc% AND
		SF3.F3_DTCANC  >= %Exp:dDataCanc% AND
		SF3.F3_ESPECIE <> 'CF' AND
		SF3.F3_CODRET <> 'T' AND
		%Exp:cWhere% AND
		SF3.%notdel%
	EndSql

	While (cAliasSF3)->(!Eof())
		cModNota := AModNot((cAliasSF3)->F3_ESPECIE)
		If ( cModNota $ "55,57")

			lNFeCan:= .T.

			//tratamento para saber se a nota esta desclassificada o que impediria de transmissao
			if (cAliasSF3)->F3_CFO < "5"
				SF1->(dbSetOrder(1)) //F1_FILIAL+F1_DOC+F1_SERIE+F1_FORNECE+F1_LOJA+F1_TIPO
				SF1->(dbSeek( xFilial("SF1") + (cAliasSF3)->F3_NFISCAL + (cAliasSF3)->F3_SERIE + (cAliasSF3)->F3_CLIEFOR + (cAliasSF3)->F3_LOJA ))
				while SF1->(!EOF()) .and.;
						SF1->(F1_FILIAL + F1_DOC + F1_SERIE + F1_FORNECE + F1_LOJA) == Alltrim((cAliasSF3)->F3_FILIAL) + (cAliasSF3)->F3_NFISCAL + (cAliasSF3)->F3_SERIE + (cAliasSF3)->F3_CLIEFOR + (cAliasSF3)->F3_LOJA

					if (cAliasSF3)->F3_FORMUL == SF1->F1_FORMUL //desclassificação
						lNFeCan := .F.
					endIf

					SF1->(dbSkip())
				end
			endIf

			If lNFeCan .and. !(cUFEnt == "MT") .and. (val((cAliasSF3)->F3_SERIE)  > 919 .AND. val((cAliasSF3)->F3_SERIE) < 969) .and. (cAliasSF3)->F3_CODRSEF  <> '100'
				lNFeCan := ConNfeAut(cAliasSF3)
			Endif

			If lNFeCan
				aadd(aNotas,{})
				nX := Len(aNotas)
				aadd(aNotas[nX],IIF((cAliasSF3)->F3_CFO<"5","0","1"))
				aadd(aNotas[nX],(cAliasSF3)->F3_ENTRADA)
				aadd(aNotas[nX],(cAliasSF3)->F3_SERIE)
				aadd(aNotas[nX],(cAliasSF3)->F3_NFISCAL)
				aadd(aNotas[nX],(cAliasSF3)->F3_CLIEFOR)
				aadd(aNotas[nX],(cAliasSF3)->F3_LOJA)
				If cModNota $ "55,57"
					aadd(aNotas[nX], cModNota)
				ElseIf FunName()$"SPEDCTE,TMSA200"
					aadd(aNotas[nX],"57")
				Else
					aadd(aNotas[nX],"55")
				EndIf
				aadd(aNotas[nX], (cAliasSF3)->F3_CODRSEF)
			EndIf
		EndIf
		(cAliasSF3)->( dbSkip() )
	EndDo

	If lTrfNfeCanc .and. !Empty(aNotas)
		aNotas := ExecBlock("TRFNFECANC",.F.,.F.,{aNotas})
	Endif

	(cAliasSF3)->(dbCloseArea())
	dbSelectArea("SF3")

	// Transmite as NFe canceladas
	If lUsaColab
		oDoc := ColaboracaoDocumentos():new()
		oDoc:cModelo 	:= IIf(cModelo=="55","NFE",IIf(cModelo=="57","CTE",""))
		oDoc:cTipoMov	:= "1"
	Else
		oWs:= WsNFeSBra():New()
		oWs:cUserToken := "TOTVS"
		oWs:cID_ENT    := cIdEnt
		oWS:_URL       := AllTrim(cURL)+"/NFeSBRA.apw"
		oWs:oWsNFe:oWsNotas :=  NFESBRA_ARRAYOFNFES():New()
	EndIf

	if !lCTE .and. len(aNotas) > 0 .and. empty(SM0->M0_ESTCOB)
		aNotas := {}
		if(!isInCallStack("SpedNFeTrf"), MsgAlert(STR0819 + FWCodFil() + STR0820 + CRLF + CRLF + STR0821), ConOut("UF do Endereço Cobrança da Filial " + FWCodFil() + " não está preenchido." + CRLF + CRLF + "Acesse: SIGACFG > Ambiente > Empresas > Filial")) // STR0819 =  "UF do Endereço Cobrança da Filial ", STR0820 =  " não está preenchido.", STR0821 =  "Acesse: SIGACFG > Ambiente > Empresas > Filial"
		endif

		cUFCode := if(len(aNotas) > 0, GetUFCode(SM0->M0_ESTCOB) , "")

		For nX := 1 To Len(aNotas)
			cModelo := aNotas[nX][7]
			cXjust := ""
			nY++

			If lUsaColab
				cSerie		:= aNotas[nX][3]
				cNumero 	:= aNotas[nX][4]
				cMod		:= IIf(cModelo == "55","NFE","CTE")
				aInfXml := ColExpDoc(cSerie,cNumero,cMod)
			Else
				aadd(oWs:oWsNFe:oWsNotas:oWSNFES,NFESBRA_NFES():New())
				oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cID := aNotas[nX][3]+aNotas[nX][4]
				oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML:= cUFCode+FsDateConv(aNotas[nX][2],"YYMM")+PADL(ALLTRIM(SM0->M0_CGC),14,"0")+cModelo+StrZero(Val(aNotas[nX][3]),3)+StrZero(Val(aNotas[nX][4]),9)+cModalidade+Inverte(StrZero(Val(SubStr(aNotas[nX][4],2,8)),8))
				oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML:= 'Id="' +	oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML + Modulo11(oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML) + '"'
			EndIf

			If ( empty( allTrim(aNotas[nX][8]) )  )
				If lAviso
					cAviso := allTrim ( STR0822 + aNotas[nX][3] + " " + aNotas[nX][4]) // STR0822 =  "Motivo de inutilizacao nota "
				Else
					cAviso := allTrim (STR0823 + aNotas[nX][3] + " " + aNotas[nX][4]) // STR0823 =  "Motivo de cancelamento nota "
				EndIf
			Else
				cAviso := allTrim (STR0823 + aNotas[nX][3] + " " + aNotas[nX][4]) //STR0823 =  "Motivo de cancelamento nota "
			EndIf

			If lUsaColab

				If 	aInfXml[1]	.OR.  aInfXml[11]	/*lInutiliza-Nota na SF3-DTCANC preenchido*/
					if (aInfXml[10])/*lCancela*/ .And. (aInfXml[9]) /*lAutorizado*/
						lAviso	:= .F.
						lInutiliza	:= .F.
						lCancela	:= .T. //pode cancelar
						cAviso := allTrim ( STR0823 + aNotas[nX][3] + " " + aNotas[nX][4]) // STR0823 =  "Motivo de cancelamento nota "
						cLabelEven:= STR0824 // STR0824 =  "Cancelamento de NF-e como Evento"
					Elseif	(aInfXml[11]) .And. (aInfXml[9]) //lInutilizacao lAutorizada*/
						lInutiliza	:= .F.
						lCancela := .F.
					Elseif	(aInfXml[11]) .And. (!aInfXml[9])
						lAviso := .T.
						lInutiliza	:= .T.
						lCancela 	:= .F.
						cAviso := allTrim ( STR0822 + aNotas[nX][3] + " " + aNotas[nX][4]) // STR0822 =  "Motivo de inutilizacao nota "
						cLabelEven:= STR0825 // STR0825 =  "Inutilização de NF-e como Evento"
					EndIf

					If ((lCancela .Or. lInutiliza) .And. GetCodRet(aNotas[nX],cModelo) .And. !(aNotas[nX][8] $ "151-101-102-155") )	//Lê o F3_CODRET = T |Se Transmitido não envia|se existir o campo na SF3

						If lXjust
							While .T.
								If Aviso(cAviso, @cXjust, {STR0713, STR0677}, 3,cLabelEven,,, .T.) == 1 // STR0713 =  "Confirmar", STR0677 =  "Cancelar"
									If SpedCCeTexto(cXjust,250)
										lGera := .T.
										Exit
									Else
										Loop
									Endif
								Else
									lGera := .F. // Cancelada a operação sem justifcativa do cancelamento
									Exit
								Endif
							Enddo
						Else
							lGera := .T.
						EndIf

						If lGera
							//Adicionando no aNFe para manter o padrao das funcoes SpedCCeXml e ColEnvEvento
							aNFeCol := {}
							aAdd(aNFeCol,aInfXml[2]) 				//01 - Chave da Nfe
							aAdd(aNFeCol,"")			 				//02 - em branco
							aAdd(aNFeCol,cSerie) 					//03 - Serie
							aAdd(aNFeCol,cNumero) 					//04 - Numero

							oDoc:cNomeArq := "" 		// Limpando o nome do Arq. pois sera criado outro arquivo.

							If lCancela
								cXml := SpedCCeXml( aNFeCol , cXjust , cTpEvento , aInfXml[7] , , , , lCte )
								lArqGer := ColEnvEvento( cMod , aNFeCol , cXml , @cIdEven , @cErro )
								If lArqGer
									lCancOk  := ColAtuTrans( /*cEntSai*/ , cSerie, cNumero ,/*cCliente*/,/*cLoja*/,/*lCTe*/,/*cChvCtg*/,/*nTpEmisCte*/)
								Endif
								lCancela := .F.
							ElseIf lInutiliza
								cXml := ColInutTrans( aNFeCol , cXjust , cModelo )
								lArqGer := ColEnvEvento( cMod , aNFeCol , cXml , nil , @cErro , lInutiliza )
								If lArqGer
									lCancOk    := ColAtuTrans( /*cEntSai*/ , cSerie, cNumero ,/*cCliente*/,/*cLoja*/,/*lCTe*/,/*cChvCtg*/,/*nTpEmisCte*/)
								Endif
								lInutiliza := .F.
							EndIf
						EndIf
					endif
				EndIf
				//Monitoramento da nota
				getListBox(cIdEnt, cUrl, aInfNota, 1, cModelo, lCte,.F.,/*lMDfe*/,/*lTMS*/,lUsaColab)
			Else
				If lXjust .and. !(aNotas[nX][8] $ "151-101-102-155") .And. aNotas[nX][7] != "57"
					//-- Tratamento do Cancelamento por Evento
					If ( lNfeCancEven .And. !( Empty(AllTrim(aNotas[nX][8])) ) )
						While .T.
							If Aviso(cAviso, @cXjust, {STR0713, STR0677}, 3,STR0824,,, .T.) == 1 // STR0824 =  "Cancelamento de NF-e como Evento", STR0713 =  "Confirmar", STR0677 =  "Cancelar"
								If SpedCCeTexto(cXjust,250)
									oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML += '<xJust>xJust="'+cXjust+'"</xJust>'
									Exit
								Else
									Loop
								Endif
							Else
								Exit
							Endif
						Enddo
					Else
						If Aviso(cAviso, @cXjust, {STR0713, STR0677}, 4,,,, .T.) == 1 .And. !Empty(cXjust) // STR0713 =  "Confirmar", STR0677 =  "Cancelar"
							oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML += '<xJust>xJust="'+cXjust+'"</xJust>'
						Endif
					Endif
				ElseIf lCTeXjust .And. aNotas[nX][7] = "57"
					//-- Tratamento do Cancelamento por Evento
					If ( lNfeCancEven .And. !( Empty(AllTrim(aNotas[nX][8])) ) )
						While .T.
							If Aviso(cAviso, @cXjust, {STR0713, STR0677}, 3,"Cancelamento do CT-e como Evento",,, .T.) == 1 // STR0713 =  "Confirmar", STR0677 =  "Cancelar"
								If SpedCCeTexto(cXjust,250)
									oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML += '<xJust>xJust="'+cXjust+'"</xJust>'
									Exit
								Else
									Loop
								Endif
							Else
								Exit
							Endif
						Enddo
					Else
						If Aviso(cAviso, @cXjust, {STR0713, STR0677}, 4,,,, .T.) == 1 .And. !Empty(cXjust) // STR0713 =  "Confirmar", STR0677 =  "Cancelar"
							oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML += '<xJust>xJust="'+cXjust+'"</xJust>'
						Endif
					Endif
				ElseIf lFisCannfe
					oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML := ExecBlock("FISCANNFE",.F.,.F.,{oWs:oWsNFe:oWsNotas:oWSNFES[nY]:cXML,aNotas[nX],aNotas[nX]})
				EndIf
			EndIf

			aAdd(aNFAtu, aClone(aNotas[nX] ) )

			If nY >=50 .Or. nX == Len(aNotas)
				If !lUsaColab
					If oWs:CancelaNotas()
						oWs:= WsNFeSBra():New()
						oWs:cUserToken := "TOTVS"
						oWs:cID_ENT    := cIdEnt
						oWS:_URL       := AllTrim(cURL)+"/NFeSBRA.apw"
						oWs:oWsNFe:oWsNotas :=  NFESBRA_ARRAYOFNFES():New()
						nY := 0

						for nZ := 1 to len(aNFAtu)
							gravaCodRet( aNFAtu[nZ] ) //Grava na F3_CODRET = T |Transmitido|se existir o campo na SF3
						next nZ

						aSize(aNFAtu,0)
						aNFAtu := {}

					Else
						Aviso(STR0647,IIf(Empty(GetWscError(3)),GetWscError(1),GetWscError(3)),{STR0114},3) //"Execute o módulo de configuração do serviço, antes de utilizar esta opção!!!", STR0647 = "SPED"
					EndIf
				EndIf
			EndIf
			IF !lUsaColab
				If lCTE
					If (nParCanc := aScan(aParCanc, {|x| x[1] == aNotas[nX][3]}) ) == 0
						aAdd(aParCanc,{aNotas[nX][3],aNotas[nX][4],aNotas[nX][4]})
					Else
						aParCanc[nParCanc][2] := Iif(aParCanc[nParCanc][2] > aNotas[nX][4], aNotas[nX][4], aParCanc[nParCanc][2])
						aParCanc[nParCanc][3] := Iif(aParCanc[nParCanc][3] < aNotas[nX][4], aNotas[nX][4], aParCanc[nParCanc][3])
					EndIf
				EndIf

			EndIf

			if( lXjust .and. lNfeCanc01 )
				ExecBlock("NFECANC01",.f.,.f.,{cIdEnt, aNotas, cXjust})
			Endif

		Next nX

		RestArea(aArea)

		//Filtra as notas fiscais canceladas do dia para atribuir valores ao campo F3_CODRSEF
		if lCTE
			For nX := 1 To Len(aParCanc)
				StartJob( "getMntFaixa", GetEnvServer(), .F., cIdEnt, cUrl, aParCanc[nX], 1, cModelo, lCte,.F.,/*lMDfe*/,/*lTMS*/,lUsaColab, cEmpAnt, cFilAnt )
			Next nX
		EndIf

		aSize(aNFAtu,0)
		aNFAtu := {}

		Return(.T.)

Static Function Inverte(uCpo)

	Local cCpo	:= uCpo
	Local cRet	:= ""
	Local cByte	:= ""
	Local nAsc	:= 0
	Local nI		:= 0
	Local aChar	:= {}

	Aadd(aChar,	{"0", "9"})
	Aadd(aChar,	{"1", "8"})
	Aadd(aChar,	{"2", "7"})
	Aadd(aChar,	{"3", "6"})
	Aadd(aChar,	{"4", "5"})
	Aadd(aChar,	{"5", "4"})
	Aadd(aChar,	{"6", "3"})
	Aadd(aChar,	{"7", "2"})
	Aadd(aChar,	{"8", "1"})
	Aadd(aChar,	{"9", "0"})

	For nI:= 1 to Len(cCpo)
		cByte := Upper(Subs(cCpo,nI,1))
		If (Asc(cByte) >= 48 .And. Asc(cByte) <= 57) .Or. ;	// 0 a 9
			(Asc(cByte) >= 65 .And. Asc(cByte) <= 90) .Or. ;	// A a Z
			Empty(cByte)	// " "
			nAsc	:= Ascan(aChar,{|x| x[1] == cByte})
			If nAsc > 0
				cRet := cRet + aChar[nAsc,2]	// Funcao Inverte e chamada pelo rdmake de conversao
			EndIf
		Else
			// Caracteres <> letras e numeros: mantem o caracter
			cRet := cRet + cByte
		EndIf
	Next
Return(cRet)



