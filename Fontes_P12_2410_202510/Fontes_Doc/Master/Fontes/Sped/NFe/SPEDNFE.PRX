					Else
						cCombo1 := STR0349	//"2-Homologacao"
					EndIf
					// Versao do leiaute
					If ValType(aCfgCCe[8]) <> "U"
						cCombo2	:= aCfgCCe[8]
					Else
						cCombo2	:= "1.00"
					EndIF
					// Versao do leiaute do evento
					If ValType(aCfgCCe[7]) <> "U"
						cCombo3	:= aCfgCCe[7]
					Else
						cCombo3	:= "1.00"
					EndIF
					// Versao do evento
					If ValType(aCfgCCe[6]) <> "U"
