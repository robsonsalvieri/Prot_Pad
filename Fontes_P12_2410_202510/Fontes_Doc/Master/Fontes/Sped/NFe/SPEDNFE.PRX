/*/{Protheus.doc} SpedVldDir
Verifica se o diretório de destino para exportação do XML existe.

@author  renan.franco
@since   22/12/2020
@version 1.0
/*/
//-----------------------------------------------------------------------
function SpedVldDir(cDestino, cArquivo)

	local lOk		:= .T.
	local cDrive	:= ""
	local cNome		:= ""
	local cExt		:= ""

	Default cDestino	:= ""
	default cArquivo	:= MV_PAR04

	SplitPath(cArquivo,@cDrive,@cDestino,@cNome,@cExt)
	cDestino := cDrive+cDestino

	If !(lOk := (ExistDir(cDestino,,.T.)) )
		dirModal(cDestino)
		cArquivo := ""
	EndIf

return lOk

static function dirModal(cDestino)

	Local oContainer, oMemo, oModal := FWDialogModal():New()
	Local cTexto := STR0631+CRLF+CRLF+cDestino+CRLF+CRLF+STR0632

	oModal:SetEscClose(.T.)
	oModal:setTitle(STR0126)
	oModal:setSize(133.5, 155) // Altura x Largura

	oModal:createDialog()
	oModal:addCloseButton(nil, "Fechar")
	oContainer := TPanel():New( ,,, oModal:getPanelMain())
	oContainer:Align := CONTROL_ALIGN_ALLCLIENT

	@ 5,5 GET oMemo VAR cTexto MEMO HSCROLL SIZE 145,103.5 READONLY NO BORDER MULTILINE OF oContainer PIXEL

	oMemo:lWordWrap := .T.
	oMemo:EnableVScroll(.t.)
	oMemo:EnableHScroll(.t.)
	oModal:Activate()

return

