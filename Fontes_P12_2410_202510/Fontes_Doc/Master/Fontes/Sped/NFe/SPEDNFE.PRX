/*/{Protheus.doc} GetXmlInu
	Faz a Leitura do XML de Inutilização
	@type  Static Function
	@author Ricardo Henrique de Mello Lima
	@since 08/08/2024
	@version 12.1.2310
	@param cXml, character, String do XML da Sefaz
	@return cCodRet, character, Codigo do retorno da Inutilizacao do Manual NFe 4.01
/*/
Static Function GetXmlInu(cXml)

	Local cCodRet := ""
	Local oXmlUnit := nil

	if Alltrim(cXml) != ""
		cXml := EncodeUtf8(cXml)
		oXmlUnit := TXMLManager():new()
		if oXmlUnit:Parse(cXml)
			oXmlUnit:xPathRegisterNs("ns","http://www.portalfiscal.inf.br/nfe")
			cCodret := Alltrim(oXmlUnit:XPathGetNodeValue("/ns:retInutNFe/ns:infInut/ns:cStat"))
		endif
	endif

	FreeObj(oXmlUnit)

Return cCodRet

/*/{Protheus.doc} SpedGerChv
	Gera Chave da Nota Fiscal 
	@type  Function
	@author Ricardo Henrique de Mello Lima
	@since 21/09/2024
	@version 12.1.2310	
	/*/
Function SpedGerChv(aNota,aUF,cNumNf)

	Local cChave 		:= ""
	local cUFFil 		:= allTrim(SM0->M0_ESTENT)
	local cCgc			:= alltrim(SM0->M0_CGC)
	Local aModalidade 	:= SpedModTpEmis()

	// A UF no TSS (para Protheus) é pega do mesmo local do XML: <emit><enderEmit><uf>
	if !GetNewPar("MV_SPEDEND",.F.)
		cUFFil := allTrim(SM0->M0_ESTCOB)
	endIf
	cUFFil := upper(cUFFil)

	/*
					UF	+ AAMM	+ CNPJ	+ Modelo + Série + nNF	+ tpEmis + cNF	+ DV
	Qtd caracteres:	02	+ 04	+ 14	+ 02	 + 03	 + 09	+ 01	 + 08	+ 01
	*/
	cChave := aUF[aScan(aUF,{|x| x[1] == cUFFil})][02]
	cChave += FsDateConv(aNota[03],"YYMM")
	cChave += replic("0",14-len(cCgc)) + cCgc
	cChave += "55"
	cChave += StrZero(Val(aNota[01]),3)
	cChave += StrZero(Val(aNota[02]),9)
	cChave += aModalidade[1]
	cChave += strZero(val(cNumNf),8)
	cChave += Modulo11(cChave)

	aModalidade := fwFreeArray(aModalidade)

Return cChave

/*/{Protheus.doc} SpedGrvChv
	Grava a Chave gerada na Tabela SF1 e ou SF2 
	@type  Static Function
	@author Ricardo Henrique de Mello Lima
	@since 26/09/2024
	@version 12.1.2310
/*/
Static Function SpedGrvChv(cAlias , cChaveNota)

	if !empty(cChaveNota) .AND. Len(cChaveNota) == 44
		if cAlias == "SF2" .AND. Alltrim(SF2->F2_CHVNFE) != Alltrim(cChaveNota)
			if empty(SF2->F2_CHVNFE) .or.;
					SubStr(Alltrim(SF2->F2_CHVNFE),35,1) != SubStr(Alltrim(cChaveNota),35,1) .or.; //troca de modalidade
				SubStr(Alltrim(SF2->F2_CHVNFE),1,2) != SubStr(Alltrim(cChaveNota),1,2) //troca de UF
				RecLock("SF2",.F.)
				SF2->F2_CHVNFE := cChaveNota
				SF2->(MsUnlock())
			endif
		elseif cAlias == "SF1" .AND. Alltrim(SF1->F1_CHVNFE) != Alltrim(cChaveNota)
			if empty(SF1->F1_CHVNFE) .or. ;
					SubStr(Alltrim(SF1->F1_CHVNFE),35,1) != SubStr(Alltrim(cChaveNota),35,1) .or.; //troca de modalidade
				SubStr(Alltrim(SF1->F1_CHVNFE),1,2) != SubStr(Alltrim(cChaveNota),1,2) //troca de UF
				RecLock("SF1",.F.)
				SF1->F1_CHVNFE := cChaveNota
				SF1->(MsUnlock())
			endif
		endif
	endif
Return

/*/{Protheus.doc} SpedModTpEmis
	Tratamento para pegar o tipo de emissao tPEmis correto conforme selecionado nos paramentros da NFE
	@type  Static Function
	@author Ricardo Henrique de Mello Lima
	@since 14/10/2024
	@version 12.1.2310	
/*/
Static Function SpedModTpEmis()

	Local aRet := ""
	Local cModal := Substr(getCfgModalidade(),1,1)
	Local lCont := .F.
	Local cRet := ""

	Do case
	Case cModal =="1"	//Normal
		cRet := "1"

	Case cModal == "2"	//Contingencia Off-Line
		cRet := "2"
		lCont:= .T.

	Case cModal == "4"	//Ambiente Nacional
		cRet := "1"

	Case cModal == "5"	//DPEC
		cRet := "4"
		lCont:= .T.

	Case cModal == "6"	//SEFAZ Virtual
		cRet := "1"

	Case cModal == "7"	//Contingencia FS-DA
		cRet := "5"
		lCont:= .T.

	Case cModal == "8"	//Contingencia SVC-AN
		cRet := "6"
		lCont:= .T.

	Case cModal == "9"	//Contingencia SVC-RS
		cRet := "7"
		lCont:= .T.
	EndCase

	aRet := {cRet,lCont}
Return aRet

/*/{Protheus.doc} setAdvplFilter
Responsavel por montar as condicoes em ADVPL para filtra da tela de Transmissao e monitoramento de NF-e
@type function
@version 12.1.2410
@author fs.martinez
@since 10/21/2024
@param cTable, character, nome da tabela SF1 ou SF2
@param lSdoc, logical, Se utiliza o SDOC
@return character, retorno com as condicoes montadas para filtro ADVPL
/*/
static function setAdvplFilter(cTable, lSdoc)
	local cCondicao	:= ""
	local cField	:= substr(cTable,2)

	cCondicao := cField + "_FILIAL=='"+xFilial(cTable)+"' "

	if cTable == "SF1"
		cCondicao += ".AND. F1_FORMUL=='S' "
	endIf

	If !Empty(MV_PAR03)
		If lSdoc
			cCondicao += ".AND. " + cField + "_SDOC== '"+MV_PAR03+"' "
		Else
			cCondicao += ".AND. " + cField + "_SERIE== '"+MV_PAR03+"' "
		EndIf
	EndIf

	If SubStr(MV_PAR02,1,1) == "1" //"1-NF Autorizada"
		cCondicao += ".AND. " + cField + "_FIMP$'S' "
	ElseIf SubStr(MV_PAR02,1,1) == "3" //"3-Não Autorizadas"
		cCondicao += ".AND. " + cField + "_FIMP$'N' "
	ElseIf SubStr(MV_PAR02,1,1) == "4" //"4-Transmitidas"
		cCondicao += ".AND. " + cField + "_FIMP$'T' "
	ElseIf SubStr(MV_PAR02,1,1) == "5" //"5-Não Transmitidas"
		cCondicao += ".AND. " + cField + "_FIMP$' ' "
	EndIf

return cCondicao

/*/{Protheus.doc} setSQlFilter
Responsavel por montar as condicoes em SQL para filtra da tela de Transmissao e monitoramento de NF-e
@type function
@version 12.1.2410
@author fs.martinez
@since 10/21/2024
@param cTable, character, nome da tabela SF1 ou SF2
@param lSdoc, logical, Se utiliza o SDOC
@return character, retorno com as condicoes montadas para filtro SQL
/*/
static function setSQlFilter(cTable, lSdoc)
	local cCondicao	:= ""
	local cField	:= substr(cTable,2)

	cCondicao := cField + "_FILIAL = '"+xFilial(cTable)+"' "

	if cTable == "SF1"
		cCondicao += "AND F1_FORMUL = 'S' "
	endIf

	If !Empty(MV_PAR03)
		If lSdoc
			cCondicao += "AND " + cField + "_SDOC = '"+MV_PAR03+"' "
		Else
			cCondicao += "AND " + cField + "_SERIE = '"+MV_PAR03+"' "
		EndIf
	EndIf

	If SubStr(MV_PAR02,1,1) == "1" //"1-NF Autorizada"
		cCondicao += "AND " + cField + "_FIMP = 'S' "
	ElseIf SubStr(MV_PAR02,1,1) == "3" //"3-Não Autorizadas"
		cCondicao += "AND " + cField + "_FIMP = 'N' "
	ElseIf SubStr(MV_PAR02,1,1) == "4" //"4-Transmitidas"
		cCondicao += "AND " + cField + "_FIMP = 'T' "
	ElseIf SubStr(MV_PAR02,1,1) == "5" //"5-Não Transmitidas"
		cCondicao += "AND " + cField + "_FIMP = ' ' "
	EndIf

	if !empty(MV_PAR04)
		cCondicao += "AND  " + cField + "_EMISSAO >= '"+dtos(MV_PAR04)+"' "
	endIf
	if !empty(MV_PAR05)
		cCondicao += "AND " + cField + "_EMISSAO <= '"+dtos(MV_PAR05)+"' "
	endIf

return cCondicao

//--------------------------------------------------------------------
/*/{Protheus.doc} VldRegra
Valida se a Regra de Tributação presentte no parâmetro  
@type		function
@version	12.1.2410
@author		Rodrigo.Pirolo
@since		19/03/2025
@param		cIDTRIB,	character, codigo da Regra de Tributação presente no MV_INUTPRO
			cCliente,	character, codigo do Produto presente no MV_INUTCLI
			cProduto,	character, codigo do Produto presente no MV_INUTRCT

@return logical,	Indica se a Regra de Tributação está de acordo 
					com o processo de Inutilização
/*/
//--------------------------------------------------------------------

Static Function VldRegra( cIDTRIB, cCliente, cProduto )

	Local cQuery	:= ""
	Local cAlias	:= GetNextAlias()
	Local lRet		:= .F.

	Default cIDTRIB		:= ""
	Default cCliente	:= ""
	Default cProduto	:= ""

	If  _oQryInu == Nil

		cQuery := " SELECT F2B_REGRA "
		cQuery += " FROM " + RetSqlName('F2B') + " F2B "
		cQuery += " INNER JOIN " + RetSqlName('F22') + " F22 ON F22.F22_FILIAL = ? AND F22.F22_CODIGO = F2B.F2B_PERFPA AND F22.D_E_L_E_T_ = ' ' "
		cQuery += " INNER JOIN " + RetSqlName('F24') + " F24 ON F24.F24_FILIAL = ? AND F24.F24_CODIGO = F2B.F2B_PERFPR AND F24.D_E_L_E_T_ = ' ' "
		cQuery += " INNER JOIN " + RetSqlName('CJ2') + " CJ2 ON CJ2.CJ2_FILIAL = ? AND CJ2.CJ2_CODIGO = F2B.F2B_CODESC AND CJ2.CJ2_ALTERA = '2' "
		cQuery += 											" AND CJ2.CJ2_INCIDE = '3' AND CJ2.CJ2_STOTNF = '1' AND CJ2.D_E_L_E_T_ = ' ' "
		cQuery += " WHERE F2B_FILIAL = ? "
		cQuery += 		" AND F2B.F2B_REGRA = ? "
		cQuery += 		" AND F22.F22_CLIFOR = ? "
		cQuery += 		" AND F24.F24_CDPROD = ? "
		cQuery += 		" AND ( "
		cQuery += 				" ( ? >= F2B.F2B_VIGINI  AND F2B.F2B_VIGFIM = ' ' ) OR "
		cQuery += 				" ( ? >= F2B.F2B_VIGINI  AND ? <= F2B.F2B_VIGFIM ) "
		cQuery += 		" ) "
		cQuery += 		" AND F2B.D_E_L_E_T_ = ' ' "

		cQuery := ChangeQuery(cQuery)

		_oQryInu:= FWPreparedStatement():New()
		_oQryInu:SetQuery(cQuery)

	EndIf

	_oQryInu:SetString( 1, FWxFilial("F22")	)
	_oQryInu:SetString( 2, FWxFilial("F24")	)
	_oQryInu:SetString( 3, FWxFilial("CJ2")	)
	_oQryInu:SetString( 4, FwxFilial("F2B")	)
	_oQryInu:SetString( 5, cIDTRIB			)
	_oQryInu:SetString( 6, cCliente			)
	_oQryInu:SetString( 7, cProduto			)
	_oQryInu:SetString( 8, DToS(dDataBase)	)
	_oQryInu:SetString( 9, DToS(dDataBase)	)
	_oQryInu:SetString( 10, DToS(dDataBase)	)

	cQuery  := _oQryInu:GetFixQuery()

	cAlias := MPSYSOpenQuery( cQuery, cAlias )

	If (cAlias)->(!EOF())
		lRet := .T.
	EndIf

	(cAlias)->(DbCloseArea())

Return lRet

