Function SPEDNFe()

	Local aArea     := GetArea()
	Local lRetorno  := .T.
	Local nVezes    := 0
	cTipoNfe := ""

	PRIVATE lBtnFiltro:= .F.

	if(!accessPD())
		return
	endif

	aGlbCache[1] := .T. //atualiza cache de variavel global de contingencia automatica?

	If UPPER(SM0->M0_ESTCOB) == "RJ" .And.;
			(ExistFunc("STBHomolPaf") .And. STBHomolPaf()) .And.;
			(SuperGetMV("MV_LJPAFEC",,.F.) .Or. (ExistFunc("LjNfPafEcf") .And. LjNfPafEcf(SM0->M0_CGC)))

		MsgAlert(STR0784) // STR0784 = "Função não permitida para este estado"
		lRetorno := .F.
	EndIf

	While lRetorno
		lBtnFiltro:= .F.
		lRetorno := SpedNfe2(nVezes==0)
		nVezes++
		If !lBtnFiltro
			Exit
		EndIf
	EndDo
	RestArea(aArea)
Return Nil

Static Function SPEDNFe2(lInit,cAlias)

	Local aCores        := {}
	Local aIndArq       := {}
	Local aPerg         := {}
	Local aPerg2        := {}
	Local cEvento		:= ""
	Local cParamEven	:= SM0->M0_CODIGO+SM0->M0_CODFIL
	Local cUF			:= Upper(Left(LTrim(SM0->M0_ESTENT),2))
	Local cUseAlias		:= "SF2"
	local cError		:= ""
	Local lEntAtiva     := .T.
	Local lEvento		:= .F.
	Local lRetorno      := .T.
	Local lVerPerg 		:= .T.
	local lUseOldFilter	:= .F.
	Local cGrupo		:= FWGrpCompany()		//Retorna o grupo
	Local cFil 			:= FWCodFil()			//Retorna o código da filial
	Local cCondAgro	    := ""
	Local aCoresAux		:= {}
	Local oSpedCFNfe    := nil
	Local oSpedISNfe    := nil
	Local oSpedNfCom    := nil
	local cTipoAtor		:= "1"

	Private lSdoc     	:= TamSx3("F2_SERIE")[1] == 14
	Private aFilBrw     := {}
	Private aPergEvento := {}
	Private aRotina     := {}
	Private bFiltraBrw
	Private lUsaColab	:= UsaColaboracao("1")
	Private cUSACOLAB	:= GetNewPar("MV_SPEDCOL","N")
	Private cDOCSCOL	:= GetNewPar("MV_DOCSCOL","")

	Private cCondicao  	:= ""
	Private cCondQry   	:= ""
	Private cIdEnt		:= ""
	Private cCadastro  	:= ""
	Private cVersaoTSS	:= ""

	if lUsaColab .And. AliasIndic("CKQ") .And. AliasIndic("CKO") .And. AliasIndic("CKP") .And. !(RetSqlName("CKO") == "CKOCOL")
		MsgInfo(STR0785) //STR0785 = "A tabela CKO deve ser única para todas as empresas do sistema, desta forma o campo X2_ARQUIVO na SX2 deve estar preenchido obrigatoriamentecom o conteúdo CKOCOL, para que a importação de arquivos funcione corretamente."
	endif

	if !lUsaColab
		cIdEnt := GetIdEnt()
		cVersaoTSS	:= getVersaoTSS()
		if Upper(cUSACOLAB) $ "S" .And. ("0" $ cDOCSCOL .Or. "1" $ cDOCSCOL)
			cCadastro	:= 	STR0001 + STR0786 + STR0787 + cIdEnt + STR0788 + cVersaoTSS // STR0786 = " - TOTVS Colaboração 1.0 ",  STR0787 = " - Entidade : ", STR0788 = " - TSS: "
		Else
			cCadastro  	:= STR0001 + STR0787 + cIdEnt+ STR0788 + cVersaoTSS  //"Monitoramento da NFe-SEFAZ", STR0787 = " - Entidade : ", STR0788 = " - TSS: "
		Endif
	else
		cCadastro	:= 	STR0001 + STR0789 // STR0789 = " - TOTVS Colaboração 2.0"
	endif

	if _lPEFilNFE == nil
		_lPEFilNFE := ExistBlock("FILSPEDNFE")
	endif

	if( !lUsaColab )

		//Obtem a versao do TSS - Totvs Services SPED
		cVersaoTSS := getVersaoTSS(@cError)

		lOk := empty(cError)

	else
		cVersaoTSS := STR0790 // STR0790 = "Colab2.0"
	endif

	aRotina := MenuDef(,cVersaoTSS)


	// Serviço a ser utilizado - Nfe/Comprovante de Entrega/CCe/RGS/ECONF 
	if existfunc("SPEDCMPNFE") .and. !lUsaColab .and. FindClass("backoffice.fiscal.tss.nfe.econf.NfeEconf") .and. FindClass("backoffice.fiscal.tss.nfe.insucesso.NfeInsucesso") .and. existfunc("SPEDATORINT")
		aadd(aPerg2,{2,STR0008,STR0791,{STR0792,STR0884,STR0793,STR0794,STR1030,STR1031,STR1032},120,".T.",.T.,".T."}) // STR0791 = "1-NFe      ", STR0792 = "1-NFe", STR0793 = "3-CCe", STR0794 = "4-Reg Saida" STR0884 = "2-Comprovante de Entrega da NF-e" STR1030 "5-ECONF" STR1031 6-Insucesso na entrega da NFe STR1032 7-Ator Interessado
	else
		aadd(aPerg2,{2,STR0008,STR0791,{STR0792,STR0793,STR0794,STR1030},120,".T.",.T.,".T."}) // STR0791 = "1-NFe      ", STR0792 = "1-NFe", STR0793 = "3-CCe", STR0794 = "4-Reg Saida"
	endif
	
	//Adicionando NFCOM - "8-NFCom"
	if FindClass("backoffice.fiscal.tss.nfe.nfcom.NFCom") .and. existFunc("getCfgNFCom") .and. len(aPerg2) > 0 .and. len(aTail(aPerg2)[4]) == 7 //STR1037 = "8-NFCom"
		aAdd(aTail(aPerg2)[4],STR1037)
	endIf

	lVerPerg := .T.

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ 
//³                        Agroindustria                          ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	If FindFunction("OGXUtlOrig") //Encontra a função
		If OGXUtlOrig() // Retorna se tem integração com Agro/originação modulo 67
			If FindFunction("AGRXPERGROM")
				lVerPerg := AGRXPERGROM()
			EndIf
		EndIf
	Endif

	If lVerPerg
		If !ParamBox(aPerg2,STR0223,,,,,,,,"TOTVSSPED",.T.,)//"TOTVS Services SPED - NFe/CCe/Reg. Saida/NFCom
			Return Nil
		EndIf
	EndIf


//Verifica se o serviço foi configurado - Somente o Adm pode configurar
	If lInit .And. !lUsaColab
		If (!IsReady() .Or. !IsReady(,2))
			If PswAdmin( /*cUser*/, /*cPsw*/,RetCodUsr()) == 0
				SpedNFeCFG()
			Else
				HelProg(,"FISTRFNFe")
			EndIf
		EndIf
		lEntAtiva := SpedEntAtiv()
	EndIf


	If Substr(MV_PAR01,1,1) == "1"	//NFe
		aRotina := MenuDef("1")

		If lUsaColab .Or. ( lEntAtiva .And. (!lInit .Or. IsReady()) )

			lVerPerg := .T.

			//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
			//³                        Agroindustria                          ³
			//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
			If FindFunction("OGXUtlOrig") //Encontra a função
				If OGXUtlOrig() // Retorna se tem integração com Agro/originação modulo 67
					If FindFunction("AGRFILNFE")
						cCondAgro +=  AGRFILNFE()

						If !Empty(cCondAgro)
							lVerPerg := .F.
						EndIf
					EndIf
				EndIf
			Endif

			lUseOldFilter := (ExistBlock("FISFILNFE") .and. !_lPEFilNFE) .or. (OGXUtlOrig() .and. !Empty(cCondAgro))
			If lVerPerg
				//Montagem das perguntas
				aPerg := {	{2,STR0075,space(9),{STR0076,STR0077},120,".T.",.T.,".T."},; //"Tipo de NFe"###"1-Saída"###"2-Entrada"
				{2,STR0082,space(18),{STR0084,STR0083,STR0110,STR0111,STR0112},120,".T.",.T.,".T."},; //"Filtra"###"1-Autorizadas"###"2-Sem filtro"###"3-Não Autorizadas"###"4-Transmitidas"###"5-Não Transmitidas"
				{1,STR0010,space(Len(If(lSdoc,SF2->F2_SDOC,SF2->F2_SERIE))),"",".T.","",".T.",30,.F.},; //"Serie da Nota Fiscal"
				{1,STR0826,ctoD(" "),"@R 99/99/9999",".T.","",cValToChar(!lUseOldFilter),50,.F.,lUseOldFilter},; //STR0826 =  "Dt. Emissão De"
				{1,STR0827,ctoD(" "),"@R 99/99/9999",".T.","",cValToChar(!lUseOldFilter),50,.F.,lUseOldFilter} } ///STR0827 =  "Dt. Emissão Até"
				If !ParamBox(aPerg,"SPED - NFe",,,,,,,,"SPEDNFE"+cGrupo+cFil,.T.,.F.)
					lRetorno := .F.
				EndIf
			EndIf

			If lRetorno
				cTipoNfe	:= If(Substr(MV_PAR01,1,1) == "2","ENTRADA","SAIDA")
				If SubStr(MV_PAR01,1,1) == "1" // nota de saída
					aCores    := {{"F2_FIMP==' ' .AND. AllTrim(F2_ESPECIE)=='SPED'",'DISABLE' },;	//NF não transmitida
					{"F2_FIMP=='S'",'ENABLE'},;									//NF Autorizada
					{"F2_FIMP=='T'",'BR_AZUL'},;									//NF Transmitida
					{"F2_FIMP=='D'",'BR_CINZA'},;									//NF Uso Denegado
					{	"F2_FIMP=='N'",'BR_PRETO'}}									//NF nao autorizada

					If ExistBlock("FISLEGNFE")
						aCoresAux := ExecBlock("FISLEGNFE", .F., .F., {aCores, cTipoNfe})
						If ValType(aCoresAux) == "A"
							aCores := aCoresAux
						EndIf
					EndIf

					//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
					//³Verifica se vai utilizar o filtro mbrowse ou o filtro recebido                        ³
					// ExistBlock("FISFILNFE") - Ponto de entrada que retorna um filtro                      ³
					// FindFunction("AGRFILNFE") - Integração com agro/originação modulo 67, retorna filtro  ³
					//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
					IF lUseOldFilter

						cCondicao := setAdvplFilter("SF2", lSdoc)

						If ExistBlock("FISFILNFE")
							cCondicao := ExecBlock("FISFILNFE",.F.,.F.)
						EndIf

						lVerPerg := .T.

						//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
						//³                        Agroindustria                          ³
						//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
						If FindFunction("OGXUtlOrig") //Encontra a função
							If OGXUtlOrig() // Retorna se tem integração com Agro/originação modulo 67
								If FindFunction("AGRFILNFE")
									cCondicao +=  cCondAgro
								EndIf
							EndIf
						Endif

						aFilBrw		:=	{'SF2',cCondicao}
						bFiltraBrw := {|| FilBrowse("SF2",@aIndArq,@cCondicao) }
						Eval(bFiltraBrw)
						//Acrescenta mensagem para o usuario
						//MensPadrao()
						mBrowse( 6, 1,22,75,"SF2",,,,,,aCores,/*cTopFun*/,/*cBotFun*/,/*nFreeze*/,/*bParBloco*/,/*lNoTopFilter*/,.F.,.F.,,,,,,{|oBrowse| oBrowse:SetMainProc("SPEDNFE")})
						//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
						//³Restaura a integridade da rotina                                ³
						//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
						dbSelectArea("SF2")
						RetIndex("SF2")
						dbClearFilter()
						aEval(aIndArq,{|x| Ferase(x[1]+OrdBagExt())})
					else
						//Realiza a filtragem dos parâmetros e compatibiliza com a filtragem via mbrowse()
						cCondicao := setSQlFilter("SF2", lSdoc)

						if _lPEFilNFE
							cCondicao := ExecBlock("FILSPEDNFE", .F., .F., {Substr(MV_PAR01,1,1), cCondicao})
						endif

						lVerPerg := .T.

						//Acrescenta mensagem para o usuario
						//MensPadrao()
						mBrowse( 6, 1,22,75,"SF2",,,,,,aCores,/*cTopFun*/,/*cBotFun*/,/*nFreeze*/,/*bParBloco*/,/*lNoTopFilter*/,.F.,.F.,cCondicao,,,,,{|oBrowse| oBrowse:SetMainProc("SPEDNFE")})
						//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
						//³Restaura a integridade da rotina                                ³
						//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
						dbSelectArea("SF2")
						RetIndex("SF2")
					endif


				Else // nota de entrada
					If SF1->(FieldPos("F1_FIMP"))>0
						aCores    := {{"F1_FIMP==' ' .AND. AllTrim(F1_ESPECIE)=='SPED'",'DISABLE' },;	//NF não transmitida
						{"F1_FIMP=='S'",'ENABLE'},;									//NF Autorizada
						{"F1_FIMP=='T'",'BR_AZUL'},;									//NF Transmitida
						{"F1_FIMP=='D'",'BR_CINZA'},;									//NF Uso Denegado
						{"F1_FIMP=='N'",'BR_PRETO'}}									//NF nao autorizada

						If ExistBlock("FISLEGNFE")
							aCoresAux := ExecBlock("FISLEGNFE", .F., .F., {aCores, cTipoNfe})
							If ValType(aCoresAux) == "A"
								aCores := aCoresAux
							EndIf
						EndIf
					Else
						aCores := Nil
					EndIf

					nPosInut := aScan( aRotina, { |X| if( !valtype(X[2])=="C", .F. , alltrim(upper(X[2])) == "SPEDNFEINUT") })
					if nPosInut > 0
						aRotina[nPosInut] := {"", ""		,0,2,0 ,NIL}
					endif

					//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
					//³Verifica se vai utilizar o filtro mbrowse ou o filtro recebido                        ³
					// ExistBlock("FISFILNFE") - Ponto de entrada que retorna um filtro                      ³
					// FindFunction("AGRFILNFE") - Integração com agro/originação modulo 67, retorna filtro  ³
					//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
					IF lUseOldFilter

						//Realiza a Filtragem
						cCondicao := setAdvplFilter("SF1", lSdoc)

						If ExistBlock("FISFILNFE")
							cCondicao := ExecBlock("FISFILNFE",.F.,.F.)
						EndIf

						//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
						//³                        Agroindustria                          ³
						//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
						If FindFunction("OGXUtlOrig") //Encontra a função
							If OGXUtlOrig() // Retorna se tem integração com Agro/originação modulo 67
								If FindFunction("AGRFILNFE")
									cCondicao +=  cCondAgro
								EndIf
							EndIf
						Endif

						aFilBrw		:=	{'SF1',cCondicao}
						bFiltraBrw := {|| FilBrowse("SF1",@aIndArq,@cCondicao) }
						Eval(bFiltraBrw)
						//Acrescenta mensagem para o usuario
						//MensPadrao()
						mBrowse( 6, 1,22,75,"SF1",,,,,,aCores,/*cTopFun*/,/*cBotFun*/,/*nFreeze*/,/*bParBloco*/,/*lNoTopFilter*/,.F.,.F.,,,,,,{|oBrowse| oBrowse:SetMainProc("SPEDNFE")})
						//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
						//³Restaura a integridade da rotina                                ³
						//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
						dbSelectArea("SF1")
						RetIndex("SF1")
						dbClearFilter()
						aEval(aIndArq,{|x| Ferase(x[1]+OrdBagExt())})
					else
						//Realiza a filtragem dos parâmetros e compatibiliza com a filtragem via mbrowse()
						cCondicao := setSQlFilter("SF1", lSdoc)

						if _lPEFilNFE
							cCondicao := ExecBlock("FILSPEDNFE", .F., .F., {Substr(MV_PAR01,1,1), cCondicao})
						endif

						//Acrescenta mensagem para o usuario
						//MensPadrao()
						mBrowse( 6, 1,22,75,"SF1",,,,,,aCores,/*cTopFun*/,/*cBotFun*/,/*nFreeze*/,/*bParBloco*/,/*lNoTopFilter*/,.F.,.F.,cCondicao,,,,,{|oBrowse| oBrowse:SetMainProc("SPEDNFE")})
						//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
						//³Restaura a integridade da rotina                                ³
						//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
						dbSelectArea("SF1")
						RetIndex("SF1")
						cTipoNFE := ""
					endif
				EndIf
			EndIf
		Else
			HelProg(,"FISTRFNFe")
			lRetorno := .F.
		EndIf

	ElseIf Substr(MV_PAR01,1,1) == "2" // Evento Comprovante de entrega da NFe

		SPEDCMPNFE( cIdEnt, cVersaoTSS, lUsaColab)

	ElseIf Substr(MV_PAR01,1,1) $ "3|4"	// CCe-Registro de saida

		cUseAlias := If(Substr(MV_PAR01,1,1) == "2","SF1","SF2")//Verifica Se é entrada

		If Substr(MV_PAR01,1,1) == "3"	//CCe
			if lUsaColab
				cCadastro	:= STR0344 + STR0789 //"Monitoramento da CCe-SEFAZ", STR0789 = " - TOTVS Colaboração 2.0"
			else
				cCadastro	:= STR0344 + STR0787 + cIdEnt		//"Monitoramento da CCe-SEFAZ", STR0787 = " - Entidade : "
			endif
			aRotina		:= MenuDef("3")

			cParamEven += "SPEDCCE"

			lEvento:=SF2->(FieldPos("F2_EVENFLG")) > 0 .And. SF2->(FieldPos("F2_IDCCE")) > 0 .And. SF1->(FieldPos("F1_EVENFLG")) > 0 .And. SF1->(FieldPos("F1_IDCCE")) > 0

			aadd(aPergEvento,{2,STR0075,PadR("",Len("2-Entrada")),{STR0076,STR0077},120,".T.",.T.,".T."}) //"Tipo de NFe"###"1-Saída"###"2-Entrada
			aadd(aPergEvento,{1,STR0010,PadR("",Len(If(lSdoc,SF2->F2_SDOC,SF2->F2_SERIE))),"",".T.","",".T.",30,.F.})	//"Serie da Nota Fiscal"

			cEvento:="3"
		ElseIf Substr(MV_PAR01,1,1) == "4"	//Regsitro de Saida

			cCadastro	:= STR0388 + STR0787 + cIdEnt//"Monitoramento do Registro de Saída", STR0787 = " - Entidade : "

			aRotina		:= MenuDef("4")

			cParamEven += "SPEDREGS"

			lEvento:=SF2->(FieldPos("F2_FLAGRGS")) > 0 .And. SF2->(FieldPos("F2_IDRGS")) > 0 .And. SF2->(FieldPos("F2_CODRGS")) > 0 .And. SF1->(FieldPos("F1_FLAGRGS")) > 0 .And. SF1->(FieldPos("F1_IDRGS")) > 0  .And. SF1->(FieldPos("F1_CODRGS")) > 0

			aadd(aPergEvento,{2,STR0075,PadR("",Len("2-Entrada")),{STR0076,STR0077},120,".T.",.T.,".T."}) //"Tipo de NFe"###"1-Saída"###"2-Entrada
			aadd(aPergEvento,{1,STR0010,PadR("",TAMSX3(If(lSdoc,Substr(cUseAlias,2)+"_SDOC",Substr(cUseAlias,2)+"_SERIE"))[1]),"",".T.","",".T.",30,.F.})	//"Serie da Nota Fiscal"

			cEvento:="4"
		EndIf

		If lEvento

			If ( lUsaColab .And. cEvento == "3" ) .Or. ( lEntAtiva .And. (!lInit .Or. IsReady()) )
				If ParamBox(aPergEvento,If(cEvento == "4",STR0392,STR0345),,,,,,,,cParamEven,.T.,)

					cTipoNfe	:= If(Substr(MV_PAR01,1,1) == "2","ENTRADA","SAIDA")
					cUseAlias	:= If(Substr(MV_PAR01,1,1) == "2","SF1","SF2")
					cCondicao	:= Substr(cUseAlias,2)+"_FILIAL=='"+xFilial(cUseAlias)+"' .And. "+Substr(cUseAlias,2)+"_FIMP$'S' .And. !Empty("+Substr(cUseAlias,2)+"_CHVNFE) "
					cCondQry	+= Substr(cUseAlias,2)+"_FILIAL='"+xFilial(cUseAlias)+"'  AND "+Substr(cUseAlias,2)+"_FIMP='S' AND "+Substr(cUseAlias,2)+"_CHVNFE <> ' '"

					if !empty(MV_PAR02)
						If lSdoc
							cCondicao	+= " .And. "+Substr(cUseAlias,2)+"_SDOC=='"+MV_PAR02+"'"
							cCondQry	+= " AND "  +Substr(cUseAlias,2)+"_SDOC='"+MV_PAR02+"'"
						Else
							cCondicao	+= " .And. "+Substr(cUseAlias,2)+"_SERIE=='"+MV_PAR02+"'"
							cCondQry	+= " AND "  +Substr(cUseAlias,2)+"_SERIE='"+MV_PAR02+"'"
						EndIF
					endIf

					If cEvento == "3"
						aCores := {	{Substr(cUseAlias,2)+"_EVENFLG == '1'",'ENABLE'},;	//NF vinculada
						{Substr(cUseAlias,2)+"_EVENFLG $ ' 2'",'BR_CINZA'}}	//NF nao vinculada

						mBrowse( 6, 1,22,75,cUseAlias,,,,,,aCores,/*cTopFun*/,/*cBotFun*/,/*nFreeze*/,/*bParBloco*/,/*lNoTopFilter*/,.F.,.F.,cCondQry,,,,,{|oBrowse| oBrowse:SetMainProc("SPEDNFE")})
					ElseIf 	cEvento == "4"

						aCores := {	{Substr(cUseAlias,2)+"_CODRGS =='" +Space(TAMSX3(Substr(cUseAlias,2)+"_CODRGS")[1])+"'",'DISABLE'},;	//NFe nao vinculada
						{Substr(cUseAlias,2)+"_CODRGS == 'T'","BR_AZUL"		},;	//Evento transmitido
						{Substr(cUseAlias,2)+"_CODRGS == 'N'","BR_CINZA"	},; //Evento Rejeitado
						{Substr(cUseAlias,2)+"_CODRGS == 'S'","ENABLE"		},; //NFe vinculada
						{Substr(cUseAlias,2)+"_CODRGS == 'M'","BR_LARANJA"	},; //Cancelamento transmitido
						{Substr(cUseAlias,2)+"_CODRGS == 'R'","BR_PRETO"	},; //Cancelamento rejeitado
						{Substr(cUseAlias,2)+"_CODRGS == 'C'","BR_AMARELO"	}}	//Cancelamento autorizado

						If cUF <> "MG"
							MsgAlert(STR0396+cUF)
						Else
							aFilBrw		:= {cUseAlias,cCondicao}
							bFiltraBrw	:= {|| FilBrowse(cUseAlias,@aIndArq,@cCondicao) }
							Eval(bFiltraBrw)
							MarkBrow(cUseAlias,Substr(cUseAlias,2)+"_FLAGRGS",Substr(cUseAlias,2)+"_CODRGS $'T|S|M'",,.F.,GetMark(,cUseAlias,Substr(cUseAlias,2)+"_FLAGRGS"),,,,,,,,,aCores)
						EndIF
					EndIF

					// Restaura a integridade da rotina                                ³
					dbSelectArea(cUseAlias)
					RetIndex(cUseAlias)

					dbClearFilter()
					aEval(aIndArq,{|x| Ferase(x[1]+OrdBagExt())})

				Else
					lRetorno := .F.
				EndIf
			Else
				HelProg(,"FISTRFNFe")
				lRetorno := .F.
			EndIf
		Else

			If Substr(MV_PAR01,1,1) == "3"	//CCe
				Aviso(STR0647,STR0371,{STR0114},3) //"Execute o compatibilizador (NFE10R106) para Carta de Correção Eletrônica - CC-e !!!", STR0647 = "SPED"
			ElseIf 	Substr(MV_PAR01,1,1) == "4"	//Registro de Saida
				Aviso(STR0647,STR0391,{STR0114},3) //"Execute o compatibilizador para o registro de saida", STR0647 = "SPED"
			EndIF

		Endif
	Elseif Substr(MV_PAR01,1,1) == "5" // Conciliação Financeira

		oSpedCFNfe := backoffice.fiscal.tss.nfe.econf.NfeEconf():New()
		oSpedCFNfe:Active(cIdEnt,cVersaoTSS,lUsaColab)
		FreeObj(oSpedCFNfe)

	Elseif Substr(MV_PAR01,1,1) == "6" // Insucesso na Entrega da NFe

		oSpedISNfe := backoffice.fiscal.tss.nfe.insucesso.NfeInsucesso():New()
		oSpedISNfe:Active(cIdEnt,cVersaoTSS,lUsaColab)
		FreeObj(oSpedISNfe)
	Elseif Substr(MV_PAR01,1,1) == "7" // Ator Interessado

		If ExistFunc("SPEDATORINT")
			aRotina   := MenuDef("2")
			SPEDATORINT(oMainWnd, cTipoAtor, cIdEnt, cVersaoTSS, "Entidade : " + cIdEnt + " - TSS: " + getVersaoTSS(@cError))
		EndIf
	Elseif Substr(MV_PAR01,1,1) == "8" // NFcom
		oSpedNfCom := backoffice.fiscal.tss.nfe.nfcom.NFCom():New()
		oSpedNfCom:Active(cIdEnt,cVersaoTSS,lUsaColab)
		FreeObj(oSpedNfCom)	
	EndIf

Return(lRetorno)

