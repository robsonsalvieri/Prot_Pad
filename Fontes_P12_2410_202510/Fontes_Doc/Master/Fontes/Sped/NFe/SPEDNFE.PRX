						cCombo7	:= aCfgCCe[2]
					Else
						cCombo7	:= "2-Brasilia"
					EndIF
					If lEpp
						aCfg :=  getCfgCCe(cError, cIdEnti, Val(Substr(aCfgCCe[1],1,1))  , , , , , , , , , , , ,.T.,)
						// Ambiente EPP
						If ValType(aCfgCCe[9]) <> "U"
							cCombo13	:= aCfgCCe[9]
						Else
							cCombo13 := STR0349	//"2-Homologacao"
