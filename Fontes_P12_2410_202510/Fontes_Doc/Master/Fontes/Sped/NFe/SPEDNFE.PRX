Function SpedEpecPar(cAlias,nRecno,nOpc,aParamAuto)

	Local aPerg  		:= {}
	Local aParam 		:= {"","","","","","",""}
	local aCfgEpecCTe 	:= {}
	Local aCombo1		:= {}	//Ambiente
	Local aCombo2		:= {}	//VersaoGeralEPEC
	Local aCombo3		:= {}	//VersaoEvenEPEC
	Local aCombo4		:= {}	//VersaoGeralCanc
	Local aCombo5		:= {}	//VersaoEvenCanc
