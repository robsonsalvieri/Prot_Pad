	@ 000, 000 MSPANEL oPanelRight OF oDlg SIZE 000, 000
	oPanelRight:Align := CONTROL_ALIGN_ALLCLIENT

	oTree := xTree():New(000,000,000,000,oPanelLeft,,,)
