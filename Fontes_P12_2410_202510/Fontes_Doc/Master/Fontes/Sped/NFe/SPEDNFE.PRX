		AADD(aCombo13,STR0348)	//"1-Producao"
		AADD(aCombo13,STR0349)	//"2-Homologacao"

	EndIF

	if ( lAdminNfe .And. PswAdmin( /*cUser*/, /*cPsw*/,RetCodUsr()) == 0 ) .Or. !lAdminNfe
		If lAtuparam
			aParamAtu:=	Array(Len(aPergEvento))
		EndIf

		If IsReady(,,,lUsaColab)
			if lUsaColab
