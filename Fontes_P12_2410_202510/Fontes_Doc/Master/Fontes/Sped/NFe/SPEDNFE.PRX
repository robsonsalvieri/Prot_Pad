Static Function IsCDReady(nTipo,cCert,cKey,cPassWord,cSlot,cLabel,cModulo,cIdHex,lPassFormat)

	Local cCertRes := ""
	Local cIdEnt   := ""
	Local lRetorno := .T.
	Local cURL     := PadR(GetNewPar("MV_SPEDURL","http://"),250)

	Default cIdHex := ""

	If	FunName() == "LOJA701"
		If !Empty(GetNewPar("MV_NFCEURL",""))
			cURL := PadR(GetNewPar("MV_NFCEURL","http://"),250)
		Endif
	Endif

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³Obtem o codigo da entidade                                              ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	If ((!Empty(cCert) .And. !Empty(cKey) .And. !Empty(cPassWord) .And. nTipo == 1) .Or.;
			(!Empty(cSlot) .And. !Empty(cLabel) .And. !Empty(cPassword) .And. nTipo == 3) .Or.;
			(!Empty(cSlot) .And. !Empty(cIdHex) .And. !Empty(cPassword) .And. nTipo == 3) .Or.;
			(!Empty(cCert) .And. !Empty(cPassWord) .And. nTipo == 2)) .Or. !IsReady(,2)

		cIdEnt := AllTrim(GetIdEnt())
		If nTipo <> 3 .And. !File(cCert)
			Aviso(STR0647,STR0048,{STR0114},3) //"Arquivo não encontrado", STR0647 = "SPED"
			lRetorno := .F.
		EndIf
		If nTipo == 1 .And. !File(cKey) .And. lRetorno
			Aviso(STR0647,STR0048,{STR0114},3) //"Arquivo não encontrado" , STR0647 = "SPED"
			lRetorno := .F.
		EndIf

		if lRetorno
			lRetorno := ConfCert(cIdEnt,cUrl,,nTipo,cCert,cKey,cPassWord,cSlot,cLabel,cModulo,cIdHex,@cCertRes,lPassFormat)
			Aviso(STR0647, cCertRes ,{STR0114},3) // STR0647 = "SPED"

			// Ajusta o certificado da FX7
			if getRPORelease() >= "12.1.033" .and. TableInDic( "FX7", .F. ) .and. FindFunction("SpedStCert")
				SpedStCert(lRetorno,nTipo, "0", cUrl, cIdEnt)
			endif
		endif
	EndIf

Return(lRetorno)

