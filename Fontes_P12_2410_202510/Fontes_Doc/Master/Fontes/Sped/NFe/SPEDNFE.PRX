							SF2->F2_HORA := SUBSTR(oXml:OWSERRO:OWSLOTENSE[nY]:cHORALOTE,1,5)
							MsUnlock()
						EndIf
					EndIf
				Next nY
			EndIf
			aadd(aListBox,{ IIf(Empty(oXml:cPROTOCOLO),oNo,oOk),;
