Function SpedNSeCan(cCodEmp,cCodFil,cIdEnt,cAmbiente,cVersao)

	MsgRun(STR0130,STR0156,{|| JobNSeCan(cIdEnt,cAmbiente,cVersao)}) //"Aguarde"###"Processando Transmissão NS-e"

