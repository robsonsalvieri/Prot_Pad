				oWS:cVERCCELAYEVEN	:= "1.00"
				oWS:cVERCCEEVEN		:= "1.00"
				oWS:cVERCCE			:= "1.00"
				oWS:cHORAVERAOCCE	:= "2-Nao"
				oWS:cHORARIOCCE		:= "2-Brasilia"
