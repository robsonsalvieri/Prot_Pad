Static Function SpedCleRelt()
	Local cPath		:= SuperGetMV('MV_RELT',,"\SPOOL\")
	Local aArquivos := {}
	Local nX		:= 0
	Local cNome		:= ""

	aArquivos := Directory(cPath + "*.rel", "D")

	For nX := 1 to Len(aArquivos)
		cNome := LOWER(aArquivos[nX,1])
		If AT("danfe", cNome) > 0
			FERASE(cPath + cNome)
		EndIf
	Next nX

