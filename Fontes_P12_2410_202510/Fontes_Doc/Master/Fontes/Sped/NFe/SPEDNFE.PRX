				{STR0109,"SpedVisualDoc" ,0,2,0 ,NIL},; //"Visualiza Doc."
				{STR0113,"SpedFiltro"	 ,0,3,0 ,NIL},; //"Filtro"
				{STR0117,"SpedCCeLeg"    ,0,3,0 ,NIL}} //"Legenda"

