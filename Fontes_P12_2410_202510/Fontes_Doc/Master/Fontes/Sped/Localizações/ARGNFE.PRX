Function ARGNNFe()

Local aArea     := GetArea()
Local lRetorno  := .T.
Local nVezes    := 0

PRIVATE lBtnFiltro:= .F.
PRIVATE cAliasMnt := ""
PRIVATE nTamPV := TamSX3("F2_PV")[1]
PRIVATE lAutoNf := .F.
PRIVATE lAutomato := isBlind()
PRIVATE cTpMetric := ""

While lRetorno
	lBtnFiltro:= .F.
    lRetorno := ARGNNfe2(nVezes==0)
    nVezes++
    If !lBtnFiltro
    	Exit
    EndIf
EndDo
RestArea(aArea)
Return Nil

