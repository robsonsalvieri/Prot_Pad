			ElseIf SubStr(MV_PAR02,1,1) == "3" //"Rejeitada"
				cCondic += ".AND. F1_FLFTEX$'E' "              
			ElseIf SubStr(MV_PAR02,1,1) == "4" .And. !(cModeloWS $ "1|5") //"Manual
				cCondic += ".AND. F1_FLFTEX$'M' " 								
			EndIf			
			
