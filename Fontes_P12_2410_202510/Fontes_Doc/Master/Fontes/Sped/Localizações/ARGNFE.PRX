/*/{Protheus.doc} GetMonCot
	Consulta cotización de monedas.
	@type  Function
	@author luis.samaniego
	@since 21/05/2025
	@version 12.1.2410
	/*/
Function GetMonCot()
Local aPergunte   := {}
Local cCRLF       := CHR(10) + CHR(13)
Local cErrMsg     := ""
Local cFchCot     := DTOS(dDataBase - 1)
Local cFchCotiz   := ""
Local cIdEnt      := ""
Local cMoneda     := ""
Local cMonId      := ""
Local cMsg        := STR0314
Local cPerg       := "ARGNFE01"
Local cURL        := FindURL()
Local cWsService  := "wsfe"
Local lAutomato   := isBlind()
Local lFEMonCotiz := ExistBlock("NFEMonCotiz")
Local lRet        := .F.
Local nErrCod     := 0
Local nMonCotiz   := 0
Local nMoneda     := 0
Local oSx1Util    := FwSX1Util():New()
Local oWsResult   := Nil

	oSx1Util:AddGroup(cPerg)
	oSx1Util:SearchGroup()
	aPergunte := oSx1Util:GetGroup(cPerg)
	If Len(aPergunte) >= 2
		If Pergunte(cPerg)
			lRet    := .T.
			nMoneda := MV_PAR01 //Moneda de SM2
			cMoneda := TipMonARCA(nMoneda) //Id de moneda
			If !Empty(MV_PAR02)
				cFchCot := DTOS(MV_PAR02) //Fecha de cotización
			EndIf
		EndIf
	EndIf

	If lRet .And. fValidURL(cUrl) //Valida URL
		If !ValidFX7(cUrl) //Verificación de autenticación para versión 12.1.33 o superior
			Return
		EndIf
		
		cIdEnt := GetIdEnt() //Obtiene código de entidad.
		If !Empty(cIdEnt)
			If !lAutomato
				Processa({|| GetAuthWS(cURL, cIdEnt, cWsService) }) //Autenticación para factura electronica
				Processa({|| oWsResult := ExecMonCot(cURL, cIdEnt, cMoneda, cFchCot) }) //Realiza consulta a WS del ARCA
			Else
				GetAuthWS(cURL, cIdEnt, cWsService) //Autenticación para factura electronica
				oWsResult := ExecMonCot(cURL, cIdEnt, cMoneda, cFchCot)//Realiza consulta a WS del ARCA
			EndIf
			
			If Valtype(oWsResult) == "O"
				If AttIsMemberOf(oWsResult, 'cMonId')
					cMonId := oWsResult:cMonId //ID de moneda
				EndIf
				If AttIsMemberOf(oWsResult, 'nMonCotiz')
					nMonCotiz := oWsResult:nMonCotiz //Cotización de Moneda
				EndIf
				If AttIsMemberOf(oWsResult, 'cFchCotiz')
					cFchCotiz := DTOC(STOD(oWsResult:cFchCotiz)) //Fecha de cotización
				EndIf
				If AttIsMemberOf(oWsResult, 'oWsErrors')
					If AttIsMemberOf(oWsResult:oWsErrors, 'oWsErrors')
						If ValType(oWsResult:oWsErrors:oWsErrors) == "A" .And. Len(oWsResult:oWsErrors:oWsErrors) > 0
							nErrCod := oWsResult:oWsErrors:oWsErrors[1]:nNErrCode //Código de error
							cErrMsg := oWsResult:oWsErrors:oWsErrors[1]:cCErrMSG //Mensaje de error
						EndIf
					EndIf
				EndIf

				cMsg := STR0311 + cMonId + cCRLF //"ID de moneda: "
				cMsg += STR0310 + Alltrim(Str(nMonCotiz)) + cCRLF //"Cotización de Moneda: "
				cMsg += STR0309 + cFchCotiz + cCRLF//"Fecha de cotización: "
				If !Empty(cErrMsg)
					cMsg += cCRLF + cCRLF
					cMsg += STR0261 + Alltrim(Str(nErrCod)) + cCRLF //"Codigo: "
					cMsg += STR0262 + cErrMsg + cCRLF //"Mensaje: "
				EndIf
			EndIf

			If lFEMonCotiz
				ExecBlock("NFEMonCotiz",.f.,.f.,{nMoneda, cMonId, nMonCotiz, cFchCotiz}) //PE para permitir grabar cotización
			EndIf

			If !lAutomato
				Aviso(STR0312, cMsg, {STR0313}, 3) //"Servicio de factura electrónica nacional - Cotización de Moneda" "OK"
			EndIf
		EndIf
	EndIf

	FreeObj(oWsResult)
	FreeObj(oSx1Util)
	FwFreeArray(aPergunte)

Return

/*/{Protheus.doc} ExecMonCot
	Realiza consulta a WS del ARCA para cotización de monedas.
	@type  Function
	@author luis.samaniego
	@since 21/05/2025
	@version 12.1.2410
	@param cUrl, Character, URL del servicio de TSS
	@param cIdEnt, Character, Código de entidad
	@param cMoneda, Character, Id de moneda
	@param cFchCot, Character, Fecha de cotización (string)
	@return oWsResult, Object, Resultado de consulta de WSFEV1FEPARAMGETCOTIZACION.
	/*/
Static Function ExecMonCot(cURL, cIdEnt, cMoneda, cFchCot)
Local oWsFEV1   := Nil
Local oWsResult := Nil

Default cURL       := ""
Default cIdEnt     := ""
Default cMoneda    := ""
Default cFchCot    := ""

	oWsFEV1            := WSWSFEV1():New()
	oWsFEV1:cUserToken := "TOTVS"
	oWsFEV1:cID_ENT    := cIdEnt
	oWsFEV1:_URL       := AllTrim(cURL) + "/WSFEV1.apw"
	oWsFEV1:cMonId     := cMoneda
	oWsFEV1:cFchCotiz  := cFchCot
	If oWsFEV1:WSFEV1FEPARAMGETCOTIZACION() //Obtiene cotización de moneda
		oWsResult := oWsFEV1:OWSWSFEV1FEPARAMGETCOTIZACIONRESULT
	EndIf

	FreeObj(oWsFEV1)

Return oWsResult

/*/{Protheus.doc} GetAuthWS
	Obtiene token para autenticación de factura electronica.
	@type  Function
	@author luis.samaniego
	@since 21/05/2025
	@version 12.1.2410
	@param cURL, Character, URL del servicio de TSS
	@param cIdEnt, Character, Código de entidad
	@param cWsService, Character, Tipo de servicio
	/*/
Static Function GetAuthWS(cURL, cIdEnt, cWsService)
Local cData  := ""
Local oWsNFE := Nil

Default cURL       := ""
Default cIdEnt     := ""
Default cWsService := ""

	cData               := FsDateConv(Date(), "YYYYMMDD")
	cData               := SubStr(cData, 1, 4) + "-" + SubStr(cData, 5, 2) + "-" + SubStr(cData, 7)
	oWsNFE              := WSNFESLOC():New()
	oWsNFE:CDATETIMEGER := cData + "T00:00:00"
	oWsNFE:cDATETIMEEXP := cData + "T23:59:59"
	oWsNFE:cUserToken   := "TOTVS"
	oWsNFE:cID_ENT      := cIdEnt
	oWsNFE:_URL         := AllTrim(cURL) + "/NFESLOC.apw"
	oWsNFE:cCWSSERVICE  := cWsService
	oWsNFE:GETAUTHREM()

	FreeObj(oWsNFE)

Return

/*/{Protheus.doc} fValidURL
	Valida si fue informada la URL del servicio de TSS.
	@type  Function
	@author luis.samaniego
	@since 21/05/2025
	@version 12.1.2410
	@param cUrl, Character, URL del servicio de TSS
	@return lRet, Logical, Indica si fue informada la URL (.T.)
	/*/
Static Function fValidURL(cUrl)
Local cCRLF := CHR(10) + CHR(13)
Local lRet  := .T.

Default cURL := ""

	If Empty(cUrl)
		Help(" ", 1, "URL_TSS", , STR0298, 2, 0,,,,,, {STR0299 + cCRLF + STR0300}) //"No se detectó configuración de conexión con TSS." "Por favor, ejecute opción Wizard de Configuración." "Siga atentamente los pasos para a configuración de la nota fiscal eletrónica."
		lRet := .F.
	EndIf

Return lRet

/*/{Protheus.doc} TipMonARCA
	Obtiene código de moneda informado en YF_COD_GI
	@type  Function
	@author luis.samaniego
	@since 21/05/2025
	@version 12.1.2410
	@param nMoneda, Numeric, Moneda de SM2
	@return cIdMoneda, Character, Id de moneda del ARCA
	/*/
Static Function TipMonARCA(nMoneda)
Local aAreaSYF   := {}
Local cAuxMoneda := ""
Local cIdMoneda  := ""

Default nMoneda  := 1

	If nMoneda > 0
		cAuxMoneda := SuperGetMV("MV_SIMB" + Alltrim(Str(nMoneda)))
		aAreaSYF   := GetArea()
		dbSelectArea("SYF")
		dbSetOrder(1)
		If SYF->(MsSeek(xFilial("SYF") + cAuxMoneda))
			cIdMoneda  := SYF->YF_COD_GI
		EndIf
		RestArea(aAreaSYF)
		FwFreeArray(aAreaSYF)
	EndIf

Return cIdMoneda
