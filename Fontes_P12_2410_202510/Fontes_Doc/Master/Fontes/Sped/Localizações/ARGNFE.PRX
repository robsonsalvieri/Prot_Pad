	dbSelectArea("SF3")
	dbSetOrder(5)
    IF(cModeloWS $ "1|3|4|5") //nacional,reg. nomin,bono e nominacion CAEA
		For nX := 1 To Len(oRetorno:OWSRETCOMP)
	  		aMsg := {}
