/*/
Static Function AcertoChv(cChave)
Local aArea  := SE1->(GetArea())
Local cRet   := ""
Local nRecno := SE1->(Recno())

Default cChave := ""

If !Empty(cChave)
	SE1->(DbSetorder(1))
	SE1->(DbSeek(cChave))
	While !SE1->(Eof()) .And. xFilial("SE1") == SF3->F3_FILIAL .And.;
		cChave == SE1->(xFilial("SE1")+E1_PREFIXO+E1_NUM +E1_PARCELA )
		If !Empty(SE1->E1_SERIE)
			cRet  := SE1->E1_SERIE
			Exit
		EndIf
		SE1->(DbSkip())
	End
EndIf
RestArea(aArea)
SE1->(DbGoto(nRecno))

Return(cRet)
