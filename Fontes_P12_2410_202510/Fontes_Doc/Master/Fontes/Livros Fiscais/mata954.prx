Local cOrgArrec := Space(30)
Local cObserv   := ""
Local cNumero   := ""
Local nCtd002	:= 0
Local lGeraSubEmp	:= .T.
Local lLoadDesc	:= .F.
Local dDataIni	:= FirstDay(dDtIni)
