a954TotLi7(n111,"111",lFirst,@aCols7)
nRecebe := a954Ret(.F.,"",mv_par01,mv_par02)
aCols7[n112,9]:= nRecebe[1]
nLin112	:= nRecebe[1]

If !lFirst
	If ReadVar() == "M->NVALOR"
		nLin113 := if(aCols[n,1]==aCols[n113,1],M->NVALOR,aCols[n113,9])
	ElseIf Empty(ReadVar())
		nLin113 := aCols[n113,9]
