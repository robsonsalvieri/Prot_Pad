Local cForISS  := ""
Local aFornISS := {{cMunSgmt,PadR(SuperGetMV("MV_MUNIC",.F.,""),TamSX3("A2_COD")[1]),PadR("00",TamSX3("A2_LOJA")[1])}}
Local nTamGnre := TamSx3("F6_NUMERO")[1]
Local aGnre    := {}
Local cLojISS  := ""
