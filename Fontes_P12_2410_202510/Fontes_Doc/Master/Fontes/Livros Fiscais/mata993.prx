Function a993TudOk()
Local lRet  := .T.
Local cData := ""

dbSelectArea("SFK")
IF INCLUI
	If dbSeek(xFilial("SFK")+M->FK_PRODUTO)
		cData:= Substr (DTOS (M->FK_DATA),1,4)+Substr (DTOS (M->FK_DATA),5,2)
        While SFK->(!eof()) .And. xFilial("SFK")+M->FK_PRODUTO == xFilial("SFK")+SFK->FK_PRODUTO
        	If cData == Substr (DTOS (SFK->FK_DATA),1,4)+Substr (DTOS (SFK->FK_DATA),5,2)
				lRet := .F.	                   		
				Exit
         	EndIf  
         	SFK->(dbSkip())
         	Loop
        EndDo           
	EndIf    
    
    If !lRet
		Help(" ", 1, "JAGRAVADO")
	EndIf
EndIf	
Return lRet

