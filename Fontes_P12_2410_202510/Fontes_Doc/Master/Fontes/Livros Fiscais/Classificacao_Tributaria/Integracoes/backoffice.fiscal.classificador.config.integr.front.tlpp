Function ParametrosClassifis(lAutomato)

	Local aParam    := {}
	Local aUser     := {"",""}

	Local cIntRegra := ".F."

	Default lAutomato := .F.

	If lAutomato
		aUser     := {"TESTE","1234"}
		cIntRegra := ".T."
	EndIf


	//Parametro   Conteudo
	aParam := { {"URLREGRA",            "https://api-gateway.systax.com.br"     },;
		{"USERNAMEDEFAULT",     aUser[1]                                },;
		{"SENHADEFAULT",        aUser[2]                                },;
		{"CODREGALIQ",          "001"                                   },;
		{"CODREGBASE",          "001"                                   },;
		{"CODREGREGRA",         "001"                                   },;
		{"URLPRINCIPAL",        "https://app.systax.com.br"             },;
		{"INTEGRAREGRA",        cIntRegra                               },;
		{"INTEGRANCM",          ".F."                                   },;
		{"CODPERFOPER",         "001"                                   },;
		{"CODREGCST",           "001"                                   },;
		{"URLREGRA2",           "https://wscockpit.systax.com.br/api"   },;
		{"GRVLOGENVPRODSYSTAX", ".T."                                   } }


	FISImportCarga():ImpParamClassif(aParam)

	FWFreeArray(aParam)

Return

