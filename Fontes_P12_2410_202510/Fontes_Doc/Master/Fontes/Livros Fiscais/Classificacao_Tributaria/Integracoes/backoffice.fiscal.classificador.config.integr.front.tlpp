#Include "Protheus.ch"
#Include "RwMake.ch"
#Include "FwBrowse.ch"
#Include "FwMvcDef.ch"
#Include "TOPCONN.ch"
#INCLUDE "TBICONN.CH"
#include 'tlpp-core.th'
#include 'tlpp-rest.th'
#include 'classificadorfiscal.ch'

Using Namespace totvs.protheus.backoffice.fiscal.Classifier

Function classFis()

	Local lExtCJHCJF := AliasInDic("CJH") .And. AliasInDic("CJF")
	Local lExistCJE := AliasInDic("CJE")
	Local lExistCJK  := AliasInDic("CJK")
	Local jParam := Nil
	Local lAutomato := IsBlind()

	If lExtCJHCJF .And. lExistCJE .And. lExistCJK

		//Abertura das tabelas da classificação Tributária
		AbertTabela()

		//Importação dos parametros iniciais do classificador
		ParametrosClassifis(lAutomato)

		//Importação do De/Para de formulas de base de calculo
		ImpDeParaFormul()

		//Importação da natureza de operação e finalidade
		ImpNatOper(lAutomato)

		//Importação dos enquadramentos
		ImpEnquad()

		jParam := GetParamCJE()

		If &(jParam:INTEGRAREGRA) .And. !Empty(jParam:USERNAMEDEFAULT) .And. !Empty(jParam:SENHADEFAULT)
			//Importação das regras
			oGetRegra := FISRetornaRegra():new()

			Processa({|| oGetRegra:FISRetornaRegra()}, STR0213 + "...", STR0214) //"Aguarde" "Atualizando as Regras"

			oIntegrRegras := FISGetRegrasClassificador():new()

			Processa({|| oIntegrRegras:IntegrConfig()}, STR0213 + "...", STR0215) //"Aguarde" "Integrando as Regras"

		EndIf

		If !lAutomato
			//Abertura do classificador fiscal
			FwCallApp("clasfis")
		EndIf
	Else
		MsgAlert(STR0216) //"Dicionário desatualizado, favor baixar o último pacote!"
	EndIf

return

