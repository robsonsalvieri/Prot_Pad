Static Function _PrintP10()
//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Variaveis utilizadas na segunda regua de processamento - impressao       ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
dbSelectArea("TRB")
dbGotop()
SetRegua(LastRec())

Do While !Eof() .and. !lEnd
	IncRegua()
	If Interrupcao(@lEnd)
		Exit
	EndIf

	If li > 55
		Cabecalho(cFilAnt)
	EndIf
	
	cCod := CODIGO+" / "+LOJA
	nCol := Round((25 - Len(cCod)) / 2,0)
	If nCol < 1
		cCod := Substr(CODIGO+"/"+LOJA,1,25)
		nCol := 1
	EndIf
	
	@ Li,000 pSay "|"
	@ Li,nCol pSay cCod
	@ Li,026 pSay "|"
	@ Li,028 pSay Substr(NOME,1,37)
	@ Li,063 pSay "|"
	@ Li,069 pSay UF
	@ Li,077 pSay "|"
	@ Li,083 pSay CGC		Picture If(Len(Alltrim(CGC))==14,"@R! NN.NNN.NNN/NNNN-99","@R 999.999.999-99")
	@ Li,104 pSay "|"
	@ Li,109 pSay INSCRI	Picture PesqPict("SA2","A2_INSCR")
	@ Li,131 pSay "|"
	Li++
	dbSkip()
EndDo

If lMovimento== .F.
	Cabecalho(cFilAnt)
   	cLinha:=FmtLin(Array(5),"|#########################|####################################|#############|##########################|##########################|",,,Li,.F.)
	R95xFillPage(Limite,cLinha,STR0017,@Li,nLimPag) //"*** NAO HOUVE MOVIMENTO ***"
Endif

If !li==80 .and. !lEnd
	@ li,00 pSay __PrtThinLine()
EndIf                    

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³Fecha as areas de trabalho³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
dbSelectArea("TRB")
dbCloseArea()
Ferase(cNomarq+GetDBExtension())
Ferase(cNomarq+OrdBagExt())

If lQuery
	dbSelectArea(cAliasSF3)
	dbCloseArea()
Else
   	dbSelectArea("SF3")
	RetIndex("SF3")
	dbClearFilter()
	Ferase(cIndex+OrdBagExt())
Endif
Li := 80 //Quebra pagina por filial 	

Return

