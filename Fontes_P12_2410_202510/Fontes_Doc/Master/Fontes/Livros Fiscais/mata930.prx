/*/{Protheus.doc} M930NLvr

Função para guardar informações das referências antes da exclusão da SF3/SFT
quando a NF não possui livro e foi alterado o TES para gravar livro.

@param cAlias		- Alias temporário com os registros filtrados da SF1/SF2
@param cTabD1D2		- Tabela para decisão dos campos
@param nDbOrder		- Índice da tabela
@param cChvF1F2		- Chave para Seek na tabela SF1/SF2
@param lMaFisItem	- FindFunction para função MaFisItem

@author Renato Rezende
@since 02/07/2021
@version 12.1.33
/*/
//-------------------------------------------------------------------
Static Function m930NLvr(cAlias,cTabD1D2,nDbOrder,cChvF1F2,lMaFisItem)

Local cTes		:= ""
Local cItem		:= ""
Local cChvD1D2	:= ""
Local nItem		:= 0

(cTabD1D2)->(DbSetOrder(nDbOrder))

If (cTabD1D2)->(MsSeek(cChvF1F2,.T.))

	cTes:= Iif(cTabD1D2=="SD2",SD2->D2_TES,SD1->D1_TES)

	SF4->(DbSetOrder(1))
	//Validar se será gerado livro fiscal - Mesma validação que tem na função MaFisLF/xFisLF para gravar livro
	//Feita a validação apenas no primeiro iem, porque não existe nota gerando livro pela metade, ou todos os itens geram ou não
	If SF4->(MsSeek(xFilial("SF4")+cTes)) .And. (SF4->F4_LFICM <> "N" .Or. SF4->F4_LFIPI <> "N" .Or. SF4->F4_LFISS $"TIO")

		cChvD1D2:= IIF(cTabD1D2=="SD2",(cTabD1D2)->(D2_FILIAL+D2_DOC+D2_SERIE+D2_CLIENTE+D2_LOJA),(cTabD1D2)->(D1_FILIAL+D1_DOC+D1_SERIE+D1_FORNECE+D1_LOJA))
		
		//Guardar informações para o reprocessamento não recalcular caso algum cadastro seja alterado
		While (cTabD1D2)->(!Eof()) .And. cChvD1D2 == cChvF1F2

			cItem:= IIF(cTabD1D2=="SD2",SD2->D2_ITEM,SD1->D1_ITEM)

			nItem   := Iif(lMaFisItem,MaFisItem('IT_ITEM',  Alltrim(cItem)), 0)
			//Realizado ajuste para diferença de tamanho de Item entre o D1_ITEM e D2_ITEM ou bancos com tamanho do _ITEM alterado
			If nItem == 0
				nItem   := Iif(lMaFisItem,MaFisItem('IT_ITEM',  cItem), 0)
			EndIf

			//Nota Fiscal de Saída
			If cTabD1D2 == "SD2"
				//Guardar Aliquota do ISS
				If SD2->D2_ALIQISS > 0 .And. nItem > 0
					MaFisAlt("IT_ALIQISS", SD2->D2_ALIQISS, nItem)
				EndIf

			//Nota Fiscal de Entrada
			Else
				//Guardar Aliquota do ISS
				If SD1->D1_ALIQISS > 0 .And. nItem > 0
					MaFisAlt("IT_ALIQISS", SD1->D1_ALIQISS, nItem)
				EndIf
			EndIf	

			(cTabD1D2)->(DbSkip())

			//Atualizando a Chave após o DBSkip, para que seja feita a verificação de igualdade no While, evitando Leitura da SD1/SD2 inteira
			cChvD1D2:= IIF(cTabD1D2=="SD2",(cTabD1D2)->(D2_FILIAL+D2_DOC+D2_SERIE+D2_CLIENTE+D2_LOJA),(cTabD1D2)->(D1_FILIAL+D1_DOC+D1_SERIE+D1_FORNECE+D1_LOJA))

		EndDo
	EndIf
EndIf

Return nil


