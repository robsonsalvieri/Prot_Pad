Function AUTOCOM(nID, aParams, cBuffer, nBuffSize)
Local cString	:= ""
Local nPos		:= 0
Local aParm 	:= {}
Local cFunction	:= ""
Local lLinux 	:= .F.
Local nHandle	:= 0
Local cCompl	:= ""	// Complemento de retorno
Local xRet

Local nInteger1, nInteger2, nInteger3, nInteger4, nInteger5, nInteger6, nInteger7, nInteger8 ,nInteger9
Local cPChar1, cPChar2, cPChar3, cPChar4, cPChar5, cPChar6, cPChar7, cPChar8, cPChar9, cPChar10, cPChar11, cPChar12, cPChar13, cPChar14, cPChar15, cPChar16, cPChar17

EXPORT FUNCTION AS ExecInClientDLL(nFuncID AS INTEGER, aParams AS PCHAR, cBuf AS PCHAR, nTam AS INTEGER) RETURNING INTEGER

If nId == 999
	cBuffer := "3.4.1.7"
	Return 1
ElseIf nId == 997
	cBuffer := "11"
	Return 1
EndIf

cString := aParams
cString := DoDecode(cString)

While .T.
	// O Chr(1) È o separador de campos
	nPos := At(Chr(1), cString)
	If nPos == 0
		// O Chr(2) È o marcador do final do aParams, o resto È somente buffer		
		If At(Chr(2), cString) == 1					// Foi enviado o ultimo parametro vazio
			AAdd(aParm, "")
		Else
			AAdd(aParm, Substr(cString,1,At(Chr(2),cString)-1))
		EndIf
		Exit
	EndIf
	If nPos == 1									// Foi enviado um parametro vazio
		AAdd(aParm, "")
	Else
		AAdd(aParm, Substr(cString,1,nPos-1))
	EndIf
	cString := Substr(cString,nPos+1)
End


             
cFunction := Upper(AllTrim(aParm[1]))


/* ----------------------------
[ INICIO DAS FUNCOES DO SITEF ]
------------------------------*/
If cFunction == Upper("ConfiguraIntSiTefInterativo")
	xRet := SIT_CONF( DoEncode(aParm[2]), DoEncode(aParm[3]), DoEncode(aParm[4]), Val(aParm[5]) )
	cBuffer := AllTrim(Str(xRet))

ElseIf cFunction == Upper("ConfiguraIntSiTefInterativoEx")
	xRet := SIT_CONFEX( DoEncode(aParm[2]), DoEncode(aParm[3]), DoEncode(aParm[4]), Val(aParm[5]), DoEncode(aParm[6]) )
	cBuffer := AllTrim(Str(xRet))

ElseIf cFunction == Upper("ObtemDadoPinPadDiretoEx")

	cPChar1	:= DoEncode(aParm[2])	// Chave	-> string vazia
	cPChar2	:= DoEncode(aParm[3])	// Codigo 	-> string vazia
	cPChar3	:= DoEncode(aParm[4])	// Entrada	-> "011111DIGITE SEU CPF                  CONFIRME O CPF  "
	cPChar4	:= Space(20000)			// Saida	-> "011100000000000"

	xRet 	:= SIT_DIGCPF( cPChar1, cPChar2, cPChar3, cPChar4 )
	cCompl 	:= SubStr( AllTrim(DoDecode(cPChar4) ) ,5, 12)	
	cBuffer	:= AllTrim(Str(xRet)) + Chr(1) + cCompl

ElseIf cFunction == Upper("IniciaFuncaoSiTefInterativo")
	xRet := SIT_INICIA( Val(aParm[2]), DoEncode(aParm[3]), DoEncode(aParm[4]), DoEncode(aParm[5]), DoEncode(aParm[6]), DoEncode(aParm[7]), DoEncode(aParm[8]))
	cBuffer := AllTrim(Str(xRet))
		
ElseIf cFunction == Upper("ContinuaFuncaoSiTefInterativo")
	nInteger1 := Val(aParm[2])				//Proximo comando a ser executado
	nInteger2 := Val(aParm[3])				//Tipo de Campo para tratamento (Vide Tabela no Manual)
	nInteger3 := Val(aParm[4])				//Tamanho MÌnimo
	nInteger4 := Val(aParm[5])				//Tamanho M·ximo
	cPChar1   := DoEncode(AllTrim(aParm[6]))			//Conteudo para o buffer
	nInteger5 := Len(cPChar1) 				//Tamanho do Buffer				
	nInteger6 := Val(aParm[8])				//Indica o comando para continuacao da navegacao dentro da rotina

	SETBUFFSIZE(cPChar1,20000)

	xRet := SIT_CONTIN( nInteger1, nInteger2, nInteger3, nInteger4, cPChar1, nInteger5, nInteger6 )
	cPChar1 := 	StrTran( cPChar1, CHR(3), "")
	cBuffer :=	AllTrim(Str(xRet))		+ Chr(1) + ;
				AllTrim(Str(nInteger1))	+ Chr(1) + ;
				AllTrim(Str(nInteger2))	+ Chr(1) + ;
				AllTrim(Str(nInteger3))	+ Chr(1) + ;
				AllTrim(Str(nInteger4))	+ Chr(1) + ;
				DoDecode(cPChar1)

ElseIf cFunction == Upper("FinalizaTransacaoSiTefInterativo")
	cPChar1 := DoEncode(aParm[3])
	cPChar2 := DoEncode(aParm[4])
	cPChar3 := DoEncode(aParm[5])
	SIT_FINAL( Val(aParm[2]) , cPChar1, cPChar2, cPChar3 )
	cBuffer :=	DoDecode(cPChar1) + Chr(1) + ;
				DoDecode(cPChar2) + Chr(1) + ;
				DoDecode(cPChar3) + Chr(1)
	
ElseIf cFunction == Upper("EnviaRecebeSitefDireto")
	nInteger1 	:= Val(aParm[2])				// Parametro 1
	nInteger2 	:= Val(aParm[3])				// Parametro 2
	nInteger3 	:= Val(aParm[4])				// Parametro 3
	cPChar1		:= DoEncode(aParm[5])			// Parametro 4
	nInteger4 	:= Val(aParm[6])				// Parametro 5
	cPChar2		:= DoEncode(aParm[7])			// Parametro 6
	nInteger5 	:= Val(aParm[8])				// Parametro 7
	nInteger6 	:= Val(aParm[9])				// Parametro 8
	nInteger7 	:= Val(aParm[10])				// Parametro 9
	cPChar3		:= DoEncode(aParm[11]) 			// Parametro 10
	cPChar6	 	:= DoEncode(aParm[12])			// Parametro 11
	cPChar4		:= DoEncode(aParm[13])			// Parametro 12
	cPChar5		:= DoEncode(aParm[14])			// Parametro 13
	nInteger9 	:= Val(aParm[15])				// Parametro 14

	xRet := SIT_ENVDIR( 	nInteger1	, nInteger2	, nInteger3	, cPChar1	, ;
							nInteger4	, cPChar2	, nInteger5	, nInteger6	, ;
							nInteger7	, cPChar3	, cPChar6	, cPChar4	, ;
							cPChar5		, nInteger9 ) 
	
	cBuffer := 	AllTrim(Str(xRet))		+ Chr(1) + ;
				Alltrim(Str(nInteger1))	+ Chr(1) + ;
				Alltrim(Str(nInteger2))	+ Chr(1) + ;
				Alltrim(Str(nInteger3))	+ Chr(1) + ;
				DoDecode(cPChar1)		+ Chr(1) + ;
				Alltrim(Str(nInteger4))	+ Chr(1) + ;
				DoDecode(cPChar2)		+ Chr(1) + ;
				Alltrim(Str(nInteger5))	+ Chr(1) + ;
				Alltrim(Str(nInteger6))	+ Chr(1) + ;
				Alltrim(Str(nInteger7))	+ Chr(1) + ;
				DoDecode(cPChar3)		+ Chr(1) + ;
				DoDecode(cPChar6)		+ Chr(1) + ;
				DoDecode(cPChar4)		+ Chr(1) + ;
				DoDecode(cPChar5)		+ Chr(1) + ;
				Alltrim(Str(nInteger9))
				
ElseIf cFunction == Upper("CorrespondenteBancarioSiTefInterativo")
	xRet := SIT_CORRES(DoEncode(aParm[2]), DoEncode(aParm[3]), DoEncode(aParm[4]), DoEncode(aParm[5]), DoEncode(aParm[6]))
	cBuffer := AllTrim(Str(xRet))
		
ElseIf cFunction == Upper("ValidaCampoCodigoEmBarras")
	xRet := SIT_VLDBAR(DoEncode(aParm[2]),DoEncode(aParm[3]))
	cBuffer := AllTrim(Str(xRet))
		
ElseIf cFunction == Upper("VerificaPresencaPinPad")
	xRet := SIT_VERPIN( )
	cBuffer := AllTrim(Str(xRet))

ElseIf cFunction == Upper("EscreveMensagemPermanentePinPad")
	xRet := SIT_MSNPIN(DoEncode(aParm[2]))
	cBuffer := AllTrim(Str(xRet))

ElseIf cFunction == Upper("IniciaFuncaoSiTefInterativoConsultaVidalink")
	xRet := SIT_VDLCON(DoEncode(aParm[2]),DoEncode(aParm[3]),DoEncode(aParm[4]),DoEncode(aParm[5]),DoEncode(aParm[6]),DoEncode(aParm[7]))
	cBuffer := AllTrim(Str(xRet))

ElseIf cFunction == Upper("InformaProdutoVendaVidalink")
	xRet := SIT_VDLPRO( Val(aParm[2]),DoEncode(aParm[3]),Val(aParm[4]),DoEncode(aParm[5]))
	cBuffer := AllTrim(Str(xRet))
		
ElseIf cFunction == Upper("IniciaFuncaoSiTefInterativoVendaVidalink")
	xRet := SIT_VDLVEN(DoEncode(aParm[2]),Val(aParm[3]),DoEncode(aParm[4]),DoEncode(aParm[5]),DoEncode(aParm[6]),DoEncode(aParm[7]))
	cBuffer := AllTrim(Str(xRet))
		
ElseIf cFunction == Upper("IniciaFuncaoSiTefInterativoCancelamentoVidalink")
	xRet := SIT_VDLCAN( Val(aParm[2]),DoEncode(aParm[3]),DoEncode(aParm[4]),DoEncode(aParm[5]),DoEncode(aParm[6]))
	cBuffer := AllTrim(Str(xRet))
		
ElseIf cFunction == Upper("LeCartaoDireto")
	cPChar1	:= DoEncode(aParm[2])
	cPChar2	:= DoEncode(aParm[3])
	cPChar3	:= DoEncode(aParm[4])
	xRet	:= SIT_LECART( cPChar1 , cPChar2, cPChar3 )
	cBuffer	:= AllTrim(Str(xRet))	+ Chr(1) + ;				//Retorno da funcao 0=OK <>0 erro
			   DoDecode(cPChar1)	+ Chr(1) + ;				//Mensagem
			   DoDecode(cPChar2)	+ Chr(1) + ;				//Trilha 1 qdo existir
			   DoDecode(cPChar3)								//Trilha 2 do cart„o quando existir

ElseIf cFunction == Upper("InterrompeLeCartaoDireto")
	xRet := SIT_CANCLE()
	cBuffer := AllTrim(Str(xRet))

ElseIf cFunction == Upper("LeSenhaDireto")
	xRet := SIT_LESENH(DoEncode(aParm[2]), DoEncode(aParm[3]))
	cBuffer := AllTrim(Str(xRet))
		
ElseIf cFunction == Upper("LeSimNaoPinPad")
	xRet := SIT_LSNPIN(DoEncode(aParm[2]))
	cBuffer := AllTrim(Str(xRet))

ElseIf cFunction == Upper("ObtemQuantidadeTransacoesPendentes")
	xRet := SIT_OBTQTR(DoEncode(aParm[2]), DoEncode(aParm[3]))
	cBuffer := AllTrim(Str(xRet))

/* --------------------------
[ FINAL DAS FUNCOES DO SITEF ]
----------------------------*/

/* -----------------------------------
[ INICIO DAS FUNCOES DO ECF BEMATECH ]
-------------------------------------*/
ElseIf cFunction == Upper("Bematech_FI_AbrePortaSerial")
	xRet := BEMA_ABRSE()
