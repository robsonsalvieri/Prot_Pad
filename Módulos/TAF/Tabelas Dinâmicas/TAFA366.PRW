//-------------------------------------------------------------------
/*/{Protheus.doc} CH8Unique
Adiciona indice único da tabela CH8

@author Caique 
@since 26/07/2018
@version P12 
/*/
//-------------------------------------------------------------------
Function CH8Unique(nVerEmp,lAutomato)

Local cQuery 	:= ""
Local cTmpAlias := ""
Local aAreaAt	:= {}
Local lFind		:= .F.

Default nVerEmp   := 0
Default lAutomato := .f.

If nVerEmp >= 1033.00
	cQuery := " SELECT CH8_FILIAL, CH8_ID, R_E_C_D_E_L_, COUNT(*)"
	cQuery += " FROM " + RetSqlName("CH8") 
	cQuery += " WHERE D_E_L_E_T_ = ' ' "
	cQuery += " GROUP BY CH8_FILIAL, CH8_ID, R_E_C_D_E_L_"
	cQuery += " HAVING COUNT(*) > 1 "
	cQuery += " ORDER BY CH8_FILIAL, CH8_ID, R_E_C_D_E_L_"

	cTmpAlias := getNextAlias()
	DbUseArea(.T.,"TOPCONN",TcGenQry(,,cQuery),cTmpAlias,.F.,.T.)

	While !(cTmpAlias)->(EOF()) .or. lAutomato
		cQuery := " DELETE FROM  " + RetSqlName("CH8") 
		cQuery += " WHERE "
		cQuery += "		CH8_FILIAL = '" + (cTmpAlias)->CH8_FILIAL +"' "
		cQuery += " 	AND CH8_ID = '" + (cTmpAlias)->CH8_ID +"' "
		cQuery += " 	AND R_E_C_D_E_L_ = "+ AllTrim(STR((cTmpAlias)->R_E_C_D_E_L_ ))
		cQuery += " 	AND R_E_C_N_O_ = " 
		cQuery += " 		( SELECT MAX(R_E_C_N_O_) "
		cQuery += " 		FROM " + RetSqlName("CH8") 
		cQuery += " 		WHERE "
		cQuery += " 		CH8_FILIAL = '" + (cTmpAlias)->CH8_FILIAL +"' "
		cQuery += " 		AND CH8_ID = '" + (cTmpAlias)->CH8_ID +"' "
		cQuery += " 		AND R_E_C_D_E_L_ = "+ AllTrim(STR((cTmpAlias)->R_E_C_D_E_L_ ))
		cQuery += " 		) "
	
		If TcSqlExec(cQuery) < 0 ; TAFConOut(RetSqlName("CH8") +" - TCSQLError() " + TCSQLError() ,3,.F.,"AUTCONT"); EndIf

		if lAutomato
			exit
		endif
		(cTmpAlias)->(DbSkip())
	EndDo
	(cTmpAlias)->(DbCloseArea())	
Else
	cQuery := "SELECT CH8_FILIAL, CH8_CODREG,CH8_CODIGO, CH8_DTINI,CH8_DTFIN, R_E_C_D_E_L_, COUNT(*) "
	cQuery += " FROM " + RetSqlName("CH8") 
	cQuery += " GROUP BY CH8_FILIAL, CH8_CODREG,CH8_CODIGO, CH8_DTINI,CH8_DTFIN, R_E_C_D_E_L_ "
	cQuery += " HAVING COUNT(*) > 1 "
	cQuery += " ORDER BY CH8_FILIAL, CH8_CODREG,CH8_CODIGO, CH8_DTINI,CH8_DTFIN, R_E_C_D_E_L_ "

	aAreaAt := getArea()

	cTmpAlias := getNextAlias()

	If Select(cTmpAlias) > 0
		(cTmpAlias)->(DbCloseArea())
	EndIf

	cQuery := ChangeQuery(cQuery)
	DbUseArea(.T.,"TOPCONN",TcGenQry(,,cQuery),cTmpAlias,.F.,.T.)

	While !(cTmpAlias)->(EOF()) .or. lAutomato
		lFind := .T.

		cQuery := " DELETE FROM  " + RetSqlName("CH8") 
		cQuery += " WHERE "
		cQuery += " 		CH8_FILIAL = '" + (cTmpAlias)->CH8_FILIAL +"' "
		cQuery += " 	AND CH8_CODREG = '" + (cTmpAlias)->CH8_CODREG +"' "
		cQuery += " 	AND CH8_CODIGO = '" + (cTmpAlias)->CH8_CODIGO +"' "
		cQuery += " 	AND CH8_DTINI = '" + (cTmpAlias)->CH8_DTINI +"' "
		cQuery += " 	AND CH8_DTFIN = '" + (cTmpAlias)->CH8_DTFIN +"' "
		cQuery += " 	AND R_E_C_D_E_L_ = "+ AllTrim(STR((cTmpAlias)->R_E_C_D_E_L_ ))
		cQuery += " 	AND R_E_C_N_O_ > " 
		cQuery += " 					( SELECT MIN(R_E_C_N_O_) "
		cQuery += " 						FROM " + RetSqlName("CH8") 
		cQuery += " 						WHERE "
		cQuery += " 							CH8_FILIAL = '" + (cTmpAlias)->CH8_FILIAL +"' "
		cQuery += " 							AND CH8_CODREG = '" + (cTmpAlias)->CH8_CODREG +"' "
		cQuery += " 							AND CH8_CODIGO = '" + (cTmpAlias)->CH8_CODIGO +"' "
		cQuery += " 							AND CH8_DTINI = '" + (cTmpAlias)->CH8_DTINI +"' "
		cQuery += " 							AND CH8_DTFIN = '" + (cTmpAlias)->CH8_DTFIN +"' "
		cQuery += " 							AND R_E_C_D_E_L_ = "+ AllTrim(STR((cTmpAlias)->R_E_C_D_E_L_ ))
		cQuery += " 					) "
		
		If TcSqlExec(cQuery) < 0 ; TAFConOut(RetSqlName("CH8") +" - TCSQLError() " + TCSQLError() ,3,.F.,"AUTCONT"); EndIf
		
		if lAutomato
			exit
		endif
		(cTmpAlias)->(DbSkip())
	Enddo

	If Select(cTmpAlias) > 0
		(cTmpAlias)->(DbCloseArea())
	EndIf

	If lFind
		TcRefresh(RetSqlName("CH8") )
		TAFUnique(RetSqlName("CH8"),"CH8_FILIAL+CH8_CODREG+CH8_CODIGO+CH8_DTINI+CH8_DTFIN")
	EndIf

	RestArea(aAreaAt)
Endif
Return 

