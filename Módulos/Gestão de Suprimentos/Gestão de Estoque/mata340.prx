//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
cNumseq := ProxNum()
dbSelectArea("SD3")
RecLock("SD3",.T.)
Replace	D3_FILIAL  With xFilial("SD3")	,;
		D3_COD     With aNumSerie[4]	,;
		D3_DOC     With "INVENT"		,;
		D3_EMISSAO With mv_par01		,;
		D3_CC      With mv_par02		,;
