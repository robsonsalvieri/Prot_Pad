EndIf
If !SubStr (TCAPIBuild(),10,8) > '20230109' // STR0051 MATA340 - A data da DBAPI / STR0050 - não é superior á 20230109, portanto não terá tratamento multithread
	Conout (OemToAnsi(STR0051) + ' [' +SubStr (TCAPIBuild(),10,8)+ '] ' + OemToAnsi(STR0050))
	lNewTEMP := .F.
EndIf
If !FWLibVersion() > '20230109' 			// STR0052 MATA340 - A data da LIB / STR0050 - não é superior á 20230109, portanto não terá tratamento multithread
	Conout (OemToAnsi(STR0052) + ' [' +FWLibVersion()+ '] ' + OemToAnsi(STR0050))
	lNewTEMP := .F.
