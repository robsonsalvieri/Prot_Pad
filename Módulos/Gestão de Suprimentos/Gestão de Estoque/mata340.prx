//-------------------------------------------------------------------
/*/{Protheus.doc} CRIATEMP
Função para retornar um array statico em uma tela será aberta temporáriamente

@author TOTVS SA
@since 16/12/2016
@version 1.0
/*/
//-------------------------------------------------------------------
Function A340CRITMP(cEmpJob,cFilJob,lNewTEMP,cInvRes,cSB7Comp,lWmsNew)
Local aCampos 	
Local aTam		
Local cGetRealName := ''
Local oInvRes
Local oSB7Comp
Local cInvRName
Local cSB7RName

// Seta job para nao consumir licensas
RpcSetType(3)

// Seta job para empresa filial desejadas
RPCSetEnv(cEmpJob,cFilJob,,,'EST')

//- verifica se o objeto está criado 
If oInvRes == nil 
	//-- Criação da Tabela de Dados de Registros que serão contabilizados
	aCampos := {}
	aAdd(aCampos, {"CLOG"	,'C',20,0})
	
	aTam := TamSX3("B7_COD"		)
	aAdd(aCampos, {"CODPRO"	,aTam[03],aTam[01],aTam[02]})
	
	aTam := TamSX3("B7_LOCAL"	)
	aAdd(aCampos, {"LOCAL"		,aTam[03],aTam[01],aTam[02]})
	
	aTam := TamSX3("B7_LOTECTL"	)
	aAdd(aCampos, {"LOTECTL"	,aTam[03],aTam[01],aTam[02]})
	
	aTam := TamSX3("B7_NUMLOTE"	)
	aAdd(aCampos, {"LOTE"		,aTam[03],aTam[01],aTam[02]})
	
	aTam := TamSX3("B7_LOCALIZ"	)
	aAdd(aCampos, {"LOCALIZA"	,aTam[03],aTam[01],aTam[02]})
	
	aTam := TamSX3("B7_NUMSERI"	)
	aAdd(aCampos, {"NUMSERIE"	,aTam[03],aTam[01],aTam[02]})
	
	aTam := TamSX3("B2_QATU"	)
	aAdd(aCampos, {"SLDATUAL"	,aTam[03],aTam[01],aTam[02]})
	
	aTam := TamSX3("B7_QUANT"	)
	aAdd(aCampos, {"QUANT"		,aTam[03],aTam[01],aTam[02]})
	
	aTam := TamSX3("B7_QUANT"	)
	aAdd(aCampos, {"EMPENHO"	,aTam[03],aTam[01],aTam[02]})
	
	aTam := TamSX3("B7_DOC"		)
	aAdd(aCampos, {"DOCUMENTO",aTam[03],aTam[01],aTam[02]})
	
	//- criação da tabela temporária compartilhada 
	If lNewTEMP
		oInvRes := totvs.framework.database.temporary.SharedTable():New(cInvRes,aCampos)
	else
		//-------------------
		//Criaç?o do objeto
		//-------------------
		oInvRes := FWTemporaryTable():New( cInvRes )
		//--------------------------
		//Monta os campos da tabela
		//--------------------------
		oInvRes:SetFields( aCampos )
	EndIf
	oInvRes:Create() 
	aSize(aCampos,0)
	aCampos := nil 
	aSize(aTam,0)
	aTam := nil

	cInvRName := oInvRes:GETREALNAME()
Else  
	//- efetua a exclusão dos registros que possam 
	//- estar presentes de outras execuções 
	If lNewTEMP
		oInvRes:Zap()
	Else
		cGetRealName := oInvRes:GETREALNAME()
		TcSqlExec('Truncate table '+cGetRealName)
	EndIf
EndIf 


If lWmsNew
	//- verifica se o objeto está criado 
	if oSB7Comp == nil 
		aCampos := {}
		aTam := TamSX3("D11_PRDORI"	)
		aAdd(aCampos, {"PRODORIG"	,aTam[03],aTam[01],aTam[02]})
		
		aTam := TamSX3("B7_COD"		)
		aAdd(aCampos, {"CODPRO"		,aTam[03],aTam[01],aTam[02]})
		aAdd(aCampos, {"RECNO"		,'N',8,0})

		//- criação da tabela temporária compartilhada 
		If lNewTEMP
			oSB7Comp := totvs.framework.database.temporary.SharedTable():New(cSB7Comp,aCampos) 
		Else
			//-------------------
			//Criaç?o do objeto
			//-------------------
			oSB7Comp := FWTemporaryTable():New( cSB7Comp )
			//--------------------------
			//Monta os campos da tabela
			//--------------------------
			oSB7Comp:SetFields( aCampos )
		EndIf 
		oSB7Comp:Create()
		aSize(aCampos,0)
		aCampos := nil 

		aSize(aTam,0)
		aTam := nil 

		cSB7RName := oSB7Comp:GETREALNAME()
	Else 
		//- efetua a exclusão dos registros que possam 
		//- estar presentes de outras execuções 
		If lNewTEMP
			oSB7Comp:Zap()
		Else
			cGetRealName := oSB7Comp:GETREALNAME()
			TcSqlExec('Truncate table '+cGetRealName)
		EndIf
	EndIf 
EndIf

RpcClearEnv()

Return({cInvRes,cSB7Comp,cInvRName,cSB7RName})

