If !lRastroO .And. dDtVldDest < dDataBase
	dDtVldDest	:= dDtValid
	lHelp		:= .T.
EndIf
If lHelp
	HelpAutoma(" ",1,"A240DTVALI",,,,,,,,,.F.)
EndIf
RestArea(aAreaSB8)
Return lRet

