Local aAreaSB1   := SB1->(GetArea())
Local aAreaSF5   := SF5->(GetArea())

dbSelectArea('SB1')
dbSetOrder(1)
lRet := MsSeek(xFilial('SB1')+cProduto, .F.) .And. (B1_APROPRI == 'I')

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
