		If self:OJSONOBJ:OJSONOBJ['items'][nY]['percposit3'] == 0
			self:OJSONOBJ:OJSONOBJ['items'][nY]['status3'] := STR0005 //Neutro
		Elseif self:OJSONOBJ:OJSONOBJ['items'][nY]['percposit3'] > 0
			self:OJSONOBJ:OJSONOBJ['items'][nY]['status3'] := STR0006 //Positivo
		Else 
			self:OJSONOBJ:OJSONOBJ['items'][nY]['status3'] := STR0007 //Negativo
		EndIf

		If self:OJSONOBJ:OJSONOBJ['items'][nY]['percposit4'] == 0
			self:OJSONOBJ:OJSONOBJ['items'][nY]['status4'] := STR0005 //Neutro
		Elseif self:OJSONOBJ:OJSONOBJ['items'][nY]['percposit4'] > 0
