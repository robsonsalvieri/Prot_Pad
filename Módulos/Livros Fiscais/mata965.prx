ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß
*/
FUNCTION A965CFO()

Local cVar:=ReadVar(), Conteudo, lHelp:=.f., i

Conteudo:=Alltrim(&cVar)

If Empty(Conteudo)
	Return .t.
Endif

cCFO:=""
aCFO:={}
j:=1
If Substr(aListBox[nAt],5,1) <> "9" .and. mv_par01 == 2
	For i:=1 to Len(Conteudo)
		cDigito:=Substr(Conteudo,i,1)
		// No campo so podem ser digitados numeros e virgulas
		If !(cDigito$"0123456789,") .or. ((i%4==0 .and. cDigito!=",").AND. (i%5==0 .and. cDigito!=",");
			.AND. (i%6==0 .and. cDigito!=","))
			lHelp:=.t.
			Exit
		Endif
		If j==1 .and. cDigito!=","
			AADD(aCFO,"")
		Endif
		If cDigito!=","
			aCFO[Len(aCFO)]+=cDigito
			j++
		EndIf
		j:=IIf(cDigito==",",1,j)
	Next
Endif
If !lHelp
	For i:=1 to Len(aCFO)
		If (Len(aCFO[i])!=3 .and. Len(aCFO[i])!=4 .and. Len(aCFO[i])!=5) .or. "*"$VerCFO(aCFO[i])
			lHelp:=.t.
			Exit
		Endif
	Next
Endif

If lHelp
	Help(" ",1,"A965CFO")
	Return .f.
Endif

Return .t.
