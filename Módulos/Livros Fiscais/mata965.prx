*/
STATIC FUNCTION A965LoadIni(ArqIni)

LOCAL i, cConteudo, oFile, aLido

If File(ArqIni)
	oFile := FWFileReader():New(ArqIni)
	If (oFile:Open())
   		While (oFile:hasLine())
      		aLido := oFile:getAllLines()      	
   		End
   		oFile:Close()
	EndIf	
	For i:=1 to Len(aListBox)
		cConteudo:=""
		If IsDigit(Substr(aListBox[i],5,3)) 
			cConteudo:=A965LeIni(Substr(aListBox[i],5,3),.F.,ArqIni,aLido)
			If Len(cConteudo)<500
				cConteudo:=cConteudo+Space(500-Len(cConteudo))
			EndIf
		Endif
		AADD(aConteudo,cConteudo)
	Next
Else
	For i:=1 to Len(aListBox)
		AADD(aConteudo,Space(500))
	Next
Endif

RETURN
