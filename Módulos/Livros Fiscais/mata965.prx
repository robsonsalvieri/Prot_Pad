*/
FUNCTION A965LeIni(cCPO,lEnd,cArqIni,aLido)

Local		cIni:="", cConteudo:="", i:=0
Default	cArqIni:="SIGADMEF.CFP"
Default	aLido	:=	{}


lEnd:=If(lEnd==NIL,.F.,lEnd)
cCPO:="["+cCPO+"]="

If !File(cArqIni)
	Help(" ",1,"SIGADMEF")
	lContinua:=.f.
	lEnd:=.T.
	RETURN (cIni)
Else	
	If Len(aLido) > 0
		nLinhas := Len(aLido)		
		For i:=4 to nLinhas
			cLinha:=aLido[i]
			If cCPO$cLinha
				cIni:=Substr(cLinha,7)
				Exit
			Endif
		Next
	Else
		cConteudo:=MemoRead(cArqIni)
		nLinhas:=MlCount(cConteudo,254)
		For i:=4 to nLinhas
			cLinha:=AllTrim(MemoLine(cConteudo,254,i))
			If cCPO$cLinha
				cIni:=Substr(cLinha,7)
				Exit
			Endif
		Next
	Endif
Endif

RETURN (cIni)
