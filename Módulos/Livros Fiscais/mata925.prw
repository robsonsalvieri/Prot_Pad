Function a925Alter()
Local nReg	:=CC1->(Recno())
Local nOpc	:=4
Local cAlias:="CC1"
Local nX    :=0
Local aCpos :={}

For nX := 1 To FCount()
	M->&(FieldName(nX)) := CriaVar(FieldName(nX))
    // Campos nao editaveis
	If !(FieldName(nX)) $ "CC1_CODIGO"
		Aadd(aCpos,FieldName(nX))
	EndIf
Next nX

AxAltera( cAlias, nReg, nOpc,,aCpos)
Return(.T.)

