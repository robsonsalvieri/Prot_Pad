				//Adiciona valor de IPI ou ST não creditado ao valmerc
				If cEntSai == "1" .And. (!(cAliasSF4)->F4_LFIPI$"T" .Or. (cAliasSFT)->FT_CREDST $ "3|4" ) .And. !(cAliasSFT)->FT_TIPO$"DB"
