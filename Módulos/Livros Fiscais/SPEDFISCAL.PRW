/*/{Protheus.doc} IndEmit()
@description Função para verificar o indicador de emissão do documentos,
			   baseando-se no indicador de formulário próprio do livro
			   e se é documento de saída ou entrada.
@param	cFormProp,String,Indica se documento é formulário próprio S=Sim/N=Não
@param	cFormProp,String,Indica se é documento de 1==Entrada ou 2 ==Saída
@return cIndEmit,String,1=Emissão de terceiros/2=Emissão própria
@author Erick G. Dias
@since 12/11/2014
@version 11.90
/*/
//-------------------------------------------------------------------
Static Function IndEmit(cFormProp, cEntSai)
Local cIndEmit	:= ''
If (Empty (cFormProp)) .And. cEntSai=="1"
	//Documento de entrada com sem formulário próprio é emissão de terceiros
	cIndEmit := "1"
ElseIf (Empty (cFormProp)) .And. cEntSai=="2"
	//Documento de saída sem formulário próprio é emissão própria
	cIndEmit := "0"
Else
	If ("S"$cFormProp)
		//Se estiver preenchido como formulário próprio será emissão própria
		cIndEmit := "0"
	Else
		//Se não estiver preenchido como formulário próprio será emissão de terceiros
		cIndEmit := "1"
	EndIf
EndIf
Return cIndEmit
/*/{Protheus.doc} RetCdalan
    Função que filtrar as notas emitidas em períodos anteriores e canceladas 
    no período atual, a fim de facilitar o processamento de estorno ou crédito.
    @type  Function
    @author Bruce Mello
    @since 05/04/2024
    @version version
    @param cAliasCDA alias da tabela CDA
    @param dData data de inicio do período
    @param lOption opção para abrir ou fechar o alias
    @return cAliasCDA alias da tabela CDA

    @see (links_or_references)
    /*/
Function RetCdaLanc(cCodLan, cAliasCDA, dDtIni, lOption,cNrLivro)
Local cQuery    := ""
Local cTipoMov  := "S"
Local oMontQry  := Nil
Local cLivro    := ""
Local nPos      := 0

If lOption // Se a opção for verdadeira, cria o alias

	 IF cNrLivro <> '*'
        cLivro := "AND SFT.FT_NRLIVRO = ? "
    Endif

    cQuery := " SELECT CDA.CDA_CODLAN, CDA.CDA_ESPECI, CDA.CDA_NUMERO, CDA.CDA_SERIE, SUM(CDA.CDA_VALOR) CDA_VALOR, SFT.FT_EMISSAO, SFT.FT_CHVNFE "    
    cQuery += "	FROM " + RetSqlName("CDA") + " CDA"
    cQuery += "	INNER JOIN " + RetSqlName("SFT") + " SFT ON"
	cQuery += "	    SFT.FT_FILIAL = ?"
    cQuery += "		AND SFT.FT_TIPOMOV = CDA.CDA_TPMOVI"
    cQuery += "		AND SFT.FT_SERIE = CDA.CDA_SERIE"
	cQuery += "		AND SFT.FT_NFISCAL = CDA.CDA_NUMERO"
    cQuery += "		AND SFT.FT_CLIEFOR = CDA.CDA_CLIFOR"
    cQuery += "		AND SFT.FT_LOJA = CDA.CDA_LOJA"
    cQuery += "		AND SFT.FT_ITEM = CDA.CDA_NUMITE"
    cQuery += "		AND SFT.D_E_L_E_T_ = ? "
    cQuery += "	WHERE CDA.CDA_FILIAL =  ?"
    cQuery += "	AND CDA.CDA_TPMOVI = ?"
    cQuery += "	AND CDA.CDA_ESPECI IN ( ? )"
    cQuery += "	AND CDA.CDA_ORIGEM = ?"
	cQuery += "	AND CDA.CDA_CODLAN = ?"
    cQuery += "	AND SFT.FT_DTCANC BETWEEN ? AND ? "
	cQuery +=  cLivro
    cQuery += "	AND CDA.D_E_L_E_T_ = ?	"
    cQuery += " GROUP BY CDA.CDA_CODLAN, CDA.CDA_ESPECI, CDA.CDA_NUMERO, CDA.CDA_SERIE, SFT.FT_EMISSAO, SFT.FT_CHVNFE "    
    cQuery += " ORDER BY CDA.CDA_ESPECI,CDA_SERIE, CDA.CDA_NUMERO"

    oMontQry := FwExecStatement():New(cQuery)
	oMontQry:SetString(nPos += 1, xFilial("SFT"))
	oMontQry:SetString(nPos += 1, ' ')
    oMontQry:SetString(nPos += 1, xFilial("CDA"))
    oMontQry:SetString(nPos += 1, cTipoMov)
    oMontQry:SetIn(nPos += 1, ConvModxEsp(Firstday(dDtIni), LastDay(dDtIni), "67#63#57"))
    oMontQry:SetString(nPos += 1, "6")
	oMontQry:SetString(nPos += 1, cCodLan)
    oMontQry:SetDate(nPos += 1, Firstday(dDtIni))
    oMontQry:SetDate(nPos += 1, LastDay(dDtIni))
	If cNrLivro <> '*'
        oMontQry:SetString(nPos += 1, cNrLivro)
    Endif
	oMontQry:SetString(nPos += 1, ' ')

    cAliasCDA := oMontQry:OpenAlias()

    oMontQry:Destroy()
    oMontQry := Nil
Else // Se a opção for falsa, Fecha o alias
    If Select(cAliasCDA) <> 0
        (cAliasCDA)->(DbCloseArea())
    Endif    
Endif

RETURN cAliasCDA

/*/{Protheus.doc} RetApurE113
    @type  Função que monta o registro E113 do SPED Fiscal para documento cancelados.
    @author Bruce Mello
    @since 24/04/2024
    @version 12.1.2410
    @param aRegE110 {Array} - Array contendo os registros E110.
    @param aRegE113 {Array} - Array vazio para armazenar os registros E113.
    @param aRegE111 {Array} - Array contendo os registros E111.
    @param cFilSA1 {Character} - Código da filial SA1.
    @param aRegApur {Array} - Array contendo os registros de apuração.
    @param dDataDe {Date} - Data de referência para a apuração.
    @returns {Array} - Array contendo os registros E113 de apuração, produtos e participantes.
    /*/
Function RetApurE113(aRegE110,aRegE113,aRegE111,cFilSA1,aRegApur,dDataDe,cNrLivro,jE113Reg)
    Local nQtdAjuste := 0
    Local nCount     := 0
    Local lReg       := .F.
    Local cCodLan    := ""
    Local cDescLanc  := ""
    Local aProd      := {}
    Local aSa2       := {}
    Local aE113Apur  := {}
    Local nSeqReg111 := 0
    Local dEmissao   := StoD("")
    Local cAliasCDA  := ""
	Local jRegE111 	 := JsonObject():New()
	Local jRegE113 	 := JsonObject():New()
	Local cChaveE111 := ""
	Local cChaveE113 := ""	
	Local cDescrE111 := ""
	Local lSeekCDO	 := .F.

	DEFAULT cNrLivro := '*'
	DEFAULT jE113Reg := JsonObject():New()

    nQtdAjuste := Len(aRegApur)

	if nQtdAjuste > 0 
	
		For nCount := 1 to nQtdAjuste
			cCodLan := AllTrim(aRegApur[nCount][2])
			
			If aSPDSX3[FP_CDO_GRE113]
				lSeekCDO := fisCDOSeek(cCodLan)
			EndIf
			
			If geraE113CDO(lSeekCDO)
				if aRegApur[nCount][9] == ''// CDO_DESCR2
					cDescLanc := aRegApur[nCount][7] // CDO_DESCR
					cChaveE113 := MD5(cCodLan+Substring(cDescLanc,1,50))
				Else
					cDescLanc := UPPER(FwNoAccent(MSMM(AllTrim(aRegApur[nCount][9]))))
					cChaveE113 := MD5(cCodLan+Substring(cDescLanc,1,50))
				Endif
				jRegE113[cChaveE113] := nCount
			EndIf
		Next
			
		For nCount := 1 to Len(aRegE111)
			cDescrE111 := Substring(UPPER(FwNoAccent(alltrim(aRegE111[nCount][4]))),1,50)
			cChaveE111 := MD5(alltrim(aRegE111[nCount][3])+cDescrE111)
			cCodlan := alltrim(aRegE111[ncount][3])
			
			jRegE111[cChaveE111] := nCount
			
			If jRegE113:hasProperty(cChaveE111)					
				nSeqReg111 := jRegE111[cChaveE111]

				If !jE113Reg:hasProperty(cValToChar(nSeqReg111))
					
					RetCdaLanc(cCodLan, @cAliasCDA, dDataDe, .T.,cNrLivro)

					Do While !(cAliasCDA)->(EOF())
						lReg := .T.

						dEmissao := Stod((cAliasCDA)->FT_EMISSAO)

						AADD(aRegE113,{;
								nSeqReg111,; //Sequencial do Registro 111
								"E113",; // Código do Registro
								"",;//aConfPart[1]+iif(lConcFil,(cAliasCDA)->FT_FILIAL,"")+alltrim((cAliasCDA)->FT_CLIEFOR)+alltrim((cAliasCDA)->FT_LOJA),; //Código do Participante
								AModNot((cAliasCDA)->CDA_ESPECI),; // Código da especie do Documento
								(cAliasCDA)->CDA_SERIE,; // Serie do Documento
								"",;
								(cAliasCDA)->CDA_NUMERO,; // Numero do Documento
								StrZero(Day((dEmissao)),2)+StrZero(Month(dEmissao),2)+StrZero(Year(dEmissao),4),; // data do movimento
								"",;//(cAliasCDA)->FT_PRODUTO+iif(lConcFil,alltrim((cAliasCDA)->FT_FILIAL),""),; // Código do produto
								(cAliasCDA)->CDA_VALOR,; // Valor do ICMS
								(cAliasCDA)->FT_CHVNFE; // Chave da NFe
								})
						//aAdd(aProd,{(cAliasCDA)->FT_PRODUTO+IIF(lConcFil,(cAliasCDA)->FT_FILIAL,"")})				

						(cAliasCDA)->(DbSkip())
					Enddo

					//Fecha o alias
					RetCdaLanc("",cAliasCDA, dDataDe, .F.)
				EndIf	
			Endif
		Next
	Endif
    
    aE113Apur := {aRegE113,aProd,aSa2,lReg}

	FreeObj(jRegE111)
	jRegE111 := Nil

Return aE113Apur

/*/{Protheus.doc} GeraMunici
    @type  Função responsvel pela verificação dos codigos dos municipios para geração do Registro E300 e E310.
    @author Carlos Silva
    @since 05/02/2025
    @version 12.1.2410
    /*/
Static Function GeraMunici(cEntSai, cCdMunOri, cCdMunDes, aCmpAntSFT)
    If cEntSai == "2"
        cCdMunOri := IIf(aCmpAntSFT[54]=="EX","9999999",UfCodIBGE(aCmpAntSFT[54]) + AllTrim(aCmpAntSFT[55]))
        cCdMunDes := IIf(aCmpAntSFT[56]=="EX","9999999",UfCodIBGE(aCmpAntSFT[56]) + AllTrim(aCmpAntSFT[57]))

	ElseIf cEntSai == "1" 
		cCdMunOri := IIf(aCmpAntSFT[49]=="EX","9999999",UfCodIBGE(aCmpAntSFT[49]) + AllTrim(aCmpAntSFT[50]))
		cCdMunDes := IIf(aCmpAntSFT[51]=="EX","9999999",UfCodIBGE(aCmpAntSFT[51]) + AllTrim(aCmpAntSFT[52]))
	Endif		
    
Return
	
/*/{Protheus.doc} BpeMunDife
    @type  Função responsvel pela verificação dos digitos iniciais dos codigos dos municipios para geração do Registro E300 e E310.
    @author Carlos Silva
    @since 05/02/2025
    @version 12.1.2410
    @returns lBpeMunDif {Logical} - Retorno lógico da comparação dos municipios.
    /*/
Static Function BpeMunDife(cEspecie, cCdMunOri, cCdMunDes)
    Local lBpeMunDif := .F.

    lBpeMunDif := ( !Empty(cCdMunOri) .And. !Empty(cCdMunDes) .And. cEspecie == "63" .And. SubStr(cCdMunOri,1,2) <> SubStr(cCdMunDes,1,2) .And. cCdMunDes <> "9999999" ) 	              

Return lBpeMunDif

/*/{Protheus.doc} geraE113CDO()
    Função de determina se gera ou não o registro E113 para aquele código de ajuste/lançamento, valiando o parâmetro MV_ESTE113 e o campo CDO_GRE113
	@param 		lSeekCDO, logical, Verifica se deverá fazer um seek na tabela CDO
	@return		lGeraE113, logical, retorna .T. se com base no campo CDO_GRE113 deve gerar ou não o E113
	@type		Static Function
	@author 	Matheus Bispo
    @since 		24/02/2025
    @version 	12.1.2410
/*/
Static Function geraE113CDO(lSeekCDO as logical)
	Local lGeraE113 := .T. as logical

	If lSeekCDO
		lGeraE113 := (CDO->CDO_GRE113 == "1" .Or. CDO->CDO_GRE113 == " ")
	EndIf
Return lGeraE113

/*/{Protheus.doc} fisCDOSeek()
    Função componentizada para dar seek na tabela CDO ordenando pelo indice 1 (CDO_FILIAL, CDO_CODAJU, R_E_C_N_O_, D_E_L_E_T_)
	@param 		cCodAjApur, character, Informa o código de lançamento/ajuste para fazer seek na CDO
	@return		lRet, logical, retorna .T. se houver achado o registro na tabela
	@type		Static Function
	@author 	Matheus Bispo
    @since 		24/02/2025
    @version 	12.1.2410
/*/
Static Function fisCDOSeek(cCodAjApur as character)
	Local lRet := .F. as logical

	lRet := CDO->(MsSeek(aSPDFil[PFIL_CDO]+cCodAjApur))
Return lRet


/*/{Protheus.doc} fdIndMov
	Função que faz a verificação do preenchimento do campo IND_MOV 
	@type  Static Function
	@author Talita Teixeira
	@since 17/03/2025
	@version 12.1.2410
	@param cAliasSFT, character, alias contendo o retorno da query com os registros | cAliasSF4, character, alias contendo retorno das informações da tabela SF4
	@return cRetMov, caracter, retorna com o código do IND_MOV
/*/

Static Function fdIndMov(cAliasSFT, cAliasSF4, lAchouSF4)
    Local cRetMov    := "1"
    Local lMOVComp   := .F.
    Local lTipoCompl := (cAliasSFT)->FT_TIPO $ "IPC"
    Local lMovFis    := lAchouSF4 .And. (cAliasSF4)->F4_MOVFIS == "S"

//Para Notas Fiscais de complemento de IPI/ICMS o campo referente ao valor do item deve ser zero.   
If lTipoCompl
	If aSPDSX3[FP_F1_TPCOMPL] .And. (cAliasSFT)->FT_TIPOMOV == "E" .And. (cAliasSFT)->F1_TPCOMPL == "2"
		lMOVComp := .T.
	ElseIf aSPDSX3[FP_F2_TPCOMPL] .And. (cAliasSFT)->FT_TIPOMOV == "S" .And. (cAliasSFT)->F2_TPCOMPL == "2"
		lMOVComp := .T.
	EndIf
EndIf

//Tratamento movimentacao fisica sem movimentacao de estoque
If (lTipoCompl .And. lMOVComp .And. lMovFis) .Or. (!lTipoCompl .And. lMovFis)   
    cRetMov := "0"
EndIf

Return cRetMov

/*/{Protheus.doc} CpoNfOrige
    (Verifica se possui numero da nota original.)
    @type  Static Function
    @author Delleon Fernandes
    @since 16/04/2025
    @version 12.1.2410
	@param cNFOri     , Caracter, Numero da Nota Original
	@param cItemOri   , Caracter, Item da Nota Original
	@param cSerOri    , Caracter, Serie da Nota Original
	@param aCmpAntSFT , Array   , Array com as informações que se referem ao cabecalho do documento fiscal
	@param cEntSai    , Caracter, Tipo de Operação da NF 1-Entrada;2-Saida
	@param cDocbpe    , Caracter, Código da Situação do documento. TP_CT-e do registro D100
    @return lRet, Logico, Informa se tem documento origem amarrado
/*/
Static Function CpoNfOrige(cNFOri as character, cItemOri as character, cSerOri as character, aCmpAntSFT as array, cEntSai as character, cDocbpe as character)
    Local lRet := .F. as logical

    If (cDocbpe=="3" .And. cEntSai=="2") .Or. (cEntSai=="1" .And. !Empty(aCmpAntSFT[38]) .And. !Empty(aCmpAntSFT[39]) .And. !Empty(aCmpAntSFT[65]))
        cNFOri   := aCmpAntSFT[38] 
        cItemOri := aCmpAntSFT[65]
        cSerOri  := aCmpAntSFT[39]
        lRet     := .T.
    ElseIf cEntSai=="2" .and. !Empty(aCmpAntSFT[67]) .And. !Empty(aCmpAntSFT[66])
        cNFOri   := aCmpAntSFT[67] 
        cItemOri := aCmpAntSFT[46]
        cSerOri  := aCmpAntSFT[66]
        lRet     := .T.
    EndIf

Return lRet


/*/{Protheus.doc} CTESeekCDD
    @description Função para posicionar no CTe substituido lançado na tabela CDD 
		que não está lançado no sistema para atender o campo 14 do registro D100.
    @type Static Function
	@author Delleon Fernandes
	@since 25/04/2025
	@version 12.1.2410
	@param cEntSai   , Caracter, Tipo de Operação da NF 1-Entrada;2-Saida
	@param aSPDFil   , Array   , Filial da tabela CDD
	@param aParFil   , Array   , Dados da nota fiscal
    @return lRet     , Logico  , Informa se tem documento origem amarrado.
/*/
Static Function CTESeekCDD(cEntSai as character, aSPDFil as array, aParFil as array, cEspecieDoc as character) as logical
	Local lRet    := .F. as logical
	Local cQry    := ""  as character
	Local nRecno  := 0   as numeric

	If oCTECDD == Nil
		cQry := "SELECT R_E_C_N_O_ AS RECNOCDD "
		cQry += " FROM "+RetSqlName("CDD")
		cQry += " WHERE CDD_FILIAL = ? "
		cQry += " AND CDD_TPMOV = ? "
		cQry += " AND CDD_DOC = ? AND CDD_SERIE = ? "
		cQry += " AND CDD_CLIFOR = ? AND CDD_LOJA = ? "
		cQry += " AND CDD_ENTSAI = ? AND CDD_MODREF = ? "
		cQry += " AND D_E_L_E_T_ = ? "

		oCTECDD := FwExecStatement():New(cQry)
	EndIf

	oCTECDD:SetString(1, aSPDFil[PFIL_CDD])
	oCTECDD:SetString(2, aParFil[1])
	oCTECDD:SetString(3, aParFil[2])
	oCTECDD:SetString(4, aParFil[3])	
	oCTECDD:SetString(5, aParFil[4])
	oCTECDD:SetString(6, aParFil[5])
	oCTECDD:SetString(7, cEntSai)
	oCTECDD:SetString(8, cEspecieDoc)	
	oCTECDD:SetString(9, " ")

	nRecno := oCTECDD:ExecScalar('RECNOCDD')

	lRet := nRecno > 0
	If lRet
		CDD->(DbGoto(nRecno))
	EndIf
	
Return lRet
