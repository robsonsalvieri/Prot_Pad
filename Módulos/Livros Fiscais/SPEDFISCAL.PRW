				    EndIf       
				    
					If (cAliasSFT)->FT_CREDST $ "3|4" .And. !(cAliasSFT)->FT_ANTICMS == "1"
						aTotaliza[10]	+= 	RetStSpd(lResF3FT, cAliasSFT)												
					EndIf
