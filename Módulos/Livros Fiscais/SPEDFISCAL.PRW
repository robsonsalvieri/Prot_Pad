					If lOldLan
						cCodAjApur := cUf+"0"+"4"+"9999"
					EndIf
				EndIf
				
				//Case que determina o calculo do (13)VL_ICMS_RECOLHER = Valor total de
				//"ICMS A RECOLHER"
				
			Case (cAliasCDH)->CDH_LINHA == "013"
