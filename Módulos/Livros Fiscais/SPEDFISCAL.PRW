								cIndEmit := Iif(SF2->F2_FORMUL<>"N","0","1")
								cIndOper := Iif(cIndEmit == "1","0","1")
								cCodMod  := aModNot(SF2->F2_ESPECIE)
								dDtEmiss := SF2->F2_EMISSAO		
