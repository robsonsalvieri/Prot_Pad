Local	cIndOper 	:=	''
Local	cCodMod  	:=	''
Local  	cCodPArt	:= 	''
Local 	cCDDChv		:= (cAliasCDD)->(CDD_FILIAL+CDD_TPMOV+CDD_DOC+CDD_SERIE+CDD_CLIFOR+CDD_LOJA)
Local	dDtEmiss 	:=	CToD ("//")     
