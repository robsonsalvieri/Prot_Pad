/*/{Protheus.doc} SPEDDUD() 
@description Função para buscar dados da DUD, Issue DSERFIS1-33468
@author Matheus Massarotto
@since 03/08/2023
@version 12.1.2210
/*/
//-------------------------------------------------------------------
Static Function SPEDDUD(cFilDoc,cDoc,cSerie,cFilOri)
Local cQuery	:= ""

if oExecDUD==Nil

	cQuery 	:= " SELECT DUD_FILIAL, DUD_FILDOC, DUD_DOC, DUD_SERIE, DUD_FILORI, MIN(DUD_VIAGEM) DUD_VIAGEM, COUNT(DUD_DOC) COUNTDUD1, MIN(R_E_C_N_O_) DUD1RECNO "
	cQuery 	+= " FROM "+RetSqlName("DUD")
	cQuery 	+= " WHERE DUD_FILIAL=? AND D_E_L_E_T_= ' ' AND DUD_FILDOC = ? AND DUD_DOC = ? AND DUD_SERIE = ? AND DUD_FILORI = ? "
	cQuery 	+= " GROUP BY DUD_FILIAL, DUD_FILDOC, DUD_DOC, DUD_SERIE, DUD_FILORI, DUD_FILORI "
		
	cQuery := ChangeQuery(cQuery)
	
	oExecDUD := FwExecStatement():New(cQuery)
	oJsonDUD := JsonObject():New()

endif

if oJsonDUD[aSPDFil[PFIL_DUD]+cFilDoc+cDoc+cSerie+cFilOri]==Nil

	oExecDUD:SetString(1,aSPDFil[PFIL_DUD])
	oExecDUD:SetString(2,cFilDoc)
	oExecDUD:SetString(3,cDoc)
	oExecDUD:SetString(4,cSerie)
	oExecDUD:SetString(5,cFilOri)

	oJsonDUD[aSPDFil[PFIL_DUD]+cFilDoc+cDoc+cSerie+cFilOri]:= oExecDUD:ExecScalar('DUD_VIAGEM')

endif

Return(oJsonDUD[aSPDFil[PFIL_DUD]+cFilDoc+cDoc+cSerie+cFilOri])

/*/{Protheus.doc} ChvDocOri
    @description Função para buscar dados da SFT origem.
    @type Static Function
	@author Sandro Farizel
	@since 09/10/2023
	@version 12.1.2210
	@param cNFOri     , Caracter, Numero da Nota Original
	@param cItemOri   , Caracter, Item da Nota Original
	@param cSerOri    , Caracter, Serie da Nota Original
	@param aCmpAntSFT , Array   , Array com as informações que se referem ao cabecalho do documento fiscal
	@param cEntSai    , Caracter, Tipo de Operação da NF 1-Entrada;2-Saida
    @return lRet, Logico, Informa se tem documento origem amarrado
/*/
Static Function ChvDocOri(cNFOri as character, cItemOri as character, cSerOri as character, aCmpAntSFT as array, cEntSai as character, lDocTms as logical)
	Local cQry     := "" as character
	Local cChave   := "" as character
	Local cTipoMov := "S" as character
	Local nBind    := 0 as numeric
	Local oSFTOri  := Nil as object

	If cEntSai=="1"
		cTipoMov := "E"
	EndIf

	cQry := " SELECT FT_CHVNFE "
	cQry += " FROM "+RetSqlName("SFT")
	cQry += " WHERE FT_FILIAL = ? AND FT_TIPOMOV = ? AND FT_SERIE = ? AND FT_NFISCAL = ? "

	If cTipoMov == "S" .And. lDocTms
		cQry += " AND FT_EMISSAO  >= ? "
	Else
		cQry += " AND FT_CLIEFOR  = ? AND FT_LOJA    = ? "
	EndIf

	cQry += " AND FT_ITEM  = ? "
	cQry += " AND FT_ESPECIE = ? "
	cQry += " AND D_E_L_E_T_ = ?"

	oSFTOri := FwExecStatement():New(cQry)

	oSFTOri:SetString(++nBind, aSPDFil[PFIL_SFT])
	oSFTOri:SetString(++nBind, cTipoMov)
	oSFTOri:SetString(++nBind, cSerOri)
	oSFTOri:SetString(++nBind, cNFOri)
	
	If cTipoMov == "S" .And. lDocTms
		// Prazo de 60 dias para Substituição de CTE. 
		// Utilizo também como proteção pois não utilizo a chave de cliente e loja 
		// pois pode haver mudanças de cliente no processo de substituição, evitando conflito de numeração DOC+SERIE.
		oSFTOri:SetDate(++nBind, (aCmpAntSFT[6] - 60))
	Else
		oSFTOri:SetString(++nBind, aCmpAntSFT[3])
		oSFTOri:SetString(++nBind, aCmpAntSFT[4])
	EndIf

	oSFTOri:SetString(++nBind, cItemOri)
	oSFTOri:SetString(++nBind, aCmpAntSFT[42])	
	oSFTOri:SetString(++nBind, " ")

	cChave := oSFTOri:ExecScalar('FT_CHVNFE')

	If oSFTOri != Nil	
		oSFTOri:Destroy()
		oSFTOri := Nil
	EndIf
	
Return(cChave)


