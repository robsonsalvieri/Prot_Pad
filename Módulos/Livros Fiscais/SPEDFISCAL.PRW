While !(cAliasCDG)->(Eof()) .AND. ( (cAliasCDG)->(CDG_FILIAL+CDG_TPMOV+CDG_DOC+CDG_SERIE+CDG_CLIFOR+CDG_LOJA)==cCDGChv)

	//³Processo o mesmo codigo de OBS da CDT e da CDG³
	If !(cAliasCDG)->(Empty(CDG_IFCOMP))

