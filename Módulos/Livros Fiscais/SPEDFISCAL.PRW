/*/{Protheus.doc} Add0221
@Sandro Farizel
@since 22/11/2023
@version 12.1.2210
 Parametros por referência: @aReg0200,@nPos0221
Função que adiciona nova linha no array do registro 0221
/*/
//-------------------------------------------------------------------
Static Function Add0221(cAlias,dDataDe,dDataAte,aReg0190,aReg0200,aReg0220,aWizard,nPos0221, cChave, cDescPro, cUnid, cNCM, cExNCM, nQtdAtom,cChavePai)
	Local nPos        := 0
	Local nHash0221   := 0
	Local nPos0200    := 0 //Passado por referência para a função SFRG0200 e retornar a posição do 0200 criado/usado
	Default cChavePai := ""

	If cChavePai == ""
		nPos      := FisFindHash(oHash0200, cChave)
		nPos0200  := nPos 	
		nHash0221 := FisFindHash(oHash0221, cChave)
		If nPos == 0
		//³Tratamento para que os itens utilizados seja gerados no 0200³
			SPEDSeek("SB1",,aSPDFil[PFIL_SB1]+cChave)
			SFRG0200( cAlias, @aReg0200, @aReg0190, dDataDe, dDataAte, ,cChave , @aReg0220,,,,,,,,,,,,,,,,,,,aWizard,,,,,.T.,@nPos0200)		
		EndIf		
	else
		nPos0200 := FisFindHash(oHash0200, cChavePai) //Posiciona registro 0200 pai para criar os 0221 filhos
	Endif
		
	If nHash0221 == 0 .Or. (cChavePai != "" .And. nPos0200 > 0)
		aAdd(aReg0221, {})
		nPos0221 += 1
		aAdd(aReg0221[nPos0221], nPos0200)	//01 - Relacionamento com o PAI
		aAdd(aReg0221[nPos0221], "0221")	//02 - Registro
		aAdd(aReg0221[nPos0221], cChave)	//03 - Produto atômico
		aAdd(aReg0221[nPos0221], nQtdAtom)	//04 - Qtde
		FisAddHash(oHash0221,cChave,Len(aReg0221))
	Endif	
           
Return

