				If cEntSai == "1" .AND. (cAliasSF4)->F4_LFIPI $ "O" .And. (cAliasSFT)->FT_TIPO $ "DB" .AND. AllTrim((cAliasSFT)->FT_CFOP) $ "1908/2908/1909/2909"
				    aTotaliza[10] += (cAliasSFT)->FT_IPIOBS
