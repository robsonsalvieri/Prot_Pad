				If !(lOldLan .And. SubStr((cAliasCDH)->CDH_CODLAN,1,1)=="1") .And.;	//No formato antigo, os que iniciam por 1 sao lancamentos de NF
					!(!lOldLan .And. Len(Alltrim((cAliasCDH)->CDH_CODLAN))==10)		//Tamanho 10 eh soh os lancamentos de NF
