			 																						//COMPLEMENTO
				If cEntSai =='1' 																							//ENTRADA
						If !SF1->(MsSeek(aSPDFil[PFIL_SF1]+(cAliasCDD)->(CDD_DOCREF+CDD_SERREF+CDD_PARREF+CDD_LOJREF)))
							lRet := .F.
						Else
							cIndEmit := Iif(SF1->F1_FORMUL=="S","0","1")
							cIndOper := "0"
