							ElseIf SA2->(MsSeek (aSPDFil[PFIL_SA2]+(cAliasCDD)->(CDD_CLIFOR)+(cAliasCDD)->(CDD_LOJA)))
								If SF1->F1_FORMUL=="S"
									aPartDoc :=	InfPartDoc("SM0")
