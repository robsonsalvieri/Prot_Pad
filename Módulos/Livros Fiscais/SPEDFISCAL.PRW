/*/{Protheus.doc} SPEDD4C() 
@description Função para buscar dados da D4C-Correlacao de Produtos, Issue DSERFIS1-30835
@author Sandro Farizel
@since 13/11/2023
@version 12.1.2210
/*/
//-------------------------------------------------------------------
Static Function SPEDD4C(cProd)
	Local aArea   := GetArea()
	Local cQuery  := ""
	Local cAlias  := ""
	Local aRetD4C := {}

	if oReg0221 == Nil
		cQuery   := " Select D4C_PRDATO, D4C_QTPRAT, B1_DESC, B1_UM, B1_POSIPI, B1_EX_NCM "
		cQuery   += " From "+RetSqlName("D4C")+" D4C"
		cQuery   += " Inner Join "+RetSqlName("SB1")+" SB1 "
		cQuery   += " On B1_FILIAL = ? And B1_COD = D4C_PRDATO And SB1.D_E_L_E_T_ = ' ' "
		cQuery   += " Where D4C_FILIAL = ? And D4C_PRODUT = ? AND D4C.D_E_L_E_T_= ' ' "
		cQuery   := ChangeQuery(cQuery)
		oReg0221 := FwExecStatement():New(cQuery)
	endif

	oReg0221:SetString(1,aSPDFil[PFIL_SB1])
	oReg0221:SetString(2,aSPDFil[PFIL_D4C])
	oReg0221:SetString(3,cProd)
	cQuery	:= oReg0221:GetFixQuery()
	cAlias  := MPSysOpenQuery(cQuery)	

	While ! (cAlias)->(Eof())
		Aadd(aRetD4C,(cAlias)->({D4C_PRDATO,D4C_QTPRAT,B1_DESC,B1_UM,B1_POSIPI,B1_EX_NCM}))
		(cAlias)->(DBSkip())
	EndDo
	
	(cAlias)->(DbCloseArea())
	RestArea(aArea)
Return(aRetD4C)

