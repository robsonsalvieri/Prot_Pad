For nI := 1 To Len(aRegD590)
	If (nPos := aScan(aRegD690,{|aX| aX[1]==nPosD600 .And. aX[3]==aRegD590[nI][02] .And. aX[4]==aRegD590[nI][03] .And. aX[5]==aRegD590[nI][04]}))==0
