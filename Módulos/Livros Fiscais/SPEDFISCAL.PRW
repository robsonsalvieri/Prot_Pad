/*/{Protheus.doc} Reg0221
@Sandro Farizel
@since 24/11/2023
@version 12.1.2210

Função para tratamento do registro 0221
/*/
//-------------------------------------------------------------------
Static Function Reg0221(cAlias, dDataDe, dDataAte, aReg0190, aReg0200, aReg0220, aWizard, cProdPai, cChave, cDesc, cUm, cPosIPI, cEx_NCM)
	Local aRetD4C     := {}
	Local nIdx        := 0
	Local nTamD4C     := 0 
	Local nPos0221    := 0 
	Local cChavePai   := cChave
	Default cAliasSB1 := ""

	aRetD4C  := SPEDD4C(Alltrim(cProdPai))
	nTamD4C  := Len(aRetD4C)
	nPos0221 :=	Len(aReg0221)
	If nTamD4C > 0 //Tratamento dos produtos filhos
		For nIdx := 1 to nTamD4C 
			cChave := Substr(aRetD4C[nIdx,1],1,nTamB1COD) + Iif(lConcFil,cFilAnt,"")
			Add0221(cAlias,dDataDe,dDataAte,@aReg0190,@aReg0200,@aReg0220,aWizard,@nPos0221,cChave,aRetD4C[nIdx,3],aRetD4C[nIdx,4],aRetD4C[nIdx,5],aRetD4C[nIdx,6],1) //geração individual dos registros 0200 e 0221 dos produtos filhos com quantidade atômica = 1
			Add0221(cAlias,dDataDe,dDataAte,@aReg0190,@aReg0200,@aReg0220,aWizard,@nPos0221,cChave,aRetD4C[nIdx,3],aRetD4C[nIdx,4],aRetD4C[nIdx,5],aRetD4C[nIdx,6],aRetD4C[nIdx,2],cChavePai) //geração apenas dos registros 0221 dos produtos filhos relacionados ao 0200 pai
		Next 
	Else 
		Add0221(cAlias,dDataDe,dDataAte,@aReg0190,@aReg0200,@aReg0220,aWizard,@nPos0221,cChave,cDesc,cUm,cPosIPI,cEx_NCM,1)
	Endif

