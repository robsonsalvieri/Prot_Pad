									If SA2->(MsSeek (aSPDFil[PFIL_SA2]+ cCodPArt))
										aPartDoc :=	InfPartDoc ("SA2")
										SDPF150175(@aReg0150,aPartDoc,cAlias,aWizard,aExistBloc)
									Else
										lRet := .F.
									EndIf
