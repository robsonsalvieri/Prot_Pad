									xRet := Round((MaRetBasT(aInfo[2],nItem,MaFisRet(nItem,'IT_ALIQIV' + aInfo[2])) - nBaseAb) * ((MaFisRet(nItem,'IT_ALIQIV' + aInfo[2])) / 100) * _nReducir, MsDecimais(_nMoeda))
								EndIf
							EndIf
