		aArea := SF1->(GetArea())
		SF1->(dbSetOrder(1))
			If SF1->(MsSeek(xFilial("SF1") + cRemito + cSerie + cCliente + cLoja))
				lRet := .T.
			EndIf
		SF1->(RestArea(aArea))
	EndIf

Return lRet

