		Case Upper(Alltrim(cArqMag)) == "CONV76" //Novo Convenio 76/03
			lConv76	:= IIf(Year(MV_PAR01)>2003.And.Year(MV_PAR02)>2003,.T.,.F.)
			lReg61I := IIf(MV_PAR17==2.And.lConv76,.T.,.F.) //Gera Registro 61I (Convenio 76)
