		Case Upper(Alltrim(cArqMag)) == "SINTEGMG" .Or. Upper(Alltrim(cArqMag)) == "SINTMG" //Sintegra de Minas Gerais - Registro 88A
			lSintMG := .T.
			lConv76 := IIf(Year(MV_PAR01)>2003.And.Year(MV_PAR02)>2003,.T.,.F.)
