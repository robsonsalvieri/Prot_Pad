			EndIf
		ElseIf cCodTP =='3' //Fecp Difal
			If SF6->(DbSeek (xFilial ("SF6")+SubStr(aCols[2][3],At("(",aCols[2][3]) + 1,2)+cGnre))
							aCols[n,4] := SF6->F6_VALOR

				cBloco	:= Alltrim(aCols[n,1])
				cValor	:= 0
