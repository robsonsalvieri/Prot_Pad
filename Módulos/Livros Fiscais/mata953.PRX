			aCOLS12[nPos,06]	:=	aDifEst[aScan(aDifEst,{|x|x[2] == '023.00'} )][4]
			aCOLS12[nPos,07]	+=	aDifEst[aScan(aDifEst,{|x|x[2] == '024.00'} )][4]
		EndIf
	EndIf


	If lProcRef
		aCOLS12[nPos,12]	:=	.F.
	Else
		aCOLS12[nPos,08]	:=	.F.
	EndIf

	//Armazena array do estado no array com todos os estados
	Aadd(aApurDifal,{aEstado[nEst][2],aDifEst})

next nEst

