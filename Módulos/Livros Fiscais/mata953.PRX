	aCoCredAcu[nPosicao][1] := "004"
	aCoCredAcu[nPosicao][2] := "TOTAL CRÉDITOS UTILIZADOS NO PERÍODO"

	// Existe saldo anterior
	If !Empty(aCrdUtiPr)
