	Local nOutDebF		:= 0
	Local nEstCrdF		:= 0
	Local nTotCrdFCP	:= 0
	Local nOutCrdF		:= 0
	Local nEstDebF		:= 0
	Local nSldDevF		:= 0
	Local nDedDifF		:= 0
	Local nDebEspF		:= 0
