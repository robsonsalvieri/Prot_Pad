							If lUsaSped
								dbSelectArea("SE2")
								MsGoto(aRecTit[Len(aRecTit)][01])

								AADD ( aTitCDH, {	SE2->E2_PREFIXO,SE2->E2_NUM,SE2->E2_PARCELA,;
