		Next
	EndIf

	// Adiciona uma linha
	nPosicao := fAddArray(@aCoCredAcu,"aCoCredAcu")

	aCoCredAcu[nPosicao][1] := "003"
	aCoCredAcu[nPosicao][2] := "TOTAL CRÉDITOS RECEBIDO POR TRANSFERÊNCIA"

	// Existe saldo anterior
	If !Empty(aCrdTrnPr)
