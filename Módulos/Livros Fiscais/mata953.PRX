						0,"MATA953",lContab,@cNumero,@aGNRE,,,,,,.T.,,@aRecTit,@lConfTit,aCols5[nPosFumac,4],,,"RFUMAC",,,,,,,,,,,,,,,lAutomato,aRetAuto,)

