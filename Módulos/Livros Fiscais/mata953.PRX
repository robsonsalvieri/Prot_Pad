			nPosicao := fAddArray(@aCoCredAcu,"aCoCredAcu")

			aCoCredAcu[nPosicao][3] := aCrdAprPr[1][2][nCount][1]
			aCoCredAcu[nPosicao][4] := aCrdAprPr[1][2][nCount][2]
			aCoCredAcu[nPosicao][5] := aCrdAprPr[1][2][nCount][3]
