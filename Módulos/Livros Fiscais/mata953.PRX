							If lGTitFluig
								Aadd( aTitCDH,{ CH3->CH3_PREFIX, CH3->CH3_NUM, "", CH3->CH3_TIPO, CH3->CH3_FORNEC, CH3->CH3_LOJA, "IC", "Desenvolve Ant.", nSolicFlg } )
