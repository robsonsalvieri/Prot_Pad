			    	aCols[n011][4]	+= (aCols[n008][4] * -1)
			    	aCols[n008][4]	:= 0
			    	aCols[n011][4]  += aCols[n009][4]
			    	aCols[n010][4] := 0

			    ElseIF aCols[n010][4] < 0 //Se valor a recolher for negativo então acumula saldo credot
