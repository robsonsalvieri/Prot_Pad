						//Realiza validação do código na tabela CDO
						xMagHelpFis ('Atenção',;										//"Atenção"
						"Código de Ajuste já informado")
						lRet	:= .F.

					EndIf

				EndIf

			EndIf

		EndIf
	EndIf

