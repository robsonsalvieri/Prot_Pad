	Local cCod

	If nTipo == 2 //Especialidade
		cCod := Left(aFUN[2],Len(ST2->T2_ESPECIA))
		If aSCAN(a330EspB,{|x| x[1]+x[2]+x[3]+x[4]+DtoS(x[5])+x[6] == aFUN[8]+aFUN[9]+aFUN[1]+cCod+DTOS(aFUN[4])+aFUN[5] }) = 0
