			EndIf
		
		EndIf

		IncProc(Oemtoansi(STR0011)) //"Selecionando Bens para Manuten‡ao"

		If lPriIf

			If Empty( cQuerySTF )

				cQuerySTF := 'SELECT '
				cQuerySTF += 'STF.R_E_C_N_O_ AS REGSTF '
				cQuerySTF += 'FROM ' + RetSqlName( 'STF' ) + ' STF '
				cQuerySTF += 'WHERE '
				cQuerySTF += 	'STF.TF_FILIAL = ? '
				cQuerySTF += 	'AND STF.TF_CODBEM = ? '
