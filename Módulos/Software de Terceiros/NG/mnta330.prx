		fGravPrb(If(!lPlaEstu,STR0065,;  //"Não existem bens válidos para o intervalo de centro de custo selecionado."
		STR0066))  //"Não existem bens válidos para a estrutura selecionada."
	EndIf

	aT9Area := ST9->(GetArea())

	If !MNT330VCAL()
