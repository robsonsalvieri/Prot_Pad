		// Se o bem não estiver na lista dos selecionados, vai pro próximo registro,
		If lFiltBem .And. ( aScan( aBensTrb, { | x | AllTrim( x ) == AllTrim( ST9->T9_CODBEM ) } ) == 0 )
			
