	EndIf
	
	RestArea(aT9Area)
	nIcEst := 1

	cQueryST9 := 'SELECT '
	cQueryST9 += 'ST9.R_E_C_N_O_ AS REGST9 '
	cQueryST9 += 'FROM ' + RetSqlName( 'ST9' ) + ' ST9 '
	cQueryST9 += 'WHERE '
	cQueryST9 +=	'ST9.T9_FILIAL = ' + ValToSQL( FWxFilial( 'ST9' ) ) + ' '
	cQueryST9 +=	'AND (ST9.T9_CCUSTO >= ' + ValToSQL( M->TI_CCUSINI ) + ' '
	cQueryST9 +=	'AND ST9.T9_CCUSTO <= ' + ValToSQL( M->TI_CCUSFIM ) + ') '

	If !Empty(M->TI_FAMIINI)
	
		cQueryST9 += 'AND (ST9.T9_CODFAMI >= ' + ValToSQL( M->TI_FAMIINI ) + ' '
		cQueryST9 += 'AND ST9.T9_CODFAMI <= ' + ValToSQL( M->TI_FAMIFIM ) + ') '
	
	EndIf
	If !Empty(M->TI_CTRAINI)
	
		cQueryST9 += 'AND (ST9.T9_CENTRAB >= ' + ValToSQL( M->TI_CTRAINI ) + ' '
		cQueryST9 += 'AND ST9.T9_CENTRAB <= ' + ValToSQL( M->TI_CTRAFIM ) + ') '
	
	EndIf

	If lPlaEstu

		cQueryST9 += 'AND ST9.T9_CODBEM IN ( '
	
		For nX := 1 To Len( vRetEst )

			cQueryST9 += ValToSQL( vRetEst[ nX ] )

			If nX < len( vRetEst )

				cQueryST9 += ', '

			EndIf

		Next nX

		cQueryST9 += ') '
	
	Else
	
		cQueryST9 += 'AND (ST9.T9_CODBEM >= ' + ValToSQL( M->TI_BEMINI ) + ' '
		cQueryST9 += 'AND ST9.T9_CODBEM <= ' + ValToSQL( M->TI_BEMFIM ) + ') '
	
