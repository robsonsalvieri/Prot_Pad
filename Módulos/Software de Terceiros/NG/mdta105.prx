Function MDT105EST()

DbSelectArea("TM1")
DbSetOrder(1)
IF DbSeek( xFilial("TM1") + M->TM3_CODMED)
	IF M->TM3_INDMOV $  "5/6/7"
 		If TM1->TM1_QTMINI > 0
			If (TM1->TM1_QTATUA - M->TM3_QTMOVI) < TM1->TM1_QTMINI     
				If !MsgYesNo(STR0006) //"É recomendável que o estoque de medicamentos não atinja uma quantidade inferior à quantidade mínima. Deseja continuar mesmo assim?"
					Return .f.
				Else
					If (TM1->TM1_QTATUA - M->TM3_QTMOVI) < 0              
						Msgstop(STR0007) //"Estoque de Medicamentos sem saldo suficiente."
						Return .f.
					Endif                               	
				EndIf
			Endif                               	
		Else	
			If (TM1->TM1_QTATUA - M->TM3_QTMOVI) < 0    
				Msgstop(STR0007) //"Estoque de Medicamentos sem saldo suficiente."
				Return .f.
			EndIf
		Endif	
   	ENDIF
EndIf

Return .T.

