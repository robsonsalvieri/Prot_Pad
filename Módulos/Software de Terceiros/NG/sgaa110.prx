		EndIf

		If lFirst
			cAval1 := TA6->TA6_CODAVA
			lFirst:= .F.
		EndIf

		DbSelectArea('TAD')
		DbSetOrder(1)
		If DbSeek(xFilial('TAD')+cOrdem+TA6->TA6_CODAVA)
			nPeso := (TA6->TA6_PESO*TAD->TAD_PESO)/100
		EndIf
		aAdd( aColsImp,{TA6->TA6_CODAVA,TA6->TA6_DESCRI,TA6->TA6_PESO,nPeso,.F.} )

		DbSelectArea('TA6')
		DbSkip()

	EndDo

Return .T.

