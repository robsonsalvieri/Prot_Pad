/*/
//-------------------------------------------------------------------
Function Sg110Get(nOpcx)

	Local lFirst := .T.
	Local nPeso  := 0

	DbSelectArea('TA6')
	DbSetOrder(1)
	DbSeek(xFilial('TA6'))
	While TA6->(!Eof()) .And. xFilial('TA6') == TA6->TA6_FILIAL

