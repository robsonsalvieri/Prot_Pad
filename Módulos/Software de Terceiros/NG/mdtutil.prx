/*/{Protheus.doc} MDTX3Tam
Função criada para tratar problemas com X3_TAMANHO de campos 
especificos que estão com divirgencias de tamanho e apresentar
mensagem quando forem diferentes.

**DEVE SER RETIRADA APÓS LIBERAÇÃO DA PRÓXIMA RELEASE 12.1.2410**

@author	Eloisa Anibaletto
@since 31/01/2024

/*/
//---------------------------------------------------------------------
Function MDTX3Tam()

	Local lMDTA120 := FWIsInCallStack( "mdta120" ) .Or. FWIsInCallStack( "mdta120a" )
	Local lMDTA155 := FWIsInCallStack( "mdta155" ) .Or. FWIsInCallStack( "mdta155a" )
	Local lMDTA156 := FWIsInCallStack( "mdta156" )
	Local lMDTA200 := FWIsInCallStack( "mdta200" )
	Local lMDTA007 := FWIsInCallStack( "mdta007" )

	Local aTamTM0N  := {}
	Local aTamTM0F  := {}
	Local aTamTM4   := {}
	Local aTamTM5   := {}
	Local aTamTMT   := {}
	Local aTamTMYN  := {}
	Local aTamTMYU  := {}
	Local aTamTMK   := {}
	Local aTamSRJ   := {}

	Local cMsg      := ""

	Do Case
		Case lMDTA120
			aTamTM4 := TAMSX3( "TM4_NOMEXA" )
			aTamTM5 := TAMSX3( "TM5_NOMEXA" )

			If aTamTM5[1] != aTamTM4[1]
				cMsg := " Nome Exame (TM5_NOMEXA) da rotina Exames do Funcionário (MDTA120) e o campo Nome Exame (TM4_NOMEXA) da rotina de Exames (MDTA020), "
				MDTMEMOLINK( STR1557, STR1558 + cMsg + STR1559, "https://tdn.totvs.com/x/k2TyM", STR1560 )
			EndIf
		Case lMDTA155 .Or. lMDTA156
			aTamTMK := TAMSX3( "TMK_NOMUSU" )
			aTamTMT := TAMSX3( "TMT_NOMUSU" )

			If aTamTMT[1] != aTamTMK[1]
				cMsg := " Nome do Médico (TMT_NOMUSU) da rotina Diagnóstico e o campo Nome do Médico (TMK_NOMUSU) da rotina Sesmt / Usuarios Aten. (MDTA070), "
				MDTMEMOLINK( STR1557, STR1558 + cMsg + STR1559, "https://tdn.totvs.com/x/TQUJMQ", STR1560 )
			EndIf
		Case lMDTA200
			aTamTM0N  := TAMSX3( "TM0_NOMFIC" )
			aTamTMYN  := TAMSX3( "TMY_NOMFIC" )
			aTamTMYU  := TAMSX3( "TMY_NOMUSU" )
			aTamTMK   := TAMSX3( "TMK_NOMUSU" )

			If aTamTMYN[1] != aTamTM0N[1]
				cMsg := " Nome da Ficha (TMY_NOMFIC) da rotina Atestado ASO (MDTA120) e o campo Nome da Ficha (TM0_NOMFIC) da rotina Ficha Médica, "
				MDTMEMOLINK( STR1557, STR1558 + cMsg + STR1559, "https://tdn.totvs.com/x/rwcJMQ", STR1560 )
			EndIf
			If aTamTMYU[1] != aTamTMK[1]
				cMsg := " Nome do Usuário (TMY_NOMUSU) da rotina Atestado ASO (MDTA120) e o campo Nome do Médico (TMK_NOMUSU) da rotina Sesmt / Usuarios Aten. (MDTA070), "
				MDTMEMOLINK( STR1557, STR1558 + cMsg + STR1559, "https://tdn.totvs.com/x/xwgJMQ", STR1560 )
			EndIf
		Case lMDTA007
			aTamTM0F := TAMSX3( "TM0_DESCFU" )
			aTamSRJ  := TAMSX3( "RJ_DESC" )

			If aTamTM0F[1] != aTamSRJ[1]
				cMsg := " Descrição da Função (TM0_DESCFU) da rotina Ficha Médica (MDTA007) e o campo Descrição da Função (RJ_DESC) da rotina Funções (GPEA030), "
				MDTMEMOLINK( STR1557, STR1558 + cMsg + STR1559, "https://tdn.totvs.com/x/d1XyM", STR1560 )
			EndIf
	EndCase

Return
