	dbGoTop()
	do While ! Eof()
		If SM0->M0_CODIGO == cEmpAnt
			Aadd(aSM0CodFil, SM0->M0_CODFIL)
		Endif
		dbSkip()
	Enddo
	RestArea(aAreaSM0)

	//Verificacoes nas tabelas do modulo SIGAMDT - Medicina e Seguranca do Trabalho
	dbSelectArea("TN3") //Fornecedores de Epi
	dbSetOrder(2)
	If lFunc
