/*/{Protheus.doc} MdtWizard
Cria um wizard para definição dos parâmetros

@author	Gabriel Sokacheski
@since 08/06/2023

@param cMsgIni, mensagem utilizada na página inicial
@param aParametro, array contendo os parâmetros
@param bPagPar, bloco de código com a função da página de parâmetros

/*/
//---------------------------------------------------------------------
Function MdtWizard( cMsgIni, aParametro, bPagPar )

    Local nPagina   	:= 0
    Local nPaginas  	:= Ceiling( Len( aParametro ) / 5 )

    Local oTela     	:= Nil
    Local oPainel   	:= Nil
    Local oWizard   	:= Nil
    Local oPagina   	:= Nil
    Local oFonteM   	:= TFont():New( Nil, Nil, -15, .T. )
    Local oFonteG   	:= TFont():New( Nil, Nil, -20, .T. )

	Private aParImp 	:= aClone( aParametro ) // Array de parâmetros que faltam ser impressos em tela

	Define Dialog oTela Style nOr( WS_POPUP, WS_VISIBLE ) Pixel

    oTela:nWidth    := 600
    oTela:nHeight   := 600

    oPainel := TPanel():New( 0, 0, '', oTela, Nil, Nil, Nil, Nil, Nil, 300, 300 )

    oWizard := FWWizardControl():New( oPainel, { 600, 600 } )

    oWizard:ActiveUISteps()

    oPagina := oWizard:AddStep( '1', { | oPainel | fPagIni( oPainel, oFonteG, cMsgIni ) } )

    oPagina:SetStepDescription( STR1491 ) // "Configuração de Parâmetros"
    oPagina:SetCancelAction( { || oTela:End() } )

    For nPagina := 1 To nPaginas

        oPagina := oWizard:AddStep( cValToChar( nPagina + 1 ), bPagPar )
        oPagina:SetStepDescription( STR1492 ) // "Parâmetros"
        oPagina:SetPrevTitle( STR1493 ) // "Voltar"
        oPagina:SetCancelAction( { || oTela:End() } )

    Next nPagina

    oPagina := oWizard:AddStep( cValToChar( nPagina + 1 ), { | oPainel | fPagFim( oPainel, oFonteG, oFonteM ) } )

    oPagina:SetStepDescription( STR1494 ) // "Confirmação"

    oPagina:SetNextAction( { || fSalPar( aClone( aParametro ) ), oTela:End() } )

    oPagina:SetCancelAction( {|| oTela:End() } )

    oWizard:Activate()

	Activate Dialog oTela Center

	oWizard:Destroy()

Return

