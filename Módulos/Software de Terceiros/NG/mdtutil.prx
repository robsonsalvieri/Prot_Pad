/*/{Protheus.doc} MDTUSRLOG
Retorna o código do usuario logado no sistema
Uso Genérico

@return cCodRet

@sample
MDTUSRLOG()

@author Jackson Machado
@since 21/08/2012
@version 1.0
/*/
//---------------------------------------------------------------------
Function MDTUSRLOG(nRet,lTNP)

	Local cCodRet := ""

	Default nRet := 1
	Default lTNP := .F.

	If FwIsInCallStack( "mdta076" )
		cCodRet := cMedico
	Else
		cCodRet := SPACE( LEN( TMK->TMK_CODUSU ) )
	EndIf

	If nRet <> 1
		If FwIsInCallStack( "mdta076" )
			cCodRet := Posicione( "TMK", 1, FwxFilial( "TMK" ) + cMedico, "TMK_NOMUSU" )
		Else
			cCodRet := SPACE( LEN( TMK->TMK_NOMUSU ) )
		EndIf
	Endif

	If SuperGetMV("MV_NG2SEG",.F.,"2") == "1" .AND. !(SuperGetMV("MV_MDTPS",.F.,"N") == "S")
		dbSelectArea("TMK")
		dbSetOrder(4)
		If dbSeek(cUserName)
			If nRet == 1
				cCodRet := TMK->TMK_CODUSU
			Else
				If Type("INCLUI") == "L" .AND. INCLUI
					cCodRet := TMK->TMK_NOMUSU
				Endif
			Endif
		Endif
		If Empty(cCodRet) .AND. lTNP
			dbSelectArea("TNP")
			dbSetOrder(3)
			If dbSeek(cUserName)
				If nRet == 1
					cCodRet := TNP->TNP_EMITEN
				Else
					If Type("INCLUI") == "L" .AND. INCLUI
						cCodRet := TNP->TNP_NOME
					Endif
				Endif
			Endif
		Endif
	Endif

Return cCodRet

