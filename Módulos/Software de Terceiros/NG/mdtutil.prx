	Local cMsg1 := STR0028  //"Este funcionário está demitido. Deseja continuar?"
	Local cMsg  := STR0029  //"Não será possível selecioná-lo, pois o funcionário está demitido."
	Local cTit  := STR0013  //"ATENCAO"
