/*/{Protheus.doc} NEXTNUMASO
Obtem o proximo numero da ASO

@author Inacio Luiz Kolling
@since 24/03/2000
/*/
//---------------------------------------------------------------------
Function NEXTNUMASO( cFilSXE )

	Local cNumAso   := ""
	Local cAliasTMY := ""
	Local cFil      := ""

	Local lTransf   := ( FWIsInCallStack( 'MDTA970' ) .Or. FWIsInCallStack( 'GPEA180' ) )

	Default cFilSXE := ""

	If !FWIsInCallStack( 'InitPad' ) .Or. FWIsInCallStack( 'MDTA410' )

		cNumAso	:= GETSXENUM( "TMY", "TMY_NUMASO", IIf( !Empty( cFilSXE ), cFilSXE, Nil ) )

		If lTransf
			cAliasTMY := RetFullName( "TMY", mv_par01 )
			cFil      := FwxFilial( 'TMY',, FWModeAccess( 'TMY', 1, mv_par01 ), FWModeAccess( 'TMY', 2, mv_par01 ),FWModeAccess( 'TMY', 3, mv_par01 ) )
		EndIf

