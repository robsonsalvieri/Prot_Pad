/*/{Protheus.doc} fCriaNR7
Criar o exame NR7 Automaticamente na abertura do sistema

@author Thiago Olis Machado
@since 05/04/2002
/*/
//---------------------------------------------------------------------
Static Function fCriaNR7()

	Local nTamExa := IIf( TAMSX3( "TM4_EXAME" )[1] < 1, 6, TAMSX3( "TM4_EXAME" )[1] )

	dbSelectArea( "TM4" )
	dbSetOrder( 1 )
	If !dbSeek( xFilial( "TM4" ) + PadR( "NR7", nTamExa, Space( 1 ) ) )

		RecLock( "TM4", .T. )
			TM4->TM4_FILIAL := xFilial( "TM4" )
			TM4->TM4_EXAME  := "NR7"
			TM4->TM4_NOMEXA := STR0011 //"AVALIACAO CLINICA"
			TM4->TM4_DESEXA := STR0011 //"AVALIACAO CLINICA"
			TM4->TM4_INDRES := "1"
			TM4->TM4_ADMISS := "1"
			TM4->TM4_DEMISS := "1"
			TM4->TM4_RETORN := "1"
			TM4->TM4_PROCRE := "0295" // AVALIAÇÃO CLÍNICA OCUPACIONAL (ANAMNESE E EXAME FÍSICO)
		MsUnlock( "TM4" )

	ElseIf dbSeek( xFilial( "TM4" ) + PadR( "NR7", nTamExa, Space( 1 ) ) )

		If Empty( TM4->TM4_PROCRE )

			RecLock( "TM4", .F. )
				TM4->TM4_PROCRE := "0295" // AVALIAÇÃO CLÍNICA OCUPACIONAL (ANAMNESE E EXAME FÍSICO)
			MsUnlock( "TM4" )

		EndIf

	EndIf

Return .T.

