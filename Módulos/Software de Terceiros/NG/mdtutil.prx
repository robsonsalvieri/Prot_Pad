		Else
			cFiltro := "SQB->QB_CC == '" + Padr(cMemoryCC, Len(SQB->QB_CC))+ "' .OR. Empty(SQB->QB_CC)"
		EndIf
	EndIf

Return "@#" + cFiltro + "@#"
