//---------------------------------------------------------------------
/*/{Protheus.doc} fAgendaLem
Agenda de lembretes para usuarios do SIGAMDT

@author Thiago Olis Machado
@since 10/09/2001
/*/
//---------------------------------------------------------------------
Static Function fAgendaLem()

	Local aUsuarios  := { cUserName, Upper( cUserName ) }

	Local cUsuLogin  := ""

	Local nDiasAntes := 0
	Local nDifDatas  := 0
	Local nUsu       := 0

	Local oDlg1, oCheck
	Local dData := dDataBase
	Local Time  := Substr(Time(),1,5)
	lUmaVez := .F.

	For nUsu := 1 To Len( aUsuarios ) //Necessário para posicionar na TNW com usuário em caixa alta e caixa baixa

		cUsuLogin := aUsuarios[ nUsu ]

		dbSelectArea( "TNW" )
		dbSetOrder( 3 )
		If msSeek( FwxFilial( "TNW" )+ cUsuLogin )
			While TNW->( !Eof() ) .and. FwxFilial( "TNW" ) == TNW->TNW_FILIAL .and. ( AllTrim( Upper( TNW->TNW_LOGIN ) ) == AllTrim( Upper( cUserName ) );
					.or. AllTrim( TNW->TNW_LOGIN ) == AllTrim( cUserName ) )
				lUmaVez := .F.

				If TNW->TNW_ANTES > 0 .And. TNW->TNW_DTINIC > dData
					//Verfica quantos dias antes deve emitir a mensagem
					nDiasAntes := TNW->TNW_ANTES
					nDifDatas  := Day( dData ) - Day( TNW->TNW_DTINIC )

					If nDifDatas > nDiasAntes
						DbSelectArea("TNW")
						DbSkip()
						Loop
					EndIf

				Else

					If TNW->TNW_DTINIC > dData
						DbSelectArea("TNW")
						DbSkip()
						Loop
					EndIf

					If TNW->TNW_DTINIC == dData .and. TNW->TNW_HRINIC > Time
						DbSelectArea("TNW")
						DbSkip()
						Loop
					EndIf

					If !Empty(TNW->TNW_DTFIM) .and. TNW->TNW_DTFIM < dData
						DbSelectArea("TNW")
						DbSkip()
						Loop
					EndIf

					If TNW->TNW_DTFIM == dData .and. (!Empty(TNW->TNW_HRFIM) .And. TNW->TNW_HRFIM < Time)
						DbSelectArea("TNW")
						DbSkip()
						Loop
					EndIf

				EndIf

				If TNW->TNW_MOSTRA == "1"
					DbSelectArea("TMK")
					DbSetOrder(1)
					IF DbSeek(xFilial("TMK")+TNW->TNW_CODUSU)
						cNome := TMK->TMK_NOMUSU

						DEFINE MSDIALOG oDlg1 TITLE STR0009+ cNome FROM 00,00 TO 400,420 PIXEL//"Mensagem para "

						cMemo := TNW->TNW_MENSAG
						@ 005,005 GET oMemo  VAR cMemo MEMO SIZE 200,150 OF oDlg1 PIXEL
						oMemo:lReadOnly:=.t.

						@180,010 checkbox oCheck Var lUmaVez Prompt OemToAnsi(STR0010) Size 95,7 OF ODLG1 PIXEL //"Não mostrar mais essa mensagem"

						DEFINE SBUTTON FROM 180,170 TYPE 1 ENABLE action GRAVA(oDlg1)
						ACTIVATE MSDIALOG oDlg1 CENTERED
					EndIf
				EndIF
				DbSelectArea("TNW")
				DbSkip()
			End
		EndIF

	Next nUsu

Return

Static Function Grava(oDlg1)
	If lUmaVez
		DbSelectArea("TNW")
		RecLock("TNW",.f.)
		TNW->TNW_MOSTRA := "2"
		MsUnlock("TNW")
	EndIf
	oDlg1:End()
Return
