Function MDT610VDES()
Local lPrest := .F.

If Type("cCliMdtPs") == "C"
	If !Empty(cCliMdtPs)
		lPrest := .T.
	Endif
Endif

If lPrest
	Return EXISTCHAV("TNL",cCliMdtps+M->TNL_CODDES,3)
Else
	Return EXISTCHAV("TNL",M->TNL_CODDES)
Endif

Return .t.