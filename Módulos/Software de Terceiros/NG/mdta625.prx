//------------------------------------------------------
Static Function OrdMarkEPI( oCombo, oGet, oMark )

	Local nIndEPI

	nIndEPI := oCombo:nAt
	//Ordena TRB conforme selecionado no combo
	dbSelectArea(cTRBEPI)
	dbSetOrder(nIndEPI)
	dbGoTop()
	//Caso for marcação, limpa Get
	If nIndEPI == 3 //B1_OK
		oGet:cText	:= Space(60)
	EndIf

	oMark:oBrowse:Refresh()

Return .T.
