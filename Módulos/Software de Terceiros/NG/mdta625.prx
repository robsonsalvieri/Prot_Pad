Function MDTA625INV(cMarca)

	Local aArea := (cTRBEPI)->(GetArea())

	Dbselectarea(cTRBEPI)
	dbSetOrder(1)
	dbGoTop()
	While !Eof()
		(cTRBEPI)->B1_OK := IF(Empty((cTRBEPI)->B1_OK),cMARCA,Space(Len(cMARCA)))
