Function MNT525TOTR()
	Local i
	Store 0 To nAno1,nAno2

	If Len(aTot) > 0
		aTot := aSort( aTot,,, { |x,y| x[1] < y[1] } )
		nMes := 0
		nTotal := 0
		For i:= 1 To Len(aTot)
			If aTot[i][1] = nMes
				nTotal += aTot[i][2]
				nPos := aScan(aTot2,{|x| x[1] = nMes } )
				If nPos = 0
					aAdd(aTot2 ,{nMes,nTotal})
				Else
					aTot2[nPos][2] := nTotal
				EndIf
			Else
				nMes   := aTot[i][1]
				nTotal := aTot[i][2]
				aAdd(aTot2 ,{nMes,nTotal})
			EndIf
			nAno1 += aTot[i][3]
		Next
	EndIf

Return .t.
