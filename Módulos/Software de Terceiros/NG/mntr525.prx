	DbSelectArea(cTRB)
	DbSetOrder(1)
	DbGotop()
	ProcRegua(RecCount())

	While !Eof()

		IncProc()

		If lEnd
			@ Prow()+1,001 PSay STR0033  //"CANCELADO PELO OPERADOR"
			Exit
		EndIf
		
		If !lExibObs .And. (cTRB)->VIDA == 'OR'

			lExibObs := .T.

		EndIf

		If MV_PAR09 = 1
			If cMes <> (cTRB)->MES .OR. cAno <> (cTRB)->ANO .OR. cHUB <> (cTRB)->CODHUB .OR. cFil <> (cTRB)->FILIAL
				cMes := (cTRB)->MES
				cAno := (cTRB)->ANO
				NgSomaLi(58)
				@ Li,000 PSay STR0034+(cTRB)->MES //"Mês: "
				@ Li,015 PSay STR0035+(cTRB)->ANO //"Ano: "

				cHUB := (cTRB)->CODHUB
				DbSelectArea("TRW")
				TRW->(DbSetOrder(1))
				If DBSeek(xFilial("TRW")+(cTRB)->CODHUB)
					@ Li,029 PSay "HUB: "+SUBSTR(TRW->TRW_DESHUB,1,15)
				EndIf

				cFil := (cTRB)->CODHUB
				DbSelectArea("SM0")
				SM0->(DbSetOrder(1))
				If DBSeek(SM0->M0_CODIGO+(cTRB)->FILIAL)
					@ Li,058 PSay STR0036+(cTRB)->FILIAL+" - "+SUBSTR(SM0->M0_FILIAL,1,20) //"Filial: "
				EndIf
				NgSomaLi(58)
				NgSomaLi(58)
			EndIf

			@ Li,000 PSay (cTRB)->FAMILIA
			@ Li,009 PSay (cTRB)->MEDIDA
			@ Li,017 PSay (cTRB)->DESMED
			@ Li,041 PSay (cTRB)->MODELO
			@ Li,053 PSay (cTRB)->DESMOD
			@ Li,077 PSay (cTRB)->PNEU
			@ Li,096 PSay (cTRB)->NUMFOG
			@ Li,108 PSay (cTRB)->DOT
			@ Li,114 PSay (cTRB)->VIDA
			@ Li,120 PSay (cTRB)->SULCO
			@ Li,127 PSay (cTRB)->RENDOR
			@ Li,137 PSay (cTRB)->RENDREF
			@ Li,148 PSay (cTRB)->PREVROD
			@ Li,158 PSay (cTRB)->VARDIA
			@ Li,162 PSay (cTRB)->QTDDUR Picture "@E 999,999.99"
			@ Li,175 PSay (cTRB)->QTDARR
			@ Li,184 PSay (cTRB)->CUSTOTR Picture "@E 999,999.99"

			++nQtdPneusM
			nCustoM += (cTRB)->CUSTOTR
			++nQtdPneusF
			nCustoF += (cTRB)->CUSTOTR
			++nQtdPneusH
			nCustoH += (cTRB)->CUSTOTR
			NgSomaLi(58)

			DbSelectArea(cTRB)
			DbSetOrder(1)
			DbSkip()

			If cMes <> (cTRB)->MES .OR. cAno <> (cTRB)->ANO
				NgSomaLi(58)
				@ Li,000 PSay STR0037+AllTrim(STR(nQtdPneusM)) //"Total Mês......:                                                                                                                                               Qtd.Pneus.....: "
				@ Li,184 PSay nCustoM Picture "@E 999,999.99"
				NgSomaLi(58)
				nQtdPneusM := 0
				nCustoM    := 0
			EndIf

			If cFil <> (cTRB)->FILIAL
				NgSomaLi(58)
				@ Li,000 PSay STR0038+AllTrim(STR(nQtdPneusF)) //"Total Filial...:                                                                                                                                               Qtd.Pneus.....: "
				@ Li,184 PSay nCustoF Picture "@E 999,999.99"
				NgSomaLi(58)
				nQtdPneusF := 0
				nCustoF    := 0
			EndIf

			If cHUB <> (cTRB)->CODHUB
				NgSomaLi(58)
				@ Li,000 PSay STR0039+AllTrim(STR(nQtdPneusH)) //"Total HUB......:                                                                                                                                               Qtd.Pneus.....: "
				@ Li,184 PSay nCustoH Picture "@E 999,999.99"
				NgSomaLi(58)
				nQtdPneusH := 0
				nCustoH    := 0
			EndIf
		ElseIf MV_PAR09 = 2  //Resumido - Qtd
			If cHUB <> (cTRB)->CODHUB
				cHUB := (cTRB)->CODHUB
				NgSomaLi(58)
				DbSelectArea("TRW")

				TRW->(DbSetOrder(1))
				If DBSeek(xFilial("TRW")+(cTRB)->CODHUB)
					@ Li,000 PSay SUBSTR(TRW->TRW_DESHUB,1,15)
				EndIf

				MNT525RES((cTRB)->CODHUB)
				nAno1 := 0
				nAno2 := 0
				nTotHub := 0

				For i := 1 to Len(aDados)    //imprime a qtd de pneus nos respectivos meses de troca
					If aDados[i][5] <= 12
						ny := 9 + (aDados[i][5] * 8)
						@ Li,ny PSay aDados[i][3]
						nTotHub += aDados[i][3]
						Aadd(aTot,{aDados[i][5],aDados[i][3],aDados[i][6],aDados[i][7]})
					EndIf
					If !Empty(aDados[i][6])
						If aDados[i][5] > 12
							nTotHub += aDados[i][3]
						EndIf
						nAno1 += aDados[i][6]
					EndIf
					If !Empty(aDados[i][7])
						nAno2 += aDados[i][7]
					EndIf
				Next i
				nTotAno1 += nAno1
				nTotAno2 += nAno2
				nTotHub  += nAno2
				nTotGeral += nTotHub
				@ Li,113 PSay nAno1
				@ Li,121 PSay nAno2
				@ Li,129 PSay nTotHub
				NgSomaLi(58)
			EndIf

			DbSelectArea(cTRB)
			DbSetOrder(1)
			DbSkip()

			If Eof()
				MNT525TOTR()
				NgSomaLi(58)
				@ Li,000 PSay STR0040 //"Total Geral....:"
				For i:=1 To Len(aTot2)
					ny := 9 + (aTot2[i][1] * 8)
					@ Li,ny PSay aTot2[i][2]
				Next i
				@ Li,113 PSay nTotAno1
				@ Li,121 PSay nTotAno2
				@ Li,129 PSay nTotGeral
			EndIf

		ElseIf MV_PAR09 = 3
			If cHUB <> (cTRB)->CODHUB
				cHUB := (cTRB)->CODHUB
				NgSomaLi(58)
				DbSelectArea("TRW")
				TRW->(DbSetOrder(1))
				If DBSeek(xFilial("TRW")+(cTRB)->CODHUB)
					@ Li,000 PSay SUBSTR(TRW->TRW_DESHUB,1,15)
				EndIf
				MNT525RES((cTRB)->CODHUB)
				nAno1 := 0
				nAno2 := 0
				nTotHub := 0

				For i := 1 to Len(aDados)    //imprime o custo dos pneus nos respectivos meses de troca
					If aDados[i][5] <= 12 //!Empty(aDados[i][5])
						ny := 6 + (aDados[i][5] * 11)
						@ Li,ny PSay aDados[i][4]
						nTotHub += aDados[i][4]
						Aadd(aTot,{aDados[i][5],aDados[i][4],aDados[i][6],aDados[i][7]})
					EndIf
					If !Empty(aDados[i][6])
						If aDados[i][5] > 12
							nTotHub += aDados[i][4]
						EndIf
						nAno1 += aDados[i][6]
					EndIf
					If !Empty(aDados[i][7])
						nAno2 += aDados[i][7]
					EndIf
				Next i
				nTotAno1 += nAno1
				nTotAno2 += nAno2
				nTotHub  += nAno2
				nTotGeral += nTotHub
				@ Li,149 PSay nAno1
				@ Li,160 PSay nAno2
				@ Li,171 PSay nTotHub
				NgSomaLi(58)
			EndIf

			DbSelectArea(cTRB)
			DbSetOrder(1)
			DbSkip()

			If Eof()
				MNT525TOTR()
				NgSomaLi(58)
				@ Li,000 PSay STR0040 //"Total Geral....:"
				For i:=1 To Len(aTot2)
					ny := 6 + (aTot2[i][1] * 11)
					@ Li,ny PSay aTot2[i][2]
				Next i
				@ Li,149 PSay nTotAno1
				@ Li,160 PSay nTotAno2
				@ Li,171 PSay nTotGeral
			EndIf
		EndIf
	End

	If lExibObs

		@ Li+1,000 PSay SUBSTR(STR0054,1,145)   // "Obs: Para realizar o cálculo da previsão os campos TQU_KMESP1 e TQU_SULCO do desenho são utilizados, alguns pneus impressos no relatório utilizam"
		@ Li+2,000 PSay SUBSTR(STR0054,146,245) // " banda original e não é informado o desenho, por isso não é possível realizar o cálculo da previsão."
	
	EndIf
	
	RODA(nCNTIMPR,cRODATXT,TAMANHO)
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Devolve a condicao original do arquivo principal             ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	RetIndex('TQS')
	Set Device To Screen
	If aReturn[5] == 1
		Set Printer To
		dbCommitAll()
		OurSpool(WNREL)
	EndIf
	MS_FLUSH()

Return Nil
