Function MNR525IMP(lEND,WNREL,TITULO,TAMANHO)

	Local cFil       := " ",cHUB := " ",cMes := " ",cAno := " "
	Local i,nTotAno1 := 0,nTotAno2 := 0,nTotGeral := 0
	Local lExibObs   := .F.
	Private aTot     := {},aTot2 := {}
	Private cRODATXT := ""
	Private nCNTIMPR := 0
	Private li       := 80 ,m_pag := 1
	Store 0 To nQtdPneusM,nCustoM,nQtdPneusF,nCustoF,nQtdPneusH,nCustoH


	nTIPO  := IIf(aReturn[4]==1,15,18)
	If MV_PAR09 = 1
		CABEC1 := "                                                                                                                               Rend.     Rend.      Previsão  Var.   Qtd.Mês   Qtd.Mês       Custo"
		CABEC2 := STR0032 //"Família  Medida  Des.Medida              Modelo      Des.Modelo              Cod.Pneu           Num.Fogo    DOT   Vida  Sulco  OR        Reformas   Rodagem   Dia    Duração   Arred.        Troca"
	ElseIf MV_PAR09 = 2 .or. MV_PAR09 = 3
		CABEC1 := "HUB"+Space(14)+MNT525CAB()
		CABEC2 := " "
	EndIf


	/*
	MV_PAR09 = 1 (DETALHADO)
