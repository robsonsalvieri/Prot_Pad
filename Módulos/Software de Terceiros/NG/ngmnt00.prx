Static Function NGAlterTUB(cCusto,cCentrab)
	Local aArea := GetArea()
	Local lVisivel, lExist
	Local cCodNiv
	Local cCodBem   := ST9->T9_CODBEM
	Local cCustoOld := ST9->T9_CCUSTO
	Local cCTrabOld := ST9->T9_CENTRAB

	Local cOpcCTT := "1" //CENTRO DE CUSTO
	Local cOpcSHB := "2" //CENTRO DE TRABALHO

	If !AliasInDic("TUA") .Or. !AliasInDic("TUB")
		Return
	EndIf

	dbSelectArea("TAF")
	dbSetOrder(6)
	If dbSeek(xFilial("TAF")+"X1"+cCodBem) .And. (cCustoOld != cCusto .Or. cCTrabOld != cCentrab)
		cCodNiv := TAF->TAF_CODNIV

		dbSelectArea("TUA")
		dbSetOrder(1)
		dbSeek(xFilial("TUA"))
		While !Eof() .And. xFilial("TUA") == TUA->TUA_FILIAL

			dbSelectArea("TUB")
			dbSetOrder(1)
			lExist   := dbSeek(xFilial("TUB")+TUA->TUA_TIPRES+TUA->TUA_TIPO+TUA->TUA_GRPUSR+"8"+cCodNiv)
			lVisivel := NGVerifTUB(TUA->TUA_TIPO,TUA->TUA_GRPUSR,cOpcCTT,cCusto,TUA->TUA_TIPRES) .And. NGVerifTUB(TUA->TUA_TIPO,TUA->TUA_GRPUSR,cOpcSHB,cCentrab,TUA->TUA_TIPRES)

			//Se existir porem registro esta desmarcado
			If lExist
				If TUB->TUB_MARCA == "2" .Or. lVisivel
					dbSelectArea("TUA")
					dbSkip()
				EndIf
			ElseIf !lVisivel
				dbSelectArea("TUA")
				dbSkip()
			EndIf


			If lExist
				//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
				//³Se localizacao existir na TUB porem nao eh mais visivel ³
				//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
				If !lVisivel
					RecLock("TUB",.F.)
					dbDelete()
					MsUnLock()
				EndIf
			Else

				dbSelectArea("TUB")
				dbSetOrder(1)
				If dbSeek(xFilial("TUB")+TUA->TUA_TIPRES+TUA->TUA_TIPO+TUA->TUA_GRPUSR+"8"+TAF->TAF_NIVSUP) .And. TUB->TUB_MARCA == "1"

					dbSelectArea("TUB")
					dbSetOrder(1)
					If !dbSeek(xFilial("TUB")+TUA->TUA_TIPRES+TUA->TUA_TIPO+TUA->TUA_GRPUSR+"8"+cCodNiv)
						RecLock("TUB",.T.)
					Else
						RecLock("TUB",.F.)
					EndIf
					TUB->TUB_FILIAL := xFilial("TUB")
					TUB->TUB_TIPRES := TUA->TUA_TIPRES
					TUB->TUB_TIPO   := TUA->TUA_TIPO
					TUB->TUB_GRPUSR := TUA->TUA_GRPUSR
					TUB->TUB_OPCAO  := "8"
					TUB->TUB_CODIGO := cCodNiv
					TUB->TUB_FILTRO := "1"
					TUB->TUB_MARCA  := "1"
					MsUnLock()

				EndIf
			EndIf

			dbSelectArea("TUA")
			dbSkip()
		EndDo
	EndIf

	RestArea(aArea)
Return

//-----------------------------------------------------------------------------
/*/{Protheus.doc} NgTranfMul
Realiza a transferência entre Centro de Custo das Multas cometidas 
após a movimentação.
@type function

@author João Ricardo Santini Zandoná
@since 26/02/2024

@param  cFil , Caracter, Filial origem do bem transferido.
@param  cBem , Caracter, Código do bem transferido.
@param  dData , Data, Data da movimentação.
@param  cHora , Caracter, Hora da movimentação.
@param  cCusto , Caracter, Centro de custo do bem após a movimentação.

@return
/*/
//-----------------------------------------------------------------------------
Function NgTranfMul( cFil, cBem, dData, cHora, cCusto)

	Local cAliasQry  := GetNextAlias()
	Local lIntFin    := SuperGetMv("MV_NGMNTFI",.F.,"N") == "S"
	Local aBind      := {}
	Local cAliasSE2 := ''
	Local oIntFIN
	
	dbSelectArea('ST9')
	dbSetOrder(01)
	msSeek(cFil+cBem)

	BeginSQL Alias cAliasQry

		SELECT TRX.R_E_C_N_O_ FROM %table:TRX% TRX
		WHERE
			TRX.TRX_FILIAL = %exp:cFil%   AND
			TRX.TRX_DTINFR || TRX.TRX_RHINFR >= %exp:DTOS(dData) + cHora% AND
			TRX.TRX_PLACA = %exp:ST9->T9_PLACA%    AND
			TRX.%NotDel%

	EndSQL

	While (cAliasQry)->(!Eof())
		dbSelectArea( 'TRX' )
		dbGoTo( (cAliasQry)->R_E_C_N_O_ )
		RecLock('TRX', .F.)
			TRX->TRX_CCUSTO := cCusto
		TRX->(MSUnlock())

		If  lIntFin

			If Empty( cQuerySE2 )

				cQuerySE2 := 'SELECT E2_PARCELA, E2_FORNECE, E2_LOJA'
				cQuerySE2 += 'FROM' + RetSqlName( 'SE2' ) + ' SE2 '
				cQuerySE2 += ' WHERE SE2.E2_FILIAL = ? '
				cQuerySE2 += '   AND SE2.E2_PREFIXO = ? '
				cQuerySE2 += '   AND SE2.E2_NUM = ? '
				cQuerySE2 += '   AND SE2.D_E_L_E_T_ = ? '

				cQuerySE2 := ChangeQuery( cQuerySE2 )

			EndIf

			aBind := {}
			aAdd( aBind, FWxFilial('SE2') )
			aAdd( aBind, TRX->TRX_PREFIX )
			aAdd( aBind, TRX->TRX_NUMSE2 )
			aAdd( aBind, Space( 1 ) )
			
			cAliasSE2 := GetNextAlias()

			dbUseArea( .T., 'TOPCONN', TcGenQry2( , , cQuerySE2, aBind ), cAliasSE2, .T., .T. )

			While (cAliasSE2)->( !EoF() )

				oIntFIN := NGIntFin():New()
				oIntFIN:setOperation(4)
				oIntFIN:setRelated("TRX")
				oIntFIN:Load({FWxFilial('SE2')+TRX->TRX_PREFIX+TRX->TRX_NUMSE2+(cAliasSE2)->E2_PARCELA+TRX->TRX_TIPO+(cAliasSE2)->E2_FORNECE+(cAliasSE2)->E2_LOJA})
				oIntFIN:setValue("E2_CCD",TRX->TRX_CCUSTO)
				oIntFIN:setValue("E2_CCUSTO",TRX->TRX_CCUSTO)

				If oIntFIN:valid()

					oIntFIN:Upsert()

				EndIf

				(cAliasSE2)->(dbSkip())

			End

			(cAliasSE2)->(DbCloseArea())

		EndIf

		(cAliasQry)->(dbSkip())

	End

	( cAliasQry )->( DbCloseArea() )

Return
