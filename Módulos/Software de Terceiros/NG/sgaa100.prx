		Else
			cCargo := 'TAR'
		EndIf

		cCodNiv := (cTRBSGA)->CODPRO

		nTRBSGA := (cTRBSGA)->(Recno())

		dbSelectArea(cTRBSGA)
		If dbSeek(cCodEst+cCodNiv)
			nPos := aScan(aItensCar, {|x| x == cCodNiv+cCargo})
			If nPos > 0
				aDel(aItensCar,nPos)
				aSize(aItensCar, Len(aItensCar)-1)
			EndIf
			Sg100DelAF((cTRBSGA)->CODPRO,cCargo)
		EndIf

		dbSelectArea(cTRBSGA)
		dbGoto(nTRBSGA)
		(cTRBSGA)->(dbSkip())
	End

Return .T.

