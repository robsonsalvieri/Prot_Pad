Function Sg100DelAr(cFather,cCarSup)

	Local nPos
	Local cCargo

	nPos := aScan(aItensCar, {|x| x == cFather+cCarSup})
	If nPos > 0
		aDel(aItensCar,nPos)
		aSize(aItensCar, Len(aItensCar)-1)
	EndIf

	dbSelectArea(cTRBSGA)
