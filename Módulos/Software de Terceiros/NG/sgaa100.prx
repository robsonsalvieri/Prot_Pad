Function Sg100Cpy()

	Local nTam	  	:= FWTamSX3( 'TAF_CODNIV' )[ 1 ]
	Local nIndex  	:= nTam + 1
	Local cLoc

	cNivCpy := oTree:GetCargo()

	If Len( Substr( cNivCpy, nIndex, 3 ) ) == 3
		cLoc := "LOC"
	EndIf

	If Substr( cNivCpy, nIndex, 3 ) <> cLoc
		MsgStop(STR0105,STR0082) //"O item selecionado não é uma localização. Cópia cancelada."###"Atenção"
		Return .F.
	EndIf

	If oMenu <> Nil
		oMenu:hide()
		MsFreeObj(@oMenu,.T.)
		oMenu:End()
		oMenu:=NIL
	EndIf

	aSMenu := SG100PERF(.T.)

	Sg100Pop(@AsMenu,@oMenu)

Return

