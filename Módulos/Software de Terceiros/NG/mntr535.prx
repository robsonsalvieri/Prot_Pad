Function MNTR535()

	Local oARQTR535
	Local nSizeFil := IIf(FindFunction("FWSizeFilial"), FwSizeFilial(), Len(TR4->TR4_FILIAL))

	WNREL      := "MNTR535"
	LIMITE     := 132
	cDESC1     := STR0001 //"O relatório apresentará o resumo de pneus"
	cDESC2     := STR0002 //"sucateados prematuramente por HUB e Filial "
	cDESC3     := STR0003 //"totalizando por Motivo,Fabricante,Medida,Modelo e Geral."
	cSTRING    := "TR4"

	Private aINDR535 := {}
	Private aVETINR  := {}
	Private aPerg 	 := {}
	Private aImprime := {}
	Private aRETURN  := {STR0004,1,STR0005,1,2,1,"",1}  //"Zebrado"###"Administracao"
	Private cTRB 	 := GetNextAlias()
	Private cPERG 	 := "MNR535"
	Private NOMEPROG := "MNTR535"
	Private TAMANHO  := "M"
	Private TITULO   := STR0006 //"Relatório de Sucateamento Prematuro de Pneus"
	Private nTIPO    := 0
	Private nLASTKEY := 0
	Private CABEC1,CABEC2


	SetKey( VK_F9, { | | NGVersao( "MNTR535" , 2 ) } )

	Pergunte(cPERG,.F.)

	aPos1      := {15,1,95,315 }

	aDBFR535 := {}
		Aadd(aDBFR535,{"CODHUB"  ,"C", 02, 0})
		Aadd(aDBFR535,{"FILIAL"  ,"C", nSizeFil, 0})
		Aadd(aDBFR535,{"MOTIVO"  ,"C", 06, 0})
		Aadd(aDBFR535,{"CODBEM"  ,"C", 16, 0})
		Aadd(aDBFR535,{"MODELO"  ,"C", 10, 0})
		Aadd(aDBFR535,{"FABRIC"  ,"C", 06, 0})
		Aadd(aDBFR535,{"MEDIDA"  ,"C", 06, 0})
		Aadd(aDBFR535,{"DESENHO" ,"C", 06, 0})
		Aadd(aDBFR535,{"VIDA"    ,"C", 01, 0})

	If MV_PAR03 = 1
		TITULO   := STR0015 //"Relatório de Sucateamento Prematuro de Pneus - Geral"
	ElseIf MV_PAR03 = 2
		TITULO   := STR0016 //"Relatório de Sucateamento Prematuro de Pneus por Motivo"
	ElseIf MV_PAR03 = 3
		TITULO   := STR0017 //"Relatório de Sucateamento Prematuro de Pneus por Medida"
	ElseIf MV_PAR03 = 4
		TITULO   := STR0018 //"Relatório de Sucateamento Prematuro de Pneus por Fabricante"
	ElseIf MV_PAR03 = 5
		TITULO   := STR0019	 //"Relatório de Sucateamento Prematuro de Pneus por Modelo"
	EndIf

	//Indice Tabela Temporária
	aINDR535 := {{"CODHUB","FILIAL","CODBEM"},{"CODHUB","FILIAL","MOTIVO"},{"CODHUB","FILIAL","MEDIDA"},{"CODHUB","FILIAL","FABRIC"},{"CODHUB","FILIAL","MODELO"}}

	//Criação Tabela Temporária
	oARQTR535 := NGFwTmpTbl(cTRB, aDBFR535, aINDR535)

	//+-----------------------------------------+
	//| Envia controle para a funcao SETPRINT   |
	//+-----------------------------------------+
	WNREL:=SetPrint(cSTRING,WNREL,cPERG,TITULO,cDESC1,cDESC2,cDESC3,.F.,"")
	If nLASTKEY = 27
		DbSelectArea("TR4")
		Return
	EndIf

	SetDefault(aReturn,cSTRING)

	Processa({ |lEnd| MNR535PROC()}, STR0020,STR0021 )  //"Aguarde..."###"Processando Registros..."
	RptStatus({|lEND| MNR535IMP(@lEND,WNREL,TITULO,TAMANHO)},STR0020,STR0021)  //"Aguarde..."###"Processando Registros..."

	Dbselectarea(cTRB)
	oARQTR535:Delete()

Return .T.
