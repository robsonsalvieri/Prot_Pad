			DbSelectArea("ST7")
			DbSetOrder(1)
			If DbSeek(xFilial("ST7")+(cTRB)->FABRIC)
				cDesPar := ST7->T7_NOME
			EndIf
		ElseIf MV_PAR03 = 5
			cPARAM := (cTRB)->MODELO

			DbSelectArea("TQR")
			DbSetOrder(1)
			If DbSeek(xFilial("TQR")+(cTRB)->MODELO)
				cDesPar := TQR->TQR_DESMOD
			EndIf
		EndIf

		DbSelectArea(cTRB)
		DbSetOrder(nIdxTrb)
		cFilP := (cTRB)->FILIAL

		nRecno := Recno()
		aBANDAS := MNR535BAN((cTRB)->CODHUB,cFilP,cPARAM)
		DbGoTo(nRecno)

		If cHUB <> (cTRB)->CODHUB
			cHUB := (cTRB)->CODHUB
			NgSomaLi(58)
			DbSelectArea("TRW")
			TRW->(DbSetOrder(1))
			If DBSeek(xFilial("TRW")+(cTRB)->CODHUB)
				@ Li,000 PSay SUBSTR(TRW->TRW_DESHUB,1,15)
			EndIf
		EndIf
		lImp := .f.
		If cFil <> (cTRB)->FILIAL
			cFil := (cTRB)->FILIAL
			cDesParAnt := cDespar
			lImp := .t.
			DbSelectArea("SM0")
			SM0->(DbSetOrder(1))
			If DBSeek(SM0->M0_CODIGO+(cTRB)->FILIAL)
				@ Li,017 PSay SUBSTR(SM0->M0_FILIAL,1,20)
			EndIf
			If !Empty(cDesPar)
				@ Li,039 PSay SubStr(cDesPar,1,20)
			EndIf

			@ Li,064 PSay aBANDAS[1]
			@ Li,070 PSay aBANDAS[2]
			@ Li,076 PSay aBANDAS[3]
			nPREM := aBANDAS[1]+aBANDAS[2]+aBANDAS[3]
			@ Li,088 PSay nPREM
			nTOTPN := MNR535PNF(cFilP)
			@ Li,094 PSay nTOTPN
			@ Li,099 PSay (aBANDAS[1]*100)/nTOTPN Picture "@E 999.99%"
			@ Li,108 PSay (aBANDAS[2]*100)/nTOTPN Picture "@E 999.99%"
			@ Li,117 PSay (aBANDAS[3]*100)/nTOTPN Picture "@E 999.99%"
			@ Li,126 PSay (nPREM*100)/nTOTPN Picture "@E 999.99%"
			NgSomaLi(58)
		EndIf
		If cDesPar <> cDesParAnt .And. !lImp
			cDesParAnt := cDespar

			If !Empty(cDesPar)
				@ Li,039 PSay SubStr(cDesPar,1,20)
			EndIf

			@ Li,064 PSay aBANDAS[1]
			@ Li,070 PSay aBANDAS[2]
			@ Li,076 PSay aBANDAS[3]
			nPREM := aBANDAS[1]+aBANDAS[2]+aBANDAS[3]
			@ Li,088 PSay nPREM
			nTOTPN := MNR535PNF(cFilP)
			@ Li,094 PSay nTOTPN
			@ Li,099 PSay (aBANDAS[1]*100)/nTOTPN Picture "@E 999.99%"
			@ Li,108 PSay (aBANDAS[2]*100)/nTOTPN Picture "@E 999.99%"
			@ Li,117 PSay (aBANDAS[3]*100)/nTOTPN Picture "@E 999.99%"
			@ Li,126 PSay (nPREM*100)/nTOTPN Picture "@E 999.99%"
			NgSomaLi(58)
		EndIf

		DbSelectArea(cTRB)
		DbSetOrder(nIdxTrb)
		DbSkip()
	End
	RODA(nCNTIMPR,cRODATXT,TAMANHO)
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Devolve a condicao original do arquivo principal             ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	RetIndex('TR4')
	Set Device To Screen
	If aReturn[5] == 1
		Set Printer To
		dbCommitAll()
		OurSpool(WNREL)
	EndIf
	MS_FLUSH()

Return Nil
