			(cTRB)->SERVICO  := STS->TS_SERVICO
			(cTRB)->SEQRELA  := STS->TS_SEQRELA
			(cTRB)->ORDEM    := STS->TS_ORDEM
			(cTRB)->TIPOREG  := cTIPOREG
			(cTRB)->NOMTIPO  := cNOMTIPO
			(cTRB)->PRODUTO  := STT->TT_CODIGO
			(cTRB)->NOMPROD  := cNOME
