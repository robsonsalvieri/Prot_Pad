					Endif
					(cTRB2)->QUANTPRE := (cTRB2)->QUANTPRE + nPRE
					(cTRB2)->QUANTREA := (cTRB2)->QUANTREA + nREA
					(cTRB2)->VALORPRE := (cTRB2)->VALORPRE + (nPRE * ST9->T9_CUSTOHO)
					(cTRB2)->VALORREA := (cTRB2)->VALORREA + (nREA * ST9->T9_CUSTOHO)
