					hFIM := STS->TS_HOPRFIM
				Else
					dFIM := STS->TS_DTMRFIM
					hFIM := STS->TS_HOMRFIM
				Endif

				nREA := NGCALEBEM(dINI, hINI, dFIM, hFIM, STS->TS_CODBEM)
				nREA := HtoM(nREA)/60
				nREA := If(nREA < 0.00, 0.00, nREA)

				lPARADA := .T.
				If STS->(FIELDPOS("TS_PARADA")) > 0
					If STS->TS_PARADA == "N"
						lPARADA := .F.
					Endif
				Endif

				If lPARADA
					dbSelectArea(cTRB2)
					If !dbSeek(STS->TS_ORDEM + STS->TS_PLANO)
						DbAppend()
						(cTRB2)->ORDEM  := STS->TS_ORDEM
