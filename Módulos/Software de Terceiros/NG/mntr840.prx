			cObs := NGMEMOSYP(STS->TS_MMSYP)
		Else
			cObs := MntEspCar( STS->TS_OBSERVA )
		EndIf

		ckey := STS->TS_CODBEM + STS->TS_ORDEM + cTIPOREG + cCODPROD

		dbSelectArea(cTRB)
		dbSetOrder(1)
		If !(cTRB)->(dbSeek(cKey))
			(cTRB)->(DbAppend())
			(cTRB)->PLANO    := STS->TS_PLANO
