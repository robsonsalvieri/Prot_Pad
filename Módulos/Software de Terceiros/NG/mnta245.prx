Function MNT245CKSE(cSERVICO)

	If !ST4->(DbSeek(xFilial("ST4") + cSERVICO))
		Help(" ",1,"SERVICONAOEXIST")
		Return .f.
	EndIf

	If !STE->(DbSeek(xFilial('STE') + ST4->T4_TIPOMAN))
		Help(" ",1,"TIPONAOEXIST")
		Return .f.
	EndIf

	If STE->TE_CARACTE != "C"
		Help(" ",1,"SERVNAOCORRET")
		Return .f.
	EndIf

Return .T.

