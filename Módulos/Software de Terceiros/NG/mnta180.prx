   aOldTPM      := aClone( aCols )
   n            := Len(aCOLS)

   oGet02 := MsNewGetDados():New( 0, 0, 125, 315, GD_INSERT + GD_UPDATE + GD_DELETE, 'NG180LINOK( 2 )', 'AllwaysTrue', '',;
