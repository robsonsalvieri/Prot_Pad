   //+-------------------------------------------------------------------+
   dbSelectArea( 'TP5' )
   dbSetOrder( 3 ) // TP5_FILIAL + TP5_CODFAM + TP5_TIPMOD + TP5_SERVIC + TP5_SEQREL + TP5_TAREFA
