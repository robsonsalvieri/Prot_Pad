/*/{Protheus.doc} ALTPADSTF
Atualiza o STF com os campos modificados do TPF
@author Inacio Luiz Kolling
@since XX/XX/XXXX
@version undefined
@type function
/*/
//---------------------------------------------------------------------
Function ALTPADSTF()

   Local x := 0,y

   dbSelectArea(cTRB)
   For x := 1 To FCOUNT()
      xNOME := AllTrim(FIELDNAME(x))
      lAlFor := .F.
 
      If xNOME != "TPF_FILIAL" .And. xNOME != "TPF_CODFAM" .And. xNOME != "TPF_TIPMOD" .And. xNOME != "TPF_SERVIC" .And.;
         xNOME != "TPF_SEQREL"
         lAlFor := .t.
      EndIf
    
      If lAlFor
         For y := 1 To Len(arraySTF)
            If AllTrim(SUBSTR(arraySTF[y],3,7)) == AllTrim(SUBSTR(xNOME,4,7))
               xx := "STF->"+ arraySTF[Y]
               yy := "(cTRB)->"+ xNOME
               zz := "M->"  + xNOME
               dbSelectArea(xALIAS)
               RecLock(xALIAS,.F.)
               If &xx == &yy .And. &yy <> &zz
                  &xx.:= &zz.
               Elseif MV_PAR02 == 2 .And. &yy <> &zz
                  &xx.:= &zz.
               EndIf
               MsUnLock()
               Exit
            EndIf
         Next y
      EndIf
      dbSelectArea(cTRB)
   Next x

Return .T.
