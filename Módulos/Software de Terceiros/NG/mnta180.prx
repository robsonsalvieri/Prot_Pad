   cQuery := "SELECT * FROM "+RetSqlName("TPH")+" TPH WHERE TPH.TPH_FILIAL = '"+xFilial("TPH")+"' AND TPH.TPH_CODFAM = '"+TPF->TPF_CODFAM+"'"+;
