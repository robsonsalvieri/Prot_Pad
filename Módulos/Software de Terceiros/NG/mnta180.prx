/*/{Protheus.doc} NG180CHECK
Validações código de INSUMO.
@type function

@author Inacio Luiz Kolling
@since 30/07/2001

@param
@return boolean, Define se o conteúdo do campo esta valido.
/*/
//-----------------------------------------------------------------------------------
Function NG180CHECK()

   Local cCodIns := M->TPG_CODIGO
   Local cCodTar := GDFieldGet( 'TPG_TAREFA' ) 
   Local cCodTip := GDFieldGet( 'TPG_TIPORE' ) 
   Local nQtdIns := 0
   Local lRet    := .T.

   M->TPG_TIPORE := cCodTip

   If ( Empty( cCodTar ) .Or. Trim( cCodTar ) == '0' ) .And.;
      !Empty( cCodIns ) .And. M->TPF_PERIOD != 'M'

      GDFieldPut( 'TPG_TAREFA', '0' + Replicate( ' ', Len( TP5->TP5_TAREFA ) - 1 ) )
      oGet03:oBrowse:Refresh()

      cCodTar := GDFieldGet( 'TPG_TAREFA' )

      If !NGIFDBSEEK( 'TP5', M->TPF_CODFAM + M->TPF_TIPMOD + M->TPF_SERVIC + M->TPF_SEQREL + cCodTar, 3 )
         
         RecLock( 'TP5', .T. )

            TP5->TP5_FILIAL := IIF(Empty(cAliQrySTF), xFilial("TP5"), (cAliQrySTF)->TF_FILIAL)
            TP5->TP5_CODFAM := M->TPF_CODFAM
            TP5->TP5_TIPMOD := M->TPF_TIPMOD
            TP5->TP5_SERVIC := M->TPF_SERVIC
            TP5->TP5_SEQREL := M->TPF_SEQREL
            TP5->TP5_TAREFA := '0'
            TP5->TP5_DESCRI := STR0011 // SEM ESPECIFICAO DE TAREFA

         MsUnLock()

         lUpsertTP5 := .T.

      EndIf

   EndIf

   If !Empty( cCodTar )

      aEval( oGet03:aCols, { |x| IIf( ( !aTail( x ) .And. x[nTarG] == cCodTar .And.;
			x[nTipoRe] == cCodTip .And. x[nCodigo] == cCodIns ), nQtdIns++, Nil ) } )

      If nQtdIns > 0

         Help( ' ', 1, 'JAGRAVADO', , CRLF + CRLF + STR0071 + cCodTar + CRLF +; // Tarefa: XXXXXX
            STR0072 + cCodTip + CRLF +; // Tipo: X
			   STR0073 + cCodIns + CRLF, 3, 1 ) // Código: XXXXX 

         lRet := .F.

      EndIf

   EndIf

   If lRet

      Do Case
         
         Case cCodTip == 'M'

            dbSelectArea( 'ST1' )
            dbSetOrder( 1 )
            If !msSeek( FWxFilial( 'ST1' ) +;
					Left( cCodIns, Len( ST1->T1_CODFUNC ) ) )
					
					Help( '', 1, 'FUNCNEXIST' )
					
					lRet := .F.

            Else

               If !NGFUNCRH( ST1->T1_CODFUNC, .T. )

						lRet := .F.
               
               Else

                  GDFieldPut( 'TPG_NOMECO', ST1->T1_NOME )

               EndIf

            EndIf

         Case cCodTip == 'P'

            dbSelectArea( 'SB1' )
            dbSetOrder( 1 )
            If !msSeek( FWxFilial( 'SB1' ) +;
					Left( cCodIns, Len( SB1->B1_COD ) ) )

					Help( '', 1, 'PRODNEXIST' )
				
					lRet := .F.

				Else

               If !NGPRODESP( SB1->B1_COD ) .Or.;
						!NGPROBLQ( SB1->B1_COD )

						lRet := .F.

					Else

						GDFieldPut( 'TPG_NOMECO', SB1->B1_DESC )
						GDFieldPut( 'TPG_UNIDAD', SB1->B1_UM )
						GDFieldPut( 'TPG_LOCAL' , SB1->B1_LOCPAD )

					EndIf

            EndIf

         Case cCodTip == 'T'
            
            dbSelectArea( 'SA2' )
            dbSetOrder( 1 )
            If !msSeek( FWxFilial( 'SA2' ) +;
					Left( cCodIns, Len( SA2->A2_COD ) ) )

					Help( '', 1, 'TERCNEXIST' )
					
					lRet := .F.

            Else

               GDFieldPut( 'TPG_FORNEC', SA2->A2_COD )
					GDFieldPut( 'TPG_LOJA'  , SA2->A2_LOJA )
					GDFieldPut( 'TPG_NOMECO', SA2->A2_NOME )
            
            EndIf

         Case cCodTip == 'F'

            dbSelectArea( 'SH4' )
            dbSetOrder( 1 )
            If !msSeek( FWxFilial( 'SH4' ) +;
					Left( cCodIns, Len( SH4->H4_CODIGO ) ) )
					
					Help( '', 1, 'FERRNEXIS' )

					lRet := .F.

				Else

					GDFieldPut( 'TPG_NOMECO', SH4->H4_DESCRI )

				EndIf

         Case cCodTip == 'E'

            dbSelectArea( 'ST0' )
            dbSetOrder( 1 )
            If !msSeek( FWxFilial( 'ST0' ) +;
					Left( cCodIns, Len( ST0->T0_ESPECIA ) ) )
					
					Help( '', 1, 'ESPENEXIST' )

					lRet := .F.

            Else

               dbSelectArea( 'ST2' )
               dbSetOrder( 2 )
               If !msSeek( FWxFilial( 'ST2' ) +;
                  Left( cCodIns, Len( ST0->T0_ESPECIA ) ) )

						Help( '', 1, 'NAOHAESP' )

						lRet := .F.
					
					Else

						GDFieldPut( 'TPG_NOMECO', ST0->T0_NOME )

					EndIf
				
				EndIf

      End Case

   EndIf

Return lRet


