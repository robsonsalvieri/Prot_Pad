Function NG405TUDO

	Local yX
	Local lNaoMar    := .F.
	Local cOpcao
For yx := 1 to nItens
		cOpcao     := IIf( SubStr( aCols[yX][nBEMIMN], 1, 1 ) == "M", '1', '2' )
		If !Empty( aCols[yX][nOK] ) .And. cOpcao == '2' .And. Empty( aCols[yX][nRESPOS] )
     Help(" ",1,"OPSEMINFO")
     Return .F.
  EndIf
  If !lNAOMAR
     If !Empty(aCols[yx][nOK])
        lNAOMAR := .T.
     EndIf
  EndIf
Next
If !lNAOMAR
   dbSelectArea(cTRBQ400)
   (cTRBQ400)->TQ_OK := "   "
EndIf

Return .T.

