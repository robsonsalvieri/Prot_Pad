Function NG405TPVAR()

If findfunction("NGTPCONTCAR")
   dbSelectArea("TPC")
   dbSetOrder(1)
   If dbSeek(xFilial('TPC')+cetapa+aCOLS[n,nnOPCA])
      If !NGTPCONTCAR(TPC->TPC_TIPCAM,M->TPQ_RESPOS)
         Return .F.
      EndIf
   EndIf
EndIf
