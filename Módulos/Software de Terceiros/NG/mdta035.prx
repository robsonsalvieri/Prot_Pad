/*/
Function D035CHK()
Local xx := 0, npos, lRET := .T.
Local nX

nPOS := aSCAN( aHEADER, { |x| Trim( Upper(x[2]) ) == "TMB_EXAME"})

If nPOS > 0
   For nX := 1 to Len(aCOLS)
       If aCOLS[nX][nPOS] == m->TMB_EXAME .and. !aCOLS[nX][Len(aCOLS[nX])]
          xx := xx + 1
       Endif
   Next
Endif

If xx > 0
   Help(" ",1,"JAEXISTINF")
   lRet := .f.
Endif

Return lRET

