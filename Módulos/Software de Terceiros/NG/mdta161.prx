		                 "TMK_DTTERM", "TMK_SESMT", "TMK_INDFUN", "TMK_CC", "TMK_CIC"}

	// Variaveis FillGetDados--
	Local aNoFields	 := {}
	Local cSeek		 := "TY3->TY3_CODMED"
	Local cWhile	 := "TY3->TY3_FILIAL == xFilial( 'TY3' )"

