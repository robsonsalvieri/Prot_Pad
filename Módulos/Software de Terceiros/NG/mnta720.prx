/*/{Protheus.doc} MNTA720VLD
Validação de campos

@param cField, string, nome do campo
@author Maria Elisandra de Paula
@since 10/11/2020
@return boolean
/*/
//----------------------------------------------------
Function MNTA720VLD( cField )

	//Passar pela função toda

	Local nPosItemCp := 0
	Local lRet       := .T.
	Local cItem      := ''
	Local aTerceiro  := {}

	If cField == 'TR8_NUMCP'

		nPosItemCp := GDFieldPos( 'TR8_ITEMCP' )

		If !Empty( M->TR8_NUMCP )

			If Select('SC3') == 0 .Or. M->TR8_NUMCP != SC3->C3_NUM
				dbSelectArea('SC3')
				dbSetOrder(1)
				dbSeek( xFilial( 'SC3' ) + M->TR8_NUMCP )
			EndIf

			cItem := SC3->C3_ITEM
			lRet := ExistCpo( 'SC3', M->TR8_NUMCP + cItem )

			If lRet 
				aTerceiro := NGProdMNT( 'T' )
				If aScan( aTerceiro, SC3->C3_PRODUTO ) == 0
					Help(" ",1,STR0013,,'O produto do contrato informado não condiz com a configuração de produtos de terceiros referente ao parâmetro "MV_PRODTER".',3,1)
					lRet := .F.
				EndIf
			EndIf

			If lRet
				aCols[n,nPosItemCp] := cItem
			EndIf

		EndIf

		If !lRet .Or. Empty( M->TR8_NUMCP )

			aCols[n,nPosItemCp] := Space( TamSx3('TR8_ITEMCP')[1] )

		EndIf

	EndIf

Return lRet

//----------------------------------------------------------------
/*/{Protheus.doc} GeraAutEnt
Gera Autorização de Entrega de acordo com contrato de Parceria

@param aItemTr8, array, valores dos campos do item selecionado
@param nRecStl, numerico, Recno do insumo
@author Maria Elisandra de Paula
@since 10/11/2020
@return boolean
/*/
//----------------------------------------------------------------
Static Function GeraAutEnt( aItemTr8, nRecStl )

	//Passar pela função toda

	Local lMnta720A := ExistBlock( 'MNTA720A' )
	Local lRet      := .T.
	Local aCabec    := {}
	Local aLinha    := {}
	Local aItens    := {}
	Local aExec     := {}
	Local cContrato := aItemTr8[ GDFieldPos('TR8_NUMCP') ]
	Local cItem     := aItemTr8[ GDFieldPos('TR8_ITEMCP') ]

	Private lMsErroAuto := .F. // Controle de execução
	Private lMSHelpAuto := .T. // Não apresenta erro em tela

	dbSelectArea('SC3')
	dbSetOrder(1) // C3_FILIAL + C3_NUM + C3_ITEM
	dbSeek( xFilial('SC3') + cContrato + cItem )

	aadd( aCabec, { 'C7_EMISSAO', M->TR7_DTLOTE } )
	aadd( aCabec, { 'C7_FORNECE', M->TR7_FORNECE } )
	aadd( aCabec, { 'C7_LOJA', M->TR7_LOJA } )
	aadd( aCabec, { 'C7_COND', SC3->C3_COND } )
	aadd( aCabec, { 'C7_CONTATO', '' } )
	aadd( aCabec, { 'C7_FILENT', cFilAnt } )

	aLinha := {}
	aadd( aLinha, { 'C7_PRODUTO', SC3->C3_PRODUTO, Nil } )
	aadd( aLinha, { 'C7_QUANT' , 1, Nil } )
	aadd( aLinha, { 'C7_PRECO' , SC3->C3_PRECO, Nil } )
	aadd( aLinha, { 'C7_TOTAL' , SC3->C3_PRECO, Nil } )
	aadd( aLinha, { 'C7_NUMSC' , SC3->C3_NUM, Nil } )
	aadd( aLinha, { 'C7_ITEMSC', SC3->C3_ITEM, Nil } )
	aadd( aLinha, { 'C7_ORIGEM', 'MNTA720', Nil } )

	//----------------------------------------------
	// PE para adicionar campos no array do execauto
	//----------------------------------------------
	If lMnta720A
		aExec  := ExecBlock( 'MNTA720A', .F., .F., { aItemTr8, aCabec, aLinha } )
		aCabec := aClone( aExec[1] )
		aLinha := aClone( aExec[2] )
	EndIf

	aadd( aItens, aLinha )

	MSExecAuto({|a,b,c,d,e,f,g| MATA120( a, b, c, d, e, f,, g )}, 2, aCabec, aItens, 3, .F., {}, {} )

	If !lMsErroAuto

		lRet := .T.

		If STL->( FieldPos('TL_NUMAE') ) > 0 .AND. STL->( FieldPos('TL_ITEMAE') ) > 0

			//---------------------------------------------------
			// Grava numero Autorização de Entrega e contrato
			//---------------------------------------------------
			dbSelectArea('STL')
			dbGoto( nRecStl )
			RecLock('STL', .F.)
			STL->TL_NUMAE  := SC7->C7_NUM   // Autorização de entrega
			STL->TL_ITEMAE := SC7->C7_ITEM  // Item da autorização
			STL->( MsUnLock() )

		EndIf

	Else

		MostraErro()
		lRet := .F.

	EndIf

Return lRet

//----------------------------------------------------------------
/*/{Protheus.doc} DelAutEnt
Exclui Autorização de Entrega caso exista

@param cNumAe, string, numero do contrato
@param cItemAe, string, numero do item
@author Maria Elisandra de Paula
@since 17/11/2020
@return boolean
/*/
//----------------------------------------------------------------
Static Function DelAutEnt( cNumAe, cItemAe )

	//Passar pela função toda

	Local lRet   := .T.
	Local aCabec := {}
	Local aLinha := {}
	Local aItens := {}

	If STL->( FieldPos('TL_NUMAE') ) > 0 .And. !Empty( cNumAe ) .And. !Empty( cItemAe )

		Private lMsErroAuto := .F. // Controle de execução
		Private lMSHelpAuto := .T. // Não apresenta erro em tela

		dbSelectArea('SC7')
		dbSetorder(1)
		If dbSeek( xFilial('SC7') + cNumAe + cItemAe )

			aadd( aCabec, { 'C7_NUM', SC7->C7_NUM } )

			aLinha := {}
			aadd( aLinha, { 'C7_ITEM', SC7->C7_ITEM, Nil } )

			aadd( aItens, aLinha )

			MSExecAuto({|a,b,c,d,e,f,g| MATA120( a, b, c, d, e, f,, g )}, 2, aCabec, aItens, 5, .F., {}, {} )

			If !lMsErroAuto

				lRet := .T.

			Else

				MostraErro()
				lRet := .F.

			EndIf

		EndIf

	EndIf

Return lRet
