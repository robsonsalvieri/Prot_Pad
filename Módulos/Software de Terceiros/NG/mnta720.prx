/*/{Protheus.doc} MNTA720SC3
Filtro para consulta NGSC3 - CONTRATO DE PARCERIA

@author Maria Elisandra de Paula
@since 10/11/2020
@return string
/*/
//----------------------------------------------------
Function MNTA720SC3()
	//Passar pela função toda

	Local cRet      := ''
	Local aTerceiro := NGProdMNT( 'T' )
	Local nIndex    := 0
	Local cCondSql  := ''

	For nIndex := 1 To Len( aTerceiro )
		If nIndex > 1
			cCondSql +=	','
		EndIf
		cCondSql += ValtoSql( aTerceiro[nIndex] )
	Next nIndex

	cRet += "@C3_FORNECE = " + ValToSql( M->TR7_FORNEC )
	cRet += " AND C3_LOJA = " + ValToSql( M->TR7_LOJA )
	cRet += " AND C3_CONAPRO <> 'B'"
	cRet += " AND C3_QUJE < C3_QUANT "
	If !Empty( cCondSql )
		cRet += " AND C3_PRODUTO IN (" + cCondSql + ")"
	EndIf

	If SuperGetMV( 'MV_CONTVLD', .F. ,'2' ) == '1'
		cRet += " AND C3_DATPRF >= " + ValToSQL( DtoS( dDataBase ) )
	EndIf

	If SuperGetMV( 'MV_DTINVLD', .F., .F. )
		cRet += " AND C3_DATPRI <= " + ValToSQL( DtoS( dDataBase ) )
	EndIf

Return cRet

