		M700LINOk(oGetAbas:nAt)

	Activate MsDialog oDlgC On Init EnchoiceBar(oDlgC,{||nOpcao:=1,If(!M700GRAVA(),nOpcao := 0,oDlgC:End())},{|| oDlgC:End()}) Centered

