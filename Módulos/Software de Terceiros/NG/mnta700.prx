Function MNT700VALP()

	If Empty(MV_PAR04)
		Help(" ",1,"ATEINVALID")
		Return .F.
	Elseif MV_PAR04 < MV_PAR03
		Help(" ",1,"DEATEINVAL")
		Return .F.
	EndIf

	If MV_PAR04 = Replicate('Z',Len(MV_PAR04))
		Return .T.
	Else
		If !ExistCpo("SX5","ER"+MV_PAR04)
			Return .F.
		EndIf
	EndIf

Return .T.

