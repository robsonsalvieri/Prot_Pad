Function MNTR970()

	Local oReport
	Local aArea := GetArea()

	Private cTRB	:= GetNextAlias()
	Private cTRB1	:= GetNextAlias()
	Private cTRB2	:= GetNextAlias()
	Private nTamPro := TamSX3("B1_COD")[1]

	If FindFunction("TRepInUse") .And. TRepInUse()
		//-- Interface de impressao
		oReport := ReportDef()
		oReport:SetLandscape()
		oReport:PrintDialog()
	Else
		MNTR970R3()
	EndIf
	RestArea(aArea)  

Return .T.

