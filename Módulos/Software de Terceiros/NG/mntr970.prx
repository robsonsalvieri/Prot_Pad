Function MNTR970R3()

	Local cString	:= "TPV"
	Local cDesc1	:= STR0005 //"Relatorio do Roteiro de Lubrificacao. Roteiro a ser seguido com os bens e"
	Local cDesc2	:= STR0006 //"seus respectivos pontos de lubrificacao."
	Local cDesc3	:= ""
	Local wnrel		:= "MNTR970"

	Private aReturn		:= {"Zebrado", 1,"Administracao", 2, 2, 1, "",1 }
	Private nLastKey	:= 0
	Private cPerg		:= "MNT97R"
	Private Titulo		:= STR0004 //"Roteiro de Lubrificao"
	Private Tamanho		:= "G"

	/*
	Ŀ
	 Variaveis utilizadas para qarametros!                        
	 mv_par01     // De Data                                      
	 mv_par02     // Ate Data                                     
	 mv_par03     // De Roteiro                                   
	 mv_par04     // Ate Roteiro                                  
	
	*/
	Pergunte(cPerg,.F.)

	//Ŀ
	// Envia controle para a funcao SETPRINT                        
	//
	wnrel:=SetPrint(cString,wnrel,cPerg,titulo,cDesc1,cDesc2,cDesc3,.F.,"")
	If nLastKey = 27
		Set Filter To
		dbSelectArea("TPV")
		Return
	EndIf

	SetDefault(aReturn,cString)
	RptStatus({|lEnd| R970Imp(@lEnd,wnRel,titulo,tamanho)},titulo)
	DbSelectArea("TPV")

Return NIL

