/*/
Static Function ReportPrint(oReport)

	Local oSection1 := oReport:Section(1)
	Local oSection2 := oReport:Section(2)

	Private aVETINR  := {}
	Private oARQTRB

	Processa({|lEND| MNTR970TPV()},STR0021) //"Processando Arquivo..."

	DbSelectArea(cTRB)  
	DbSetOrder(01)
	DbGotop()
	oReport:SetMeter(LastRec())
	cRot := ""
	If !Eof()
		While !Eof() .And. !oReport:Cancel()

			cRot := (cTRB)->ROTEIRO

			oSection1:Init()
			oSection1:PrintLine()
			oSection2:Init()

			DbSelectArea(cTRB)
			While !Eof() .And. (cTRB)->ROTEIRO = cRot .And. !oReport:Cancel()

				oReport:IncMeter()
				oSection2:PrintLine()

				DbSelectArea(cTRB)
				DbSkip()
			End

			oSection1:Finish()
			oSection2:Finish()
		End
	Else
		MsgInfo(STR0024,STR0025) //"No existem dados para montar o relatrio."###"ATENO"
		oARQTRB:Delete()
		Return .F.
	Endif

	//Deleta o arquivo temporario fisicamente
	oARQTRB:Delete()

Return .T.
