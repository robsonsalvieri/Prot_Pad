		cQuery += 								"TRIM( B.TUB_CODIGO ) = TRIM( A.TAF_CODCON ) ) ) AND "
		cQuery += 							"B.TUB_TIPRES = '1' AND "
		cQuery +=							"B.TUB_MARCA  = '1' AND "
		cQuery += 							"B.D_E_L_E_T_ = ' ' "
		cQuery += 							cJoin
		cQuery += 					"INNER JOIN ( "
		cQuery += 						"SELECT "
		cQuery += 							"TAF_CODNIV, "
		cQuery += 						 	"TAF_NIVSUP, "
		cQuery += 							"TAF_CODCON  "
		cQuery += 						"FROM "
		cQuery += 							RetSQLName( 'TAF' ) + " ) R ON "
		cQuery += 								" A.TAF_NIVSUP = R.TAF_CODNIV "
		cQuery += 					"WHERE "
		cQuery += 						"A.TAF_FILIAL = " + ValToSQL( FWxFilial( 'TAF' ) ) + " AND "
