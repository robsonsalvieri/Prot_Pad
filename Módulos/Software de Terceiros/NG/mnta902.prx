/*/{Protheus.doc} MNTA902OS
Carrega as Solicitações de Serviço e Ordens de Serviço.
@type static

@author Rafael Diogo Richter
@since 17/07/2009

@sample MNTA902OS( 'B', .T. )

@param  cTipItem, Caracter, Tipo do item que será carregado ( 'B', 'L', 'F' e 'T')
@param  [lClose], Lógico  , Define se a localização deve se manter fechada.
@return .T.
/*/
//---------------------------------------------------------------------------------
Static Function MNTA902OS( cTipItem, lClose )
	
	Local aAreaTRB := (cTRBSGA)->( FWGetArea() )
	Local aAreaSTJ := {}
	Local aAreaTQB := {}
	Local aBind    := {}
	Local cAlsSTJ  := GetNextAlias()
	Local cAlsTQB  := GetNextAlias()
	Local lIncFil  := .F.
	
	Default lClose := .F.

	If cTipItem == "L"
		cNivSup := (cTRBSGA)->CODPRO+Space(Len(STJ->TJ_CODBEM)-Len((cTRBSGA)->CODPRO))
	Else
		cNivSup := (cTRBSGA)->CODPRO
	EndIf

	lIncSS := .F.

	If nMod902 == 19

		If nExibOsSs == 0 .Or. nExibOsSs == 2
			
			aAreaTQB := TQB->( FWGetArea() )

			If Empty( cQryTreeSS )
					
				cQryTreeSS := 	"SELECT "
				cQryTreeSS +=		"TQB.R_E_C_N_O_ "
				cQryTreeSS += 	"FROM "
				cQryTreeSS +=		RetSQLName( 'TQB' )     + " TQB "
				cQryTreeSS +=	"WHERE "
				cQryTreeSS +=		"TQB.TQB_FILIAL = ? AND "
				cQryTreeSS +=		"TQB.TQB_TIPOSS = ? AND "
				cQryTreeSS +=		"TQB.TQB_CODBEM = ? AND "
				cQryTreeSS +=		"TQB.TQB_DTFECH = ? AND "
				cQryTreeSS +=		"TQB.TQB_SOLUCA <> ? AND "
				cQryTreeSS +=		"TQB.D_E_L_E_T_ = ?  "

				cQryTreeSS := ChangeQuery( cQryTreeSS )

			EndIf
			
			aBind := {}
			aAdd( aBind, FwxFilial( 'TQB' ) )
			aAdd( aBind, cTipItem )

			If cTipItem == 'L'
				
				aAdd( aBind, cNivSup )

			Else

				aAdd( aBind, (cTRBSGA)->CODTIPO )

			EndIf

			aAdd( aBind, Space( 1 ) )
			aAdd( aBind, 'C' )
			aAdd( aBind, Space( 1 ) )

			dbUseArea( .T., 'TOPCONN', TcGenQry2( , , cQryTreeSS, aBind ), cAlsTQB, .T., .T. )

			While (cAlsTQB)->( !EoF() )

				dbSelectArea( 'TQB' )
				dbSetOrder( 13 ) // TQB_FILIAL + TQB_TIPOSS + TQB_CODBEM
				msGoTo( (cAlsTQB)->R_E_C_N_O_ )

				If !NGValidTUA( 'TQB', , cNivSup )

					dbSelectArea("TQB")
					dbSetOrder(13)
					dbSkip()
					Loop

				EndIf

				fIncSS( TQB->TQB_SOLICI, AllTrim( cNivSup ), If(cTipItem=="L",'LOC','BEM') )

				lIncSS := .T.

				Exit

			End

			(cAlsTQB)->( dbCloseArea() )

			FWRestArea( aAreaTQB )

		EndIf
		
		If nExibOsSs == 0 .Or. nExibOsSs == 1
			
			//Carrega as Ordens de Servico
			If !lIncSS
				
				aAreaSTJ := STJ->( FWGetArea() )
				
				If Empty( cQryTreeOS )
					
					cQryTreeOS := 	"SELECT "
					cQryTreeOS +=		"STJ.R_E_C_N_O_ "
					cQryTreeOS += 	"FROM "
					cQryTreeOS +=		RetSQLName( 'STJ' )     + " STJ "
					cQryTreeOS += 	"LEFT JOIN "
					cQryTreeOS +=		oARQTA100:GetRealName() + " TRB ON "
					cQryTreeOS +=			"TRB.FILIAL  = ?            AND "
					cQryTreeOS +=			"TRB.TIPO    = ?            AND "
					cQryTreeOS +=			"TRB.CODTIPO = STJ.TJ_ORDEM  "
					cQryTreeOS +=	"WHERE "
					cQryTreeOS +=		"STJ.TJ_FILIAL  = ? AND "
					cQryTreeOS +=		"STJ.TJ_TIPOOS  = ? AND "
					cQryTreeOS +=		"STJ.TJ_CODBEM  = ? AND "
					cQryTreeOS +=		"STJ.TJ_TERMINO = ? AND "
					cQryTreeOS +=		"STJ.TJ_SITUACA = ? AND "
					cQryTreeOS +=		"STJ.D_E_L_E_T_ = ? AND "
					cQryTreeOS +=		"TRB.CODTIPO IS NULL "

					cQryTreeOS := ChangeQuery( cQryTreeOS )

				EndIf
				
				aBind := {}
				aAdd( aBind, AllTrim( cValToChar( 6 ) ) )
				aAdd( aBind, cFilAnt )
				aAdd( aBind, FwxFilial( 'STJ' ) )
				aAdd( aBind, cTipItem )

				If cTipItem == 'L'
					
					aAdd( aBind, cNivSup )

				Else

					aAdd( aBind, (cTRBSGA)->CODTIPO )

				EndIf

				aAdd( aBind, 'N' )
				aAdd( aBind, 'L' )
				aAdd( aBind, Space( 1 ) )

				dbUseArea( .T., 'TOPCONN', TcGenQry2( , , cQryTreeOS, aBind ), cAlsSTJ, .T., .T. )

				While (cAlsSTJ)->( !EoF() )

					dbSelectArea( 'STJ' )
					dbSetOrder( 2 ) // TJ_FILIAL + TJ_TIPOOS + TJ_CODBEM + TJ_SERVICO + TJ_SEQRELA + TJ_ORDEM + TJ_PLANO
					msGoTo( (cAlsSTJ)->R_E_C_N_O_ )

					If !NGValidTUA( 'STJ', , cNivSup, , ( STJ->TJ_PLANO == '000000' ) )

						dbSelectArea("STJ")
						dbSetOrder(2)
						dbSkip()
						Loop

					EndIf

					fIncOS( STJ->TJ_ORDEM, Trim(cNivSup), If(cTipItem=="L","LOC","BEM"), .F., , lClose )

					Exit

				End

				(cAlsSTJ)->( dbCloseArea() )

				FWRestArea( aAreaSTJ )

			EndIf

		EndIf

	ElseIf nMod902 == 35
		If cTipItem == "L" .Or. cTipItem == "F" .Or. cTipItem == "T"
			If lOHSAS
				If cTipItem == "L"
					cNivSup := (cTRBSGA)->CODPRO
				EndIf
         		//Carrega os perigos, danos e análises
				aAreaTG9 := TG9->(GetArea())
				dbSelectArea("TG9")
				dbSetOrder(2)
				dbSeek(xFilial("TG9")+cCodEst+cNivSup)
				While !EoF() .And. TG9->TG9_FILIAL == xFilial("TG9") .And. TG9->TG9_CODEST == cCodEst .And. TG9->TG9_CODNIV == cNivSup

					nRec:=(cTRBSGA)->(Recno())
					dbSelectArea(cTRBSGA)
					dbSetOrder(7)
					If dbSeek(_nPosPerigo+Padr( TG9->TG9_CODPER , Len( (cTRBSGA)->CODTIPO ) )+cCodEst+cNivSup)
						(cTRBSGA)->(dbGoto(nRec))
						dbSelectArea("TG9")
						dbSetOrder(2)
						dbSkip()
						Loop
					EndIf
					(cTRBSGA)->(dbGoto(nRec))

					If !NGValidTUA("TG9",,cNivSup)
						dbSelectArea("TG9")
						dbSetOrder(2)
						dbSkip()
						Loop
					EndIf

					IncProc() // Incrementa regua de processamento

					fIncPer( TG9->TG9_CODPER , Trim(cNivSup) , If(cTipItem == "L","LOC",If(cTipItem=="F","FUN","TAR")) , .F. , TAF->TAF_MAT )

					dbSelectArea("TG9")
					dbSetOrder(2)
					dbSkip()
				End
				RestArea(aAreaTG9)
			EndIf
		EndIf

	ElseIf nMod902 == 56

		If cTipItem == "L"
			cNivSup := (cTRBSGA)->CODPRO

			If !lIncFil
         		//Carrega as definições e ocorrências
				aAreaTAV := TAV->(GetArea())
				dbSelectArea("TAV")
				dbSetOrder(2)
				dbSeek(xFilial("TAV")+cCodEst+cNivSup)
				While !EoF() .And. TAV->TAV_FILIAL == xFilial("TAV") .And. TAV->TAV_CODEST == cCodEst .And. TAV->TAV_CODNIV == cNivSup

					lIncFil := .T.
					SgaIncRes( TAV->TAV_CODRES, Trim(cNivSup), "LOC", TAF->TAF_MAT, 0 )

					Exit
				End
				RestArea(aAreaTAV)
			EndIf

			cNivSup := (cTRBSGA)->CODPRO

			If !lIncFil
         		//Carrega os aspectos
				aAreaTAG := TAG->(GetArea())
				dbSelectArea("TAG")
				dbSetOrder(2)
				dbSeek(xFilial("TAG")+cCodEst+cNivSup)
				While !EoF() .And. TAG->TAG_FILIAL == xFilial("TAG") .And. TAG->TAG_CODEST == cCodEst .And. TAG->TAG_CODNIV == cNivSup

					lIncFil := .T.
					SgaIncAsp( TAG->TAG_CODASP , Trim(cNivSup) , "LOC" , TAF->TAF_MAT, 0 )

					Exit
				End
				RestArea(aAreaTAG)
			EndIf

			cNivSup := (cTRBSGA)->CODPRO

			If !lIncFil
         		//Carrega os planos emergenciais
				aAreaTBC := TBC->(GetArea())
				dbSelectArea("TBC")
				dbSetOrder(2)
				dbSeek(xFilial("TBC")+cCodEst+cNivSup)
				While !EoF() .And. TBC->TBC_FILIAL == xFilial("TBC") .And. TBC->TBC_CODEST == cCodEst .And. TBC->TBC_CODNIV == cNivSup

					lIncFil := .T.
					SgaIncPE( TBC->TBC_CODPLA , Trim(cNivSup) , "LOC" ,  , 0 )

					Exit
				End
				RestArea(aAreaTBC)
			EndIf
		EndIf
	EndIf

	FWRestArea( aAreaTRB )

	FWFreeArray( aAreaTRB )
	FWFreeArray( aAreaSTJ )
	FWFreeArray( aAreaTQB )

Return .T.

