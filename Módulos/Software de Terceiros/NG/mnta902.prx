Static Function fIncSS(cCodSS, cNivSup, cCarSup )

	Local aAreaTMP := (cTRBSGA)->( FWGetArea() )
	Local aAreaTQB := TQB->( FWGetArea() )
	Local cDesPro  := ''
	Local cFolderA := ''
	Local cFolderB := ''
	Local cCargo   := ''
	Local cRespons := (cTRBSGA)->RESPONS
		
	cFolderA := fRetImgSS()[1]
	cFolderB := cFolderA
	cCargo   := fRetImgSS()[2]

	cDesPro := AllTrim( Msmm( TQB->TQB_CODMSS , , , , 3 ) )

	cCodNiv := fRetCodNiv()

	dbSelectArea( cTRBSGA )
	dbSetOrder( 3 ) // TIPO + CODTIPO + FILIAL
	If !msSeek( '5' + PadR( TQB->TQB_SOLICI, Len( (cTRBSGA)->CODTIPO ) ) + cFilAnt )
	
		RecLock( (cTRBSGA), .T. )
			
			(cTRBSGA)->FILIAL  := cFilAnt
			(cTRBSGA)->CODEST  := cCodEst
			(cTRBSGA)->CODPRO  := cCodNiv
			(cTRBSGA)->DESCRI  := cDesPro
			(cTRBSGA)->NIVSUP  := cNivSup
			(cTRBSGA)->RESPONS := cRespons
			(cTRBSGA)->TIPO    := "5"
			(cTRBSGA)->CODTIPO := TQB->TQB_SOLICI
			(cTRBSGA)->ORDEM   := cCodNiv
			(cTRBSGA)->CARGO   := cCargo

			If nMod902 == 19
				(cTRBSGA)->MODMNT  := 'X'
			ElseIf nMod902 == 35
				(cTRBSGA)->MODMDT  := 'X'
			ElseIf nMod902 == 56
				(cTRBSGA)->MODSGA  := 'X'
			EndIf

		(cTRBSGA)->( MsUnLock() )

		oTree:TreeSeek( cNivSup + cCarSup + cFilAnt )
		
		oTree:AddItem( TQB->TQB_SOLICI + ' - ' + cDesPro, cCodNiv + cCargo + cFilAnt,;
			cFolderA, cFolderB, , , 2 )

	EndIf

	FWRestArea( aAreaTQB )
	FWRestArea( aAreaTMP )

	FWFreeArray( aAreaTQB )
	FWFreeArray( aAreaTMP )

Return cCargo

