/*/{Protheus.doc} fNewCode
Busca o proximo codigo de nivel disponivel
@type function

@author Alexandre Santos
@since 03/12/2024

@param 
@return integer, novo código do nivel. 
/*/ 
//------------------------------------------------------------------------------
Static Function fNewCode()

	Local cNewCode := '000000'
	Local cAlsTAF  := GetNextAlias()
	Local cDBase   := Upper( TCGetDB() )
	Local cWhere   := '%%'
	
	If cDBase == 'ORACLE' .Or. cDBase == 'POSTGRES'

		cWhere := "%AND LENGTH( RTRIM( TAF.TAF_CODNIV ) ) = 6%"

	Else

		cWhere := "%AND LEN( RTRIM( TAF.TAF_CODNIV ) ) = 6%"

	EndIf

	BeginSQL Alias cAlsTAF

		SELECT
			TAF.TAF_CODNIV
		FROM
			%table:TAF% TAF
		WHERE
			TAF.TAF_FILIAL = %xFilial:TAF% AND
			TAF.%NotDel%
			%exp:cWhere%
		ORDER BY
			TAF.TAF_CODNIV DESC

	EndSQL

	If (cAlsTAF)->( !EoF() )

		cNewCode := Soma1( AllTrim( (cAlsTAF)->TAF_CODNIV ) )
		
	EndIf

	(cAlsTAF)->( dbCloseArea() )
	
Return cNewCode

