				(cTRBSGA)->DESCRI  := M->TAF_NOMNIV
				(cTRBSGA)->RESPONS := M->TAF_MAT
				(cTRBSGA)->CC      := M->TAF_CCUSTO
				(cTRBSGA)->PLANTA  := M->TAF_PLANTA
				If lDepto
