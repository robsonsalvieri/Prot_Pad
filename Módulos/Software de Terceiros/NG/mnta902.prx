	cLocal := cCod
	dbSelectArea(cTRBSGA)
	dbSetOrder(2)
	dbSeek("001"+cCod+cFilAnt)

	If SubStr( oTree:GetCargo(), nTamTAF2, 3 ) == 'LOC'

		dbSelectArea("TAF")
		dbSetOrder(2)
		If msSeek( FWxFilial( 'TAF') + '001' + cCod )
			nRecno := TAF->(Recno())
		Else
