			EndIf

			If lRetS
				(cTRBSGA)->ETAPA  := If( nMod902 <> 35 .And. nMod902 <> 56, M->TAF_ETAPA, "" )
			EndIf
			If lRateio
				(cTRBSGA)->RATEIO  := M->TAF_RATEIO
			EndIf
			If lFamilia
