		oBmpArv:Hide()

	Activate MSDIALOG oDlg ON INIT (EnchoiceBar(oDlg,{||nOpca := 1,oDlg:End()},{||oDlg:End()},,aButton), fIndCreate())

	If nOpca == 1
		Processa({|| MNT902Grava()},STR0019,STR0144,.F.) //"Salvando Alterações..."
	EndIf

	For nX := 1 To Len(aTblTemp)
		aTblTemp[nX][2]:Delete()
	Next nX

Return .T.

