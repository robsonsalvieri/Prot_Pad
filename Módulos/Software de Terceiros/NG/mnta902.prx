			aLegenda[ 7 ]  := { 'NG_ICO_OCO_RES',     STR0294 }
			aLegenda[ 8 ]  := { 'NG_ICO_FMRPC',       STR0305 }
			aLegenda[ 9 ]  := { 'NG_ICO_FMRAP',       STR0306 }
			aLegenda[ 10 ]  := { 'NG_ICO_FMRAR',       STR0307 }
			aLegenda[ 11 ] := { 'NG_ICO_FMRNC',       STR0308 }
			aLegenda[ 12 ] := { 'NG_ICO_FMRRB',       STR0309 }

	EndIf

