Function MNT902MLoc(cCod)

	Local aAreaST9 := {}

	aAreaST9 := ST9->(GetArea())
	oPnlEstB:Hide()
	cLoc := ""

	dbSelectArea(cAliasTRB)
	dbSetOrder(2)
	If dbSeek(cCod)
