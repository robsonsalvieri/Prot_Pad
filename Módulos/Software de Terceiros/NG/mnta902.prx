				(cTRBTAK)->CODNIV := cLocal
				(cTRBTAK)->CODFUN := (cTRBTAP)->CODFUN
				(cTRBTAK)->MAT    := (cTRBTAP)->MAT
				(cTRBTAK)->OPERAC := (cTRBTAP)->OPERAC
