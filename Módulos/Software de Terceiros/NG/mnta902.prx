Function NGDeleteTUB()

	Local aArea := TAF->(GetArea())

	If !AliasInDic("TUA") .Or. !AliasInDic("TUB")
		Return
	EndIf

	dbSelectArea("TUA")
	dbSetOrder(1)
	dbSeek(xFilial("TUA"))
	While !EoF() .And. xFilial("TUA") == TUA->TUA_FILIAL

		dbSelectArea("TUB")
		dbSetOrder(1)
		If dbSeek(xFilial("TUB")+TUA->TUA_TIPRES+TUA->TUA_TIPO+TUA->TUA_GRPUSR+"8"+TAF->TAF_CODNIV)
			RecLock("TUB",.F.)
			dbDelete()
			TUB->( MsUnLock() )
		EndIf

		dbSelectArea("TUA")
		dbSkip()
	EndDo

	RestArea(aArea)
Return

