				EndIf
				(cTRBSGA)->CENTRAB := IIf( nMod902 <> 35 .And. nMod902 <> 56, M->TAF_CENTRA, "" )
				(cTRBSGA)->DOCFIL  := ""
				If lRetS
					(cTRBSGA)->ETAPA := IIf( nMod902 <> 35 .And. nMod902 <> 56, M->TAF_ETAPA, "" )
				EndIf
				If nMod902 == 19
					(cTRBSGA)->MODMNT  := 'X'
				ElseIf nMod902 == 35
