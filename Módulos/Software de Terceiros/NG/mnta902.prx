@type Function

@author Alexandre Santos
@since 26/08/2024

@param
@return boolean, Indica se o processo de ajuste foir realizado. 
/*/ 
//------------------------------------------------------------------------------
Function MNTA902Upd()
	
	Local cAlsTAF := GetNextAlias()
	Local cDBase  := Upper( TCGetDB() )
	Local cWhere  := '%%'
	Local lRet    := .T.

	If cDBase == 'ORACLE' .Or. cDBase == 'POSTGRES'

		cWhere := "%AND LENGTH( RTRIM( TAF.TAF_CODNIV ) ) < 6%"

	Else

		cWhere := "%AND LEN( RTRIM( TAF.TAF_CODNIV ) ) < 6%"

	EndIf

	BeginSQL Alias cAlsTAF

		SELECT
			TAF.TAF_CODNIV
		FROM
			%table:TAF% TAF
		WHERE
			TAF.TAF_FILIAL = %xFilial:TAF% AND
			TAF.%NotDel%
			%exp:cWhere%
		ORDER BY
			TAF.TAF_CODNIV

	EndSQL

	If (cAlsTAF)->( !EoF() )

		If ( lRet := MsgYesNo( STR0341,; // Atenção
			STR0091 ) ) // Devido a um aumento na capacidade de registros na árvore lógica, será necessário reprocessar os níveis existentes, este processo pode demorar alguns minutos e não deve ser interrompido.

			BEGIN TRANSACTION
				
				Processa( { || fAjustBase( cAlsTAF ) }, STR0019 ) // Aguarde...

			END TRANSACTION

		EndIf

	EndIf

	(cAlsTAF)->( dbCloseArea() )

Return lRet

//------------------------------------------------------------------------------
/*/{Protheus.doc} fAjustBase
Ajuste de base dos campos TAF_CODNIV e TAF_NIVSUP
@type Function

@author Alexandre Santos
@since 23/08/2024

@param cAlsTAF, string, Alias com registros para ajuste. 
@return  
/*/ 
//------------------------------------------------------------------------------
Static Function fAjustBase( cAlsTAF )

	Local cNewCode := fNewCode()
	Local nTotal   := 0
	Local nAtual   := 0

	dbSelectArea( cAlsTAF )
	dbGoTop()
	
	Count To nTotal

	ProcRegua( nTotal )

	dbSelectArea( cAlsTAF )
	dbGoTop()

	While (cAlsTAF)->( !EoF() )

		nAtual++
		IncProc( STR0339 + cValToChar( nAtual ) + STR0340 + cValToChar( nTotal ) + '...' ) // Atualizando registro XX de XXXX...
		
		cNewCode := Soma1( AllTrim( cNewCode ) )

		/*--------------------------+
		| Atualiza campo TAF_NIVSUP |
		+--------------------------*/
		cQuery := "UPDATE " + RetSQLName( 'TAF' ) + " "
		cQuery += "SET " 
		cQuery +=	"TAF_NIVSUP = " + ValToSQL( cNewCode ) + " "
		cQuery += "WHERE "
		cQuery +=	"TAF_FILIAL = " + ValToSQL( FWxFilial( 'TAF' ) ) + " AND "
		cQuery += 	"TAF_NIVSUP = " + ValToSQL( (cAlsTAF)->TAF_CODNIV )

		TcSqlExec( cQuery )

		/*-----------------+
		| Ordem de Serviço |
		+-----------------*/
		cQuery := "UPDATE " + RetSQLName( 'STJ' ) + " "
		cQuery += "SET " 
		cQuery +=	"TJ_CODBEM = " + ValToSQL( cNewCode ) + " "
		cQuery += "WHERE "
		cQuery +=	"TJ_FILIAL  = " + ValToSQL( FWxFilial( 'STJ' ) ) + " AND "
		cQuery +=	"TJ_CODBEM  = " + ValToSQL( (cAlsTAF)->TAF_CODNIV )    + " AND "
		cQuery += 	"TJ_TIPOOS  = 'L' AND "
		cQuery += 	"D_E_L_E_T_ = ' ' "

		TcSqlExec( cQuery )

		/*---------------+
		| O.S. Histórico |
		+---------------*/
		cQuery := "UPDATE " + RetSQLName( 'STS' ) + " "
		cQuery += "SET " 
		cQuery +=	"TS_CODBEM = " + ValToSQL( cNewCode ) + " "
		cQuery += "WHERE "
		cQuery +=	"TS_FILIAL  = " + ValToSQL( FWxFilial( 'STS' ) ) + " AND "
		cQuery +=	"TS_CODBEM = " + ValToSQL( (cAlsTAF)->TAF_CODNIV )     + " AND "
		cQuery += 	"TS_TIPOOS = 'L' AND "
		cQuery += 	"D_E_L_E_T_ = ' ' "

		TcSqlExec( cQuery )

		/*-----------------------+
		| Solicitação de Serviço |
		+-----------------------*/
		cQuery := "UPDATE " + RetSQLName( 'TQB' ) + " "
		cQuery += "SET " 
		cQuery +=	"TQB_CODBEM = " + ValToSQL( cNewCode ) + " "
		cQuery += "WHERE "
		cQuery +=	"TQB_FILIAL = " + ValToSQL( FWxFilial( 'TQB' ) ) + " AND "
		cQuery +=	"TQB_CODBEM = " + ValToSQL( (cAlsTAF)->TAF_CODNIV )    + " AND "
		cQuery += 	"TQB_TIPOSS = 'L' AND "
		cQuery += 	"D_E_L_E_T_ = ' ' "

		TcSqlExec( cQuery )

		/*--------------------+
		| Restrição de Acesso |
		+--------------------*/
		cQuery := "UPDATE " + RetSQLName( 'TUB' ) + " "
		cQuery += "SET " 
		cQuery +=	"TUB_CODIGO = " + ValToSQL( cNewCode ) + " "
		cQuery += "WHERE "
		cQuery +=	"TUB_FILIAL = " + ValToSQL( FWxFilial( 'TUB' ) )    + " AND "
		cQuery +=	"TUB_CODIGO = " + ValToSQL( (cAlsTAF)->TAF_CODNIV ) + " AND "
		cQuery += 	"TUB_OPCAO = '8' AND "
		cQuery += 	"D_E_L_E_T_ = ' ' "

		TcSqlExec( cQuery )

		/*--------------------------+
		| Estrutura organizacional |
		+--------------------------*/
		cQuery := 'UPDATE '   + RetSQLName( 'TCJ' )
		cQuery += ' SET TCJ_CODNIV   = ' + ValToSQL( cNewCode )
		cQuery += ' WHERE TCJ_CODNIV = ' + ValToSQL( (cAlsTAF)->TAF_CODNIV )
		cQuery += " AND D_E_L_E_T_ = ' ' "

		TcSqlExec( cQuery )

		cQuery := 'UPDATE '   + RetSQLName( 'TCJ' )
		cQuery += ' SET TCJ_NIVSUP   = ' + ValToSQL( cNewCode )
		cQuery += ' WHERE TCJ_NIVSUP = ' + ValToSQL( (cAlsTAF)->TAF_CODNIV )
		cQuery += " AND D_E_L_E_T_ = ' ' "

		TcSqlExec( cQuery )

		/*--------------------------+
		| Atualiza campo TAF_CODNIV |
		+--------------------------*/
		cQuery := "UPDATE " + RetSQLName( 'TAF' ) + " "
		cQuery += "SET " 
		cQuery +=	"TAF_CODNIV = " + ValToSQL( cNewCode ) + ", "
		cQuery +=	"TAF_ORDEM  = " + ValToSQL( cNewCode ) + " "
		cQuery += "WHERE "
		cQuery +=	"TAF_FILIAL = " + ValToSQL( FWxFilial( 'TAF' ) ) + " AND "
		cQuery += 	"TAF_CODNIV = " + ValToSQL( (cAlsTAF)->TAF_CODNIV )

		TcSqlExec( cQuery )

		(cAlsTAF)->( dbSkip() )

	End

	/*------------------------------+
	| Atualiza campo TAF_NIVSUP 000 |
	+------------------------------*/
	cQuery := "UPDATE " + RetSQLName( 'TAF' ) + " "
	cQuery += "SET " 
	cQuery +=	"TAF_NIVSUP = '000000' "
	cQuery += "WHERE "
	cQuery +=	"TAF_FILIAL = " + ValToSQL( FWxFilial( 'TAF' ) ) + " AND "
	cQuery += 	"TAF_NIVSUP = '000' "

	TcSqlExec( cQuery )

	/*------------------------------+
	| Atualiza campo TCJ_NIVSUP 000 |
	+------------------------------*/
	cQuery := 'UPDATE ' + RetSQLName( 'TCJ' )
	cQuery += " SET TCJ_NIVSUP   = '000000'"
	cQuery += " WHERE TCJ_NIVSUP = '000' "

	TcSqlExec( cQuery )
    
Return

