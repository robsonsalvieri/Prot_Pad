/*/{Protheus.doc} MNT902REG
Realiza o carregamento indiviual dos campos do Bem em memória.
Processo necessário pois a Classe MsmGet não recarrega os campos virtuais quando
o registro exibido é mudado.

@type Function

@author João Ricardo Santini Zandoná
@since 22/05/2024

@return Nil  
/*/ 
//------------------------------------------------------------------------------
Function MNT902REG()

	Local aHeadCamp := NGHeader( 'ST9' )
	Local nInd      := 1
	Local nTamTot   := Len( aHeadCamp )
	Local cCampo    := ''
	Local cContext  := ''
	Local cTipo     := ''
	Local cCmpo     := ''
	Local cCmpo2    := ''
	Local nTamanho  := 0
	Local xConteudo

	For nInd := 1 To nTamTot

		cCampo      := aHeadCamp[ nInd, 2 ]
		cContext 	:= aHeadCamp[ nInd, 10]
		nTamanho 	:= aHeadCamp[ nInd, 4 ]
		cTipo 		:= aHeadCamp[ nInd, 8 ]

		If cContext == "V"

			cCmpo     := "M->" + Trim( cCampo )
			xConteudo := &cCmpo.
		
			If ValType( xConteudo ) != "U"
				
				&( "M->" + Trim( cCampo ) ) := CriaVar( Trim( cCampo ), .T. )
		
			EndIf
		
		Else
		
			cCmpo  := ( "ST9->" + Trim( cCampo ) )
			cCmpo2 := "M->" + Trim( cCampo )
			
			If ValType( &cCmpo2 ) != "U"
		
				&( "M->" + Trim( cCampo ) ) := &cCmpo
			
			EndIf
		
		EndIf

	Next nInd

Return

