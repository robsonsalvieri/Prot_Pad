    cQuery += 'INNER JOIN ' + RetSqlName('ST4') + ' ST4 ON '
    cQuery += NGMODCOMP( 'ST4', 'STJ', '=' ) + ' '
    cQuery +=   "AND ST4.T4_SERVICO = STJ.TJ_SERVICO "
    cQuery +=   "AND ST4.D_E_L_E_T_ = ' ' "
    cQuery += 'INNER JOIN ' + RetSqlName('STD') + ' STD ON '
    cQuery += NGMODCOMP( 'STD', 'ST4', '=' ) + ' '
    cQuery +=   "AND STD.TD_CODAREA = ST4.T4_CODAREA "
    cQuery +=   "AND STD.D_E_L_E_T_ = ' ' "
    cQuery += 'INNER JOIN ' + RetSqlName('STE') + ' STE ON '
    cQuery += NGMODCOMP( 'STE', 'STJ', '=' ) + ' '
    cQuery +=   "AND STE.TE_TIPOMAN = STJ.TJ_TIPO "
    cQuery +=   "AND STE.D_E_L_E_T_ = ' ' "
    cQuery += 'LEFT JOIN ' + RetSqlName('ST9') + ' ST9 ON '
    cQuery += NGMODCOMP( 'ST9', 'STJ', '=' ) + ' '
    cQuery +=   "AND ST9.T9_CODBEM = STJ.TJ_CODBEM "
    cQuery +=   "AND ST9.D_E_L_E_T_ = ' ' "
    cQuery += 'LEFT JOIN ' + RetSqlName('CTT') + ' CTT ON '
    cQuery += NGMODCOMP( 'CTT', 'STJ', '=' ) + ' '
    cQuery +=   "AND CTT.CTT_CUSTO = STJ.TJ_CCUSTO "
    cQuery +=   "AND CTT.D_E_L_E_T_ = ' ' "
    cQuery += 'LEFT JOIN ' + RetSqlName('SHB') + ' SHB ON '
    cQuery += NGMODCOMP( 'SHB', 'STJ', '=' ) + ' '
    cQuery +=   "AND SHB.HB_COD = STJ.TJ_CENTRAB "
    cQuery +=   "AND SHB.D_E_L_E_T_ = ' ' "
    cQuery += 'LEFT JOIN ' + RetSqlName('STF') + ' STF ON '
    cQuery += NGMODCOMP( 'STF', 'STJ', '=' ) + ' '
    cQuery +=   "AND STF.TF_CODBEM = STJ.TJ_CODBEM "
    cQuery +=   "AND STF.TF_SERVICO = STJ.TJ_SERVICO "
    cQuery +=   "AND STF.TF_SEQRELA = STJ.TJ_SEQRELA "
    cQuery +=   "AND ST4.D_E_L_E_T_ = ' ' "
    cQuery += 'LEFT JOIN ' + RetSqlName('TQB') + ' TQB ON '
    cQuery += NGMODCOMP( 'TQB', 'STJ', '=' ) + ' '
    cQuery +=   "AND TQB.TQB_SOLICI = STJ.TJ_SOLICI "
    cQuery +=   "AND TQB.D_E_L_E_T_ = ' ' "
    cQuery += 'LEFT JOIN ' + RetSqlName('STI') + ' STI ON '
    cQuery += NGMODCOMP( 'STI', 'STJ', '=' ) + ' '
    cQuery +=   "AND STI.TI_PLANO = STJ.TJ_PLANO "
