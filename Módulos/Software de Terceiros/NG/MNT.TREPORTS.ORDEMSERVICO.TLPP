    cQuery += "END AS TF_NOMEMAN, "     
    cQuery += "CASE WHEN STJ.TJ_TIPOOS = 'B' THEN ' ' "
    cQuery += 'ELSE TAF.TAF_NOMNIV '
    cQuery += 'END AS TAF_NOMNIV '
