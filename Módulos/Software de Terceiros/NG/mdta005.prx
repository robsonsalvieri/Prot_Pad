/*/
//-------------------------------------------------------------------
Function MDT005F3()

	Local cAlias5 := Alias()
	Local cSvOrd5 := IndexOrd()
	Local OldbNGGRAVA
	Local lRet := .F.

	Private aDadIncHis := {}

	If Type( "bNGGRAVA" ) == "B"
