/*/{Protheus.doc} MDTRegBio
Registra biometria do funcionário

@author Bruno Souza
@since	02/06/2022

@param cMedRec, caracter, código da ficha médica 
@param cTable, caracter, tabela posicionada no momento 

/*/
//---------------------------------------------------------------------
Function MDTRegBio()

	Local cTable 	:= 'TM0'
	Local cMedRec	:= IIf( FwIsInCallStack( 'MDTA410' ), M->TM0_NUMFIC, cMedRec := TM0->TM0_NUMFIC )

	If ( SuperGetMv( 'MV_NG2BIOM', .F., '2' ) != '1' .Or. IIf( FwIsInCallStack( 'MDTA410' ), M->TM0_INDBIO, TM0->TM0_INDBIO ) != '1' )

			//-----------------------------------------------------------------------------------------------------------------------
            // Mensagens:
            // "Atenção"
            // "O funcionário não utiliza biometria"
            // "Configure o parâmetro MV_NG2BIOM ou altere o campo Biometria (TM0_INDBIO) para 1-Sim na ficha médica do funcionário"
            //-----------------------------------------------------------------------------------------------------------------------
            Help( Nil, Nil, STR0068, Nil, STR0104 + '.', 1, 0, Nil, Nil, Nil, Nil, Nil, { STR0105 + '.' } )

            Return .F.

	EndIf

	If SuperGetMV('MV_BIOMDT', .F., '1') == '1'
		//registro de biometria utilizando totvsBio (legado)
		MdtGetBio(cMedRec, cTable)
	Else
		If Empty(Posicione( cTable, 1, xFilial( cTable ) + cMedRec, 'TM0_REGBIO' )) .Or.;
			MsgYesNo( STR0101 ) //"Este colaborador já possui digitais cadastradas, deseja refazer o cadastro?"
		
			//registro de biometria utilizando fingerTech web
			MdtBio(cMedRec)
		EndIf
	EndIf

Return
