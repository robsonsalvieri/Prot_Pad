Function MNT610ATIV()

	If M->TQF_ATIVO == "2"
		lATIVO  := .T.
		lREATIV := .F.
	Else
		lATIVO  := .F.
		lREATIV := .T.
	EndIf

	If Altera
		If lREATIV
			If !MsgYesNo(STR0008,STR0010) // //"Deseja reativar o posto ?"###"ATENCAO" //"ATENÇÃO"
				M->TQF_ATIVO := "2"
				lATIVO       := .T.
				lREATIV      := .F.
			EndIf
		EndIf
	EndIf

Return .T.
