*/
Function MNT610FN()

	cTEL := AllTrim(SA2->A2_DDD+SA2->A2_TEL)

	n := AT("-",cTEL)
	If n > 0
		cTEL := SUBSTR(cTEL,1,n-1)+SUBSTR(cTEL,n+1,Len(cTel))
	EndIf

	nTAM := LEN(ALLTRIM(cTEL))
	If SubStr(cTEL,1,1) == "0"
		cTEL := SubStr(cTEL,2,nTAM)
	EndIf

	If nTAM <= 8
		cTEL := "xx"+cTEL
	EndIf

Return cTEL

