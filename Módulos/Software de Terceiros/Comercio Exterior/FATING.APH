  <div class="footer">
    <!-- Content -->
    <div class="report-info" style="flex-direction: column;">
      <table class="footer-table">
        <!-- Title -->
        <tr class="border-bottom">
          <td class="justify-center" colspan="2">
            <span class="sub-title bold">
              PACKING
            </span>
          </td>
          <td class="justify-center border-left-right">
            <span class="sub-title bold">
              MARKS
            </span>
          </td>
          <td class="justify-center" colspan="2">
            <span class="sub-title bold">
              TOTALS
            </span>
          </td>
        </tr>
        <!-- Line 1 -->
        <tr>
          <!-- EMPAQUE -->
          <td>
            <span class="value">
              Net Weight <%=HEADER_P->AVG_C02_10%>:
            </span>
          </td>
          <td class="justify-right">
            <span class="value">
              <%=HEADER_P->AVG_C03_20%>
            </span>
          </td>
          <!-- MARCAS -->
          <td class="border-left-right">
            <span>
              <%=HEADER_P->AVG_C06_20%>
            </span>
          </td>
          <!-- TOTALES -->
          <td>
            <span class="value">
              Subtotal
            </span>
          </td>
