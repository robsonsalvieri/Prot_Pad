<%
local nPosRecno  := 0
local cDescItem  := ""
local nRecImp    := 0
local nRecPos    := 0
local lTotal     := .F.
local lImpPesBru := .F.
local cNCM       := ""
local lFinal     := .F.
local cFileType  := "bmp"
local aFile      := {}
%>

<!DOCTYPE html>
<html>
<head>
  <title>
    Commercial Invoice
  </title>
  <style>
    * {
      font-size: 0.95em;
    }

    body {
      font-family: sans-serif;
      width: 100%;
    }

    /* Header and Footer Styles */
    .header, .footer {
      display: flex;
      width: calc(98% - 6px);
      flex-direction: column;
      padding-left: 3px;
    }
  
    .header-logo {
      padding: 5px;
      display: flex;
      justify-content: center;
      align-items: center;
    }

    .content {
      display: flex;
      width: 100%;
    }

    .content-left, .content-right {
      display: flex;
      flex-direction: column;
      width: 50%;
    }

    .logo {
      max-width: 50%;
      max-height: 95%;
    }

    .no-visible {
      display: none;
    }

    /* Main Body Styles */
    .main {
      display: flex;
      flex-direction: column;
      gap: 10px;
      width: 98%;
      height: 100%;
    }

    .report-info {
      display: flex;
      width: 100%;
    }

    .last-content {
      width: 100%;
    }

    /* Table Styles */
    .main-table {
      width: 100%;
    }

    .header-table, .footer-table {
      width: 100%;
      border: 1px solid black;
      padding: 3px;
      border-collapse: collapse;
    }

    .header-table td:nth-child(1), 
    .header-table td:nth-child(3) {
      width: 12%;
    }

    .header-table td:nth-child(2) {
      width: 38%;
      border-right: 1px solid black;
    }

    .header-table td:nth-child(4) {
      width: 38%;
    }

    .footer-table td {
      width: 20%;
    }

    .products-table, .last-table {
      border: 1px solid black;
      width: 100%;
      border-collapse: collapse;
    }

    .products-table td {
      padding: 3px;
    }

    .products-table td:nth-child(1),
    .products-table td:nth-child(2), 
    .products-table td:nth-child(4), 
    .products-table td:nth-child(5), 
    .products-table td:nth-child(6), 
    .products-table td:nth-child(7) {
      width: 10%;
      border-right: 1px solid black;
    }

    .products-table td:nth-child(3) {
      width: 30%;
      border-right: 1px solid black;
    }

    .products-table td:nth-child(8) {
      width: 10%;
    }

    .last-table {
      word-break: break-all;
    }

    .last-table td:nth-child(1),
    .last-table td:nth-child(3) {
      width: 10%;
    }

    .last-table td:nth-child(2) {
      width: 40%;
      border-right: 1px solid black;
    }
    
    .last-table td:nth-child(4) {
      width: 40%;
    }
    /* Specific style for the "merged" cells */
    .last-table td[colspan="2"] {
      width: 50%;
      border-right: 1px solid black;
    }

    .last-table td[colspan="2"] + td {
      width: 10%;
      border-right: 0px;
    }

    .last-table td[colspan="2"] + td + td{
      width: 40%;
    }

    /* Border Styles */
    .no-border-top {
      border-top: 0px;
    }

    .border-top {
      border-top: 1px solid black;
    }

    .border-bottom {
      border-bottom: 1px solid black;
    }

    .border-right {
      border-right: 1px solid black;
    }

    .border-left-right {
      border-left: 1px solid black;
      border-right: 1px solid black;
    }

    /* Span Styles */
   .main-title {
      font-weight: bold;
      font-size: 12px;
    }

    .sub-title {
      font-size: 9px;
    }

    .value, .product-value {
      font-weight: normal;
      font-size: 8px;
    }

    .bold {
      font-weight: bold;
    }

    .justify-center {
      text-align: center;
    }

    .justify-right {
      text-align: right;
    }

    /* Print Styles */
    @media print {
      thead {
        display: table-header-group;
      }

      tfoot {
        display: table-footer-group;
      }

      .header {
        position: fixed;
        top: 0;
      }

      .header, .header-placeholder {
        height: 210px;
      }

      .last-table, .last-table tr, .products-table tr {
        page-break-inside: avoid;
      }

      .footer {
        bottom: 0;
        position: fixed;
      }
      
      .footer, .footer-placeholder {
	    height: 220px;
      }
    }
  </style>
</head>

<body>
  <div class="header">
    <div class="content">
      <div class="content-left">
        <div class="header-logo">
            <%
              cLogoBase64 := ""
              cFile := comex.generics.LoadAphLogo()
              If !Empty(cFile)
                cLogoBase64 := encode64(,cFile,.F.,.F.)
                // Caso o arquivo seja de um ponto de entrada, ele não será apagado, diferente do comportamento padrão onde é copiado a logo para a pasta temporária
                IIF(GetTempPath() $ cFile, FErase(cFile),)
                aFile := EasyStrSplit(cFile, ".")
                cFileType := aFile[Len(aFile)]
              Else
                cFile := GetPvProfString(GetEnvServer(),"StartPath","",GetSrvIniName())+"LGRL"+cEmpAnt+".BMP"
                If File(cFile)
                  cLogoBase64 := encode64(,cFile,.F.,.F.)
                EndIf
              EndIf
            %>
            <img <%= iif(empty(cLogoBase64),'class="logo no-visible"','class="logo"') %> src=<%= '"data:image/' + cFileType + ';base64,'+cLogoBase64+'"' %> alt="">
        </div>
      </div>
      <div class="content-right">
        <span class="main-title">
          <%= HEADER_P->AVG_C01_60%>
        </span>
        <span class="sub-title">
