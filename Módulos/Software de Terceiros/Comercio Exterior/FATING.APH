                  <td>
                    <span class="value">
                      <%=HEADER_P->AVG_C03150%>
                    </span>
                  </td>
                </tr>
                <!--  Obs Line 4  -->
                <tr>
                  <td colspan="2">
                  </td>
                  <td>
                  </td>
                  <td>
                    <span class="value">
