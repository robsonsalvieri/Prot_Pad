*/
STATIC FUNCTION MONTAGANT( LEND )
/*
LOCAL ASIZE     := {}
LOCAL AOBJECTS  := {}
LOCAL AINFO     := {}
LOCAL NTSK
LOCAL DINI      := CTOD("")		//DDATABASE
LOCAL NBOTTOM   := -60
LOCAL NRIGHT     := -10
LOCAL ABUTTONS  := {}
