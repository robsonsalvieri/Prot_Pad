								EndIF

								// DSERLOCA-2293
								If STRZERO(_NOK,LEN(ACOLS0[ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FPP_ITEM" })])) == "001"

									// Verificar se já foi registrado um valor de seguro
									If select("TRBMED") > 0
										TRBMED->(dbCloseArea())
									EndIf
									cQuery := " SELECT SUM(FPP.FPP_VALSEG) SEG "
									cQuery += " FROM " + RETSQLNAME("FPP") + " FPP "
