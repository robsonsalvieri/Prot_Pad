Function Loca048c()
Local lRet := .T.
Local cTurno := &(ReadVar())
Local aArea := GetArea()

if !Empty(cTurno)
	dbSelectArea("FPA")
	dbSetOrder(3)
	if dbSeek(xFilial("FPA")+cAS)
		dbSelectArea("FPE")
		dbSetOrder(1)
		if !dbSeek(xFilial("FPE")+FPA->(FPA_PROJET + FPA_OBRA + FPA_GRUA + FPA_SEQGRU) + cTurno)
			Help(Nil,	Nil,alltrim(upper(Procname()))+"B",; //"RENTAL: "
					Nil,"Turno não encontrado na tabela de turnos para a AS informada.",1,0,Nil,Nil,Nil,Nil,Nil,; //"Inconsistência nos dados."
					{"Verificar a tabela de turnos ou a AS informada."}) //"A hora inicial deve ser diferente da hora final."
			lRet := .F.
		endif
	endif
	if lRet
		// Setar o valor do turno
		GDFIELDPUT("FPP_VALTUR", FPE->FPE_VROPER,OGETZLM:nAt, OGETZLM:aHeader, OGETZLM:aCols)
		GDFIELDPUT("FPP_VALHOR", FPE->FPE_VROPER,OGETZLM:nAt, OGETZLM:aHeader, OGETZLM:aCols)
		M->FPP_VALHOR :=  FPE->FPE_VROPER
	endif

else
	// Zerar o valor do turno
	GDFIELDPUT("FPP_VALTUR", 0,OGETZLM:nAt, OGETZLM:aHeader, OGETZLM:aCols)
	if FPA->FPA_TPBASE <> "H"
		GDFIELDPUT("FPP_VALHOR", FPA->FPA_VRHOR, OGETZLM:nAt, OGETZLM:aHeader, OGETZLM:aCols)
		M->FPP_VALHOR := FPA->FPA_VRHOR

	Else
		GDFIELDPUT("FPP_VALHOR", FPA->FPA_PRCUNI, OGETZLM:nAt, OGETZLM:aHeader, OGETZLM:aCols)
		M->FPP_VALHOR := FPA->FPA_PRCUNI
	EndIF

endif

if lRet // Executa as funções de calculo dos gatilhos
	LOCA04808('FPP_VALHOR')
	LOCA04808('FPP_PERMO')
endif

RestArea(aArea)

Return lRet


