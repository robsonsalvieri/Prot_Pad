									cQuery += " FPN.FPN_AS = ? AND FPN.D_E_L_E_T_ = '' AND FPN.FPN_SITUAC <> '3' AND FPN.FPN_SITUAC <> '6' "
									cQuery += " WHERE FPP.D_E_L_E_T_ = '' AND FPP.FPP_COD = FPN.FPN_COD "
