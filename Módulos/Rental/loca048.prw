	cQuery := " SELECT COUNT(FPF_FROTA) AS TOT FROM "+RETSQLNAME("FPF")+" "
	cQuery += " WHERE 	FPF_FILIAL = ? AND "
	cQuery += " FPF_DATA BETWEEN ? AND ? AND "
	cQuery += " FPF_HORAI <= ? AND FPF_HORAF >= ? AND "
	cQuery += " FPF_FROTA = ? "
	//desconsidera AS origem, se informado
	If !Empty(cAS)
		cQuery += " AND FPF_AS <> ? "
	EndIF
	cQuery += " AND D_E_L_E_T_ = ' ' "

	CQUERY := CHANGEQUERY(CQUERY) 
	If !Empty(cAS)
		aBindParam := {FwxFilial("FPF"),cDtIni,cDtFim,cHoraFim,cHoraIni,cBem,cAs}
	else
		aBindParam := {FwxFilial("FPF"),cDtIni,cDtFim,cHoraFim,cHoraIni,cBem}
	EndIF
	cAliasQry := MPSysOpenQuery(cQuery,,,,aBindParam)
	//DbUseArea(.T. , "TOPCONN" , TCGENQRY(,,cQuery) , cAliasQry , .F. , .T.) 

	//caso ache retorna verdadeiro
	If !(cAliasQry)->(Eof())
		If (cAliasQry)->TOT > 0
			lRet := .F.
		EndIF
	EndIf

	(cAliasQry)->(DbCloseArea())

	ST9->(dbSetOrder(1))
	If ST9->(dbSeek(xFilial("ST9")+cBem))
		IF !empty(ST9->T9_STATUS)
			lRet := .T.
		EndIF
	EndIf
	If empty(cBem)
		lRet := .T.
	EndIF

Return lRet

// Rotina para identificar se um pedido de vendas faz parte do faturamento de uma medicao
// Frank Z Fuga em 11/11/2022
Function LOCA04820(cPedido)
Local lMedicao  := .F.
Local cQuery    := ""
Default cPedido := ""
	If !empty(cPedido)
		/*
		+FwxFilial("FPN")+
		+cPedido+
		*/
		cQuery := " SELECT FPN_NUMPV FROM "+RETSQLNAME("FPN")+" "
		cQuery += " WHERE FPN_FILIAL = ? AND "
		cQuery += " FPN_NUMPV = ? "
		cQuery += " AND D_E_L_E_T_ = ' ' "
		cQuery := changequery(cQuery)
		If select("TRBFPN") > 0			
			TRBFPN->(DBCLOSEAREA())
		EndIf
		aBindParam := {FwxFilial("FPN"),cPedido}
		MPSysOpenQuery(cQuery,"TRBFPN",,,aBindParam)
		//TCQUERY cQuery NEW ALIAS "TRBFPN"
		If !TRBFPN->(Eof())
			lMedicao := .T.
		EndIf
		TRBFPN->(DBCLOSEAREA())
	EndIF
Return lMedicao

