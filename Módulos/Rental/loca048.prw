// Rotina para validar os campos de seguro na FPA.
// Frank Zwarg Fuga em 31/01/2024
// DSERLOCA-2293
Function LOCA048A(cCampo)
Local lRet := .T.
Local nVar := 0
//Local cTipo := ""
	If cCampo == "FPA_PERSEG"
		If ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_VRCARG"})] == 0
			lRet := .F.
		EndIF
	ElseIf cCampo == "FPA_VRSEGU"
		If ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_PERSEG"})] > 0
			lRet := .F.
		EndIF
	ElseIf cCampo == "VLRBASE"
		nVar := &(Readvar())
		ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_VRSEGU"})] := (nVar * ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_PERSEG"})]) / 100
	ElseIf cCampo == "PERCENT"	
		nVar := &(Readvar())
		ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_VRSEGU"})] := (ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_VRCARG"})] * nVar ) / 100
	EndIF
Return lRet


