							_DDTFIM := _DDTINI + NDIASTRB - 1
						ELSE
							IF _DDTINI + NDIASTRB - 1 < FPA->FPA_DTSCRT
								_DDTFIM := _DDTINI + NDIASTRB - 1
							ELSE
								_DDTFIM := FPA->FPA_DTSCRT
							ENDIF
						ENDIF
					ENDIF
				ENDIF
		


				_CPERLOC := DTOC(_DDTINI) + " A " + DTOC(_DDTFIM)
				AITENS := {}
				AADD(AITENS,{"C6_FILIAL", CFILFAT, NIL}) 
				//AADD(AITENS,{"C6_NUM", CNUMSC5, NIL})
				AADD(AITENS,{"C6_ITEM", STRZERO(NX,2), NIL})
				AADD(AITENS,{"C6_PRODUTO", AITENSMED[NX][1], NIL})
				AADD(AITENS,{"C6_UM", "UN", NIL})
				AADD(AITENS,{"C6_QTDVEN", 1, NIL})
				AADD(AITENS,{"C6_PRCVEN", AITENSMED[NX][2], NIL})
				AADD(AITENS,{"C6_VALOR", AITENSMED[NX][2], NIL})
				AADD(AITENS,{"C6_PRUNIT", AITENSMED[NX][2], NIL})
				AADD(AITENS,{"C6_DESCONT", 0, NIL})
				AADD(AITENS,{"C6_TES", CTES, NIL})
				If SC6->( FIELDPOS("C6_FROTA") ) > 0
					AADD(AITENS,{"C6_FROTA", FPA->FPA_GRUA, NIL})
				EndIF
				AADD(AITENS,{"C6_LOCAL", POSICIONE("SB1",1,XFILIAL("SB1")+AITENSMED[NX][1],"B1_LOCPAD"), NIL})
				AADD(AITENS,{"C6_ENTREG", DDATABASE, NIL})
				AADD(AITENS,{"C6_QTDLIB", 1, NIL})

				If !lMvLocBac
					AADD(AITENS,{"C6_XPERLOC", _CPERLOC, NIL})
					AADD(AITENS,{"C6_XAS", FPA->FPA_AS, NIL})
				else

					aFPY := {}
					Aadd(aFPY, {"FPY_FILIAL"	, xFilial("SC5")	,	NIL })
					Aadd(aFPY, {"FPY_PEDVEN"	, SC5->C5_NUM		,	NIL })
					Aadd(aFPY, {"FPY_PROJET"	, FPA->FPA_PROJET	,	NIL })
					Aadd(aFPY, {"FPY_TIPFAT"	, "M"				,	NIL })
					Aadd(aFPY, {"FPY_STATUS"	, "1"				,	NIL }) //1=Pedido Ativo;2=Pedido Cancelado
					aFPYTransf := aFPY

					aItemFPZ := {}
					// no caso de alterar a estrutura do array precisa alterar também o locm006
					Aadd(aItemFPZ, {"FPZ_FILIAL", xFilial("SC6"), NIL })
					xTemp := ""
					For nY:=1 to len(aItens)
						If alltrim(aItens[nY][1]) == "C6_NUM"
							xTemp := aItens[nY][2]
						EndIF
					Next
					Aadd(aItemFPZ, {"FPZ_PEDVEN", xTemp, NIL })
					Aadd(aItemFPZ, {"FPZ_PROJET", FPA->FPA_PROJET, NIL }) 
					xTemp := ""
					For nY:=1 to len(aItens)
						If alltrim(aItens[nY][1]) == "C6_ITEM"
							xTemp := aItens[nY][2]
						EndIF
					Next
					nPonteiro := xTemp
					Aadd(aItemFPZ, {"FPZ_ITEM", xTemp, NIL }) 
					Aadd(aItemFPZ, {"FPZ_AS", FPA->FPA_AS, NIL })
					Aadd(aItemFPZ, {"FPZ_EXTRA", "N", NIL })
					Aadd(aItemFPZ, {"FPZ_FROTA", FPA->FPA_GRUA, NIL })
					Aadd(aItemFPZ, {"FPZ_CCUSTO",  FPA->FPA_CUSTO, NIL })
					Aadd(aItemFPZ, {"NPONTEIRO", nPonteiro, "999" })

					Aadd(aFPZ,Aclone(aItemFPZ))
					aItemFPZ := {}
				EndIf

				If _MV_LOC051 //getmv("MV_LOCX051",,.F.)
					IF SC6->( FIELDPOS("C6_CLVL") ) > 0
						AADD(AITENS,{"C6_CLVL"			, FPN->FPN_AS, NIL})
					ENDIF
				EndIF
				IF SC6->( FIELDPOS("C6_XBEM") ) > 0
					AADD(AITENS,{"C6_XBEM"          , FPA->FPA_GRUA, NIL})
				ENDIF

				IF SC6->(FIELDPOS("C6_CC")) > 0
					AADD(AITENS, {"C6_CC"      , FPN->FPN_CC        , NIL})
				ENDIF

				AADD(ACAMPOSSC6,AITENS)	
			NEXT NX                                         
			
			NY := LEN(AITENSMED) 

			BEGIN TRANSACTION 
			
				IF LEN(ACAMPOSSC5) > 0 .AND. LEN(AITENS) > 0

					IF LTEMITMED 
						IF EMPTY(CTES) 
							MSGALERT(STR0098 , STR0020) //"PARAMETRO 'MV_LOCX186' PARA TES - MEDICAO, NÃO CONFIGURADO. INFORME O CÓDIGO DA TES PARA MEDIÇÃO NESTE PARÂMETRO !"###"GPO - LOCT035.PRW"
							LPVTUDOOK := .F. 
						ELSE
							AAREAATU := GETAREA() 
							AAREASF4 := SF4->(GETAREA()) 
							DBSELECTAREA("SF4") 
							DBSEEK(XFILIAL("SF4")+CTES) 
							IF SF4->(EOF()) 
								MSGALERT(STR0099+ALLTRIM(CTES)+STR0100 , STR0020) // "A TES ["###"] CONFIGURADA NO PARÂMETRO 'MV_LOCX186' NÃO EXISTE ! ALTERE A TES NO REFERIDO PARÂMETRO .OU. REALIZE A INCLUSÃO DA TES."###"GPO - LOCT035.PRW"
								LPVTUDOOK := .F. 
							ENDIF 
							RESTAREA(AAREASF4) 
							RESTAREA(AAREAATU) 
						ENDIF 
					ENDIF 

					IF LTEMITME2 
						IF EMPTY(CTESEXTRA) 
							MSGALERT(STR0101 , STR0020) //"PARAMETRO 'MV_LOCX079' PARA TES - MEDICAO (EXTRA), NÃO CONFIGURADO. INFORME O CÓDIGO DA TES PARA MEDIÇÃO (EXTRA) NESTE PARÂMETRO !"###"GPO - LOCT035.PRW"
							LPVTUDOOK := .F. 
						ELSE 
							AAREAATU := GETAREA() 
							AAREASF4 := SF4->(GETAREA()) 
							DBSELECTAREA("SF4") 
							DBSEEK(XFILIAL("SF4")+CTESEXTRA) 
							IF SF4->(EOF()) 
								MSGALERT(STR0102+ALLTRIM(CTESEXTRA)+STR0103 , STR0020) //"A TES ["###"] CONFIGURADA NO PARÂMETRO 'MV_LOCX079' NÃO EXISTE ! ALTERE A TES NO REFERIDO PARÂMETRO .OU. REALIZE A INCLUSÃO DA TES."###"GPO - LOCT035.PRW"
								LPVTUDOOK := .F. 
							ENDIF 
							RESTAREA(AAREASF4) 
							RESTAREA(AAREAATU) 
						ENDIF 
					ENDIF 

					IF LPVTUDOOK				

						//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
						//³ GERA PEDIDO DE VENDA DA TRANSFERENCIA.          ³
						//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
						If lMvLocBac
							aFPZTransf := aFPZ
						EndIF
						FWMSGRUN(,{|| MSEXECAUTO({|X,Y,Z| MATA410(X,Y,Z)},ACAMPOSSC5,ACAMPOSSC6,3)},,STR0104 ) //"AGUARDE...GERANDO PEDIDO DE VENDA MEDIÇÃO NR. "
					
						IF LMSERROAUTO
							// --> MOSTRA O ERRO GERADO NA PASTA ERROS
							MOSTRAERRO()
							ROLLBACKSX8()
						ELSE
							If FP0->(FieldPos("FP0_PROV")) > 0
								FP0->(DbSetOrder(1))
								If FP0->(DbSeek(FPA->FPA_FILIAL+FPA->FPA_PROJET))
									If FP0->FP0_PROV == "1" .or. FP0->FP0_PROV == "2"			
										_MV_LOC278 := .T.
									Else
										_MV_LOC278 := .F.
									EndIf
								EndIf
							EndIf

							If _MV_LOC278 // Excluir provisórios
								RecLock("FPA",.f.)
								FPA->FPA_ULTFAT	:= FPN->FPN_DTFIM
								FPA->(MsUnLock())
								DBSELECTAREA("SC6")
								SC6->(DBSETORDER(1))
								SC6->(MSSEEK(SC5->C5_FILIAL+SC5->C5_NUM))
								WHILE !EOF() .AND. SC6->C6_FILIAL == SC5->C5_FILIAL .AND. SC6->C6_NUM == SC5->C5_NUM
									DELTITPR()
									SC6->(DBSKIP())				
								ENDDO 			
								RecLock("FPA",.f.)
								FPA->FPA_ULTFAT	:= Ctod("")
								FPA->(MsUnLock())
							EndIf

							CONFIRMSX8()
							
							CBODY := REPLACE(CBODY , STR0105  , SC5->C5_NUM) //"%PEDIDO%"
							CBODY := REPLACE(CBODY , STR0106 , FPN->FPN_COD) //"%MEDICAO%"
							CBODY := REPLACE(CBODY , STR0107  , ALLTRIM(CAPITAL(FWFILIALNAME(CEMPANT,XFILIAL("FPN"))))) //"%FILIAL%"
							CBODY := REPLACE(CBODY , STR0108 , DTOC(DDATABASE+NQTDVALI)) //"%DTVALID%"
							
							IF !EMPTY(_CDEST)
								LOCA05909(_CREMET, _CDEST, _CCC, _CASSUNTO, CBODY, "", _CCCO,.F.)
							ENDIF 
							
							/*
							+xFilial("FPN")+
							+FPN->FPN_COD+
							*/
							cQuery := "SELECT R_E_C_N_O_ REC "
							cQuery += "FROM "+RetSqlName("FPN")+" "
							cQuery += "WHERE D_E_L_E_T_ = '' AND "
							cQuery += "FPN_FILIAL = ? AND "
							cQuery += "FPN_COD = ? "
							CQUERY := CHANGEQUERY(CQUERY) 
							aBindParam := {xFilial("FPN"),FPN->FPN_COD}
							CALIAS := MPSysOpenQuery(cQuery,,,,aBindParam)
							
							DBSELECTAREA(CALIAS)
							
							WHILE (CALIAS)->(!EOF())
								FPN->(DBGOTO((CALIAS)->REC))
								
								RECLOCK("FPN",.F.)
									FPN->FPN_FILPV	:= CFILANT
									FPN->FPN_NUMPV := SC5->C5_NUM
									FPN->FPN_SITUAC:= "4"
								MSUNLOCK()

								// Troca dos status da minuta para encerrada - Frank em 05/04/23
								FPP->(dbSetOrder(1))
								FPP->(dbSeek(xFilial("FPP")+FPN->FPN_COD))
								While !FPP->(Eof()) .and. FPP->FPP_FILIAL == xFilial("FPP") .and. FPP->FPP_COD == FPN->FPN_COD
									If !empty(FPP->FPP_MINUTA)
										FPF->(dbSetOrder(2))
										If FPF->(dbSeek(xFilial("FPF")+FPP->FPP_MINUTA))
											FPF->(RecLock("FPF"),.F.)
											FPF->FPF_STATUS := "4"
											FPF->(MsUnlock())
										EndIf
									EndIF
									FPP->(dbSkip())
								EndDo
								
								DBSELECTAREA("SC6")
								SC6->(DBSETORDER(1))
								IF SC6->(DBSEEK(XFILIAL("SC6") + SC5->C5_NUM))
									WHILE SC6->(!EOF()) .AND. XFILIAL("SC6") + SC5->C5_NUM == SC6->C6_FILIAL + SC6->C6_NUM
										IF _MV_LOC243 //SUPERGETMV("MV_LOCX243",.F.,.F.)
											LOCA062(CFILANT,SC5->C5_NUM,SC6->C6_ITEM,FPN->FPN_FILIAL,FPN->FPN_PROJET,FPN->FPN_AS)
										ENDIF
										SC6->(DBSKIP())
									ENDDO
								ENDIF
								(CALIAS)->(DBSKIP())
							ENDDO 
							
							(CALIAS)->(DBCLOSEAREA())
							
							RESTAREA(AAREA)
			
							DBSELECTAREA("FQ5")
							RECLOCK("FQ5",.F.)
							FQ5->FQ5_ZLFTIP := "3"
							FQ5->FQ5_NUMPV  := SC5->C5_NUM
							MSUNLOCK()

							MSGINFO(STR0109+SC5->C5_NUM+STR0110 , STR0020) //"PEDIDO DE VENDA DE NR. "###" FOI GERADO COM SUCESSO !"###"GPO - LOCT035.PRW"
						ENDIF 					// IF LMSERROAUTO 

					ENDIF						// IF LTEMITMED  

				ENDIF 							// IF LEN(ACAMPOSSC5) > 0 .AND. LEN(AITENS) > 0

			END TRANSACTION

		ENDIF
	ELSE
		MSGALERT(STR0111 , STR0020) //"NAO EXISTE MEDICAO PARA ESTE ITEM. CADASTRE A MEDICAO!!!"###"GPO - LOCT035.PRW"
	ENDIF

RETURN

