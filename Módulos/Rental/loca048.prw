									TRBMED->(dbCloseArea())

									If nSeg == 0 .and. cSequencia == "001"
										ACOLS0[ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FPP_PERSEG"})] := FPA->FPA_PERSEG
