										ACOLS0[ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FPP_TIPO"  })] := FPA->FPA_TPSEGU
										ACOLS0[ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FPP_VALSEG"})] := FPA->FPA_VRSEGU
									EndIF

									// Verificar se já foi registrado um valor de mobilização e desmobilização
									//If select("TRBMED") > 0
									//	TRBMED->(dbCloseArea())
									//EndIf
									cQuery := " SELECT SUM(FPP.FPP_VLRMOB) MOB, SUM(FPP.FPP_VLRDES) DES "
