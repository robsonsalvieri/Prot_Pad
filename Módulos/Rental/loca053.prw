		AADD(APARAMBOX,{1,STR0099        , SPACE(27) , "@!"              , "LOCA05308(MV_PAR06,.T.,If(AllTrim(MV_PAR05)  $ 'OBRA|INTEGR', .T., .F.),If(!(AllTrim(MV_PAR05) $ 'OBRA|INTEGR'), .T., .F.))" ,"FQ51" ,""  ,80, .F.}) // TIPO CARACTERE "Nº AS"
		
	ENDIF    

	IF PARAMBOX(APARAMBOX,STR0104,@ARET,,,,,,,,.F.)       			//"PARÂMETROS"
		IF NTIPO == 2
			IF VALTYPE(ARET[6]) == "C"
				ARET[6] := VAL(ARET[6])
			ENDIF
		ENDIF 
	ENDIF

