/*/{Protheus.doc} LOCA05313
@description	Retorna novo sequencial para Controle da FPQ
@author			José Eulálio
@since     		13/10/2022
/*/			
//-------------------------------------------------------------------
FUNCTION LOCA05313(cFilFpq,cFilMat,cMatFpq,dDataFpq)  		
Local cControl	:= "01"
Local cQryExe	:= ""
Local cQryAlias	:= GetNextAlias()

	/*
	+ cFilFpq +
	+ cFilMat +
	+ cMatFpq +
	+Dtos(dDataFpq)+
	*/

	cQryExe := " SELECT MAX(FPQ_CONTRO) AS MAX FROM " + RetSqlName("FPQ") + " " 
	cQryExe += " WHERE FPQ_FILIAL = ? AND FPQ_FILMAT = ? AND
	cQryExe += " FPQ_MAT = ? AND FPQ_DATA = ? AND D_E_L_E_T_ = ' ' "

	cQryExe := CHANGEQUERY(cQryExe) 
	aBindParam := {cFilFpq, cFilMat, cMatFpq, Dtos(dDataFpq)}
	cQryAlias := MPSysOpenQuery(cQryExe,,,,aBindParam)
	
	//DBUSEAREA(.T.,"TOPCONN", TCGENQRY(,,cQryExe),cQryAlias, .F., .T.)

	If !(cQryAlias)->(Eof())
		cControl	:= Soma1((cQryAlias)->MAX)
	EndIf

	(cQryAlias)->(DbCloseArea())

Return cControl


//-------------------------------------------------------------------
/*/{Protheus.doc} LOCA05314
@description	Retorna diferença entre hora inicial e final em decimais
@author			José Eulálio
@since     		07/11/2022
/*/			
//-------------------------------------------------------------------
FUNCTION LOCA05314(cHoraFin,cHoraIni)  	
Local nRet	:= 0
	// calcula a parte da hora cheia 
	nRet := Round(((Val(SubStr(cHoraFin,1,2)) - Val(SubStr(cHoraIni,1,2)))) + ((Val(SubStr(cHoraFin,4)) - Val(SubStr(cHoraIni,4))) / 60) , 2)

Return nRet

