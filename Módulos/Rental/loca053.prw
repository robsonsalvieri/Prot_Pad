				_LRET := .F.
				MSGALERT(STR0040) //"NÃO É PERMITIDO DEIXAR A AS EM BRANCO PARA STATUS IGUAL A OBRA."
			EndIf
		// 04/11/2022 - Jose Eulalio - SIGALOC94-554 - Valid - AS - Lançamento de mão de Obra
		// Executar somente a partir do Valid essa validação.
		/*CASE !ALLTRIM(_CSTATUS)$"OBRA|INTEGR" .AND. !EMPTY(_CAS)
			_LRET := .F.
			MSGALERT(STR0044) // "AS não aceita!"*/
		CASE !EMPTY(_CAS) .AND. ALLTRIM(FQ5->FQ5_AS) == ALLTRIM(_CAS)
			If ( AllTrim(ODLGLO:ACOLS[ODLGLO:NAT][ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FPQ_STATUS" })]) == "OBRA" .or.; 
				AllTrim(ODLGLO:ACOLS[ODLGLO:NAT][ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FPQ_STATUS" })]) == "INTEGR" )
