SINCE 03/12/2020
HISTORY 03/12/2020, FRANK ZWARG FUGA, FONTE PRODUTIZADO.
/*/

// ======================================================================= \\
STATIC FUNCTION FACUMUPLA()
// ======================================================================= \\
LOCAL NPOS
LOCAL ACOLS0  := {}
Local nPOBRA := ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_OBRA"})
LOcal nPProd := ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_PRODUT"})
Local nQitens := 0
Local nObraFP1 := ASCAN(ODLGOBR:AHEADER,{|X|ALLTRIM(X[2])=="FP1_OBRA"})
Local nQtdItem := ASCAN(ODLGOBR:AHEADER,{|X|ALLTRIM(X[2])=="QTDEITENS"})
Local nPosObra

	If FWIsInCallStack("FSALVAR900")
		COBRA := ODLGOBR:ACOLS[ODLGOBR:NAT][nObraFP1] // Rossana - DSERLOCA 3710 - 05/08/2024
	EndIf

	FOBRA()

	FOR NPOS:=1 TO LEN(OPLA_COLS)
		WOBRA := OPLA_COLS[NPOS][nPOBRA]
		IF COBRA != WOBRA
			AADD( ACOLS0, ACLONE( OPLA_COLS[NPOS] ) )
		ENDIF
	NEXT NPOS

	FOR NPOS := 1 TO LEN(ODLGPLA:ACOLS)
		WOBRA := ODLGPLA:ACOLS[NPOS][nPOBRA]
		IF COBRA = WOBRA
			AADD( ACOLS0, ACLONE(ODLGPLA:ACOLS[NPOS]))
			if !Empty(ODLGPLA:ACOLS[nPos, nPProd])
				nQitens++
			endif
		endif
	NEXT NPOS

	nPosObra := Ascan(ODLGOBR:aCols, {|X|ALLTRIM(X[nObraFP1])== ODLGPLA:ACOLS[1][nPOBRA]} )
	if nQtdItem > 0
		ODLGOBR:aCols[nPosObra][nQtdItem] := nQitens
	endif

	IF EMPTY(ACOLS0)
		AADD( ACOLS0, ACLONE( OPLA_COLS0[1] ) )
	ENDIF

	OPLA_COLS := ACLONE(ACOLS0)

	aSize(ACOLS0,0)
	FwFreeArray(ACOLS0)
	ODLGOBR:Refresh()
	ODlgObr:oBROWSE:ReFresh()

RETURN NIL


