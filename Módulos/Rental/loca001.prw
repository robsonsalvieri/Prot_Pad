			If FPA->(fieldpos("FPA_QTDPRC")) > 0 .and. OPLA_COLS[_NX][nPQTDPRC] > 0
				_nTempX16 += OPLA_COLS[_NX][nPVRHOR] * OPLA_COLS[_NX][nPQTDPRC]
				_nTempX14 += OPLA_COLS[_NX][nPVRISS] * OPLA_COLS[_NX][nPQTDPRC]
