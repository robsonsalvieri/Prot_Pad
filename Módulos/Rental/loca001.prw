cQuery += " WHERE FPA_FILIAL = '"+xFilial("FPA")+"'"
cQuery += " AND FPA_PROJET = ? "
Aadd(aBindParam, cProjeto)
cQuery += " AND FPA_OBRA = ? "
Aadd(aBindParam, cObra)
cQuery := CHANGEQUERY(cQuery)

MPSysOpenQuery(cQuery,"TRB",,,aBindParam)

if !TRB->(EOF())
	cSeq := TRB->ULTIMO
endif


RestArea(aArea)

Return cSeq

