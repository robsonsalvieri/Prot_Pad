Local CALIASZA0 := "FP0"
Local CPROJET   := PROJETO()
Local _aArea    := GetArea()
Local aBindParam
Local cAliasFp9
Local CSQL
Local _MV_LOC278 := supergetmv("MV_LOCX278",,.F.)
Local _aVetSE1
Local lErroExec := .f.

