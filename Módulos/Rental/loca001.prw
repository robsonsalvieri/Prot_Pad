Local nPULTFAT := ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FQZ_ULTFAT"})
Local nPRETIRA := ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FQZ_RETIRA"})
