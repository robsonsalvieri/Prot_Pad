STATIC FUNCTION FFOLDEROBR(NFOLDER , NLIN1 , NCOL1 , NLIN2 , NCOL2, lFiltra)
Local NSTYLE    := GD_INSERT + GD_UPDATE + GD_DELETE
Local CALIAS
Local CCHAVE
Local CCONDICAO
Local NINDICE
Local CFILTRO
Local CPROJET   := PROJETO()
Local LMULTOBR	:= SUPERGETMV("MV_LOCX280",.F.,.T.)

Private NINSS     := 0

DEFAULT lFiltra := .F.

	NSTYLE := IIF(NOPCMANU==2 , 0 , NSTYLE) 		// 2=VISUALIZAR

	IF EXISTBLOCK("LC001EDT") 						// --> PONTO DE ENTRADA PARA ALTERAÇÃO DA EDIÇÃO DA ABA
		NSTYLE := EXECBLOCK("LC001EDT",.T.,.T.,{"FP1",NOPCMANU, NSTYLE})
	ENDIF

	CALIAS    := "FP1"
	CCHAVE    := XFILIAL(CALIAS)+CPROJET
	CCONDICAO := 'FP1_FILIAL+FP1_PROJET=="'+CCHAVE+'"'
	cFiltro := CCONDICAO
	if lFiltra
		cFiltro += " .and. FP1_OBRA >= '"+MV_PAR01+"' .and. FP1_OBRA <= '"+MV_PAR02+"'"
	endif

	NINDICE   := 1  								// FP1_FILIAL+FP1_PROJET+FP1_OBRA

	AHEADER   := FHEADER("FP1", , , , lFiltra)
	ACOLS     := FCOLS(AHEADER,CALIAS,NINDICE,CCHAVE,CCONDICAO,CFILTRO)

	IF LEN(ACOLS)==1
		CCAMPO    := "FP1_OBRA"
		CCAMPOGET := ACOLS[1][ASCAN(AHEADER,{|X|ALLTRIM(X[2])==CCAMPO})]
		IF EMPTY(CCAMPOGET)
			ACOLS[1][ASCAN(AHEADER,{|X|ALLTRIM(X[2])==CCAMPO})] := Soma1(LastObra(FP0->FP0_PROJET))
		ENDIF
	ENDIF

	cObra :=  ACOLS[1][ASCAN(AHEADER,{|X|ALLTRIM(X[2])=="FP1_OBRA"})]

	IF LMULTOBR
		ODLGOBR := MSNEWGETDADOS():NEW(NLIN1,NCOL1,NLIN2  ,NCOL2 ,NSTYLE,        ,       ,"",      ,       ,MAXGETDAD,        ,         ,{|| ITDELZA1()}   ,OFOLDER:ADIALOGS[NFOLDER],AHEADER,ACOLS)
	ELSE
		ODLGOBR := MSNEWGETDADOS():NEW(NLIN1,NCOL1,NLIN2  ,NCOL2 ,NSTYLE,        ,       ,"",      ,       ,1			 ,        ,         ,{|| ITDELZA1()}   ,OFOLDER:ADIALOGS[NFOLDER],AHEADER,ACOLS)
	ENDIF
	ODLGOBR:OBROWSE:BCHANGE := {|| LOCA00144()}
	ODLGOBR:SetEditLine(.F.)

RETURN NIL

