HISTORY 03/12/2020, FRANK ZWARG FUGA, FONTE PRODUTIZADO.
/*/

FUNCTION LOCA00157(CCAMPO)					// VALIDAÇÕES
Local LRET     := .T.
Local CCOD
Local CDESC    := ""
Local CPROD
Local CTPGUIM
Local CTPGUID
Local NBASE
Local NPERC
Local NVALOR
Local aArea := GetArea()
Local cError
Local cSolucao
Local lLocx305  := SuperGetMV("MV_LOCX305",.F.,.T.) // Indica se podemos tem uma FPA sem Grua (.T.) ou não (.F.)

	DO CASE
	CASE UPPER(CCAMPO) == "FPA_PRODUT"
		IF VALTYPE(ODLGPLA)=="O" .AND. LRET 		// SE O OBJETO JÁ FOI CRIADO
			CPROD :=M->FPA_PRODUT
			SB1->(DBSETORDER(1))
			IF !SB1->(DBSEEK(XFILIAL("SB1") + CPROD))

				Help(Nil,	Nil,STR0029+alltrim(upper(Procname())),; //"RENTAL: "
						Nil,STR0033,1,0,Nil,Nil,Nil,Nil,Nil,; //"Inconsistência nos dados."
						{STR0284 + ALLTRIM(CPROD) + STR0285}) //"O produto: "###" não está cadastrado."

				LRET := .F.
			ENDIF
			//Rossana - limpar equipamento quando produto é alterado - DSERLOCA 2619
			DbSelectArea("FPA")
			DbSetOrder(1)
			DbSeek(FP0->FP0_FILIAL+FP0->FP0_PROJET+ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_OBRA"})] + ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_SEQGRU"})] + "  "   )
			If Found() .and. M->FPA_PRODUT==FPA->FPA_PRODUT
			Else
				ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_GRUA"})]   := Space(16)
				ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_DESGRU"})] := Space(40)
			EndIf
		ENDIF

	CASE UPPER(CCAMPO) == "FPA_GRUA"
		IF VALTYPE(ODLGPLA)=="O" .AND. LRET 		// SE O OBJETO JÁ FOI CRIADO
			CPROD := ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_PRODUT"})]
			CCOD  := M->FPA_GRUA

			if lLocx305 .and. Empty(cCod) // Card 1322 - Circenis - 04/12/2023
				ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_DESGRU"})] := CDESC
				lRet := .T.
			else
				ST9->(DBSETORDER(1))
				IF ST9->(DBSEEK(XFILIAL("ST9") + CCOD))
					CDESC := ST9->T9_NOME
//					NPES  := ST9->T9_PES
					NPES  := 0 // Rossana - DSERLOCA 2619

					// Em alguns clientes o preenchimento da grua não tem haver com o produto posicionado
					// criamos o ponto de entrada LC00157 para não validar o T9_CODESTO
					// Frank Z Fuga em 24/01/22
					IF EXISTBLOCK("LC00157")
						LRET := EXECBLOCK("LC00157" , .T. , .T. , {ODLGPLA})
					Else
						IF ALLTRIM(ST9->T9_CODESTO) <> ALLTRIM(CPROD)
							Help(Nil,	Nil,STR0029+alltrim(upper(Procname())),; //"RENTAL: "
							Nil,STR0033,1,0,Nil,Nil,Nil,Nil,Nil,; //"Inconsistência nos dados."
							{STR0286 + ALLTRIM(CCOD) + STR0287 + ALLTRIM(CPROD)}) //"O equipamento: "###" não possui vínculo com o produto: "
							LRET := .F.
						ENDIF
					EndIF

					// CARD 1240 - Frank Zwarg Fuga
					// Validações complementares de acordo com a mesma regra existente do filtro do SXB deste campo.
					cError := ""
					If ST9->T9_SITMAN <> 'A'
						cError := STR0602 //"A situação no manutenção de ativos precisa estar Ativa."
						cSolucao := STR0603 //"Veja as regras no manutenção de ativos sobre o campo T9_SITMAN."
					EndIf
					If ST9->T9_SITBEM <> 'A'
						cError := STR0604 //"A situação do bem precisa estar Ativa."
						cSolucao := STR0605 //"Veja as regras no manutenção de ativos sobre o campo T9_SITBEM."
					EndIF
					If !ST9->T9_STATUS $ LOCA00194()
						cError := STR0606 //"O status do bem é inválido."
						cSolucao := STR0607 //"O campo T9_STATUS está com um valor diferente do tratado no módulo SIGALOC."
					EndIF
					If ST9->T9_CODESTO <> ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=='FPA_PRODUT'})]
						cError := STR0608 //"Bem inválido."
						cSolucao := STR0609 //"O bem digitado não é associado ao produto informado."
					EndIf
					if FindFunction("LOCA224G") .and. LOCA224G(CCOD, .F.) // Avalia ultimo substatus bloqueia locação DSERLOCA-3906 Circenis
						cError  := STR0633 //"O Equipamento está bloqueado para locação!"
						cSolucao := STR0634 //"Avalie o substatus atual do aquipamento."
					endif
					If !empty(cError)
						lRet := .F.
						Help(Nil,	Nil,STR0029+alltrim(upper(Procname())),; //"RENTAL: "
								Nil,cError,1,0,Nil,Nil,Nil,Nil,Nil,;
								{cSolucao})
					EndIF
				ELSE
					Help(Nil,	Nil,STR0029+alltrim(upper(Procname())),; //"RENTAL: "
								Nil,STR0033,1,0,Nil,Nil,Nil,Nil,Nil,; //"Inconsistência nos dados."
								{STR0286 + ALLTRIM(CCOD) + STR0288}) //"O equipamento: "###" não está cadastrado como bem."

					LRET := .F.
				ENDIF

				IF LRET									// CONSULTA A DISPONIBILIDADE DE FROTA
					ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_DESGRU"})] := CDESC
					IF 	FPA->FPA_PES > 0
						ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_PES"})] := NPES
						IF EMPTY(ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_CONJT" })])
							CCONJT := ALLTRIM(CAPITAL(POSICIONE("SX5",1,XFILIAL("SX5")+"76" + ALLTRIM(STR(NPES,3,0)),"X5_DESCRI")))
							ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_CONJT" })] := CCONJT
						ENDIF
					ENDIF
					M->FPA_DTINI := ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_DTINI"})]
					M->FPA_DTFIM := ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_DTFIM"})]
					// 07/10/2022 - Jose Eulalio - Sprint 2 para atender Equipamento com Status vazio
					//IF ST9->T9_STATUS<>"00"
					IF ST9->T9_STATUS<>"00" .And. !(Empty(ST9->T9_STATUS))
						Help(Nil,	Nil,STR0029+alltrim(upper(Procname())),; //"RENTAL: "
									Nil,STR0033,1,0,Nil,Nil,Nil,Nil,Nil,; //"Inconsistência nos dados."
									{STR0286 + ALLTRIM(CCOD) + STR0011}) //"O equipamento: "###"Indisponível"

						LRET := .F.
					ENDIF
					//LOCA00165(CCOD , M->FPA_DTINI , M->FPA_DTFIM)
				ENDIF
			endif
		EndIf

	CASE UPPER(CCAMPO) == "FPA_TPGUIM"
		IF VALTYPE(ODLGPLA)=="O" .AND. LRET 		// SE O OBJETO JÁ FOI CRIADO
			CTPGUIM := ALLTRIM(M->FPA_TPGUIM)
			IF EMPTY(CTPGUIM)
			ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_GUIMON"})] := 0
			ENDIF
		ENDIF

	CASE UPPER(CCAMPO) == "FPA_TPGUID"
		IF VALTYPE(ODLGPLA)=="O" .AND. LRET 		// SE O OBJETO JÁ FOI CRIADO
			CTPGUID := ALLTRIM(M->FPA_TPGUID)
			IF EMPTY(CTPGUID)
			ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_GUIDES"})] := 0
			ENDIF
		ENDIF

	CASE UPPER(CCAMPO)==UPPER("FPA_VRCARG")
		IF VALTYPE(ODLGPLA)=="O" 					// SE O OBJETO JÁ FOI CRIADO
			// DSERLOCA-2293 - Frank em 31/01/2024 - inativado o código abaixo
			//NBASE  := M->FPA_VRCARG
