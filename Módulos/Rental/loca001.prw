					LRet := .F.

				// Se tiver produto deve ter quantidade informada
				elseif !Empty(ODLGIMP:aCols[nX, nPosProd]) .and. ODLGIMP:aCols[nX, nPosQuant] <= 0

					Help(Nil,	Nil,STR0029+alltrim(upper(Procname())),; //"RENTAL: "
					Nil,STR0637,1,0,Nil,Nil,Nil,Nil,Nil,; //"Quando um produto for informado a quantidade deverá ser informada!"
					{STR0638}) //"Informe a quantidade do produto ."
					LRet := .F.
				endif
			Endif
		endif
	next
	RestArea(aArea)
Return lRet

