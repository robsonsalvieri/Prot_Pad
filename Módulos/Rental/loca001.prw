SINCE 03/12/2020
HISTORY 03/12/2020, FRANK ZWARG FUGA, FONTE PRODUTIZADO.
/*/

FUNCTION LOCA00181A()
LOCAL NPORCENT := 0
Local nTemp
Local nValOper
Local nValTur

	If upper(alltrim(Readvar())) == "M->FPE_VROPER"
		nValOper := &(ReadVar())
		nValTur := OGETVT:ACOLS[N, GDFIELDPOS("FPE_VALTUR", OGETVT:AHEADER)]
	else
		nValTur := &(ReadVar())
		nValOper := OGETVT:ACOLS[N, GDFIELDPOS("FPE_VROPER", OGETVT:AHEADER)]	
	EndIF

	If nValTur > nValOper
		MsgAlert(STR0626,STR0293) //"Valor Total menor que o valor base"###"Inconsistência"
		Return .F.
	EndIF

	nTemp    := nValOper - nValTur

	NPORCENT := (nTemp / 100) * 10
	OGETVT:ACOLS[N, GDFIELDPOS("FPE_PORCEN", OGETVT:AHEADER)] := nPorcent
	OGETVT:refresh()

RETURN .T.

