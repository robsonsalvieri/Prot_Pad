			//_cTmprX08 += oDlgPla:aCols[_NX][nPTPGUIM]
			//_cTmprX09 += oDlgPla:aCols[_NX][nPTPGUID]
			_nTmprX10 += oDlgPla:aCols[_NX][nPGUIMON] + if( nPMOB >0, oDlgPla:aCols[_NX][nPMOB] ,0)
			_nTmprX11 += oDlgPla:aCols[_NX][nPGUIDES] + if( nPDESMOB >0, oDlgPla:aCols[_NX][nPDESMOB], 0)
			_nTmprX15 += oDlgPla:aCols[_NX][nPVRSEGU]

			//ifranzoi - 27/07/2021
			//Valor Total (Qtd Parcela * Valor Base)
			If FPA->(fieldpos("FPA_QTDPRC")) > 0 .and.  oDlgPla:aCols[_NX][nPQTDPRC] > 0
				_nTmprX14 += oDlgPla:aCols[_NX][nPVRISS] * oDlgPla:aCols[_NX][nPQTDPRC]
				_nTmprX16 += oDlgPla:aCols[_NX][nPQTDPRC]*oDlgPla:aCols[_NX][nPVRHOR]
				if oDlgPla:aCols[_NX][nPTPISS] = 'N' // ISS não incluso somar ao total
					_nTmprX16 += oDlgPla:aCols[_NX][nPVRISS] * oDlgPla:aCols[_NX][nPQTDPRC]
				endif
			Else
				_nTmprX14 += oDlgPla:aCols[_NX][nPVRISS]
				_nTmprX16 += oDlgPla:aCols[_NX][nPVRHOR]
				if oDlgPla:aCols[_NX][nPTPISS] = 'N' // ISS não incluso somar ao total
					_nTmprX16 += oDlgPla:aCols[_NX][nPVRISS]
				endif
			EndIF
			// Frank em 10/10/22
			_nTmprX16 += oDlgPla:aCols[_NX][nPGUIMON] + if( nPMOB >0, oDlgPla:aCols[_NX][nPMOB], 0)
			_nTmprX16 += oDlgPla:aCols[_NX][nPGUIDES] + if( nPDESMOB >0, oDlgPla:aCols[_NX][nPDESMOB], 0)
			if nTIPSEG = 0 .OR. oDlgPla:aCols[_NX][nTIPSEG] = "N"
				_nTmprX16 += oDlgPla:aCols[_NX][nPVRSEGU]
			ENDIF
			_nTmprX13 += oDlgPla:aCols[_NX][nPPERISS]
		EndIf
	Next

	_nTmprX13 := _nTmprX13 / (_NX - 1)

	// Frank em 10/10/22
	_nTmprX06 := ((_nTmprX05 - _nTmprX07) / _nTmprX05) * 100
	//_nTmprX13 := (_nTmprX14 / _nTmprX07) * 100

	//Custo Direto
	_nTmprX17 := 0
	For _NX := 1 To Len(oDlgRes:aCols)
		If ( _NX == 1 )
			_nTmprX17 := oDlgRes:aCols[_NX][nPVALOR]
		Else
			_nTmprX17 += oDlgRes:aCols[_NX][nPVALOR]
		EndIf
	Next
	//Margem de Contribuição (Valor Base - Custo Direto)
	_nTmprX18 := _nTmprX16-_nTmprX17
	//% Margem de contribuição ( Margem de Contribuição / Receita)
	_nTmprX19 := (_nTmprX18 / _nTmprX16) * 100


//------------------------------------------------------------------------------------------------
// CÁLCULO DO CONTRATO DEVE FICAR APÓS O MESMO CÁLCULO DA OBRA
// DSERLOCA - 2423 - Rossana - 05/03/24

	_nTempX17 := 0
	For _NX := 1 To Len(ORES_COLS)
		If ( _NX == 1 )
			_nTempX17 := ORES_COLS[_NX][nPVALOR]
		Else
			_nTempX17 += ORES_COLS[_NX][nPVALOR]
		EndIf
	Next

	//Margem de Contribuição (Valor Base - Custo Direto)
	_nTempX18 := _nTempX16-_nTempX17
	//% Margem de contribuição ( Margem de Contribuição / Receita)
	_nTempX19 := (_nTempX18 / _nTempX16) * 100
//------------------------------------------------------------------------------------------------



