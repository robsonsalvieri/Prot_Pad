			M->FP0_CLICEP := IIF(EMPTY(SUS->US_CODCLI) .AND. EMPTY(SUS->US_LOJACLI) , SUS->US_CEP    , SA1->A1_CEP)
			M->FP0_CLIEMA := IIF(EMPTY(SUS->US_CODCLI) .AND. EMPTY(SUS->US_LOJACLI) , SUS->US_EMAIL	 , SA1->A1_EMAIL)
