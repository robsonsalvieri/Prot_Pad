/*/LOCA001TP
ITUP BUSINESS - TOTVS RENTAL
AUTHOR FRANK ZWARG FUGA
SINCE 02/04/2025
Rotina para pesquisar na aba locações
/*/
Function LOCA001TP
Local nX
Local oOK
Local oNO
Local oPesq
Local oSay := NIL
Local OCHECKBOX1
Local lAtual := .T.
Local oObjPes

Private OFILOS
Private oDlgPes
Private aCampos := {}
Private cPesq := space(50)

	oOK := LOADBITMAP(GETRESOURCES(),"LBOK")
	oNO := LOADBITMAP(GETRESOURCES(),"LBNO")

	If oFolder:nOption <> 2 .and. oFolder:nOption <> 3 .and. oFolder:nOption <> 4 ;
		.and. oFolder:nOption <> 5 .and. oFolder:nOption <> 7 .and. oFolder:nOption <> 9 .and. oFolder:nOption <> 10

		FWAlertError(STR0664,STR0463) // "Esta aba não permite a execução da pesquisa."###"Atenção!"

	Else

		If oFolder:nOption == 2 // Localidades
			oObjPes := oDlgObr
		ElseIf oFolder:nOption == 3 // Locações
			oObjPes := oDlgPla
		ElseIf oFolder:nOption == 4 // Conj.Transportador
			oObjPes := oDlgCnp
		ElseIf oFolder:nOption == 5 // Responsabilidades
			oObjPes := oDlgRes
		ElseIf oFolder:nOption == 7 // Documentos
			oObjPes := oDlgDoc
		ElseIf oFolder:nOption == 9 // Implemento
			oObjPes := oDlgImp
		ElseIf oFolder:nOption == 10 // Turnos
			oObjPes := oDlgTur
		EndIf

		For nX := 1 to len(oObjPes:AHEADER)
			If oObjPes:AHEADER[nX,8] <> "L"
				//                  titulo                 campo                  tamanho                decimal                tipo 
				aadd(aCampos, {.T., oObjPes:AHEADER[nX,1], oObjPes:AHEADER[nX,2], oObjPes:AHEADER[nX,4], oObjPes:AHEADER[nX,5], oObjPes:AHEADER[nX,8]}  )
			EndIf
		Next

		DEFINE MSDIALOG oDlgPes FROM 10,10 TO 350,600 PIXEL TITLE STR0663 //"Pesquisar"
		@ 0.5,1 LISTBOX OFILOS FIELDS HEADER  " ",;
		STR0665,; //"Titulo"
		STR0666; //"Campo"
		SIZE 110,153 ON DBLCLICK (MARCARREGI(.F.))

		OFILOS:SETARRAY(aCampos)

		cLinha := "{|| { IF( aCampos[OFILOS:NAT,1],OOK,ONO),"
		cLinha += "aCampos[OFILOS:NAT,2],"
		cLinha += "aCampos[OFILOS:NAT,3]"
		cLinha += "}}"

		OFILOS:BLINE := &(cLinha)

		@ 06,123 SAY STR0667 SIZE 050,8 OF oDlgPes PIXEL //"Chave da pesquisa:"
		@ 14,123 MSGET oPesq VAR cPesq SIZE 150,8 OF oDlgPes PIXEL 
		@ 27,123 CHECKBOX OCHECKBOX1 VAR lAtual PROMPT STR0668 SIZE 150,8 PIXEL OF oDlgPes //"Localizar desde o início da grid"

		@ 150,178 BUTTON OMARKBUT PROMPT STR0669 SIZE 55,12 OF oDlgPes PIXEL ACTION (MARCARREGI(.T.)) //"(Des)marcar todos"
		@ 150,235 BUTTON OFILBUT PROMPT STR0670 SIZE 55,12 OF oDlgPes PIXEL ;  //"Pesquisar"
		ACTION ( FwMsgRun(NIL, {|oSay| LOCA001TP2(oSay,oFilos,lAtual,oObjPes)}, STR0670, STR0671) ) //"Pesquisar"###"Localizando o registro"
					
		ACTIVATE MSDIALOG oDlgPes CENTERED

	EndIf

Return

/*/LOCA001TP2
ITUP BUSINESS - TOTVS RENTAL
AUTHOR FRANK ZWARG FUGA
SINCE 02/04/2025
Complemento da rotina de pesquisa na aba locações
/*/
Function LOCA001TP2(oSay,oFilos,lAtual,oObjPes)
Local nX
Local nY 
Local nZ
Local xPesq
Local lLocaliza
Local nLinha := 1

	For nX := 1 to len(oObjPes:aCols)
		oSay:SetText(STR0672 + StrZero(nX, 6)) //"Localizando o registro na linha: "
		ProcessMessage()

		If !lAtual
			If nX < oObjPes:nAt
				Loop
			EndIf
		EndIf

		For nY := 1 to len(oFilos:aArray)
			If oFilos:aArray[nY,1]
				For nZ := 1 to len(oObjPes:aHeader)
					If alltrim(oObjPes:aHeader[nZ,2]) == alltrim(oFilos:aArray[nY,3])
						xPesq := ""
						llocaliza := .F.
						If oObjPes:aHeader[nZ,8] == "C" .or. oObjPes:aHeader[nZ,8] == "M"
							xPesq := alltrim(cPesq)
							If AT(upper(xPesq),upper(oObjPes:ACOLS[nX][nZ])) > 0
								lLocaliza := .T.
								nLinha := nX
								Exit
							EndIf
						ElseIf oObjPes:aHeader[nZ,8] == "N"
							If val(cPesq) > 0
								xPesq := val(cPesq)
								If oObjPes:ACOLS[nX][nZ] == xPesq
									lLocaliza := .T.
									nLinha := nX
									Exit
								EndIf
							EndIf
						ElseIf oObjPes:aHeader[nZ,8] == "D"
							If !empty(ctod(cPesq))
								xPesq := ctod(cPesq)
								If oObjPes:ACOLS[nX][nZ] == xPesq
									lLocaliza := .T.
									nLinha := nX
									Exit
								EndIf
							EndIf
						EndIf
					endif
				Next
				If lLocaliza
					Exit
				EndIf
			EndIf
		Next
		If lLocaliza
			Exit
		EndIf
	Next

	if lLocaliza
		oDlgPes:END()
		oObjPes:GOTO(nLinha)
        oObjPes:OBROWSE:REFRESH()
        oObjPes:OBROWSE:SetFocus() 
	Else
		FWAlertWarning(STR0673,STR0670) //"Nenhuma ocorrência foi localizada."###"Pesquisar"
	EndIf

Return


/*/MARCARREGI
ITUP BUSINESS - TOTVS RENTAL
AUTHOR FRANK ZWARG FUGA
SINCE 04/04/2025
Marca o flag na listbox da pesquisa avançada
/*/

Static Function MARCARREGI(lTodos)
Local nI := 0
Local lMarcados := aCampos[oFilos:nAT,1]

	If lTodos
		lMarcados := ! lMarcados
		For nI := 1 TO len(aCampos)
			aCampos[nI,1] := lMarcados
		Next
	Else
		aCampos[oFilos:nAT,1] := !lMarcados
	EndIf

	oFilos:REFRESH()
	oDlgPes:REFRESH()

Return


