/*/ FSALVARIMP
ITUP BUSINESS - TOTVS RENTAL
AUTHOR FRANK ZWARG FUGA
DSERLOCA-1982 - Frank em 30/04/2024
/*/
STATIC FUNCTION FSALVARIMP(CALIAS , AHEADER , ACOLS)
local npos
local _aarea    := getarea()
Local lDeletada := .F.
Local lAltera   := .T.
//Local NXX
//Local nCod 		:= ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_COD"  })
Local cProjet
Local cObra
Local cSeq
Local cCod

	DBSELECTAREA(CALIAS)
	DBSETORDER(1)

	SELIMP("SAINDO")

	IF NOPC != 5  //5=EXCLUI
		/*IF ODLGIMP:ACOLS[ODLGIMP:NAT][LEN(ODLGIMP:AHEADER)+1] 
			FOR NXX := 1 TO LEN(OIMP_COLS) // Marcar as Minhas para deleção
				IF OIMP_COLS[NXX][nCod] == ODLGIMP:ACOLS[ODLGIMP:NAT][nCod]
					OIMP_COLS[NXX,LEN(AHEADER)+1] := .T.
				ENDIF
			NEXT
		ENDIF*/

		FOR NPOS := 1 TO LEN(OIMP_COLS)
			lDeletada := OIMP_COLS[NPOS][len(ODLGIMP:AHEADER)+1] //Ultimo elemento da linha da Acol se .T. está deletado

			cProjet  := OIMP_COLS[nPos][ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_PROJET"})]
			cObra    := OIMP_COLS[nPos][ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_OBRA"})]
			cSeq     := OIMP_COLS[nPos][ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_SEQ"})]
			cCod     := OIMP_COLS[nPos][ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_COD"})]
	
			IF ! (CALIAS)->(DBSEEK(XFILIAL(CALIAS)+CPROJET+COBRA+CSEQ+cCod))
				if !lDeletada
					(CALIAS)->(RECLOCK(CALIAS,.T.))
				endif
				lAltera := .F.
			ELSE
				lAltera := .T.
				(CALIAS)->(RECLOCK(CALIAS,.F.))
			ENDIF

			if lDeletada
				(CALIAS)->(dbDelete())
			EndIF
			if !lDeletada
				FGRAVATUDO(CALIAS,AHEADER,OIMP_COLS[NPOS])  //GRAVA TODOS OS CAMPOS DO ACOLS
				(CALIAS)->FQG_FILIAL := XFILIAL(CALIAS)
				(CALIAS)->FQG_PROJET := CPROJET
				(CALIAS)->FQG_OBRA   := COBRA
				(CALIAS)->FQG_SEQ    := cSeq
				(CALIAS)->FQG_COD    := cCod
			endif
			(CALIAS)->(MSUNLOCK() )
		NEXT NPOS

		// Apos a gravacao de todos os itens da FQG
		// Valiar se a FP1, ou FPA foram deletados
		cProjeto := FP0->FP0_PROJET
		FQG->(dbSetOrder(1))
		FQG->(dbSeek(xFilial("FQG")+cProjeto))
		FP1->(dbSetOrder(1))
		FPA->(dbSetOrder(1))
		While !FQG->(Eof()) .and. FQG->(FQG_FILIAL+FQG_PROJET) == xFilial("FQG")+cProjeto
			If !FP1->(dbSeek(xFilial("FP1")+FQG->(FQG_PROJET+FQG_OBRA)))	
				FQG->(RecLock("FQG",.F.))
				FQG->(dbDelete())
				FQG->(MsUnlock())
			Else
				If !FPA->(dbSeek(xFilial("FPA")+FQG->(FQG_PROJET+FQG_OBRA+FQG_SEQ)))	
					FQG->(RecLock("FQG",.F.))
					FQG->(dbDelete())
					FQG->(MsUnlock())
				EndIF
			EndIF
			FQG->(dbSkip())
		EndDo

	ENDIF

	
	RestArea(_aArea)

	aSize(_aArea,0)
	FwFreeArray(_aArea)
RETURN NIL

/*/ SELIMP
ITUP BUSINESS - TOTVS RENTAL
AUTHOR FRANK ZWARG FUGA
USADO PARA O BCHANGE QUANDO SE SAI DA ABA IMPLEMENTO
DSERLOCA-1982 - Frank em 30/04/2024
/*/
Static Function SELIMP(cVar)
Local nPos	
Local nPos2
Local ACOLS0
Local aCols := {}
Local cProjeto
Local cObra
Local cSeq
Local nCod := ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_COD"  })
Local lAchou := .F.
Local nPos3
Local aIncluir := {}
Local lDeletada
	
	If cVar=="ENTRANDO"
		ODLGIMP:ACOLS := {}
		ACOLS := {}
		FOR nPos := 1 TO LEN(OIMP_COLS) // Array que contém todos os registros da FQG
			lDeletada := OIMP_COLS[nPos][len(ODLGIMP:aHeader)+1] 
			//If !lDeletada
				aCols0 := {}
				cProjeto := FP0->FP0_PROJET
				cObra    := OIMP_COLS[nPos][ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_OBRA"})]
				cSeq     := OIMP_COLS[nPos][ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_SEQ"})]
				If cProjeto == FP0->FP0_PROJET
					If cObra == ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_OBRA"})]
						If cSeq == ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_SEQGRU"})]
							For nPos2:=1 to len(ODLGIMP:AHEADER)
								aadd(aCols0,OIMP_COLS[nPos][nPos2])
							Next
							AADD(aCols0, lDeletada )
							AADD(ACOLS,ACOLS0)
						EndIf
					EndIf
				EndIf
			//EndIf
		NEXT
		If len(aCols) == 0
			ACOLS0 := {}
			FOR NPOS := 1 TO LEN(ODLGIMP:AHEADER)
				AADD(ACOLS0,CRIAVAR(ODLGIMP:AHEADER[NPOS,2]))
			NEXT
			AADD(ACOLS0 , .F.) 
			aadd(ODLGIMP:ACOLS,aClone(ACOLS0))
			ODLGIMP:ACOLS[1][ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_PROJET"})] := FP0->FP0_PROJET
			ODLGIMP:ACOLS[1][ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_OBRA"})] := ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_OBRA"})]
			ODLGIMP:ACOLS[1][ASCAN(ODLGIMP:AHEADER,{|X|ALLTRIM(X[2])=="FQG_SEQ"})] := ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_SEQGRU"})]
		Else
			ODLGIMP:ACOLS := aClone(ACOLS)
		EndIf
		ODLGIMP:REFRESH()
	ElseIf cVar=="SAINDO"
		// Tratar o acumulado no momento de salvar e no momento da troca de aba
		For nPos := 1 to len(ODLGIMP:ACOLS)
			lAchou := .F.
			For nPos2 := 1 to LEN(OIMP_COLS)
				If OIMP_COLS[nPos2][nCod] == ODLGIMP:ACOLS[nPos][nCod]
					lAchou := .T.
					For nPos3 := 1 to len(OIMP_COLS[nPos2])
						OIMP_COLS[nPos2][nPos3] := ODLGIMP:ACOLS[nPos][nPos3]
					Next
					OIMP_COLS[nPos2][len(ODLGIMP:AHEADER)+1] := ODLGIMP:ACOLS[nPos][len(ODLGIMP:AHEADER)+1]
				EndIf
			Next
			If !lAchou //.and. !ODLGIMP:ACOLS[nPos][len(ODLGIMP:AHEADER)+1]
				aadd(aIncluir,ODLGIMP:ACOLS[nPos])
				aIncluir[len(aIncluir)][len(ODLGIMP:AHEADER)+1] := ODLGIMP:ACOLS[nPos][len(ODLGIMP:AHEADER)+1]
			EndIF
		Next
		For nPos := 1 to len(aIncluir)
			aadd(OIMP_COLS, aIncluir[nPos] )
		Next
	EndIF
Return 

