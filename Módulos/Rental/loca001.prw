/*/

FUNCTION LOCA00158(NBASE , NPERC , CTPISS)
LOCAL NRETISS := 0

	DO CASE
	CASE ALLTRIM(CTPISS) $ "X|M| "
		NRETISS := 0
	CASE ALLTRIM(CTPISS) == "I"
		NRETISS := ((NBASE * NPERC) /100)
	OTHERWISE
		NRETISS := (NBASE / ((100-NPERC)/100)) - NBASE
	ENDCASE

RETURN NRETISS


