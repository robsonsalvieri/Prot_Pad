Local nPPERISS := ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_PERISS"}) // Percentual de ISS
Local nPMOB    := ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_VRMOB"}) // Valor de Mobilização
Local nPDESMOB := ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_VRDES"}) // Valor de Desmobilização
Local nTIPSEG  := ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_TPSEGU"}) // Tipo de Seguro


	//ifranzoi - 29/08/2021
	//Zera os totais para somar novamente
	_nTempX01 := 0
