STATIC FUNCTION GETVALPROJ(CPROJET , CTIPO)
LOCAL AAREA     := GETAREA()
LOCAL NRET		:= 0

	DBSELECTAREA("FPA")
	DBSETORDER(1)
	DBSEEK(XFILIAL("FPA")+CPROJET)
	WHILE !FPA->(EOF()) .AND. FPA->(FPA_FILIAL+FPA_PROJET) == XFILIAL("FPA")+CPROJET
		NRET += FPA->FPA_VRHOR
		IF FPA->FPA_TPGUIM == "C"
			NRET += FPA->FPA_GUIMON
		ENDIF
		IF FPA->FPA_TPGUID == "C"
			NRET += FPA->FPA_GUIDES
		ENDIF
		FPA->(DBSKIP())
	ENDDO

	DBSELECTAREA("FP6")
	DBSETORDER(1)
	DBSEEK(XFILIAL("FP6")+CPROJET)
	WHILE !FP6->(EOF()) .AND. FP6->(FP6_FILIAL+FP6_PROJET) == XFILIAL("FP6")+CPROJET
		NRET += FP6->FP6_VALCOB
		FP6->(DBSKIP())
	ENDDO

	RESTAREA( AAREA )

	aSize(AAREA,0)
	FwFreeArray(AAREA)

RETURN NRET

