/*/

FUNCTION LOCA00189()
LOCAL _AAREAOLD := GETAREA()
LOCAL _CGRPACE  := ""
LOCAL _CQUERY   := ""

	IF SBM->(FIELDPOS("BM_XACESS")) > 0

		/*
		+ XFILIAL("SBM") +
		*/

		_CQUERY := " SELECT DISTINCT BM_GRUPO"
		_CQUERY += " FROM " + RETSQLNAME("SBM") + " SBM"
		_CQUERY += " WHERE  BM_FILIAL = ? "
		_CQUERY +=   " AND  BM_XACESS = '1'"
		_CQUERY +=   " AND  SBM.D_E_L_E_T_ = ''"
		IF SELECT("TRBSBM") > 0
			TRBSBM->(DBCLOSEAREA())
		ENDIF
		_CQUERY := CHANGEQUERY(_CQUERY)
		aBindParam := {XFILIAL("SBM")}
		MPSysOpenQuery(_CQUERY,"TRBSBM",,,aBindParam)

		//TCQUERY _CQUERY NEW ALIAS "TRBSBM"

		WHILE TRBSBM->(!EOF())
			IF EMPTY(_CGRPACE)
				_CGRPACE := TRBSBM->BM_GRUPO
			ELSE
				_CGRPACE += ";" + TRBSBM->BM_GRUPO
			ENDIF
			TRBSBM->(DBSKIP())
		ENDDO
		TRBSBM->(DBCLOSEAREA())

		IF EMPTY(_CGRPACE)
			_CGRPACE := SUPERGETMV("MV_LOCX014",.F.,"")
			SBM->(DbSetOrder(1))
			If SBM->(DbSeek(xFilial("SBM")+SubStr(_CGRPACE,1,4)))
				SBM->(RECLOCK("SBM",.F.))
				SBM->BM_XACESS := "1"
				SBM->(MsUnLock())
			EndIf
		EndIf	

	ELSE
		_CGRPACE := SUPERGETMV("MV_LOCX014",.F.,"")
	ENDIF

	RESTAREA( _AAREAOLD )

	aSize(_AAREAOLD,0)
	FwFreeArray(_AAREAOLD)

RETURN _CGRPACE


