/*/
STATIC FUNCTION TURVAL(OPC , ORET) 
LOCAL NI
LOCAL NUSADO    := 0
LOCAL OSBUTTON1
LOCAL NSTYLE    := GD_INSERT + GD_UPDATE + GD_DELETE
LOCAL AHEADER   := {}
LOCAL ACOLS     := {}
LOCAL CALIAS    := "FPE"
LOCAL CPROJET   := PROJETO()
LOCAL ACPOALTER := {}
LOCAL CCHAVE  := 'DBSEEK(XFILIAL(CALIAS)+CPROJET+COBRA+CSEQGUI)'
LOCAL CCHAVE2 := 'XFILIAL(CALIAS)+CPROJET+COBRA+CSEQGUI'
LOCAL CCHAVE3 := 'FPE_FILIAL+FPE_PROJET+FPE_OBRA+FPE_SEQGUI'
LOCAL NFROTA
Local nPos
Local aCols0 := {}
Local cNaoAlter := "FPE_PROJET,FPE_OBRA  ,FPE_FROTA ,FPE_SEQGUI ,FPE_TURNO ,FPE_MINDIT "
Local nX
Local nObra
Local nPosObra

PRIVATE CTGET2    := STR(FP4->FP4_PREDIA)
PRIVATE LREFRESH  := .T.

	AHEADER := FHEADER("FPE")
	NUSADO  := LEN(AHEADER)

	IF OPC == 2
		ACPOALTER := {}
		NSTYLE    := 0
	ELSE
		for nI := 1 to Len(aHeader)
			if !(aHeader[ nI, 2] $ cNaoAlter)
				Aadd(aCpoAlter, aHeader[ nI, 2])
			endif
		next
	//	ACPOALTER := { "FPE_HRINIT", "FPE_HOFIMT", "FPE_DIASEM", "FPE_PORCEN", "FPE_VALTUR", "FPE_VROPER" }
	ENDIF

	NFROTA  := ORET:ACOLS[ORET:NAT][ASCAN(ORET:AHEADER,{|X|ALLTRIM(X[2])=="FPA_GRUA"})] // ODLGGUI:NAT
	CSEQGUI := ORET:ACOLS[ORET:NAT][ASCAN(ORET:AHEADER,{|X|ALLTRIM(X[2])=="FPA_SEQGRU"})]
	nPosObra := Ascan(oDLGOBR:AHEADER,{|X|ALLTRIM(X[2])=="FP1_OBRA"})

	// Processando Obras
	if LEN(ACOLSTURNO) < Len(oDLGOBR:aCols)
		for nX := 1 to Len(oDLGOBR:aCols)
			if LEN(ACOLSTURNO) = 0 .or. (nItmObra := aScan(ACOLSTURNO, {|x| x[1] = oDLGOBR:aCols[nX, nPosObra]} )) =0
				Aadd(aColsTurno, { oDLGOBR:aCols[nX, nPosObra], {}})
			endif

		next

	endif

	nObra := aScan(aColsTurno, {|x| x[1] = oDLGOBR:aCols[oDLGOBR:nAt, nPosObra]})

	IF LEN(ORET:ACOLS) > LEN(ACOLSTURNO[nObra,2])
		WHILE LEN(ORET:ACOLS) > LEN(ACOLSTURNO[nObra,2])
			AADD(ACOLSTURNO[nObra,2]  , {})
			AADD(OTURNO_COLS , {}) 
		ENDDO 
	ENDIF 

	// Se acumulador de Turnos estiver vazio ou não Contiver o item do contrato
	if Empty(ACOLSTURNO[nObra,2, oRet:nAt])
	DBSELECTAREA(CALIAS)
	FPE->(DBSETORDER(2))
//  AS LINHAS COMENTADAS ABAIXO NÃO ESTÃO SENDO EXECUTADAS, PORÉM AINDA FICA COMO INFORMAÇÃO
		IF DBSEEK(XFILIAL(CALIAS)+CPROJET+COBRA+CSEQGUI)

			WHILE  XFILIAL(CALIAS)+CPROJET+COBRA+CSEQGUI == FPE->(FPE_FILIAL+FPE_PROJET+FPE_OBRA+FPE_SEQGUI)
				ACOLS0 := {}
				FOR NPOS:=1 TO LEN(AHEADER)
					IF !AHEADER[NPOS,10]=="V"  				// X3_CONTEXT
						If alltrim(upper(AHEADER[NPOS,2])) <> "IMAGEM"
							(CALIAS)->(AADD(ACOLS0,FIELDGET(FIELDPOS(AHEADER[NPOS,2]))))
						else
//							AADD(ACOLS0,"")
						EndIF
					ELSE
//						If alltrim(upper(AHEADER[NPOS,2])) <> "IMAGEM"
//							AADD(ACOLS0,CRIAVAR(AHEADER[NPOS,2]))
//						Else
//							AADD(ACOLS0,"")
//						EndIF
					ENDIF
				NEXT
				AADD(ACOLS0,.F.  )  						// DELETED
				AADD(ACOLS,ACOLS0)
				(CALIAS)->(DBSKIP())
		ENDDO 
	ELSE
			IF EMPTY(ACOLS)
				AADD(ACOLS,ARRAY(NUSADO+1))
				FOR NI := 1 TO NUSADO
					ACOLS[1][NI] := CRIAVAR(AHEADER[NI][2])
				NEXT NI 
				ACOLS[1][1]        := CPROJET 	// PROJETO
				ACOLS[1][2]        := COBRA   	// OBRA
				ACOLS[1][3] 	   := NFROTA
				ACOLS[1][4]        := CSEQGUI 	// SEQGUI
				ACOLS[1][5]        := "001"   	// TURNO
				ACOLS[1][NUSADO+1] := .F.
			ENDIF
		ENDIF
	ELSE
//		ACOLS := ACLONE(ACOLSTURNO[nObra, 2, ORET:NAT])
	ENDIF

	REGTOMEMORY("FPE" , IIF(EMPTY(ACOLS) , .T. , .F.)) 

	ODLGVT := MSDIALOG():NEW(000,000,320,780,STR0335,,,,,,,,,.T.) //"MANUTENÇÃO DE TURNOS"
	OGETVT := MSNEWGETDADOS():NEW(05   ,05   ,145    ,390   ,NSTYLE, "LOCA00186()","LOCA00187()","+FPE_TURNO" , ACPOALTER ,  , 7 , /*"FIELDOK"*/ , /*"SPRDEL"*/ , /*"DELOK"*/ , ODLGVT  ,AHEADER,ACOLS)

	OSBUTTON1 := SBUTTON():NEW(147,350,1,{||ODLGVT:END(), TIRDEL(ORET, CSEQGUI , nObra), RETGETD() },ODLGVT,.T.,,)
	OGETVT:BCHANGE := { || TURVAL1()}
	OGETVT:Refresh()
	ODLGVT:ACTIVATE(,,,.T.,,,)

RETURN NIL 


/*/{PROTHEUS.DOC} TURVAL1
ITUP BUSINESS - TOTVS RENTAL
@TYPE FUNCTION
@AUTHOR FRANK ZWARG FUGA
@SINCE 03/12/2020
@HISTORY 03/12/2020, FRANK ZWARG FUGA, FONTE PRODUTIZADO.
ISSUE 3476 - 16/07/2024
/*/
STATIC FUNCTION TURVAL1()		// FUNÇÃO AUXILIAR DO BOTÃO TURNOS.
LOCAL CPROJET  := ""
LOCAL COBRA    := ""
LOCAL CSEQGUI  := ""

	IF LEN(OGETVT:ACOLS) > 1
		CPROJET  := GDFIELDGET("FPE_PROJET",LEN(OGETVT:ACOLS)-1)
		COBRA    := GDFIELDGET("FPE_OBRA"  ,LEN(OGETVT:ACOLS)-1)
		NFROTA   := GDFIELDGET("FPE_FROTA" ,LEN(OGETVT:ACOLS)-1)
		CSEQGUI  := GDFIELDGET("FPE_SEQGUI",LEN(OGETVT:ACOLS)-1)

		GDFIELDPUT("FPE_PROJET", CPROJET ,OGETVT:NAT)
		GDFIELDPUT("FPE_OBRA"  , COBRA   ,OGETVT:NAT)
		GDFIELDPUT("FPE_FROTA" , NFROTA  ,OGETVT:NAT)
		GDFIELDPUT("FPE_SEQGUI", CSEQGUI ,OGETVT:NAT)

		OGETVT:REFRESH()
	ENDIF

RETURN NIL

/*/{PROTHEUS.DOC} DELETZBB
ITUP BUSINESS - TOTVS RENTAL
@TYPE FUNCTION
@AUTHOR FRANK ZWARG FUGA
@HISTORY 03/12/2020, FRANK ZWARG FUGA, FONTE PRODUTIZADO.
/*/
STATIC FUNCTION DELETZBB(CINDICE)
LOCAL AAREA    := GETAREA()
LOCAL AAREAZBB := FPE->(GETAREA())

	DBSELECTAREA("FPE")
	DBSETORDER(2)
	DBSEEK(XFILIAL("FPE")+CINDICE)

	WHILE !FPE->(EOF()) .AND. FPE->(FPE_PROJET+FPE_OBRA+FPE_SEQGUI) == CINDICE
		FPE->(RECLOCK("FPE",.F.))
		FPE->(DBDELETE()) 
		FPE->(MSUNLOCK()) 
		FPE->(DBSKIP())
	ENDDO

	RESTAREA(AAREAZBB)
	RESTAREA(AAREA)

RETURN NIL

