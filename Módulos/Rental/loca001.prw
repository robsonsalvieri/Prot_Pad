/*/

FUNCTION LOCA00111(NINDO , NESTOU , ODLG , OFOLDER)
Local ACOLS
Local LRET := .T.
Local _NX
Local lTurno := .F.
Local cGrPand
Local cProduto

	ACOLS := {}

	SETKEY(VK_F12 , NIL)

	DO CASE
	CASE NESTOU==NFOLDERPLA
		FACUMUPLA()
	CASE NESTOU==NFOLDERRES
		FACUMURES()
	CASE NESTOU==NFOLDERCUS
		FTRAZMEM(.F.) 								// .T.=ATUALIZA VARIÁVEIS  /  .F.=ATUALIZA ODLGCUS:ACOLS
		FACUMUCUS()
	CASE NESTOU==NFOLDERDOC
		FACUMUDOC()
	CASE NESTOU==NFOLDERCNP							// BECHANGE CONJUNTO TRANSPORTADOR PLATAFORMA
		OCNPACOLS:SETDATA(ODLGCNP:ACOLS)
	ENDCASE

	// DSERLOCA-1982 - Frank em 30/04/2024
	If lImplemento
		If nEstou==nFolderImp // DSERLOCA-1982 - Frank em 03/05/2024
			if LOCA001A6()
			SELIMP("SAINDO")
			else
				REturn .F.
			endif
		ElseIf nIndo==nFolderImp // DSERLOCA-1982 - Frank em 03/05/2024
			If !LOCA001A9()
				Return .F.
			Else
				SELIMP("ENTRANDO")
			EndIf
		EndIF
	EndIF

	// DSERLOCA-3409 - Frank em 18/12/24 - Aba Turnos
	If nEstou==nFolderTur
		LOCA001T05("SAINDO")
	EndIf
	If nIndo==nFolderTur
		If SBM->(FIELDPOS("BM_XACESS")) > 0
			cGrPand := LOCA00189()
		Else
			cGrPand := SuperGetMV("MV_LOCX014",.F.,"")
		EndIf

		cProduto := ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_PRODUT"})]

		// Válido para mão de obra, ou base = hora
		If ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_TIPOSE"})] == "M"
			lTurno := .T.
		EndIf
		If ODLGPLA:ACOLS[ODLGPLA:NAT][ASCAN(ODLGPLA:AHEADER,{|X|ALLTRIM(X[2])=="FPA_TPBASE"})] == "H"
			lTurno := .T.
		EndIf

		SB1->(dbSetOrder(1))
		SB1->(dbSeek(xFilial("SB1")+cProduto))
		If alltrim(SB1->B1_GRUPO) $ alltrim(CGRPAND)
			lTurno := .F.
		EndIf

		If !lTurno .and.EXISTBLOCK("LC001TUR") 		// --> Ponto de Entrada para forçar abertura pasta Turnos DSERLOCA 7204
			lTurno := ExecBlock("LC001TUR",.f.,.f.)
		EndIf

		If !lTurno
			Help(Nil,	Nil,STR0029+alltrim(upper(Procname())),; //"RENTAL: "
						Nil,STR0033,1,0,Nil,Nil,Nil,Nil,Nil,; //"Inconsistência nos dados."
						{STR0658}) //"Os turnos estão liberados apenas para os serviços de mão de obra, base de cálculo hora e produtos que não aceitem quantidade > 1."
			Return .F.
		EndIf
		LOCA001T05("ENTRANDO")
	EndIF

	IF VALTYPE(ODLGPLA)=="O" .and. NINDO == NFOLDERPLA
		//FTRAZPLA()
		LOCA00141()
		ODLGPLA:OBROWSE:REFRESH()
	ENDIF

	IF VALTYPE(ODLGRES)=="O"
		FTRAZRES()
		MUDARESB()
		ODLGRES:OBROWSE:REFRESH()
	ENDIF

	IF VALTYPE(ODLGCUS)=="O"
		FTRAZCUS()
		LOCA00139()
		FTRAZMEM(.T.) 								// .T.=ATUALIZA VARIÁVEIS  /  .F.=ATUALIZA ODLGCUS:ACOLS
		FCALCCUS()
		SUMVALRES() 								// SOMA VALORES DAS RESPONSABILIDADES
		ODLGCUS:OBROWSE:REFRESH()
	ENDIF

	IF VALTYPE(ODLGDOC)=="O"
		FTRAZDOC()
		LOCA00146()
		ODLGDOC:OBROWSE:REFRESH()
	ENDIF

	DO CASE
	CASE NINDO == NFOLDERCUS						// ABA BASE DE CALCULO


		FCALCTOTAL()
		FDISPTRAN()
		FDISPGUIN()
		FDISPGRUA()

		CalcCusto()

		_oTempX01:Refresh()
		_oTempX02:Refresh()
		_oTempX03:Refresh()
		_oTempX04:Refresh()
		_oTempX05:Refresh()
		_oTempX06:Refresh()
		_oTempX07:Refresh()
		//_oTempX08:Refresh()
		//_oTempX09:Refresh()
		_oTempX10:Refresh()
		_oTempX11:Refresh()
		//_oTempX12:Refresh()
		_oTempX13:Refresh()
		_oTempX14:Refresh()
		_oTempX15:Refresh()
		_oTempX16:Refresh()
		_oTempX17:Refresh()
		_oTempX18:Refresh()
		_oTempX19:Refresh()

		_oTmprX01:Refresh()
		_oTmprX02:Refresh()
		_oTmprX03:Refresh()
		_oTmprX04:Refresh()
		_oTmprX05:Refresh()
		_oTmprX06:Refresh()
		_oTmprX07:Refresh()
		//_oTmprX08:Refresh()
		//_oTmprX09:Refresh()
		_oTmprX10:Refresh()
		_oTmprX11:Refresh()
		_oTmprX13:Refresh()
		_oTmprX14:Refresh()
		_oTmprX15:Refresh()
		_oTmprX16:Refresh()
		_oTmprX17:Refresh()
		_oTmprX18:Refresh()
		_oTmprX19:Refresh()
		//ifranzoi

	CASE NINDO == NFOLDERCNP						// BECHANGE CONJUNTO PLATAFORMA
		ODLGCNP:ACOLS := ACLONE(OCNPACOLS:GETDATA("FQ7_OBRA=='"+COBRA+"' .AND. FQ7_SEQGUI='"+CSEQGRU+"'"))

		// FRANK 23/09/20
		// TRATAMENTO PARA SABER SE A ZA1 ESTA DELETADA.
		IF ODLGOBR:ACOLS[ODLGOBR:NAT][LEN(ODLGOBR:AHEADER)+1]
			FOR _NX := 1 TO LEN(ODLGCNP:ACOLS)
				ODLGCNP:ACOLS[_NX][LEN(ODLGCNP:AHEADER)+1] := .T.
			NEXT
		ELSE
			FOR _NX := 1 TO LEN(ODLGCNP:ACOLS)
