	If upper(alltrim(ReadVar())) == "M->FPA_DTINI"
		dIni := M->FPA_DTINI
	Else
		dIni := oDlgPla:aCols[oDlgPla:nAt][aScan( oDlgPla:aHeader, { |x| AllTrim(x[02]) == "FPA_DTINI" } )]
	EndIF
	If upper(alltrim(ReadVar())) == "M->FPA_DTENRE"
		dFim := M->FPA_DTENRE
	Else
		dFim := oDlgPla:aCols[oDlgPla:nAt][aScan( oDlgPla:aHeader, { |x| AllTrim(x[02]) == "FPA_DTENRE" } )]
	EndIf
	If upper(alltrim(ReadVar())) == "M->FPA_DTFIM"
		dFat  := M->FPA_DTFIM
	Else
		dFat := oDlgPla:aCols[oDlgPla:nAt][aScan( oDlgPla:aHeader, { |x| AllTrim(x[02]) == "FPA_DTFIM" } )]
	EndIf

	//If empty(dGeraEm)
		dTemp := dFat
	//Else
	//	dTemp := dGeraEm
	//EndIf

	nMes  := 0

	// Passo 1 - Verificar os ciclos de faturamento que podem existir neste período
	While dTemp <= dFim
		if month(dTemp) <> nMes
			nMes := month(dTemp)
			aadd(aPeriodos,{dTemp})
		endif
		dTemp ++
	EndDo

	For nX:=1 to len(aPeriodos) // deixar todos os registros com o dia do próximo faturamento
		If day(lastday(ctod("01/"+str(month(aPeriodos[nX,1]))+"/"+str(year(aPeriodos[nX,1]))))) < day(dFat)
			aPeriodos[nX,1] := lastday(ctod("01/"+str(month(aPeriodos[nX,1]))+"/"+str(year(aPeriodos[nX,1]))))
		Else
			aPeriodos[nX,1] := ctod(str(day(dFat))+"/"+str(month(aPeriodos[nX,1]))+"/"+str(year(aPeriodos[nX,1])))
		EndIF
		// se a data final for maior que o último periodo gera mais uma parcela com vencimento na data final / 				
		If nX==len(aPeriodos)
			If dFim > aPeriodos[nX,1]
				aadd(aPeriodos,{dFim})
				Exit
			EndIf
		EndIf
	Next

	nRetTOT := len(aPeriodos)


Return nRetTOT


