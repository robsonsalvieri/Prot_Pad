				_MV_LOC278 := .T.
			Else
				_MV_LOC278 := .F.
			EndIf
		EndIf

		If _MV_LOC278	

			// Deleta os títulos provisórios
			LOCA013DEL(1, FP0->FP0_PROJET, "")

			/*FQB->(dbSetOrder(1))
			FQB->(dbSeek(xFilial("FQB")+FP0->FP0_PROJET))
			While !FQB->(Eof()) .and. FQB->FQB_FILIAL == xFilial("FQB") .and. FQB->FQB_PROJET == FP0->FP0_PROJET
