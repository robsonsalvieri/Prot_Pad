		If OPLA_COLS[_NX][nPTIPOSE] <> "S"
			_nTempX01 += OPLA_COLS[_NX][nPPREDIA]
			_nTempX02 += OPLA_COLS[_NX][nPQUANT]
			_nTempX03 += OPLA_COLS[_NX][nPACRESC]
			_nTempX04 += OPLA_COLS[_NX][nPPRCUNI]
			_nTempX05 += OPLA_COLS[_NX][nPVLBRUT]
			_nTempX06 += OPLA_COLS[_NX][nPPDESC]
			_nTempX07 += OPLA_COLS[_NX][nPVRHOR]
			_cTempX08 := OPLA_COLS[_NX][nPTPGUIM]
			_cTempX09 := OPLA_COLS[_NX][nPTPGUID]
			_nTempX10 += OPLA_COLS[_NX][nPGUIMON] + if( nPMOB >0, OPLA_COLS[_NX][nPMOB], 0)
			_nTempX11 += OPLA_COLS[_NX][nPGUIDES] + if( nPDESMOB >0, OPLA_COLS[_NX][nPDESMOB] , 0)
			_nTempX15 += OPLA_COLS[_NX][nPVRSEGU]

