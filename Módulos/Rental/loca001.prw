STATIC FUNCTION FTRAZCBOX(CCAMPO,CCOD)
LOCAL NPOS
LOCAL CCOMBO
LOCAL CRET := CCOD

	CCOMBO := ALLTRIM(GETSX3CACHE(CCAMPO,"X3_CBOX"))
	NPOS   := AT(CRET+"=" , CCOMBO)
	IF NPOS > 0
		CRET := SUBSTR(CCOMBO,NPOS+2,LEN(CCOMBO))
		NPOS := AT(";" , CRET)
		IF NPOS>0
			CRET := SUBSTR(CRET,1,NPOS-1)
		ENDIF
	ENDIF

RETURN CRET


