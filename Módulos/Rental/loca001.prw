		OBTNOK   := TBUTTON():NEW(45,10,STR0338,ODLG,{||LGRV:=.T.,ODLG:END()},35,12,,,,.T.) //"Gravar"
		OBTNCANC := TBUTTON():NEW(45,52,STR0339,ODLG,{||ODLG:END()},35,12,,,,.T.) //"Fechar"
