	for nX := 1 to Len(ODLGIMP:aCols)

		// Se linha não estiver deletada
		if !Atail(ODLGIMP:aCols[nX])
		// Se for gerar OS
			if ODLGIMP:aCols[nX, nPosGerOS] = "S"
				// Validar se tem serviço associado
				if Empty(ODLGIMP:aCols[nX, nPosServi])
					Help(Nil,	Nil,STR0029+alltrim(upper(Procname())),; //"RENTAL: "
