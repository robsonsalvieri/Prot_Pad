	/*//////////////////////////////////////////////////////////////////////
	// fVldQtd
	//
	// valida quantidade informada
	*///////////////////////////////////////////////////////////////////////
	function fVldQtd(obj){

		if ($( obj ).hasClass( "money" )){

			if(obj.value.search("," != -1))
				obj.value = obj.value.replace(",", ".");
		
			if(obj.value <= 0.01)
				obj.value = 0.00;

			if(obj.value.search("." != -1))
				obj.value = obj.value.replace(".", ",");

		}else{
			if(obj.value <= 1)
				obj.value = 1;
		}

	}

