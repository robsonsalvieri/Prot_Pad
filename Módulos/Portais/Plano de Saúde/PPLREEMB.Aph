	/*//////////////////////////////////////////////////////////////////////
	// getRecnoDivs
	//
	// Retorna o recno da div
	*///////////////////////////////////////////////////////////////////////
	function getRecnoDivs(){
		var recno_divs = document.getElementsByClassName('recno_despesas');
		var recnos = "";
		if((recno_divs.length) > 0){
			
			var x = "";
			//pego apenas os values
			[].forEach.call(recno_divs, function(i) {
    			if(i.value > 0)
