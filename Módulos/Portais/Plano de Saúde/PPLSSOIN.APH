function CarregaDadInt(v) {   
    var cPSol 		= "";
    var cNSol 		= "";
    var aMatCabIte 	= v.split("<");
    var aMatCab 	= aMatCabIte[0].split("|");
    //зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
