function CarregaProSaudeFil(v) {
    var aResult = v.split("|"); 
    var i = 0;
    var j = 0;
    var lEntrou = false;                    
    //зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд
