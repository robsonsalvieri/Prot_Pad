  		document.getElementById("cFace").value = "";  
  		document.getElementById("cFaceNova").value = ""; 
        document.getElementById("cFaceNova").focus();
          
    }
    else {
        if (aResult[4] != ""){
            document.getElementById("cFace").value = aResult[4];    //recebe os digitos das faces digitadas em ordem alfabetica
        }
        else{
