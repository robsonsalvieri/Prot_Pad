function RetValFace(v) {
	var aResult = v.split("|");
    
    if (aResult[0] == "1") { 
    	aResDen = "1";
  		document.getElementById("cFlagDente").value = "1";	
  	}
  	else {
  		document.getElementById("cFlagDente").value = "";    
  		if (document.getElementById("cDenteReg").value != "1"){
  			aResDen = "";
  		}
  	}
  	
    if (aResult[1] != "true") { 
