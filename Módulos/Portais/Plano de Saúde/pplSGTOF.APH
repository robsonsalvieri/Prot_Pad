function fChkFace(str) {
    var astr = str.split("");
    var i;

    for(i = 0; i < astr.length; i++) {
        // ..se o índice do valor atual "indexOf(arr[i])"
        // for diferente do índice corrente "i"
        // retornamos "true"
        if(astr.indexOf(astr[i]) != i) {
            alert("Não é permitido inserir caracteres repetidos!");
            document.getElementById("cFaceNova").value = "";
            document.getElementById("cFaceNova").focus();
            return false;
        };
    }
       
	var cCodPad = document.getElementById("cCodPadSE").value;
	var cCodPro = document.getElementById("cCodProSE").value; 
	var cDenteAux  = document.getElementById("cDente").value;  
	var cFaceAux   = document.getElementById("cFaceNova").value.toUpperCase().split("").join(",");

        Ajax.open("W_PPLVLDDEFA.APW?cTipo=F&cCodPad=" + cCodPad + "&cCodPro=" + cCodPro + "&cCodDente=" + cDenteAux + "&cCodFace=" + cFaceAux, { 
