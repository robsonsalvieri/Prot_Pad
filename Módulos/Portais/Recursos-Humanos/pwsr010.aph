   			html += '<option value=' + "<%= HttpSession->GetTables[1]:oWsListOfCity:oWsCity[m]:cCodCityOri %>"  + ">" + "<%= AllTrim(HttpSession->GetTables[1]:oWsListOfCity:oWsCity[m]:cCityOri) %>" + "</option>";
