			document.forms[0].cAreaCode.value 		= ReplaceAll( document.forms[0].cAreaCode.value, '>', '' );

			if(Trim(document.forms[0].cCompany.value) == '' )
			{
				//alert('<%=STR0002%>');	//"Digite o nome da empresa."
				document.getElementById("cCompany").style.backgroundColor = "#FFF2F0";
				document.getElementById("cCompany").style.border = "solid 1px #D3362C";
