					<td width="130" class="FundoConteudo"><span class="TituloMenor"><%=STR0057%></span><span class="links-detalhes-parcial">*</span></td><!-- /* "Instituição de Ensino:" */ -->                                         
					<td width="400" class="FundoConteudo">
						<span class="TituloMenor">
							<input name="cEntity3Code"  ID="cEntity3Code"  type="text" class="texto" size="5" value="<%=HttpPost->cEntity3Code%>" readOnly="readOnly" onBlur="return limparMsg('cEntity3Code', 'cEntity3CodeAlert')">
							<img   name="oEntity3F3"    ID="oEntity3F3"    src="imagens-rh/ico-zoom.gif" border="0" onclick="openSearch('W_PWSRF3.APW?F3Nome=BRWENTITY&CMPWS1=CENTITYCODE&CMPDEST1=cEntity3Code&CMPWS2=CENTITYDESCRIPTION&CMPDEST2=cEntity3Desc&GRIDF31=CENTITYCODE&GRIDF32=CENTITYDESCRIPTION&Tipo=4');" align="absmiddle" alt="Busca">
			   	      		<input name="cEntity3Desc"  ID="cEntity3Desc"  type="text" class="texto" size="40" value="<%=HttpPost->cEntity3Desc%>" readOnly="readOnly" maxlength="40">
							<input name="lEntity3Other" ID="lEntity3Other" type="checkbox" value=".T." onclick="showOther('lEntity3Other', 'cEntity3Code', 'cEntity3Desc', 'oEntity3F3', true, 'cEntity3Desc', '1');" <%=If(Type('HttpPost->lEntity3Other') == 'L' .AND. HttpPost->lEntity3Other == .T., 'checked="checked"', '')%> /><span class="texto"><%=STR0069%></span>		<!--"Outra"-->
						</span>
						<br /><span class="alertas" id="cEntity3CodeAlert"></span>
					</td>
			  	</tr>
			  	<!--INCLUSAO DE USERFIELD PARA CURSOS RELEVANTES-->
				<% For m:=1 To Len(HttpPost->GETCURRICULUM[1]:oWSUSERFIELDCOUR:oWSUSERFIELD) %> 
				  	<tr>
						<td width="130" class="FundoConteudo"><span class="TituloMenor"><%=HttpPost->GETCURRICULUM[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserTitle%></span></td>                                         
						<td width="400" class="FundoConteudo">
							<span class="TituloMenor">
								<% If HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserType == "D" %>
									<input type="text" name="cr<%=AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m))%>" class="texto" value="<%=IIf(Empty(&('HttpPost->cr'+AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m)))),&('HttpPost->cr'+AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m))),SToD(&('HttpPost->cr'+AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m)))))%>" onKeyPress="return data(this)" size="12" maxlength="10">&nbsp;&nbsp;<span class="texto"><%=STR0049%><!-- /* "(DD/MM/AAAA)" */ --></span> 
								<% ElseIf !Empty(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUSERCOMBOBOX) %>
									<select name="cr<%=AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m))%>" id="cr<%=AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m))%>" class="combo">                 
										<option value=""></option>
										<%	aDados := separa(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUSERCOMBOBOX,";") %>
										<% For nI := 1 to len(aDados) %>
										   	<option value="<%=subs( aDados[nI],1,at('=',aDados[nI])-1 )%>" <%=IIf( &('HttpPost->cr'+AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m))) == subs( aDados[nI],1,at('=',aDados[nI])-1 ), "selected", )%> ><%=AllTrim(subs(aDados[nI],at('=',aDados[nI])+1,len(aDados[nI])-at('=',aDados[nI])))%></option>
										<% Next nI %>
									</select>
								<% ElseIf HttpPost->GetCurriculum[1]:OWSUSERFIELDHIST:oWSUSERFIELD[m]:cUserType == "N" %> 
									<input type="text" name="cr<%=AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m))%>" class="texto" value="<%=AllTrim(&('HttpPost->cr'+AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m))))%>" size="<%=HttpPost->GETCURRICULUM[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:nUSERSIZE%>" maxlength="<%=HttpPost->GETCURRICULUM[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:nUSERSIZE%>" onKeyUp="Picture( event, this, '@E 999999,99', 'N', aBuffer )">
								<% Else %>
								  	<input type="text" name="cr<%=AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m))%>" class="texto" value="<%=AllTrim(&('HttpPost->cr'+AllTrim(HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:cUserName)+AllTrim(str(m))))%>" size="<%=HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:nUSERSIZE%>" maxlength="<%=HttpPost->GetCurriculum[1]:oWSUSERFIELDCOUR:oWSUSERFIELD[m]:nUSERSIZE%>">
								<% EndIf %>
							</span>
						</td>
				  	</tr>						
				<% Next m %>
					<tr> 
				  		<td align="right" colspan="2" class="TituloMenor">
				  			<%=STR0068%><!-- /* "Gravar Curso" */ -->          
							<input name="Incluir_Curso" type="button" class="Botoes" onClick="fSubmit( 3, 0, '1' )" value=<%="+"%>><!-- /* "Confirmar Item" */ -->
						</td>
					</tr> 	
					<tr> 
		          		<td height='20' colspan="2" class="titulo"><img src="imagens-rh/linha.gif" width="100%" height="1"></td>
					</tr>
			<% EndIf %>		
<!--/* 
-------------------------------------------------------------------------------------------------------------------------------
 Fim do Curso
------------------------------------------------------------------------------------------------------------------------------- 
*/-->				
					
					 				        
					<tr>
						<td width="130" height='40'></td>                                         
						<td width="400" align="right" height='40' class="TituloMenor">
							<input name="Gravar1" type="button" class="Botoes" onClick="fEnvia()" value=<%=STR0013%>><!-- /* "Gravar Curriculo" */ -->
						</td>
					</tr>     							
				</table>
			</td>
		</tr>
	</table>								    
</form>	
