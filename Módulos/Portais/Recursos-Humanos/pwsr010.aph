				document.forms[0].cCompany.focus();
				return;
			}
			
 
	       	if(Trim(document.forms[0].cFunctionCode.value) == '' )
			{
				//alert('<%=STR0004%>');	//"Informe a função."
				document.getElementById("cFunctionCode").style.backgroundColor = "#FFF2F0";
				document.getElementById("cFunctionCode").style.border = "solid 1px #D3362C";
				document.getElementById("cFunctionCodeAlert").innerHTML = "<%=STR0004%>";
				document.forms[0].cFunctionCode.value == '';
				document.forms[0].cFunctionCode.focus();
				return;
			} 
			

			if(Trim(cDatAdm) == '' )
			{
				//alert('<%=STR0003%>');	//"Digite a Data de Admissão."
				document.getElementById("dAdmissionDate").style.backgroundColor = "#FFF2F0";
				document.getElementById("dAdmissionDate").style.border = "solid 1px #D3362C";
