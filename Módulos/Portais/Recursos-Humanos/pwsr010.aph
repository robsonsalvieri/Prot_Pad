						return;
					}  
					
				if(MyDtos(cDatDemis) > MyDtos(cDatAtual))  	
			      	{
						//alert('<%=STR0073%>');	
						document.getElementById("dDismissalDate").style.backgroundColor = "#FFF2F0";
						document.getElementById("dDismissalDate").style.border = "solid 1px #D3362C";
						document.getElementById("dDismissalDateAlert").innerHTML = "<%=STR0073%>";
						document.forms[0].dDismissalDate.value == '';
						document.forms[0].dDismissalDate.focus();
						return;
					} 
		    }
	         
	       	if( document.forms[0].cActivity.value.length >= 18225 ) //18225 caracteres equivale aproximadamente a 5 paginas de texto no Word
			{
				alert('<%=STR0080%>');	//"Favor reduzir a quantidade de informações digitada."
				document.forms[0].cActivity.focus();
				return;
			}		
	return true;
}

