function validaData( cData1 )

{//data
	        var ano1 = cData1.substring(6,10);
            var oData = new Date()   ;
            var ano2 = oData.getFullYear().toString();                           
            
			if ( (cData1 == null) || (cData1.length < 10) ) {
			      return false;
			}           
            
            if(ano1 >= ano2){
              return false;
            }
              return true;
		         
          
}

