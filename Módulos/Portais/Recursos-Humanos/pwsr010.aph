    var cDatAtual 	=  <%= '"'+Dtoc(Date())+ '"' %> ;
    
	document.forms[0].nI.value = nLinha;
	document.forms[0].cTipo.value = cTpOpc;
	
	if ( cOpcao == 2 )
	{ 
        
 	 	cDatAdm = document.forms[0].dAdmissionDate.value;     
		cDatDemis = document.forms[0].dDismissalDate.value ;

	    if ( cTpOpc == '1' )
        {
			if (!ValidHist()){
				return;
			}

			document.forms[0].action = "W_PWSR012.APW";

		}else if (cTpOpc == '2') {
			if(document.formAtuCurric.cCompany.value != ""){
				document.formAtuCurric.cCompany.required = "true";
