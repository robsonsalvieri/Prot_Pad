					document.forms[0].dAdmissionDate.focus();
					return;
			     }
			

		  	if(Trim(cDatDemis) != '' )
			{
		     	
		     	  if (ValData(document.forms[0].dDismissalDate.value)== false )
				     {
				     	document.forms[0].dDismissalDate.value = '';
						document.forms[0].dDismissalDate.focus();
						return;
				     }
	     	
		     	
		     	if(MyDtos(cDatAdm) > MyDtos(cDatDemis))  
					{
						//alert('<%=STR0072%>');	//"Data de saída/demissão não pode ser menor que a data de admissão"
						document.getElementById("dDismissalDate").style.backgroundColor = "#FFF2F0";
						document.getElementById("dDismissalDate").style.border = "solid 1px #D3362C";
						document.getElementById("dDismissalDateAlert").innerHTML = "<%=STR0072%>";
						document.forms[0].dDismissalDate.value == '';
