function fFiltraUF(cUF){
	var oCidades = document.getElementById("cCodCityOri");
	var html = "<option value=''></option>";
	
	oCidades.length = 0;

   	<% for m := 1 to Len(HttpSession->GetTables[1]:oWsListOfCity:oWsCity) %>
