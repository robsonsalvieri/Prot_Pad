				document.formAtuCurric.dAdmissionDate.required = "true";
				alert('<%=OemToAnsi(STR0128)%>');
				return;
			}else {
				document.forms[0].action = "W_PWSR012.APW";
			}
		}else if (cTpOpc == '3') {
			document.forms[0].action = "W_PWSR012.APW";
