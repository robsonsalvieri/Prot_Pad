	oCidades.innerHTML = html;	

}

function makeRequest5() {

        http_request = false;
        
        var oUf = document.getElementById("cOrigin");
        var cUf = oUf.value;
        
        if (window.XMLHttpRequest) { // Mozilla, Safari,...
            http_request = new XMLHttpRequest();
            if (http_request.overrideMimeType) {
                http_request.overrideMimeType('text/xml');
                // See note below about this line 

            }
        } else if (window.ActiveXObject) { // IE
            try {
                http_request = new ActiveXObject("Msxml2.XMLHTTP");
            } catch (e) {
                try {
                    http_request = new ActiveXObject("Microsoft.XMLHTTP");
                } catch (e) {}
            }
        }

        if (!http_request) {
            alert('Giving up :( Cannot create an XMLHTTP instance');
            return false;
        }                           
        url = "W_PWSR018.APW?cChave="+cUf;
        http_request.onreadystatechange = alertContents5;
        http_request.open('GET', url, true);
        http_request.send(null);


    }

    function alertContents5() {
        if (http_request.readyState == 4) {
            if (http_request.status == 200) {
				if (window.ActiveXObject) { // IE
				   var xmldoc = new ActiveXObject("Microsoft.XMLDOM"); 
  				   xmldoc.async = false; 
				   xmldoc.loadXML(unescape(http_request.responseText.replace(/\+/g," ")));
	             }else{
					  var xmldoc = http_request.responseXML;
		         }

		           if(Trim(http_request.responseText)=='')
			       {
			           document.getElementById("cCodCityOri").innerHTML ="";
					   document.getElementById("cCodCityOri").style.display='none';
					}else
					{
			           document.getElementById("cCodCityOri").innerHTML =http_request.responseText;
					   document.getElementById("cCodCityOri").style.display='';                    
					}
                  
            } else {
                alert('There was a problem with the request.');
            }
        }
                         
	}

function openSearch(url) {
	var left = Math.floor( (screen.width - 700) / 2);
	var top = Math.floor( (screen.height - 400) / 2);
	var params = "top=" + top + ",left=" + left + ",height=400,width=700,scrollbars=yes";
	var oForm = new xform(document.forms[0]);
	var win = window.open(url, 'jF3', params);
   
	if (parseInt(navigator.appVersion) >= 4)
		win.window.focus();
		
	return win;
}

function showOther(cChecked, cTxtCode, cTxtDesc, oImgF3, lClear, cDesc, cTipo) {
	var oTxtCode = document.getElementById(cTxtCode);	
	var oTxtDesc = document.getElementById(cTxtDesc);
	var oImgF3 = document.getElementById(oImgF3);
	var oDesc = document.getElementById(cDesc);	
	var oCheck = document.getElementById(cChecked);	
	
	if ((oTxtCode != null) && 
		(oTxtDesc != null) && 
		(oImgF3 != null) ) {
		if( cTipo != '2' && cChecked.substring(0,7) == "lCourse")
		{
 			if(Trim(oTxtDesc.value) != Trim('<%=STR0006%>')  ) //"Outros"
			{
				oCheck.disabled = true;
				oCheck.checked = false;
				oDesc.style.display = 'none';
				return
			}
			else
			{
				oCheck.disabled = false;
				return
			
			}
		}
		if( oCheck.checked == true ) {
			if(cTipo != '2') {
				oTxtDesc.size='53';
				oTxtDesc.removeAttribute('readOnly', 'readOnly'); 
				oTxtCode.style.display = 'none';
				oImgF3.style.display = 'none';
			}
			else {
				if(	oTxtDesc.value == '' || Trim(oTxtDesc.value) != Trim('<%=STR0006%>') ) //"Outros"
				{
					alert("<%=STR0075%>");	//"Opcao 'Outro' so eh permitida se selecionar o curso 'Outros'"
					oCheck.checked = false;
					oTxtCode.focus();
					return;
				} 
				oDesc.style.display = '';
			}
		}
		else {
			if (cTipo != '2') {
				oTxtDesc.size='40';
				oTxtDesc.setAttribute('readOnly', 'readOnly');
				oTxtCode.style.display = '';
				oImgF3.style.display = '';
			}
			else {
				oDesc.style.display = 'none';
			}
		}
	}
	
	if (lClear) {
		oDesc.value = '';		
			
		if (cTipo != '2') {
			oTxtDesc.value = '';
			oTxtCode.value = '';
			}
	}
} 

function ValidHist()
{
	var cDatAdm 	= '';     
