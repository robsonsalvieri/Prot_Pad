Static Function ModelDef()

	Local oStruA70 := FWFormStruct( 1, 'A70', /*bAvalCampo*/, /*lViewUsado*/ )
	Local oModel   as object

// Cria o objeto do Modelo de Dados
	oModel := MPFormModel():New( 'GCCA018', /*bPreValidacao*/, /*bPosValidacao*/, /*bCommit*/, /*bCancel*/ )

// Crio a Enchoice com os campos do cadastro
	oModel:AddFields( 'A70MASTER', /*cOwner*/, oStruA70 )

// Adiciona a chave primaria da tabela principal
	oModel:SetPrimaryKey({ "A70_FILIAL" , "A70_CODIGO" })

// Preencho a descri็ใo da entidade
	oModel:GetModel('A70MASTER'):SetDescription( STR0009 ) // Dados da religiใo

Return(oModel)

