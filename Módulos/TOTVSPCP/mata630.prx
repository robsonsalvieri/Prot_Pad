Function A630F4(nOpcx)
Local nx,cOper,cRecPrin, cFerram,cCTrab, i
Local nHdl := GetFocus()
Local lRet := .T.
Private nAlias,nUsadoAux, nAcolsPrinc
Private aHeadOld:={},aColsOld:={}

If ReadVar() == "M->G2_RECURSO" .Or. ReadVar() == "M->G2_FERRAM" .Or. ( TipoAps(.F.,"DRUMMER") .And. ReadVar() == "M->G2_CTRAB" )

	//зддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё	Salva area de trabalho anterior                   Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддды
	nAlias	 := Alias()
	nAcolsPrinc := n

	//здддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Arrays auxiliares para armazenar a getdados      Ё
	//Ё principal.                                       Ё
	//юдддддддддддддддддддддддддддддддддддддддддддддддддды
	aHeadOld:=Aclone(aHeader)
	aColsOld:=Aclone(aCols)

	//зддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Salva linha da primeira GetDados.                 Ё
	//Ё Salva variavel p/ HotArea.                        Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддды
	nUsadoAux:=nUsado

EndIf

If ReadVar() == "M->G2_RECURSO" .or. __lAutomacao

	While .T.

		For nx := 1 To Len(aHeader)
			Do Case
				Case Trim(aHeader[nx][2]) == "G2_OPERAC"
					cOper := aCols[n][nx]
				Case Trim(aHeader[nx][2]) == "G2_RECURSO"
					cRecPrin := aCols[n][nx]
			EndCase
		Next nx

		//здддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Verifica se a opera┤└o e o recurso n└o est└o vazios. Ё
		//юдддддддддддддддддддддддддддддддддддддддддддддддддддддды
		IF Empty(cOper) .Or. Empty(cRecPrin)
			Help(" ",1,"A630F4")
			Exit
		Else
			//здддддддддддддддддддддддддддддддддддддддддддддддддддддд©
			//Ё Se for uma inclus└o.                                 Ё
			//юдддддддддддддддддддддддддддддддддддддддддддддддддддддды
			If nOpcx = 3
				//зддддддддддддддддддддддддддддддддддддддддддддддддддддд©
				//ЁVerifica se Array de recursos Alternativos ja existe.Ё
				//юддддддддддддддддддддддддддддддддддддддддддддддддддддды
				If Empty(aColsRec)
					For i:= 1 to n
						AADD(aColsRec,{})
					Next i
				Else
					//зддддддддддддддддддддддддддддддддддддддддддддддддддддд©
					//ЁVerifica se Array de recursos Alternativos tem o	    Ё
					//Ёmesmo tamanho da posi┤└o atual de aCols.			    Ё
					//юддддддддддддддддддддддддддддддддддддддддддддддддддддды
					If Len(aColsRec) < n
						For i:= (Len(aColsRec)+1) to n
							AADD(aColsRec,{})
						Next i
					EndIf
				EndIf

				//зддддддддддддддддддддддддддддддддддддддддддддддддддддд©
				//ЁVerifica se a posi┤└o correspondente a linha do aColsЁ
				//Ё esta vazia no  Array  de Recursos Alternativos.	    Ё
				//юддддддддддддддддддддддддддддддддддддддддддддддддддддды
				If Empty(aColsRec[n])
					//зддддддддддддддддддддддддддддддддддддддддддддддддддддд©
					//ЁSe .T., liga o flag p/ criar um Acols em branco.	    Ё
					//юддддддддддддддддддддддддддддддддддддддддддддддддддддды
					aCols:={}
					lPrim:=.F.
				Else
