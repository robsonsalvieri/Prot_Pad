Static Function A630DelSHJ(c630Prod,c630Cod,c630Oper)
LOCAL  cSeek
If SuperGetMV("MV_APS",.F.,"")=="DRUMMER" .And. SX2->( MsSeek("SHJ") )
	Begin Transaction

		//здддддддддддддддддддддддддддддддддддддддддд©
		//Ё Deleta recursos restritivos da operacao  Ё
		//Ё quando existir integracao                Ё
		//юдддддддддддддддддддддддддддддддддддддддддды
		dbSelectArea("SHJ")
		dbSetOrder(1)
		If c630Oper == NIL
			cSeek:=xFilial("SHJ")+c630Cod+c630Prod
			dbSeek(cSeek)
			While !Eof() .And. cSeek==HJ_FILIAL+HJ_ROTEIRO+HJ_PRODUTO
				RecLock("SHJ",.F.,.T.)
				dbDelete()
				dbSkip()
			End
		Else
			cSeek:=xFilial("SHJ")+c630Cod+c630Prod+c630Oper
			dbSeek(cSeek)
			While !Eof() .And. cSeek==HJ_FILIAL+HJ_ROTEIRO+HJ_PRODUTO+HJ_OPERAC
				RecLock("SHJ",.F.,.T.)
				dbDelete()
				dbSkip()
			End
		EndIf
	End Transaction
EndIf
Return

