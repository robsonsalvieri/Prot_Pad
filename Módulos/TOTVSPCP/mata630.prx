//Ё 4. Tipo de Transa┤└o a ser efetuada							 Ё
//Ё	 1 - Pesquisa e Posiciona em um Banco de Dados				 Ё
//Ё	 2 - Simplesmente Mostra os Campos							 Ё
//Ё	 3 - Incluir registros no Bancos de Dados 					 Ё
//Ё	 4 - Altera o registro corrente								 Ё
//Ё	 5 - Remove o registro corrente do Banco de Dados			 Ё
//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
PRIVATE aRotina := MenuDef()
Private aSize   :={}
Private aInfo   :={}
Private aMemos  :={}
Private aObjects:={}
Private aPosObj :={}

Private aItensChk :=  {}

AADD(aObjects,{100,If(SuperGetMV("MV_UNILIN",.F.,.F.),040,020),.T.,.F.,.F.})
AADD(aObjects,{100,100,.T.,.T.,.F.})
aSize:=MsAdvSize()
aInfo:={aSize[1],aSize[2],aSize[3],aSize[4],3,3}
aPosObj:=MsObjSize(aInfo,aObjects)

//Seleciona tabela utilizada na validaГЦo da integraГЦo com InspeГЦo de Processo.
dbSelectArea("QP6")

If TipoAps()
	Aadd(aRotina, {STR0028, "A630RecOpe", 0, 4}) //"TempoxRecurso"
	Aadd(aRotina, {STR0029, "A630FerOpe", 0, 4}) //"Ferram.xOper."
Endif

cCadastro := OemToAnsi(STR0006)	//"Roteiro de Opera┤■es"

//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
//Ё Ponto de entrada para adicao de campos memo do usuario   Ё
//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
If ExistBlock( "MA630MEM" )
	If Valtype(	aMemUser := ExecBlock( "MA630MEM", .F., .F. ) ) == "A"
		aEval( aMemUser, { |x| aAdd( aMemos, x ) } )
	EndIf
EndIf

//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
//Ё Ponto de Entrada para filtrar tabela SG2                 Ё
//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
If lMTA630Fil
	cFiltraSG2 := ExecBlock("MTA630FIL",.F.,.F.)
	If ValType(cFiltraSG2) == "C" .And. !Empty(cFiltraSG2)
		bFiltraBrw := {|| FilBrowse("SG2",@aIndexSG2,@cFiltraSG2)}
		Eval(bFiltraBrw)
	Else
		lMTA630Fil := .F.
	Endif
EndIf

//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
//Ё Endereca a funcao de BROWSE							     Ё
//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
dbSelectArea("SG2")
dbGoTop()

If !__lAutomacao
mBrowse( 6, 1,22,75,"SG2" )
EndIf
//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
//Ё Desfaz o filtro tabela SG2                               Ё
//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
If lMTA630Fil
	EndFilBrw("SG2",aIndexSG2)
Endif

Return

