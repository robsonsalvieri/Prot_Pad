*/
Function A630Apaga(c630Prod,c630Cod,nSavRec)
Local nDelSG2 := 0, nDelSH3 := 0,cSeek
Local bSeekWhile
//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
//Ё Indica a Integracao com o Inspecao de Processos (SIGAQIP)Ё
//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
Local lQipMat := IntQIP(c630Prod)

Begin Transaction

	//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Exclusao da Integracao PCP x QIP						 Ё
	//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	If lQipMat
		QAtuMatQIP(c630Prod,,c630Cod,"PCP",.T.)
	EndIf

	dbGoTo(nSavRec)
	//здддддддддддддддддддддддддддддддддддддддддддддддддд©
	//ЁDeleta registros da operacao em SG2 				 Ё
	//юдддддддддддддддддддддддддддддддддддддддддддддддддды

	If Empty(c630RefGrd)
		bSeekWhile	:= { || SG2->(G2_FILIAL+G2_PRODUTO+G2_CODIGO) == xFilial("SG2")+c630Prod+c630Cod }
		SG2->( DbSetOrder(1) )
		DbSeek(xFilial("SG2")+c630Prod+c630Cod)
	Else
		bSeekWhile	:= { || SG2->(G2_FILIAL+G2_REFGRD+G2_CODIGO) == xFilial("SG2")+c630RefGrd+c630Cod }
		SG2->( DbSetOrder(7) )
		DbSeek(xFilial("SG2")+c630RefGrd+c630Cod)
	EndIf

	While !Eof() .And. Eval( bSeekWhile )
		dbSelectArea("SH3")
		cSeek:=xFilial("SH3")+SG2->G2_PRODUTO+SG2->G2_CODIGO+SG2->G2_OPERAC
		dbSeek(cSeek)
		//здддддддддддддддддддддддддддддддддддддддддд©
		//Ё Deleta maquinas alternativas da operacao Ё
		//юдддддддддддддддддддддддддддддддддддддддддды
		While !Eof() .And. cSeek==H3_FILIAL+H3_PRODUTO+H3_CODIGO+H3_OPERAC
			RecLock("SH3",.F.,.T.)
			dbDelete()
			nDelSH3++
			dbSkip()
		End

		//здддддддддддддддддддддддддддддддддддддддддд©
		//Ё Deleta recursos restritivos da operacao  Ё
		//Ё quando existir integracao                Ё
		//юдддддддддддддддддддддддддддддддддддддддддды
		A630DelSHJ(c630Prod,c630Cod,SG2->G2_OPERAC)

		dbSelectArea("SG2")
		RecLock("SG2",.F.,.T.)
		dbDelete()
		nDelSG2++
		dbSkip()

		//здддддддддддддддддддддддддддддддддддддддддд©
		//Ё Deleta checklist da operacao 			 Ё
		//Ё quando estiver cadastrado                Ё
		//юдддддддддддддддддддддддддддддддддддддддддды
		SGR->(dbSetOrder(1))
		SGR->(dbSeek(xFilial("SGR")+c630Prod+c630Cod+SG2->G2_OPERAC))
		While !SGR->(EOF()) .And. SGR->(GR_FILIAL+GR_PRODUTO+GR_ROTEIRO+GR_OPERAC) == xFilial("SGR")+c630Prod+c630Cod+SG2->G2_OPERAC
			RecLock("SGR",.F.)
			SGR->(dbDelete())
			SGR->(MsUnLock())
			SGR->(dbSkip())
		End
	End

End Transaction
Return


