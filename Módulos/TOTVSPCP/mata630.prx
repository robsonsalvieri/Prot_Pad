Function A630ProdAux(lRefGrd)
Local cCampo	:= ''
Local cConteudo := ''
Local cRefGrd	:= ""
Local cAlias	:= Alias()
Local lRet 		:= .T.

If !__lAutomacao
cCampo	:= ReadVar()
cConteudo := &(ReadVar())
EndIf

lRefGrd := .F.

If !Empty(cConteudo)
	If !Empty(c630CodAux) .And. A093Prod()
		cRefGrd := c630ProdAu
		SG2->( dbSetOrder(1) )
		If MatGrdPrrf(@cRefGrd,.T.) .And. !SG2->( MsSeek(xFilial("SG2")+cConteudo+c630CodAux) )
			lRefGrd := .T.
			dbSelectArea("SG2")
			dbSetOrder(7)
			cRefGrd := PadR(cRefGrd,Len(G2_REFGRD))
			If !MsSeek(xFilial("SG2")+cRefGrd+c630CodAux)
				Help(" ",1,"A630ROTNAE")
				lRet			:= .F.
				&(ReadVar())	:= Space(15)
			EndIf
		Else
			dbSelectArea("SG2")
			dbSetOrder(1)
			If !MsSeek(xFilial("SG2")+cConteudo+c630CodAux)
				Help(" ",1,"A630ROTNAE")
				lRet			:= .F.
				&(ReadVar())	:= Space(15)
			EndIf
		EndIf
	Else
		Help(" ",1,"A630ROTNAE")
		lRet			:= .F.
		&(ReadVar())	:= Space(15)
	EndIf
Else
	If !Empty(c630CodAux)
		c630CodAux		:= Space(2)
		&(ReadVar())	:= Space(15)
	EndIf
EndIf
dbSelectArea(cAlias)
Return lRet

