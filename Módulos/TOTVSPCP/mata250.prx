/*/{Protheus.doc} A250GravLt
Grava o lote produzido automaticamente
@type Static Function
@author Breno Ferreira
@since 07/05/2025
@version P12
@param1 cNumSeq, Character, NЗmero da sequЙncia da OP
@param2 cProdPai, Character, Produto do Pai da OP
@param3 cOp, Character, Ordem de produГЦo do apontamneto
@return Nil
/*/
Static Function A250GravLt(cNumSeq, cProdPai, cOp)
	Local cAlias   := ""
	Local cLotePrd := ""
	Local cQuery   := ""
	Local cSubLote := ""
	Local oQryLote := Nil
	
	cQuery :=  " SELECT SD5.D5_PRODUTO, "
	cQuery +=         " SD5.D5_LOTECTL "
	If _lSubLote
		cQuery +=    " ,SD5.D5_NUMLOTE "
	EndIf
	cQuery +=    " FROM " + RetSqlName("SD5") + " SD5 "
	cQuery +=   " WHERE SD5.D5_FILIAL  = ? "
	cQuery +=     " AND SD5.D5_NUMSEQ  = ? "
	cQuery +=     " AND SD5.D5_PRODUTO = ? "
	cQuery +=     " AND SD5.D5_OP      = ? "
	cQuery +=     " AND SD5.D_E_L_E_T_ = ' ' "

	oQryLote := FwExecStatement():New(cQuery)

	oQryLote:SetString(1, xFilial("SD5"))
	oQryLote:SetString(2, cNumSeq)
	oQryLote:SetString(3, cProdPai)
	oQryLote:SetString(4, cOp)

	cAlias := oQryLote:OpenAlias()

	If (cAlias)->(!Eof())
		If _lSubLote
			cSubLote := (cAlias)->D5_NUMLOTE
			A250GvSLt(cSubLote, cNumSeq, cOp)
		Else
			cLotePrd := (cAlias)->D5_LOTECTL
			gravaSD5(cLotePrd, cNumSeq, cOp)
		EndIf
	EndIf

	(cAlias)->(dbCloseArea())

	oQryLote:Destroy()
    FreeObj(oQryLote)
Return

/*/{Protheus.doc} gravaSD5
