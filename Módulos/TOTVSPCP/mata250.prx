Static Function QtdReqPMS()
	Local aAreaSD3	:=	SD3->(GetArea())
	Local nQtdReq	:=	0
	Local cChave
	//Achar as requisicoes
	DbSelectArea('SD3')
	DbSetOrder(9)
	cChave	:=	xFilial("SD3")+D3_DOC+Space(Len(D3_ITEM))+"RE1"
	DbSeek(cChave)
	While !Eof() .And. cChave == D3_FILIAL+D3_DOC+D3_ITEM+D3_CF
		If !Empty(D3_PROJPMS)
			nQtdReq	+=	D3_QUANT
		EndIf
		DbSkip()
	EndDo
	RestArea(aAreaSD3)
Return nQtdReq

