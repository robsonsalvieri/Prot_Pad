Function A250TM
	LOCAL cVar
	LOCAL lRet := .T.
	cVar := &(ReadVar())
	dbSelectArea("SF5")
	MsSeek(xFilial("SF5")+cVar)
	If F5_TIPO != "P"
		Help(" ",1,"A250TM")
		lRet := .F.
	EndIf

	If F5_TIPO == "P"
		If fEnvCQProd(M->D3_COD, M->D3_TM, .F.)
			aTam := TamSx3("D3_LOCAL")
			M->D3_LOCAL := GetMvNNR('MV_CQ','98')
		EndIf
	EndIf

Return lRet

