	Local aFolder1
	Local nX      := 1

	aFolder1 := {STR0015 + ": " + cProd}
	IF !__lAutomacao
		Define MsDialog oDlg Title STR0074 From 0,0 To 17, 77 OF oMainWnd

		oFolder1:=TFolder():New(5,5,aFolder1,{},oDlg,,,, .T., .F.,295,097,)
