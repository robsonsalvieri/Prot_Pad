	cQuery := " SELECT SD5.D5_PRODUTO, "
	cQuery +=        " SD5.D5_OP, "
	cQuery +=        " SD5.D5_NUMSEQ, "
	cQuery +=        " SD5.R_E_C_N_O_ "
	cQuery +=   " FROM " + RetSqlName("SD5") + " SD5 "
	cQuery +=  " WHERE SD5.D5_FILIAL   = ? "
	cQuery +=    " AND SD5.D5_NUMSEQ   = ? "
	cQuery +=    " AND SD5.D5_OP       = ? "
	cQuery +=    " AND SD5.D_E_L_E_T_  = ' ' " 

	oQrySD5 := FwExecStatement():New(cQuery)

	oQrySD5:SetString(1, xFilial("SD5"))
	oQrySD5:SetString(2, cNumSeq)
	oQrySD5:SetString(3, cOp)

	cAlias := oQrySD5:OpenAlias()

	While (cAlias)->(!Eof())

		dbSelectArea("SD5")
    	dbGoTo((cAlias)->R_E_C_N_O_)

		RecLock("SD5", .F.)
		SD5->D5_LOTEPRD := cLotePrd
		SD5->(MsUnlock())

		(cAlias)->(dbSkip())
	End

	(cAlias)->(dbCloseArea())

	oQrySD5:Destroy()
    FreeObj(oQrySD5)
Return

