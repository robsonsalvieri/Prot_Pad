*/
Static Function A250NEstor( cD3Ident )

	Local lRet		:= .F.
	Local cAlias	:= "SH6"
	Local aArea		:= GetArea()
	Local aAreaSH6	:= SH6->( GetArea() )
	Local cIndex 	:= ""
	Local cChave 	:= ""
	Local cQuery 	:= ""
	Local lQuery	:= .T.

	dbSelectArea("SH6")
	dbSetOrder(1)

	If lQuery
		cAlias := GetNextAlias()
		BeginSql Alias cAlias
			SELECT H6_IDENT
			FROM %table:SH6% SH6
			WHERE H6_FILIAL = %xFilial:SH6% AND
			SH6.H6_IDENT = %Exp:cD3Ident% AND
			SH6.%NotDel%
		EndSql
	Else
		cIndex := CriaTrab(NIL,.F.)
		cChave := IndexKey()
		cQuery := "H6_FILIAL == '"+xFilial("SH6")+"' .And. H6_IDENT == '"+cD3Ident+"'"
		IndRegua("SH6",cIndex,cChave,,cQuery)
		dbGoTop()
	Endif

	lRet := ( (cAlias)->(!Eof()) )

	If lQuery
		dbCloseArea()
	Else
		RetIndex("SH6")
		Ferase(cIndex+OrdBagExt())
	Endif

	RestArea( aAreaSH6 )
	RestArea( aArea )

Return( lRet )

