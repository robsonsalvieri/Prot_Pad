	ElseIf Type("cProgMtSFC")=="C"
		cProg := cProgMtSFC
	EndIf

	If Empty(cProg)
		cProg := FunName()
	EndIf
	
	cUsuario := RetCodUsr()
	
	RecLock("SMO",.T.)
		Replace SMO->MO_FILIAL   With xFilial("SMO"),;
				SMO->MO_IDAPON   With cIdApon,;
				SMO->MO_TIPO     With cTipo,;
				SMO->MO_PROGRAM  With cProg,;
				SMO->MO_CODUSU   With cUsuario
    SMO->(MsUnLock())

Return

