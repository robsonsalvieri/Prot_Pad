*/
Function A250Status(cNumOP, cNumSeq)
	Static lA250AbreOp
	Local lRet := .F.
	Local cLinha1:= STR0029+cNumOp+STR0030	//"A OP "###" estА encerrada. ApСs o estorno deste "
	Local cLinha2:= STR0031	//"apontamento a OP ficarА com saldo para produzir. "
	Local cLinha3:= STR0032	//"Deseja reabrir a Ordem de Producao?"
	Local cLinha4:= STR0152 //"A Ordem de ProduГЦo serА reaberta."
	Local cAlias:=Alias()
	Local cMsg := ''
	Local nOldOrdSC2:=SC2->(IndexOrd())
	Local nOldRecSC2:=SC2->(Recno())
	Local lAponAuto := .F.
	Local lOpen 	:= .T.

	Default lAbreOP := .T.

	If Type("lReabreOp") == "U"
		Private lReabreOp := .F.
	EndIf

	If Type("lValReabr") == "U"
		Private lValReabr := .F.
	EndIf

	lProdaut := IIF( lProdaut, .T., fRetPrdAut(cNumOP) )
	If lProdaut
		lAponAuto := a250ApoAut(cNumOP, cNumSeq)
	EndIf

	dbSelectArea("SC2")
	dbSetOrder(1)
	If dbSeek(xFilial("SC2")+cNumOP) .And. !Empty(C2_DATRF)
		If (Type("l250Auto") == "L" .and. l250Auto) .or. (Type("l680Auto") == "L" .and. l680Auto) .or. (Type("l681Auto") == "L" .and. l681Auto)
			lRet := .T.
		Else
			If lReabreOp //.And. fRetParPrd(cNumOP)
				lRet := lValReabr
			Else
				If lAponAuto
					cMsg := cLinha1 + cLinha2 + cLinha4
					Help(NIL,NIL,'MATA250',NIL,cMsg, 1,0 )
					
					lRet := .T.

					If !lReabreOp //.And. fRetParPrd(cNumOP)
						lReabreOp := .T.
						lValReabr := lRet  
					EndIf
				Else
					If lA250AbreOp == NIL
							lA250AbreOp := ExistBlock("A250ABOP") 
					EndIf

					If lA250AbreOp
						lOpen:=ExecBlock("A250ABOP",.F.,.F.,{cNumOp,cNumSeq}) //PARAMIXB[1] -> Num.Op / PARAMIXB[2] -> Num.Seq // Se o ponto de entrada retornar .T., abrirА a mensagem normalmente, caso contrАrio reabre a OP sem dar a msg e segue normalmente.
						If ValType(lOpen) != "L"
							lOpen:=.T.
						EndIf
					EndIf

					If lOpen
						TONE(3500,1)
						If !(MsgYesNo(OemToAnsi(cLinha1+cLinha2+cLinha3),OemToAnsi(STR0033)+cNumOP))		//"Estorno OP "
							lRet    := .F.
							lAbreOP := .F.
						Else 
							lRet := .T.
						EndIf 
	
						If !lReabreOp //.And. fRetParPrd(cNumOP)
							lReabreOp := .T.
							lValReabr := lRet  
						EndIf
					Else
						lRet := .T.
						If !lReabreOp //.And. fRetParPrd(cNumOP)
							lReabreOp := .T.
							lValReabr := lRet  
						EndIf
					EndIf
				EndIf
			EndIf
		EndIf
	EndIf
	dbSelectArea(cAlias)
	SC2->(dbSetOrder(nOldOrdSC2))
	SC2->(MsGoto(nOldRecSC2))
Return lRet

