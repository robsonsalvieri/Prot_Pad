Function fGravaSMO(cIdApon, cTipo)	
	
	Local cProg    := ""	
	Local cUsuario := ""

	Default cIdApon  := ""
	Default cTipo    := ""	

	If !AliasInDic("SMO")
		Return
	EndIf

	If Empty(cIdApon) .Or. Empty(cTipo)
		Return
	EndIf

	If Type("cProgMt250")=="C"
		cProg := cProgMt250
	ElseIf Type("cProgMt681")=="C"
