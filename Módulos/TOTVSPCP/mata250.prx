*/
Function A250OpsPis(cOp, aOps)
	Local aSavAre := {SD4->(GetArea()), GetArea()}
	Local cSeek   := NIL
	Local nRecSD4     := 0

	Default aOps  := {cOp}

	dbSelectArea("SD4")
	SD4->(dbSetOrder(2))
	SD4->(dbSeek(cSeek := xFilial("SD4") + cOp))
	Do While !SD4->(Eof()) .And. SD4->(D4_FILIAL+D4_OP) == cSeek
		If Empty(SD4->D4_OPORIG)
			SD4->(DbSkip())
			Loop
		EndIf
		
		If aScan(aOps, {|x| x == SD4->D4_OPORIG}) == 0
			Aadd(aOps, SD4->D4_OPORIG)
			nRecSD4:= SD4->(Recno())
			aOps := A250OpsPis(SD4->D4_OPORIG, aOps)
			SD4->(MsGoto(nRecSD4))		
		EndIf
		SD4->(dbSkip())
	EndDo

	RestArea(aSavAre[1])
	RestArea(aSavAre[2])

Return(aOps)

