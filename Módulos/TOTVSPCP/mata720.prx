*/
Static Function MTA720OK(oTempTable)
Local lRet 		 := .T.
Local cText 	 := ""
Local cQuery     := ""

_lCrpOk := Iif(_lCrpOk == Nil, AliasInDic("HWF") .And. FindFunction('PCPA152'),_lCrpOk)

cText += OemToAnsi(STR0019)+CHR(13)	//"Esta rotina ir  aglutinar as Ordens de Produção"
cText += OemToAnsi(STR0020)+CHR(13) //"conforme os parƒmetros selecionados."
cText += OemToAnsi(STR0021)	        //"Confirma a aglutinação das OPs ?"

If !MsgYesNo(cText,OemToAnsi(STR0022)) //"Atenção"
	lRet := .F.
Endif

If lRet .And. _lCrpOk
	cQuery += " AND HWF.HWF_OP IN (" 
	cQuery +=                    " SELECT NUMOP FROM " + oTempTable:GetTableNameForQuery()
	cQuery +=                     " WHERE MARK <> '')"

	lRet := PCPA152Efetivacao():abreTela(cQuery)
Endif

Return lRet	

