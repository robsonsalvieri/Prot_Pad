						))
				Break
			EndIf
		EndIf
	        
		nArq := MsFCreate( cArquivo , 0 )
		If (;
				( Ferror() # 0 );
				.and.;
				( nArq = -1  );
			)
			cMsg := STR0028 + Str( Ferror() , 3 ) //-- "Erro de Gravacao do Arquivo - Codigo DOS: "
			MsgInfo( OemToAnsi( cMsg ) )
