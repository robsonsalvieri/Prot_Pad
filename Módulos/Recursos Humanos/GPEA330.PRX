Function Rc5GetTParam( cType ) 
Local cRc5CodParam
	
	If ( cType $ "C" )
		cRc5CodParam := "1"
	ElseIf ( cType == "N" )
		cRc5CodParam := "2"					
	ElseIf ( cType == "D" )
		cRc5CodParam := "3"					
	ElseIf ( cType == "L" )
		cRc5CodParam := "4"					
	ElseIf ( cType == "A" )
		cRc5CodParam := "5"					
	ElseIf ( cType == "B" )
		cRc5CodParam := "6"
	ElseIf ( cType == "O" )
		cRc5CodParam := "7"
	ElseIf ( cType == "U" )
		cRc5CodParam := "8"
	Else
		cRc5CodParam := " "
	EndIf

Return( cRc5CodParam )


