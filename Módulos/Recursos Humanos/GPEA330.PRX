		If cPaisLoc == "RUS"
			aAdd(aLinesProg, "ณFuno    ณGpFunRus      ณAutorณ Gerado pelo sistema ณ Data ณ" + Dtoc( Msdate() ) + "ณ" + CRLF)
		Else
