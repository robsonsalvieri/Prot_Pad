	EndIf
	
	aIniHdrRC4	:= RC4->( GdMontaHeader( @nUsado , @aRC4Virtual , NIL , NIL , NIL , .T. ) )
	aIniHdrRC5	:= RC5->( GdMontaHeader( @nUsado , @aRC5Virtual , NIL , NIL , NIL , .T. ) )
	
	Begin Sequence
