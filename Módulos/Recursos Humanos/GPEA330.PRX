Static Function Gpea330Dele()
Local cAlias := "RC5"  
Local nX   
	
	lRet		 := .T. 
	     
	// Verifica se a funcao ้ usada na formula 
	If ! fUseVar(@lRet,cNomFun)
		Return
	EndIf	
	
	// Deleta os Itens (Parametros da Funcao) 
	dbSelectArea("RC5")
	For nX = 1 to Len(aCols)
	    If dbSeek(xFilial(cAlias)+cNomFun+aCols[nX][1])
		    RecLock(cAlias,.F.,.T.)
		    dbDelete( )
		    MsUnlock()
	    EndIf	
	Next nX
	
	// Deleta a funcao 
	dbSelectArea("RC4")
	If dbSeek(xFilial("RC4") + cNomFun)
		RecLock("RC4",.F.,.T.)
		dbDelete( )
		MsUnlock()	
	EndIf
	
Return( Nil )

