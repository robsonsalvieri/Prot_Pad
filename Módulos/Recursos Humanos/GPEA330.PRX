	    aAdd(aLinesProg, CRLF)
	    aAdd(aLinesProg, CRLF)
	    
	
		//-- Transferir as linhas para dentro do programa                 
	    For nItem := 1 To Len( aLinesProg )
		    Fwrite( nArq , aLinesProg[ nItem ] )
		Next nItem
	
	    For nItem := 1 To Len( aLinesFunc )
