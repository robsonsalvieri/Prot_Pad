Function X3VALID(cCpo)   
Local cSavAlias := Alias()
Local cRet := ""
	
	DbSelectArea("SX3")
	DbSetOrder(2)
	If DbSeek(PadR(cCpo,Len(X3_CAMPO)),.F.)
		cRet := (X3_VALID)
	EndIf
	DbSetOrder(1)
	DbSelectArea(cSavAlias)

Return ( cRet )

