Static Function Gpem111Pergunte()

Local lOk    

lOk := Pergunte("GP111A",.T. )

If lOk 
	cTipoCont := MV_PAR01
	If cTipoCont == 1		
		Pergunte("GP111B",.T. )
	Elseif cTipoCont == 2
		Pergunte("GP111C",.T. )
	Else
		Pergunte("GP111B",.T. )
		Pergunte("GP111C",.T. )
	Endif
EndIf

Return( .T. )

