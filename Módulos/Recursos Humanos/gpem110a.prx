IF !Empty( aLogIncon )
	//"Foram Encontradas Inconsistencias na Contabilizacao."
	aAdd( aLogTitle , STR0033 )
	fMakeLog( { aLogIncon } , aLogTitle , cPerg , lGpem110ExecInRemote )
