IF ( !Empty( cTxtAlias ) .and. ( Select( cTxtAlias ) > 0 ) )
	
	//Fecha o Arquivo atualmente aberto 						  

	TxtAliasClose( cTxtAlias )
	// Exclui os Arquivos Temporarios 							  

