				/*/
				IF ( oAponta:lApontaTpTrab := (;
													( oAponta:cTipoDia == "S" ) .or.;
													(;
														( oAponta:cTipoDia != "S" ) .and.;
														( oAponta:nIntTab > 0 ) .and.;
														( oAponta:cPgIntNTrb == "S" );
