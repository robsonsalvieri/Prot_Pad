				/*/
				oAponta:lIntNtrab:= (;
											( oAponta:cTipoDia != "S" );
											.and.;
											( oAponta:nIntTab > 0 );
											.and.;
											( oAponta:cPgIntNTrb == "S");
											.and.;
											!( oAponta:lMovelFree );
