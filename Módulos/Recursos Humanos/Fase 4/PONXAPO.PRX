Function fValHrs100()

	Local aAreaSP9	:= SP9->( GetArea() )
	Local cFilSP9	:= FwxFilial("SP9")
	Local lRet		:= .T.
	Local nTamEven	:= Len("P9_CODIGO")

	Local cEvenFInt	:= SuperGetMv("MV_POHEFIN", NIL , " " ) 	// Falta Integral
	Local cEvenFNot	:= SuperGetMv("MV_POHEFNO", NIL , " " ) 	// Falta Noturna
	Local cEvenHE50	:= SuperGetMv("MV_POHE50" , NIL , " " )		// H.E.  50% AUT
	Local cEvenHE100:= SuperGetMv("MV_POHE100", NIL , " " ) 	// H.E. 100% AUT

	dbSelectArea("SP9")
	dbSetOrder(1)

    If !DbSeek( cFilSP9 + Padr(cEvenHE100,nTamEven)) .Or. ;
    	!DbSeek( cFilSP9 + Padr(cEvenHE50,nTamEven)) .Or. ;
    	!DbSeek( cFilSP9 + Padr(cEvenFInt,nTamEven)) .Or. ;
    	!DbSeek( cFilSP9 + Padr(cEvenFNot,nTamEven))
	     HELP( " ",1,"PONVAL100" )
		 lRet := .F.
	EndIf

		RestArea(aAreaSP9)

Return(lRet)

/*/{Protheus.doc} AjusEvent
	Ajusta de Cantidad Calculada de eventos para Mercado Internacional.
	@type Function
	@author oscar.lopez
	@since 20/08/2025
	@version 1.0
	@param oAponta, objeto, Objeto con informaciСn de marcaciones
	@param cRegApu, caracter, Codigo de regla de apunte del empleado
	@return Nil
	@example
		AjusEvent(oAponta, cRegApu)
	/*/
Function AjusEvent(oAponta, cRegApu)

	Local aArea			:= GetArea()
	Local aAreaSPA		:= {}
	Local nPosPriDom	:= 0
	Local dFcEntrada	:= CToD("  /  /    ")
	Local nHrEntrada	:= 0
	Local dFcSalida		:= CToD("  /  /    ")
	Local nHrSalida		:= 0
	Local nHorasCalc	:= 0
	Local nPosEnt		:= 0	//PosiciСn de arreglo oAponta:aMarClone para lectura de Fecha y Hora de Entrada
	Local nPosSal		:= 0	//PosiciСn de arreglo oAponta:aMarClone para lectura de Fecha y Hora de Salida

	Default oAponta := Nil
	Default cRegApu	:= ""

	DbSelectArea("SPA")
	aAreaSPA := SPA->(GetArea())
	SPA->(DBSetOrder(1)) //PA_FILIAL+PA_CODIGO

	If oAponta <> Nil .And. SPA->(MsSeek(FwxFilial("SPA")+cRegApu)) .And. SPA->(ColumnPos("PA_PRIDOM")) > 0 .And. SPA->PA_PRIDOM == "2"
		nPosPriDom := AScan(oAponta:aResult,{|x| x[2] == EVEN_PRIMA_DOM})
		If nPosPriDom > 0

			nPosEnt := AScan(oAponta:aMarClone,{|x| x[1] == oAponta:aResult[nPosPriDom][1]})

			If oAponta:aMarClone[nPosEnt][12] $ MARC_ENTRADA
				nPosSal := nPosEnt + 1
			ElseIf oAponta:aMarClone[nPosEnt][12] $ MARC_SALIDA
				nPosSal := nPosEnt
				nPosEnt--
			EndIf

			If nPosEnt > 0 .And. nPosSal > 0 .And. nPosSal <= Len(oAponta:aMarClone)
				dFcEntrada	:= oAponta:aMarClone[nPosEnt][1]
				nHrEntrada	:= oAponta:aMarClone[nPosEnt][2]
				dFcSalida	:= oAponta:aMarClone[nPosSal][1]
				nHrSalida	:= oAponta:aMarClone[nPosSal][2]

				If dFcEntrada < oAponta:aResult[nPosPriDom][1]
					nHrEntrada := HORA_MAX
				ElseIf dFcSalida > oAponta:aResult[nPosPriDom][1]
					nHrSalida := 0
				EndIf

				nHorasCalc := fCalHoras(dFcEntrada		,;	//01 -> Fecha de Entrada
									nHrEntrada		,;	//02 -> Hora de Entrada
									dFcSalida		,;	//03 -> Fecha de Salida
									nHrSalida		 ;	//04 -> Hora de Salida
									)

				oAponta:aResult[nPosPriDom][3] := nHorasCalc
			EndIf
		EndIf
	EndIf

	SPA->(RestArea(aAreaSPA))
	RestArea(aArea)

Return Nil
