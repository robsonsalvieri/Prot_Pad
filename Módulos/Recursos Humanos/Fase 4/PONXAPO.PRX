					oAponta:aApo[oaApo:HeNormal,nIndiceaApo]	,;	//03 -> Numero de Horas Extras Autorizadas
					oAponta:aApo[oaApo:HeNorNAut,nIndiceaApo]	,;	//04 -> Numero de Horas Extras Nao Autorizadas
