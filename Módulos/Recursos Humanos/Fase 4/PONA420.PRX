*/
Static Function PONA420TmpGrv( aStrRf2, aCposRf2 )

Local nCont := 0
Local nCpos := 0

For nCont := 1 To Len(aCposRf2)
	IF RF2->( RecLock( "RF2" , .T. ) )
		For nCpos := 1 To Len(aStrRf2)
			RF2->(&(aStrRf2[nCpos,01])) := aCposRf2[nCont,nCpos]
		Next
		RF2->( MsUnlock() )
	EndIF
Next

Return( NIL )

