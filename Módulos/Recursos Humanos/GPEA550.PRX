Function TrocaTab550(cOrigem)
	If !lRobo
		If cOrigem == "1"
			If !(M->RG1_CODTAB == oGet:aCols[n][nPosCTab])
				oGet:aCols[n][nPosLin] := 0
				oGet:aCols[n][nPosCol] := 0
			Endif
