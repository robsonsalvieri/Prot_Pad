							EndIf
						EndIF
		            Else
						lTemSubArray := If( !Empty( oAponta:aHorasAnt[ nX , 7] ), ( Valtype(oAponta:aHorasAnt[ nX , 7, 1 ] ) == "D" ), .F. )
						nPos := aScan(oAponta:aHorasAnt, {|x| x[ 3 ] == "M" .And. !x[6]}, nX)
