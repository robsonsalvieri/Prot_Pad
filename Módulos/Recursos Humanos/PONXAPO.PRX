	AppendaResAbo(  @oAponta:aResult,  oAponta:aResAbo )
	
	// Grava os resultados do funcion rio no arquivo SPC apenas se houveram novos apontamentos
	IF ( lGrava )
		
		// Verifica se Deve Gravar o Apontamento
		lGrava := (Empty( oAponta:aResult ) .or. (( oAponta:lChkTolArr ) .and. !ArrayCompare( oAponta:aResClone , oAponta:aResult )))
		
		// Se Houverem Diferencas Entre O que Entrou e o que foi Apontado ou se o aResult Estiver vazio Regrava os Apontamentos
		IF ( lGrava )
			GravaSPC( oAponta:aResult, cFilSRA, cMatSRA, cEvesNoDele, oAponta:dSvdIni, oAponta:dSvdFim, aAbonosPer )
		EndIF
	EndIF
	
