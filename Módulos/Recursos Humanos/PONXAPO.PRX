								EndIF
							EndIF
						Else
							dDiaAnt		:= oAponta:aHorasAnt[nX ,1]
							nPriMarc	:= oAponta:aHorasAnt[nX ,2]
							dDiaAtu		:= oAponta:aHorasAnt[nXPlus1  ,01]
							nUltMarc	:= oAponta:aHorasAnt[nXPlus1  ,02]
							
							lTemSubArray := If(!Empty(oAponta:aHorasAnt[nXPlus1, 7]), (Valtype(oAponta:aHorasAnt[nXPlus1, 7, 1]) == "D" .Or. Valtype(oAponta:aHorasAnt[nXPlus1, 7, 1]) == "A"), .F.)
							
