³Uso       ³Generico 													 ³
ÀÄÄÄÄÄÄÄÄÄÄÁÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ/*/
Static function TratIntProAbo()
	
	// campos para tratar o novo horário de pré-abono resultante da combinação entre o abono já existente e o interjornada
	Local nIndx1       := 0
	Local nNovaHrFim   := 0
	Local cHorIni		:= ""
	Local cHorFim		:= ""
	Local cLimIni		:= ""
	Local cLimFim		:= ""
	
	nNovaHrFim := SomaHoras(nIntPriEntradPadrao, nCredInterJ)
	
	cHorIni := DataHora2Str(oAPonta:achktolarr[oAponta:nChkTolArr][1], nIntPriEntradPadrao)
	cHorFim := DataHora2Str(oAPonta:achktolarr[oAponta:nChkTolArr][1], nNovaHrFim)
	
	For nIndx1 := 1 to Len(oAponta:apreabodia)
		
		cLimIni := DataHora2Str(oAponta:apreabodia[nIndx1][2], oAponta:apreabodia[nIndx1][3])
		cLimFim := DataHora2Str(oAponta:apreabodia[nIndx1][4], oAponta:apreabodia[nIndx1][5])
		
		// Se o abono da interjornada estiver contido em algum abono já cadastrado
		If (cHorIni <= cLimFim .AND. cHorFim >= cLimIni)
			
			If oAponta:apreabodia[nIndx1][2] < oAPonta:achktolarr[oAponta:nChkTolArr][1]
				// Quebra o abono cadastrado para incluir o abono da interjornada
				Aadd( oAponta:apreabodia, aClone(oAponta:apreabodia[nIndx1]))
				
				// Altera a data e hora final 
				oAponta:apreabodia[nIndx1][4] := oAPonta:achktolarr[oAponta:nChkTolArr][1]
				oAponta:apreabodia[nIndx1][5] := nIntPriEntradPadrao
				
				// Altera o dia e hora inicial
				oAponta:apreabodia[Len(oAponta:apreabodia)][2] := oAPonta:achktolarr[oAponta:nChkTolArr][1]
				oAponta:apreabodia[Len(oAponta:apreabodia)][3] := nNovaHrFim
				oAponta:apreabodia[Len(oAponta:apreabodia)][6] := 0
				
				// Replica para o array oAponta:apreabo
				If nIndx1 <= Len(oAponta:apreabo)
					Aadd( oAponta:apreabo, aClone(oAponta:apreabo[nIndx1]))
					oAponta:apreabo[nIndx1][4] := oAPonta:achktolarr[oAponta:nChkTolArr][1]
					oAponta:apreabo[nIndx1][5] := nIntPriEntradPadrao
					oAponta:apreabo[Len(oAponta:apreabo)][2] := oAPonta:achktolarr[oAponta:nChkTolArr][1]
					oAponta:apreabo[Len(oAponta:apreabo)][3] := nNovaHrFim
					oAponta:apreabo[Len(oAponta:apreabo)][6] := 0
				EndIf
			Else
				//Altera a hora de inicio do abono cadastrado para o fim da interjornada
				oAponta:apreabodia[nIndx1][3] := nNovaHrFim
				If nIndx1 <= Len(oAponta:apreabo)//observamos que apreabo é sempre uma copia fiel do apreabodia
					oAponta:apreabo[nIndx1][3] := nNovaHrFim
				EndIf
			EndIf
			
			EXIT
			
		EndIf
	Next
	
	// Inclui o abono da interjornada
	Aadd( oAponta:apreabodia, {SR6->R6_ABINTJ, oAPonta:achktolarr[oAponta:nChkTolArr][1], nIntPriEntradPadrao/*8*/, oAPonta:achktolarr[oAponta:nChkTolArr][1], nNovaHrFim/*10*/, 0/*obrigatorio deixar recno zerado*/} )
   	//observamos que apreabo é sempre uma copia fiel do apreabodia
	Aadd( oAponta:apreabo,    {SR6->R6_ABINTJ, oAPonta:achktolarr[oAponta:nChkTolArr][1], nIntPriEntradPadrao/*8*/, oAPonta:achktolarr[oAponta:nChkTolArr][1], nNovaHrFim/*10*/, 0/*obrigatorio deixar recno zerado*/} )
	
Return

