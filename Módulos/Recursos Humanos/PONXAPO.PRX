/*/{Protheus.doc} GetEvtAdN
Retorna o código do evento para a geração do Adicional Noturno de acordo com o cadatro da CCT
@type function
@version  
@author Cícero Alves
@since 2/19/2024
@param cFilCCT, character, Filial da CCT
@param cCodCCT, character, Código da CCT
@param cID, character, ID do ponto que será gerado, indica se é Ad. Noturno sobre horas normais ou extras
@return cEvento, character, Código do evento cadastrado na CCT, caso não encontre o retorno será uma string vazia
/*/
Function GetEvtAdN(cFilCCT, cCodCCT, cID)
	
	Local cEvento	:= ""
	Local cAdNHN	:= ""
	Local cAdNHE	:= ""
	
	DEFAULT cFilCCT := ""
	DEFAULT cCodCCT := ""
	DEFAULT cID		:= ""
	
	lCpoADN := If(lCpoADN == NIL, SWY->(ColumnPos("WY_ADNHN") > 0), lCpoADN)
	
	If lCpoADN .And. !Empty(cCodCCT) .And. !Empty(cID)
		
		If !(cFilCCT + cCodCCT == cLastCodCCT)
			
			cLastCodCCT := cFilCCT + cCodCCT
			cLastAdNHN := ""
			cLastAdNHE := ""
			
			DbSelectArea("SWY")
			SWY->(dbSetOrder(1))
			If SWY->(DbSeek(cFilCCT + cCodCCT))
				cAdNHN := SWY->WY_ADNHN
				cAdNHE := SWY->WY_ADNHE
			EndIf
			
			// Verifica se os eventos existem e são válidos para a filial do funcionário
			If !Empty(cAdNHN) .And. (Empty(Posicione("SP9", 1, xFilial("SP9", SRA->RA_FILIAL) + cAdNHN, "P9_IDPON")) .And. SP9->P9_CLASEV == "06")
				cLastAdNHN := cAdNHN
			EndIf
			If !Empty(cAdNHE) .And. (Empty(Posicione("SP9", 1, xFilial("SP9", SRA->RA_FILIAL) + cAdNHE, "P9_IDPON")) .And. SP9->P9_CLASEV == "06")
				cLastAdNHE := cAdNHE
			EndIf
		EndIf
		
		If cID $ "003N|004A|046A" // Adicional noturno Horas Normais
			cEvento := cLastAdNHN
		ElseIf cID $ "027N|028A|045A" // Adicional noturno Horas Extras
			cEvento := cLastAdNHE
		EndIf
		
	EndIf
	
Return cEvento

/*/{Protheus.doc} GetIntSum
Retorna o intervalo que será gerado a Súmula 437
@type function
@version  
@author Marco Nakahara Nakazawa
@since 20/06/2024
@param cTipoMarc, Tipo da Marcação
@return nIntervalo
/*/
Function GetIntSum(cTipoMarc)
Local aInterval		:= {"1S*2E","2S*3E","3S*4E"}
Local nIntervalo	:= 0
Local nI			:= 0

Default cTipoMarc	:= ""

If !Empty(cTipoMarc)
	For nI := 1 To Len(aInterval)
		If cTipoMarc $ aInterval[nI]
			nIntervalo := nI
			Exit
		EndIf
	Next
EndIf
Return nIntervalo

/*/{Protheus.doc} fVldSumula
Valida se a Súmnula deve ser gerada para os intervalos.
@type function
@version  
@author Marco Nakahara Nakazawa
@since 20/06/2024
@param cTipoMarc, Tipo da Marcação
@return cIntervalo
/*/
Function fVldSumula(lIndeniza)
Local nPosIndSum	:= aScan(aEvesIds, {|x| x[1] == "048A"})
Local nI			:= 0
Local nLenAhoras	:= 0
Local nLenIndSum	:= 0
Local nTotIndSum	:= 0
Local nTotInterv	:= 0
Local nLenApo		:= 0
Local nPlus			:= 0
Local nPosSI		:= 0
Local nPosEI		:= 0
Local nIntRealzd	:= 0
Local nIntSum		:= 0
Local nTotIntHe		:= 0
Local nTotIntAu		:= 0
Local nTotIntNa		:= 0
Local cInterv		:= ""
Local cCcFunc		:= ""
Local aIndSumula	:= {}
Local aInts			:= {{2,3,"1I","1S","2E"},{4,5,"2I","2S","3E"},{6,7,"3I","3S","4E"}}

Default lIndeniza	:= .F.

If !lIndeniza
	nLenApo := Len(oAponta:aApo[oaApo:HeIntNorAu])

	If oAponta:lPAPgInTot .And. !oAponta:lIndSumula
		For nI := 2 To nLenApo Step 2
			nPlus := If(nI + 1 <= nLenApo, nI + 1, nI)

			If oAponta:aApo[oaApo:HeIntNorAu, nI] > 0 .Or. oAponta:aApo[oaApo:HeIntNorAu, nPlus] > 0
				nPosAint := aScan(aInts, {|x| x[1] == nI .Or. x[2] == nPlus})

				If nPosAint > 0 
					If (nPosSI := aScan(oAponta:aHoras, {|x| x[3] == aInts[nPosAint,4]})) > 0
						oAponta:aApo[oaApo:HeIntNorAu, nPlus] := 0
						oAponta:aApo[oaApo:HeIntNorAu, nI] := oAponta:aHoras[nPosSI,9]
					EndIf
				EndIf
			ElseIf oAponta:aApo[oaApo:HeIntNotAu, nI] > 0 .Or. oAponta:aApo[oaApo:HeIntNotAu, nPlus] > 0
				nPosAint := aScan(aInts, {|x| x[1] == nI .Or. x[2] == nPlus})

				If nPosAint > 0 
					If ((nPosSI := aScan(oAponta:aHoras, {|x| x[3] == aInts[nPosAint,4]})) > 0) .And. ((nPosEI := aScan(oAponta:aHoras, {|x| x[3] == aInts[nPosAint,5]})) > 0)
						oAponta:aApo[oaApo:HeIntNotAu, nI] := 0
						oAponta:aApo[oaApo:HeIntNotAu, nPlus] := 0
						oAponta:aApo[oaApo:AdicHeNotA, nI] := 0
						oAponta:aApo[oaApo:AdicHeNotA, nPlus] := 0
						oAponta:aApo[oaApo:AdicHeNotN, nI] := 0
						oAponta:aApo[oaApo:AdicHeNotN, nPlus] := 0
						oAponta:aApo[oaApo:HeIntNotNa, nI] := 0
						oAponta:aApo[oaApo:HeIntNotNa, nPlus] := 0

						fExtras(oAponta:aHoras[nPosSI,1]	,;	//01 -> Data da 1a Marcacao
								oAponta:aHoras[nPosSI,2]	,;	//02 -> Hora da 1a Marcacao
								oAponta:aHoras[nPosEI,1]	,;	//03 -> Data da 2a Marcacao
								oAponta:aHoras[nPosEI,2]	,;  //04 -> Hora da 2a Marcacao
								aInts[nPosAint,5]			,;	//05 -> Tipo de Marcacao (Ent.,Int.,Saida...)
								aInts[nPosAint,2]			,;	//06 -> Indice de aApo
								.F.							,;	//07 -> 1o Dia do Apontamento
								.T.							 ;	//08 -> Hora Extra Intervalo ( Entrada )
						)
					EndIf
				EndIf
			EndIf
		Next
	EndIf
Else
	If oAponta:lIndSumula .And. nPosIndSum > 0 .And. Empty(aEvesIds[nPosIndSum, 02])
		lMsgIdSum	:= .T.
		Return
	EndIf

	nLenAhoras	:= Len(oAponta:aHoras)

	For nI := 1 To nLenAhoras
		If !(oAponta:aHoras[nI,10] $ "EI*SI") .Or. oAponta:aHoras[nI,3] == "M"
			Loop
		Else
			nIntSum := GetIntSum(oAponta:aHoras[nI,3])

			If nIntSum > 0
				If "S" $ oAponta:aHoras[nI,3]
					nTotInterv	:= oAponta:aHoras[nI,9]

					nTotIntAu := SomaHoras(oAponta:aApo[oaApo:HeIntNorAu, aInts[nIntSum, 1]], oAponta:aApo[oaApo:HeIntNorAu, aInts[nIntSum, 2]])
					nTotIntNa := SomaHoras(oAponta:aApo[oaApo:HeIntNorNa, aInts[nIntSum, 1]], oAponta:aApo[oaApo:HeIntNorNa, aInts[nIntSum, 2]])
					nTotIntHe := SomaHoras(nTotIntAu, nTotIntNa)

					If nTotIntHe > 0
						aAdd(aIndSumula, {nTotInterv, SubHoras(nTotInterv, nTotIntHe), oAponta:aHoras[nI,14]})
					EndIf
				EndIf
			EndIf
		EndIf
	Next

	If !Empty(aIndSumula)
		cInterv		:= ""
		nTotInterv	:= 0
		nLenIndSum	:= Len(aIndSumula)

		For nI := 1 To nLenIndSum
			If oAponta:lIndSumula .And. oAponta:nLimitSuml > 0
				If aIndSumula[nI, 2] >= oAponta:nLimitSuml
					Loop
				Else
					nIntRealzd := SubHoras(oAponta:nLimitSuml, aIndSumula[nI, 2])
				EndIf

				If nIntRealzd > oAponta:nLimitSuml
					Loop
				Else
					If nIntRealzd > 0 
						nTotIndSum := SomaHoras(nIntRealzd, nTotIndSum)
					EndIf
				EndIf
			Else
				nTotIndSum := SomaHoras(nTotIndSum, SubHoras(aIndSumula[nI, 1], aIndSumula[nI, 2]))
			EndIf

			If oAponta:lPAPgInTot
				nTotInterv := If(oAponta:lIndSumula .And. oAponta:nLimitSuml > 0, nTotIndSum, SomaHoras(nTotInterv, aIndSumula[nI,1]))
			EndIf

			If nI == nLenIndSum
				cCcFunc := aIndSumula[nI,3]
			EndIf
		Next

		If nTotIndSum > oAponta:nTolSumula .And. !oAponta:lPAPgInTot
			fGeraRes(oAponta:aResult							,;	//01 -> Array com os Resultados do Dia
				oAponta:dData									,;	//02 -> Data da Geracao
				nTotIndSum										,;	//03 -> Numero de Horas Resultantes
				aEvesIds[nPosIndSum, 02]						,;	//04 -> Codigo do Evento
				cCcFunc											,;	//05 -> Centro de Custo a ser Gravado
				Nil												,;	//06 -> Tipo de Marcacao
				.T.												,;	//07 -> True para Acumular as Horas
				""												,;	//08 -> Periodo de Apuracao
				0   											,;	//09 -> Tolerancia
				"H"							     				,;	//10 -> Tipo de Arredondamento a Ser Utilizado
				/*lSubstitui*/									,;	//11 -> Substitui a(s) Hora(s) Existente(s) ( Garanto Apenas 1 DSR por Data )
				/*cFuncao	*/									,;	//12 -> Funcao
				/*cDepto	*/									,;	//13 -> Depto para gravacao
				/*cPosto	*/									,;	//14 -> Posto para gravacao
				/*cProcesso	*/									,;	//15 -> Periodo para Gravacao
				/*cRoteiro	*/									,;	//16 -> Processo para Gravacao
				/*cPerApo	*/									,;	//17 -> Periodo para Gravacao
				/*cNumPagto	*/ 									,;	//18 -> NumPagto para Gravacao
				oAponta:cTnoCalend							    ,;	//19 -> Turno de Trabalho
				oAponta:cSeqTno									 ;  //20 -> Semana/Sequencia do Turno
			)
		ElseIf nTotIndSum > oAponta:nTolSumula
			fGeraRes(oAponta:aResult							,;	//01 -> Array com os Resultados do Dia
				oAponta:dData									,;	//02 -> Data da Geracao
				nTotInterv										,;	//03 -> Numero de Horas Resultantes
				aEvesIds[nPosIndSum, 02]						,;	//04 -> Codigo do Evento
				cCcFunc											,;	//05 -> Centro de Custo a ser Gravado
				Nil												,;	//06 -> Tipo de Marcacao
				.T.												,;	//07 -> True para Acumular as Horas
				""												,;	//08 -> Periodo de Apuracao
				0   											,;	//09 -> Tolerancia
				"H"							     				,;	//10 -> Tipo de Arredondamento a Ser Utilizado
				/*lSubstitui*/									,;	//11 -> Substitui a(s) Hora(s) Existente(s) ( Garanto Apenas 1 DSR por Data )
				/*cFuncao	*/									,;	//12 -> Funcao
				/*cDepto	*/									,;	//13 -> Depto para gravacao
				/*cPosto	*/									,;	//14 -> Posto para gravacao
				/*cProcesso	*/									,;	//15 -> Periodo para Gravacao
				/*cRoteiro	*/									,;	//16 -> Processo para Gravacao
				/*cPerApo	*/									,;	//17 -> Periodo para Gravacao
				/*cNumPagto	*/ 									,;	//18 -> NumPagto para Gravacao
				oAponta:cTnoCalend							    ,;	//19 -> Turno de Trabalho
				oAponta:cSeqTno									 ;  //20 -> Semana/Sequencia do Turno
			)
		EndIf
	EndIf	
EndIf	
Return

/*/{Protheus.doc} fGeraHESobre
Realiza a geração dos eventos de hora extra sobreaviso no aResult
@type static function
@author Cícero Alves
@since 09/09/2025
@param aSAviso, array, Informações do sobreaviso
@param nHoras, numeric, Horas que serão geradas
@param nTipo, numeric, Define qual evento deve ser gerado
@return logical, Verdadeiro caso o evento tenha sido gerado	1 = Hora Extra; 2 = Hora extra noturna; 3 = Adicional Noturno
/*/
Static Function fGeraHESobre(aSAviso, nHoras, nTipo)
	
	Local cEvento		:= ""
	Local lRet			:= .F.
	
	Static cHESobre		:= NIL
	Static cHENotS		:= NIL
	Static cAdNHES		:= NIL
	
	DEFAULT aSAviso 	:= {}
	DEFAULT nHoras 		:= 0
	DEFAULT nTipo		:= 0
	
	If cHESobre == NIL
		getIDsHE()
	EndIf
	
	If nTipo == 1
		cEvento := cHESobre
	ElseIf nTipo == 2
		cEvento := cHENotS
	ElseIf nTipo == 3
		cEvento := cAdNHES
	EndIf
	
	If nHoras > 0 .And. !Empty(cEvento)
		fGeraRes(	oAponta:aResult									,;	//01 -> Array com os Resultados do Dia
					oAponta:dData									,;	//02 -> Data da Geracao
					nHoras											,;	//03 -> Numero de Horas Resultantes
					cEvento											,;	//04 -> Codigo do Evento
					aSAviso[5]										,;	//05 -> Centro de Custo a ser Gravado
					Nil												,;	//06 -> Tipo de Marcacao
					.T.												,;	//07 -> True para Acumular as Horas
					""												,;	//08 -> Periodo de Apuracao
					0   											,;	//09 -> Tolerancia
					"H"							     				,;	//10 -> Tipo de Arredondamento a Ser Utilizado
					/*lSubstitui*/									,;	//11 -> Substitui a(s) Hora(s) Existente(s) ( Garanto Apenas 1 DSR por Data )
					/*cFuncao	*/									,;	//12 -> Funcao
					/*cDepto	*/									,;	//13 -> Depto para gravacao
					/*cPosto	*/									,;	//14 -> Posto para gravacao
					/*cProcesso	*/									,;	//15 -> Periodo para Gravacao
					/*cRoteiro	*/									,;	//16 -> Processo para Gravacao
					/*cPerApo	*/									,;	//17 -> Periodo para Gravacao
					/*cNumPagto	*/ 									,;	//18 -> NumPagto para Gravacao
					aSAviso[6]									    ,;	//19 -> Turno de Trabalho
					aSAviso[7]										 ;  //20 -> Semana/Sequencia do Turno
				)
			
		lRet := .T.
	EndIf
	
Return lRet

/*/{Protheus.doc} getIDsHE
Carrega as variáveis static com os Eventos para geração de Horas Extras Sobreaviso
@type static function
@author Cícero Alves
@since 09/09/2025
@return NIL, NIL
/*/
Static Function getIDsHE()
	
	Local nPosID		:= 0
	
	If cHESobre == NIL
		nPosID := aScan(aEvesIds, {|x| x[1] == "049A"}) // Hora extra sobreaviso
		cHESobre := If(nPosID > 0 .And. !Empty(aEvesIds[nPosID][2]), aEvesIds[nPosID][2], "")
	EndIf
	
	If cHENotS == NIL
		nPosID := aScan(aEvesIds, {|x| x[1] == "050A"}) // Hora extra sobreaviso Noturna
		cHENotS := If(nPosID > 0 .And. !Empty(aEvesIds[nPosID][2]), aEvesIds[nPosID][2], "")
	EndIf
	
	If cAdNHES == NIL
		nPosID := aScan(aEvesIds, {|x| x[1] == "051A"}) // Adicional Noturno HE sobreaviso.
		cAdNHES := If(nPosID > 0 .And. !Empty(aEvesIds[nPosID][2]), aEvesIds[nPosID][2], "")
	EndIf
	
Return 

/*/{Protheus.doc} CleanSobreA
Exclui os eventos de Hora Extra Sobreaviso gerados no aResult
@type  Static Function
@author Cícero Alves
@since 09/09/2025
@return NIL, NIL
/*/
Static Function CleanSobreA()
	
	Local nI 		:= 1
	Local cEventos 	:= 	""
	
	getIDsHE()
	
	cEventos := cHESobre + "|" + cHENotS + "|" + cAdNHES
	
	While nI <= Len(oAponta:aResult)
		If oAponta:aResult[nI][ARESULT_DATA] == oAponta:dData .And. oAponta:aResult[nI][ARESULT_EVENTO] $ cEventos
			aDel(oaponta:aResult, nI)
			aSize(oaponta:aResult, Len(oAponta:aResult)-1)
		Else
			nI++
		EndIf
	EndDo
	
Return
