/*/{Protheus.doc} f520SXB041
Consulta padrão de campos da S041
@author  isabel.noguti
@since   2018.11.07
@version 12.1.17
/*/
Function f520SXB041()
	Local MvPar		:= &(ReadVar())
	Local MvParDef	:= ""
	Local nTipo		:= 0
	Local aArea		:= GetArea()
	Local nCampo	:= aScan( aHeader, {|x| x[2] == PadR( substr( __readvar, 4, 10 ), 10 ) } )
	Local cTitulo	:= If( nCampo > 0, aHeader[nCampo][1], "" )
	Private aOpcoes	:= {}

	VAR_IXB := MvPar

	Do Case
		Case __readvar == "M->PorteEmpr"
			aOpcoes := {OemToAnsi(STR0117),;	//"1 - Micro Empresa"
						OemToAnsi(STR0118),;	//"2 - Empresa de Pequeno Porte"
						OemToAnsi(STR0119)}		//"3 - Empresa não classificada nos itens anteriores"
			MvParDef := "123"

		Case __readvar == "M->ParticPAT"
			cTitulo := OemToAnsi(STR0120)		//"Indicador de participação no Programa de Alimentação do Trabalhador"
			aOpcoes := {OemToAnsi(STR0121),;	//"1 - Estabelecimento participa do PAT"
						OemToAnsi(STR0122)}		//"2 - Estabelecimento não participa do PAT"
			MvParDef := "12"

		Case __readvar == "M->CONTRSOCFL"		//S129
			cTitulo := OemToAnsi(STR0125)		//"Percentual de contribuição social devida em caso de transformação em sociedade de fins lucrativos - Lei 11.096/2005"
			aOpcoes := {OemToAnsi(STR0126),;	//"1 - 0,2000"
						OemToAnsi(STR0127),;	//"2 - 0,4000"
						OemToAnsi(STR0128),;	//"3 - 0,6000"
						OemToAnsi(STR0129),;	//"4 - 0,8000"
						OemToAnsi(STR0130)}		//"5 - 1,0000"
			MvParDef := "12345"

		Otherwise
			aOpcoes := {OemToAnsi(STR0123),;	//"1 - Sim"
						OemToAnsi(STR0124)}		//"2 - Nao"
			MvParDef := "12"
	EndCase

	If f_Opcoes(@MvPar,cTitulo,aOpcoes,MvParDef,,,.T.,1)
		VAR_IXB := MvPar
	EndIf

	RestArea(aArea)
Return .T.
