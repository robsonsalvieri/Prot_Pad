Function R7DescTipInit()
	Local cDescTip	:= space(25)  
	Local cTipoR7 	:= ""

	If (Type( "oGetSR7") <> "U") 
	cTipoR7 := ""
	Else // #
	If SELECT("QRYSR7") > 0
		cTipoR7 := QRYSR7->R7_TIPO
	Else
		cTipoR7 := SR7->R7_TIPO
	Endif
	EndIf

	If !Empty(cTipoR7)
		If cPaisLoc == "RUS"
			cDescTip := fReasDesc(cTipoR7)
		Else
			cDescTip    :=  fDesc("SX5", "41" + cTipoR7, "X5Descri()", TamSX3("R7_DESCTIP")[1], SR7->R7_FILIAL, 01)
		EndIf	
	EndIf	

Return cDescTip     

