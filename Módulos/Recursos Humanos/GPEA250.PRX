	lAccess := aScan( aFilAcs , { | x | AllTrim( x[ 1 ] ) == cEmpVld .And. AllTrim( x[ 2 ] ) == AllTrim( cFilVld ) .And. x[ nPosFAcs ] } ) > 0

	If !lAccess
		MsgInfo( STR0044 )//"Usuário sem permiss?o para visualizar registros desta filial."
		lRet := .F.
	EndIf

