	Local cFiltraSRA	:= ""
	Local oldROTINA := aCLONE(aROTINA)
	Local nSizeSA1  := Iif((TAMSX3("A1_COD")[1]) < 1, 6, (TAMSX3("A1_COD")[1]))
	Local nSizeLo1  := Iif((TAMSX3("A1_LOJA")[1])< 1, 2, (TAMSX3("A1_LOJA")[1]))
	Local nSizeTD   := nSizeSA1+nSizeLo1
