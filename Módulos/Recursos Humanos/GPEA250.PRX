Static Function Gpea250PdDes(cPd)
	Local cDescPd	:= ""
	Local lPutDesc	:= .T.
	Local oMdl		:= FWModelActive()
	Local oGrid		:= oMdl:GetModel('MDL250DEF2')
	DEFAULT cPd		:= &(ReadVar())

	IF (cPd == "000")
		cDescPd     := STR0016  //"SALARIO BASE"
