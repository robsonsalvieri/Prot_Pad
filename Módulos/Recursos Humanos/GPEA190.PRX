Function GP190PicOut(cQuant)
Local cPicture
Local nPosColVlr	:= GdFieldPos("RO_VALOR")
Local nQuant		:= &(ReadVar(cQuant))
Local nVal			:= 0
Local lRet 		:= .T. //PICTURE NAO ESTOURADA

nVal := Round(nQuant * Acols[n][nPosColVlr], 4)

cPicture := GetSx3Cache("RO_VALTOT", "X3_PICTURE")
If SubStr(TransForm(nVal , cPicture), 1,1) == "*"
	Help(,, "QTDxTOT",, OemToAnsi(STR0057), 1, 0 )	//"Valor total excedeu seu limite permitido! Verifique a quantidade preenchida!"
	lRet := .F.
EndIf

If lRet .And. !IsBlind()
	cPicture := GetSx3Cache("RC_HORAS", "X3_PICTURE")
	If SubStr( TransForm(nQuant, cPicture), 1,1) == "*"
		Help(,, "QTDxHORAS",, OemToAnsi( STR0064 + AllTrim(Substr(cPicture,3)) ), 1,,,,,,, {STR0065})	// "Quantidade informada excede o limite do campo RC_HORAS: "
	EndIf																//"Verifique o lançamento ou solicite o aumento do campo para o administrador do sistema."
EndIf

Return lRet

