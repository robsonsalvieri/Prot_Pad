Function fValIFGTS()
Local lRet	:= .T.
Local cVerba := "  #00#11#12#21#31#91#92#93"
Local aVerba := {}
Local lValid := .T.

	aVerba := STRTOKARR(cVerba,"#")
	lValid := Iif( aScan(aVerba, M->RV_INCFGTS) > 0, .T., .F.)
	If !lValid
		//Caso o usuário informe um código que não existe na tabela
		lRet := .F.
		Help( , , 'HELP', , OemToAnsi(STR0272), 1, 0 ) //Informe um código válido
	EndIf

	//Valida o correto preenchimento da incidência FGTS do eConsignado
	If lRet .And. M->RV_INCFGTS == "31" .And. ((!Empty(M->RV_NATUREZ) .And. M->RV_NATUREZ <> "9253") .Or. M->RV_TIPOCOD <> "2")
		lRet := .F.
		 //"A incidência 31 - Desconto eConsignado só pode ser informada para verbas com Natureza de Rubrica igual a 9253 e do tipo Desconto"
		//"Se a incidência FGTS 31 - Desconto eConsignado for a correta, altere a Natureza de Rubrica para 9253 - Empréstimo eConsignado, caso contrário ajuste o valor informado para a incidência FGTS. "
		Help( , , 'HELP', , OemToAnsi(STR0426), 1, 0, , , , , , {OemToAnsi( STR0430 )} )
	EndIf

	//Valida o correto preenchimento da incidência FGTS do eConsignado
	If lRet .And. M->RV_INCFGTS <> "31" .And. !Empty(M->RV_NATUREZ) .And. M->RV_NATUREZ == "9253"
		lRet := .F.
		//"O campo Natureza de Rubrica está preenchido com 9253 - Empréstimo eConsignado. A incidência FGTS deve conter o valor 31 - Desconto eConsignado. "
		//"Preencha a incidência FGTS com o valor 31 - Desconto eConsignado ou altere o campo Natureza de Rubrica para um valor diferente de 9253 - Empréstimo eConsignado. "
		Help( , , 'HELP', , OemToAnsi(STR0428), 1, 0, , , , , , {OemToAnsi( STR0433 )} )
	EndIf

Return lRet

