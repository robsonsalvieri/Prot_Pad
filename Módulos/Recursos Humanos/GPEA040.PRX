MvParDef := "1234567890"

If f_Opcoes(@MvPar,cTitulo,aGrpVerba,MvParDef,,,l1Elem)
	For nGrupo := 1 To Len(MvPar)
		If (SubStr(MvPar, nGrupo, 1) # "*")
			MvStrRet += SubStr(mvpar, nGrupo, 1)
