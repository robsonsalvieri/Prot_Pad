Local lRet			:= .T.

	If cVersGPE >= "9.0"
		lRet := GP310SXB('S138','CODIGO')
	Else
		lRet := fRetOpcGPE('RV_INCIRF')
	Endif

	RestArea(aArea)

Return lRet


//-------------------------------------------------------------------
/*/{Protheus.doc} fChkNatSRV
Checa existência de verbas cadastradas com Natureza 1020/1021
@author		isabel.noguti
@since		19/04/2023
@version	12.1.2210
@param	cFilQry	-filial de pesquisa
@return	aVbs	-array com os códigos das verbas encontradas
/*/
//-------------------------------------------------------------------
Function fChkNatSRV(cFilQry)
Local aVbs		:= {}
Local cQrySRV	:= GetNextAlias()
Local cQryWhere	:= ""

Default cFilQry	:= ""

	cQryWhere := "RV_NATUREZ IN ('1020','1021')"
	If !Empty(cFilQry)
		cQryWhere += " AND RV_FILIAL = '" + xFilial("SRV", cFilQry) + "'"
	EndIf
	cQryWhere := "%" + cQryWhere + "%"

	BeginSql alias cQrySRV
		SELECT RV_COD
		FROM %table:SRV% SRV
		WHERE SRV.%NotDel% AND %exp:cQryWhere%
	EndSql

	While !(cQrySRV)->(EoF())
		aAdd( aVbs, (cQrySRV)->RV_COD )
		(cQrySRV)->(dbSkip())
	EndDo
	(cQrySRV)->(dbCloseArea())

	If !Empty(aVbs)
		fAvsNatFer()
	EndIf

Return aVbs

//-------------------------------------------------------------------
/*/{Protheus.doc} fAvsNatFer
Função para exibição de alerta e link para o TDN com orientação sobre Natureza de Rubrica 1020/1021
@author	 isabel.noguti
@since	 19/04/2023
@version 12.1.2210
/*/
//-------------------------------------------------------------------
Static Function fAvsNatFer()
Local oButton1
Local oCheckBo1
Local lCheckBo1 := .F.
Local oGroup1
Local oPanel1
Local oSay1
Local oSay2
Local cSession	:= "AlertNatur1020"
Local lChkMsg 	:= fwGetProfString(cSession, "MSG_NAT1020_" + cUserName,'',.T.) == ""
Local cLinkTDN	:= "https://tdn.totvs.com/pages/viewpage.action?pageId=754249157"

Static oDlg

	If lChkMsg
		DEFINE MSDIALOG oDlg TITLE OemToAnsi(STR0419) FROM 000, 000  TO 200, 500 COLORS 0, 16777215 PIXEL //"Natureza de Rubrica inválida - NT 01/2023"

			@ 000, 000 MSPANEL oPanel1 SIZE 300, 150 OF oDlg COLORS 0, 16777215 RAISED
			@ 005, 012 GROUP oGroup1 TO 058, 237 PROMPT OemToAnsi(STR0028) OF oPanel1 COLOR 0, 16777215 PIXEL //"Atenção"

			@ 013, 017 SAY oSay1 PROMPT OemToAnsi(STR0420) SIZE 215, 025 OF oPanel1 COLORS 0, 16777215 PIXEL //"Em sua Tabela de Verbas existem registros com Natureza de Rubrica 1020 ou 1021, que foram substituídas por 1016, 1017, 1018 e 1019."
			@ 028, 017 SAY oSay1 PROMPT OemToAnsi(STR0421) SIZE 215, 025 OF oPanel1 COLORS 0, 16777215 PIXEL //"Estas verbas não serão mais aceitas a partir do dia 01/05/2023, e o evento não será acatado pelo RET. É necessário rever a configuração das verbas."
			@ 046, 017 SAY oSay1 PROMPT OemToAnsi(STR0422) SIZE 215, 025 OF oPanel1 COLORS 0, 16777215 PIXEL //"Para mais informações, consulte a documentação abaixo:"

			@ 063, 012 SAY oSay2 PROMPT cLinkTDN SIZE 175, 007 OF oPanel1 COLORS 0, 16777215 PIXEL
			@ 080, 012 CHECKBOX oCheckBo1 VAR lCheckBo1 PROMPT OEMToAnsi(STR0423) SIZE 067, 008 OF oPanel1 COLORS 0, 16777215 PIXEL //#STR0242 = "Não exibir novamente"
			@ 070, 200 BUTTON oButton1 PROMPT "OK" SIZE 037, 012 OF oPanel1 PIXEL

			oSay2:bLClicked := {|| ShellExecute( "open", cLinkTDN, "", "", 1 ) }

			oButton1:bLClicked := {|| oDlg:End() }

		ACTIVATE MSDIALOG oDlg CENTERED

		If lCheckBo1
			fwWriteProfString(cSession,"MSG_NAT1020_" + cUserName, 'CHECKED', .T.)
		EndIf

	EndIf
Return .T.

