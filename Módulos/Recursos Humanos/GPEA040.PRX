Function FRCCParametros()
Local lRet 		:= .F.
Local cTipoCod	:= M->RV_TIPOCOD
Local lGpem610	:= .F.

If Select("RCC")>0
	RCC->(DbCloseArea())
	dbSelectArea("RCC")
Endif

If M->RV_TIPOCOD =='2' .Or. M->RV_CODFOL == '0210'
	lGpem610 := .T.
	cTipoCod := '2'
EndIf

If lGpem610
	lRet := GPEM601V04(cTipoCod,0,M->RV_HOMOLOG)
EndIf

lRet := ConPad1(,,,"S20")

Return lRet




