Function GetNextRecno( cAlias , nNextRecno , nRecno , nOrder )

IF Empty( nNextRecno )
	nRecno := ( cAlias )->( Recno() )
Else
	nRecno := nNextRecno
EndIF
( cAlias )->( dbSkip() )
IF ( cAlias )->( Eof() )
	nNextRecno := NIL
Else
	nNextRecno := ( cAlias )->( Recno() )
EndIF

Return( GotoNextRecno( cAlias , nRecno , nOrder ) )

