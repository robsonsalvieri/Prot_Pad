Function RstGetCache( cAlias , cFil )
         
Local bAscan
Local lFilIsNil
Local nPosAlias

IF ( cAlias == NIL )
	aGetCache := {}
Else
	lFilIsNil := ( cFil == NIL )
	IF !( lFilIsNil )
		cFil := xFilial( cAlias , cFil )
		bAscan := { |x| ( x[1] == cAlias ) .and. ( x[2] == cFil ) }
	Else
		bAscan := { |x| ( x[1] == cAlias ) }
	EndIF
	While ( ( nPosAlias := aScan( aGetCache , bAscan ) ) > 0 )
		aDel( aGetCache , nPosAlias )
		aSize( aGetCache , ( Len( aGetCache ) - 1 ) )
	End While
EndIF

Return( NIL )

