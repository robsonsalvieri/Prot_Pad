IF ( lDelOk := (;
					!( lShowLog );
					.or.;
					(;
						Iif(lRotAuto,.T.,  ( CursorArrow(),;
						MsgYesNo( cMsgYesNo , (cTitleMsg + " - " + cTitLog) )) ) ;
