Function RestartNotUse( aSelect , aNotSelect )

Local cSelect		:= ""
Local lChk			:= .T.
Local lSelect		:= .T.
Local nUse			:= 0
Local nSelect		:= 0

aNotSelect := {}

IF ( ( nSelect := Len( aSelect ) ) > 0 )
	For nUse := 1 To nSelect
		cSelect := aSelect[ nUse ]
		IF ( Select( cSelect ) == 0 )
			IF !( lChk := ChkFile( cSelect ) )
				aAdd( aNotSelect , cSelect )
			EndIF
		EndIF	
	Next nUse
EndIF

lSelect := ( Empty( aNotSelect ) .and. !Empty( aSelect ) )

Return( lSelect )

