		aAdd( aVerbas, aClone(aVerbas[nPos3]) )
		aVerbas[Len(aVerbas), 1] := aCodFol[64,1]
		aVerbas[Len(aVerbas), 5] := 0.00
		lAux := .T.
	ElseIf aVerbas[nPos4, 9] == "D"
