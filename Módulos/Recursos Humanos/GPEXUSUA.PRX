			nTotMedAux += ( nOrigHrMat - aPdOld[nPos,5] )
			aPdOld[nPos,5] := nOrigHrMat
		EndIf

		If ( nPos := aScan(aPdOld, {|x| x[1] $ aCodFol[123,1]}) ) > 0//Total de Medias em Valor
			nValAux := nOrigVal
