		nDiasMes := 0
		nDiasMse := Day(SRH->RH_DATAFIM) - SRH->RH_DIALRE1
	EndIf
	If cDifFer == "S"
		Aadd(aDifFer,{ Recno() , nDiasMes , nDiasMse })
	EndIf
	nRecSrh := Recno()
	dbSkip()
Enddo
dbSelectArea("SRH")
dbGoTo(nRecSrh)

Return

