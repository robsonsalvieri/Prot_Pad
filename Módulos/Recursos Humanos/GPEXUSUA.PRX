			If fTemHMed(dDataDe,"2",cPeriodo,cSemana,fGetCalcRot("6"))
				If nValAux > RJK->RJK_SALBAS //Compara se houve alteração salarial entre o cálculo do roteiro 132 e o cálculo do complemento na folha
					If ( nPos := aScan(aVerbasLanc, {|x| x[3] == aCodFol[28,1] }) ) > 0
						nValAux := aVerbasLanc[nPos,7] - nTotMedAux //Subtrai as diferença de médias da diferença de 13º apurada no complemento da folha
						If ( nPos := aScan(aPdOld, {|x| x[1] == aCodFol[24,1] .and. x[9] <> "D"}) ) > 0
							aPdOld[nPos,5] += nValAux //Soma a diferença de 13º pago na folha para gerar diferença correta no dissidio
						EndIf
					EndIf
				EndIf 
			EndIf 
		EndIf
		
	EndIf
EndIf

