Function fGetTipoRot( cRoteiro )

Local aArea 	:= GetArea()
Local aAreaSRY  := SRY->( GetArea() )
Local cTipo
Local cAuxRot   := Upper(cRoteiro)

If cAuxRot != SRY->RY_CALCULO
	DbSelectArea( "SRY" )
	DbSetOrder( RetOrder( "SRY", "RY_FILIAL+RY_CALCULO" ) )
	DbSeek( xFilial("SRY") + cAuxRot, .F. )
EndIf

If SRY->( !Eof() )
	cTipo := SRY->RY_TIPO
EndIf

RestArea( aAreaSRY )
RestArea( aArea )


Return( cTipo )

