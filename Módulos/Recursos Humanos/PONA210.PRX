   If !Empty(M->PK_HrsAbo)
		If !Empty(nPosHrAb) .AND. !Empty(nPosHrIn)
	  	   //Calcula e Sugere Hora Final do Abono (Hora Inicial + Saldo de Horas a Abonar)
