Function Pn210ValDt()

Local dData	:= &(ReadVar())
Local lRet := .T.

IF n <= Len(aColsAnt) .and. !lChamadaNIL
	IF aCols[n,nPosData] # M->PK_DATA .And. !Empty(aCols[n,nPosData])
	   lRet := .F.
	EndIF
ElseIF lChamadaNIL
	IF dData < dPerIni .or. dData > dPerFim
	   lRet := .F.
	EndIF
EndIF

Return( lRet )

