/*/{Protheus.doc} fTotalInt
Verifica se a hora inicial do apontamento + horas abonadas possui um intervalo dentro do período e retorna as horas de intervalo
@author Marco Nakazawa
@since 19/07/2022
@version P12.1.27
/*/
Static Function fTotalInt(aTabCalend, dDataIni, nHorasAbo, nAboIni)
Local dDataApon		:= cTod(" /  / ")
Local nTotIntHrs	:= 0
Local nI			:= 0
Local nPos			:= 0
Local nDtHrIniAb	:= 0
Local nDtHrFimAb	:= 0
Local nDtHrIniIn	:= 0
Local nDtHrFimIn	:= 0
Local nFimInt		:= 0
Local nAboFim		:= 0

DEFAULT aTabCalend	:= {}
DEFAULT dDataIni	:= cTod(" /  / ")
DEFAULT nHorasAbo	:= 0
DEFAULT nAboIni		:= 0

If !Empty(aTabCalend) .And. !Empty(dDataIni) .And. nHorasAbo > 0
	nPos := aScan(aTabCalend, {|x| x[CALEND_POS_DATA_APO] == dDataIni})

	If nPos > 0
		dDataApon := aTabCalend[nPos,CALEND_POS_DATA_APO]
		nI := nPos
		nDtHrIniAb	:= fDHtoNS(dDataIni, nAboIni) 
		nDtHrFimAb	:= fDHtoNS(dDataIni, SomaHoras(nAboIni,nHorasAbo))
		nAboFim		:= fNStoDH(nDtHrFimAb,"H")
		
		While nI <= Len(aTabCalend) .And. aTabCalend[nI,CALEND_POS_DATA_APO] == dDataApon
			If aTabCalend[nI,CALEND_POS_TIPO_MARC] $ "1S*2S*3S"	.And. aTabCalend[nI,CALEND_POS_HRS_INTER] > 0
				nDtHrIniIn	:= fDHtoNS(aTabCalend[nI,CALEND_POS_DATA],aTabCalend[nI,CALEND_POS_HORA])
				nDtHrFimIn	:= fDHtoNS(aTabCalend[nI,CALEND_POS_DATA],aTabCalend[nI,CALEND_POS_HORA] + aTabCalend[nI,CALEND_POS_HRS_INTER])
				nFimInt 	:= fNStoDH(nDtHrFimIn,"H")
				
				If (nDtHrIniAb > nDtHrIniIn .And. nDtHrIniAb <= nDtHrFimIn)
					If (nFimInt > nAboIni, nTotIntHrs := SomaHoras(nTotIntHrs,SubHoras(nFimInt,nAboIni)),nTotIntHrs := SomaHoras(nTotIntHrs,SubHoras(nAboIni,nFimInt)))
				ElseIf nDtHrIniAb <= nDtHrIniIn .And. nDtHrFimAb >= nDtHrFimIn
					nTotIntHrs := SomaHoras(nTotIntHrs, aTabCalend[nI,CALEND_POS_HRS_INTER])
				ElseIf nDtHrFimAb > nDtHrIniIn .And. nDtHrFimAb <= nDtHrFimIn
					nTotIntHrs := SomaHoras(nTotIntHrs, SubHoras(nAboFim,aTabCalend[nI,CALEND_POS_HORA]))
				EndIf
			EndIf
			nI++
		End While
	EndIf
EndIf

Return nTotIntHrs
