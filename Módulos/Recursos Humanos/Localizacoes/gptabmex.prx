Static Function Tab001( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local nVerProg		:= 0
Local nVerLoc		:= 0

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S001','Impuesto Bruto Mensual','01','LIMINF','LМmite Inferior','N',12,2,'99999999.99','','2','S','003','','',} )
aAdd( aRCBItens, { '','S001','Impuesto Bruto Mensual','02','LIMSUP','LМmite Superior','N',12,2,'99999999.99','','1','S','003','','',} )
aAdd( aRCBItens, { '','S001','Impuesto Bruto Mensual','03','PORCENTAJE','Porcentaje','N',5,2,'99.99','','2','S','003','','',} )
aAdd( aRCBItens, { '','S001','Impuesto Bruto Mensual','04','CF','Cuota Fija','N',12,2,'999999999.99','','2','S','003','','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])

If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIf

nVerProg := Val(cVersao)
nVerLoc := Val(cCliVersao)

If nVerProg > nVerLoc
	fGravaTab( aRCBHeader, aRCBItens )
EndIf

Return( NIL )

