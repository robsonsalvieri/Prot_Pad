Static Function Tab001( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local nVerProg		:= 0
Local nVerLoc		:= 0

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S001','CARGAS FAMILIARES','01','TRAMO','TRAMO','C',1,0,'@!','','','001','1','','',} )
aAdd( aRCBItens, { '','S001','CARGAS FAMILIARES','02','DESDE','DESDE','N',12,0,'999,999,999','','','001','2','','',} )
aAdd( aRCBItens, { '','S001','CARGAS FAMILIARES','03','HASTA','HASTA','N',12,0,'999,999,999','','','001','2','','',} )
aAdd( aRCBItens, { '','S001','CARGAS FAMILIARES','04','MONTO','MONTO DE CARGA','N',12,0,'999,999,999','','','001','2','','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])

If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIf

nVerProg := Val(cVersao)
nVerLoc := Val(cCliVersao)

If nVerProg > nVerLoc
	fGravaTab( aRCBHeader, aRCBItens )
EndIf

Return( NIL )

