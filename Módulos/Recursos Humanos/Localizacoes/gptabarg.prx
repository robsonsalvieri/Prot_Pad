Static Function Tab001( aRCBHeader )

Local aRCBItens	:= {}

Local cFilRCB		:= xFilial('RCB')
Local cVersao		:= ""
Local cCliVersao

Local nVerProg		:= 0
Local nVerLoc		:= 0

Local lFound

Local nPosCodigo	:= 0

DEFAULT aRCBHeader	:= {}

aAdd( aRCBItens, { '','S001','RИgimen','01','CODREG','RИgimen','N',4,0,'9999','NAOVAZIO()','','001','1','S','',} )
aAdd( aRCBItens, { '','S001','RИgimen','02','FCHDES','Vigencia Desde','N',6,0,'@R 9999/99','','','001','1','S','',} )
aAdd( aRCBItens, { '','S001','RИgimen','03','FCHHAS','Vigencia Hasta','N',6,0,'@R 9999/99','','','001','1','S','',} )
aAdd( aRCBItens, { '','S001','RИgimen','04','DESCRP','DescripciСn','C',30,0,'@!','','','001','1','S','',} )

nPosCodigo	:= GdFieldPos( 'RCB_CODIGO' , aRCBHeader )
cVersao		:= aRCBItens[1, GdFieldPos( 'RCB_VERSAO' , aRCBHeader )]

DbSelectArea("RCB")
DbSetOrder(1)    // RCB_FILIAL + RCB_CODIGO
lFound := DbSeek( cFilRCB + aRCBItens[1, nPosCodigo])

If lFound
	cCliVersao := RCB->(RCB_VERSAO)
Else
	cCliVersao := "__New_Version__"
EndIf

nVerProg := Val(cVersao)
nVerLoc := Val(cCliVersao)

If nVerProg > nVerLoc
	fGravaTab( aRCBHeader, aRCBItens )
EndIf

Return( NIL )

