method ChkApoAfas(lApHeDtM) class PonVarApon
	
	Local lRet 			:= .T.
	Local lApontaFer	:= .F.
	Local lTemMarc		:= .F.
	Local nPosData
	
	Default lApHeDtM 	:= .F.
	
	::dIniAfas := ::aTabCalend[ ::nPosCalend , CALEND_POS_INI_AFAST ]
	::dFimAfas := ::aTabCalend[ ::nPosCalend , CALEND_POS_FIM_AFAST ]
	
	If ::aTabCalend[::nPosCalend, CALEND_POS_AFAST] .And. ::dData >= ::dIniAfas .And. ::dData <= If(Empty(::dFimAfas), ::dData, ::dFimAfas)
		
		// ExceГЦo trabalhada
		If !(::aTabCalend[::nPosCalend, CALEND_POS_TIPO_DIA] == 'S' .And. ::aTabCalend[::nPosCalend, CALEND_POS_EXCECAO] == 'E')
			
			lApontaFer := ::aTabCalend[::nPosCalend, CALEND_POS_APON_FERIAS]
			
			lTemMarc := aScan(::aMarClone, {|x| x[AMARC_DATAAPO] == ::dData}) > 0 .Or. (lApHeDtm .And. ::lAponHeAfa .And. ::lAponHrAfa .And. !Empty(::aHorasAnt)) .Or. (lApHeDtm .And. !Empty(::aHorasAnt) .And. ValType(::aHorasAnt[1,28]) == "D" .And. ::aHorasAnt[1,28] < ::dIniAfas)
			
			IF !( lApontaFer ) .And. !lTemMarc
				IF ( ( nPosData := aScan( ::aResult , { |x| x[ ARESULT_DATA ] == ::dData } ) ) > 0 )
					aEval( ::aResult , { |x| x[ ARESULT_HORAS ] := 0 } )
				EndIF
				
				lRet := .F.
			ElseIf lApontaFer .And. lTemMarc 
				lRet := .T.
			ElseIf (::lAponHeAfa .Or. ::lAponHrAfa) .And. lTemMarc
				::lAtestMed := .T.
				lRet := .T.
			Else
				lRet := .F.
			EndIF
		EndIf
	EndIF
	
return lRet

