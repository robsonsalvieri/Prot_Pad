	::GetPerDia(	dDt1		,;	//01 -> Data 1
					nHr1		,;	//02 -> Hora 1
					dDt2		,;	//03 -> Data 2
					nHr2		,;	//04 -> Hora 2				
					aPerNot		,;	//05 -> Periodo da Hora Noturna
					@aPerDia	 ;	//06 -> Periodo da Hora Diurna
			  ) 

Endif

return( nHorasCalc )

