/*/{Protheus.doc} ValdPerNot
Retorna a data do inicio do adicional noturno
@type  Function
@author Marco Nakazawa
@since 29/08/2025
@Return NIL, NIL
/*/
Function ValdPerNot(dDataRef, nHoras, nIniHrNot, nFimHrNot, dAponNIni, dAponNFim, aTabCalend)
Local dDtIniNot	:= cTod("/ / ")
Local cMarcDtHr	:= ""
Local cIniNDtHr	:= ""
Local cFimNDtHr	:= ""
Local nI		:= 0

Default dDataRef	:= cTod("/ / ")
Default nHoras		:= 0
Default nIniHrNot	:= 0
Default nFimHrNot	:= 0
Default dAponNIni	:= cTod("/ / ")
Default dAponNFim	:= cTod("/ / ")
Default aTabCalend	:= {}

If !Empty(dDataRef) .And. !Empty(aTabCalend)
	// Valida o dia do inicio do horАrio noturno de acordo com a marcaГЦo
	cMarcDtHr := DataHora2Str(dDataRef, nHoras)	//-- Data hora marcaГЦo

	If (nPos := aScan(aTabCalend, {|x| x[CALEND_POS_DATA] == dDataRef .And. x[CALEND_POS_TIPO_MARC] == "1E"})) > 0 .And. nHoras > aTabCalend[nPos, CALEND_POS_FIM_H_NOT]
		nFimHrNot := aTabCalend[nPos, CALEND_POS_FIM_H_NOT]
	EndIf

	For nI := 1 To 2
		If nI == 1
			If nFimHrNot >= nIniHrNot
				cIniNDtHr := DataHora2Str(dDataRef, nIniHrNot)	//-- Data hora do inicio do horАrio noturno
			Else
				cIniNDtHr := DataHora2Str(dDataRef - 1, nIniHrNot)	//-- Data hora do inicio do horАrio noturno
			EndIf

			cFimNDtHr := DataHora2Str(dDataRef, nFimHrNot)		//-- Data hora do fim do horАrio noturno
			
			If cMarcDtHr >= cIniNDtHr .And. cMarcDtHr <= cFimNDtHr
				If nFimHrNot >= nIniHrNot
					dDtIniNot := dDataRef
				Else
					dDtIniNot := dDataRef - 1
				EndIf
				Exit
			EndIf
		Else
			If nFimHrNot >= nIniHrNot
				cFimNDtHr := DataHora2Str(dDataRef, nFimHrNot)	//-- Data hora do inicio do horАrio noturno
			Else
				cFimNDtHr := DataHora2Str(dDataRef + 1, nFimHrNot)	//-- Data hora do inicio do horАrio noturno
			EndIf
			
			cIniNDtHr := DataHora2Str(dDataRef, nIniHrNot)		//-- Data hora do fim do horАrio noturno

			If cMarcDtHr >= cIniNDtHr .And. cMarcDtHr <= cFimNDtHr
				dDtIniNot := dDataRef
			EndIf
		EndIf
	Next
EndIf

If Empty(dDtIniNot)
	dDtIniNot := dAponNIni
Else
	If dDtIniNot == dAponNFim
		dAponNFim := dAponNFim + 1
	EndIf
EndIf

Return dDtIniNot
