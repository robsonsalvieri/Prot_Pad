method Sr6GetSetCpos() class PonVarApon
	
	Local aSr6Info
	Local lGetSetOK
	
	Begin Sequence
		
		// Seta o Turno Atual
		::GetSetTnoC( aTabCalend )
		
		// Verifica se o Turno de Trabalho do funcionАrio И vАlido.
		IF !( lGetSetOK := ::PosSR6() )
			Break
		EndIF
		
		// SС verifica os Campos dos Turnos Quando Houver ModificaГЦo
		IF ( ::FilTnoCompare() )
			Break
		Else
			::cFilTnoLst := ( ::cFilSR6 + ::cTnoCalend )
		EndIF
		
		// Carrega as InformaГУes em Cache do SR6
		aSr6Info := ::PosSr6( ::oSr6Fields:aSr6Fields , .F. )
		
		// Carrega o ConteЗdo do Campo R6_INTERNT
		::cInterNt		:= aSr6Info[ ::oSr6Fields:nR6InterNt ]	//-- Tipo de Dia para Geracao Inter-jornada
		
		// Carrega o ConteЗdo do Campo R6_HRINTER
		::nHrInterJd	:= aSr6Info[ ::oSr6Fields:nR6HrInter ]	//-- Numero Minimo de Horas Inter-Jornada
		
		// Carrega o ConteЗdo do Campo R6_APTPMAR
		::lApTpMar		:= ( aSr6Info[ ::oSr6Fields:nR6AptpMar ] == "S" )	//-- Gravar Tipo de Marcacao nos Apontamentos
		
		// Carrega o ConteЗdo do Campo R6_HNOTATR
		::lHnotAtr		:= ( aSr6Info[ ::oSr6Fields:nR6HnotAtr ] == "S" )	//-- Considerar a Hora Noturna Para na Apuracao das Saidas Antecipadas/Expediente
		
		// Carrega o ConteЗdo do Campo R6_HNOTTAB
		::lHnotTab		:= ( aSr6Info[ ::oSr6Fields:nR6HnotTab ] == "S" )	//-- Considerar a Hora Noturna Para Horas Trabalhadas
		
		// Carrega o ConteЗdo do Campo R6_HNOTTBI
		::lHnotTbI		:= ( aSr6Info[ ::oSr6Fields:nR6HnotTbI ] == "S" )	//-- Considerar a Hora Noturna Para Horas de Intervalo
		
		// Carrega o ConteЗdo do Campo R6_HNOTFAL
		::lHnotFal		:= ( aSr6Info[ ::oSr6Fields:nR6HnotFal ] == "S" )	//-- Considerar a Hora Noturna Para na Apuracao das Faltas
		// Carrega o ConteЗdo do Campo R6_HNOTSAI
		
		::lHnotSai		:= ( aSr6Info[ ::oSr6Fields:nR6HnotSai ] == "S" )	//-- Considerar a Hora Noturna Para na Apuracao das Saidas Antecipadas/Expediente
		
		// Carrega o ConteЗdo do Campo R6_EXTNOT 
		::lExtNot		:= ( aSr6Info[ ::oSr6Fields:nR6ExtNot ] == "S" )	//-- Aponta Hora Noturno considerando as Horas Noturnas Reduzidas
		
		// Carrega o ConteЗdo do Campo R6_ACRENOT
		::lAcreNot		:= ( aSr6Info[ ::oSr6Fields:nR6AcreNot ] == "S" )	//-- Se vai considerar Apenas o Acrescimo Noturno
		
		// Carrega o ConteЗdo do Campo R6_ACRENOT
		::lIdAcreNot		:= ( aSr6Info[ ::oSr6Fields:nR6IdAcreNot ] == "S" )	//-- Se vai Destacar em Identificador Especifico o Acrescimo Noturno
		
		// Carrega o ConteЗdo do Campo R6_SPADNOT
		If SR6->(ColumnPos("R6_SPADNOT")) > 0
			::lSepAcrNot	:= ( aSr6Info[ ::oSr6Fields:nR6SpAcNot ] == "S" )	//-- Se irА separar o acrИscimo noturno da hora noturna
		Else
			::lSepAcrNot	:= .F.
		EndIf
		
		// Carrega o ConteЗdo do Campo R6_TPJORN
		::cTpJorn		:= aSr6Info[ ::oSr6Fields:nR6TpJorn ]	//-- Tipo de jornada do turno (Descontinuado, HorАrio diАrio e folga fixa, 12x36, HorАrio diАrio fixo e folga variАvel e Demais tipos de jornada)
		
		// Carrega o ConteЗdo do Campo R6_DSRINTE
		If SR6->(ColumnPos("R6_DSRINTE")) > 0
			::nHrDSRInter	:= ( aSr6Info[::oSr6Fields:nR6HrDSR] )	//-- Quantidade de horas de DSR interjornada
			If ::nHrDSRInter == 0
				::nHrDSRInter	:= 24
			EndIf
		Else
			::nHrDSRInter	:= 24
		EndIf

		// Carrega o ConteЗdo do Campo R6_FERINTE
		If SR6->(ColumnPos("R6_FERINTE")) > 0
			::nHrFerInter := (aSr6Info[::oSr6Fields:nR6HrFER])	//-- Quantidade de horas interjornada de feriado
		Else
			::nHrFerInter := 0
		EndIf
		
	End Sequence
	
return( lGetSetOK )

