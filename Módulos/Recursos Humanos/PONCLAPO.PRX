method ChkMarcImpar() class PonVarApon
Local nLenHor	:= Len( ::aHoras )
Local nI		:= 0

IF ( ( nLenHor % 2 ) > 0 )
	For nI := nLenHor To 1 Step -1
		If ::aHoras[nI,3] == "M"
			aAdd(::aHoras,::aHoras[nI])
			Exit
		EndIf
	Next

	::SortaHoras()

EndIF

return( NIL )

