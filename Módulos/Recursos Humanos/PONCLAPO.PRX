method fAvalTol( dData, cEventos, nTolerancia, lVerHrNr, cTpHrExt, lFalta) class PonVarApon
	
	Local nTotHoras		:= 0
	Local nX			:= 0
	Local nI			:= 0
	Local aVerHrEx		:= {}
	Local nPosSI		:= 0
	Local nPosEI		:= 0
	Local nPosSIMarc	:= 0
	Local nResultDiv	:= 0
	Local aMarcDia		:= {}
	Local aRetFimInt	:= {}
	Local lGeraExt		:= .F.
	Local dDataAnt		:= sToD("")		
	Local cTotHoras		:= ""
	Local aClonTabPd	:= aClone( ::aTabCalend )
	Local cHrIntIni		:= ""
	Local cHrIntFim		:= ""
	Local cMarcHora		:= ""
	Local cMarcTab		:= ""
	Local nHrIntIni		:= 0
	Local nHrIntFim		:= 0
	Local nTotInterv	:= 0
	Local lTolMarc		:= .F.
	Local lMarcVld		:= .T.
	Local lMemoCalc		:= GetlMemoCalc()
	
	Static dUltimaD		:= cToD("//")
	
	Default lVerHrNr 	:= .F.
	Default cTpHrExt 	:= ""
	Default lFalta		:= .F.
	
	// Verifica se a tolerБncia por marcaГЦo estА preenchida
	For nI := 1 To 8
		If fTolerancia(If(cTpHrExt == "E", ::cTolHex, ::cTolaSai), nI, 2, dData) > 0
			lTolMarc := .T.
			Exit
		EndIf
	Next
	
	IF ( nTolerancia > 0 )
		
		If lVerHrNr .and. !Empty(::aHrNReal)
			aEval( ::aHrNReal, { |x| IF(( x[1] == dData ) .and. ( x[2] $ cEventos ), nTotHoras := SomaHoras( nTotHoras, x[3] ), NIL)})		
		Else
			aEval( ::aResult, { |x| IF(( x[1] == dData ) .and. ( x[2] $ cEventos ), nTotHoras := SomaHoras( nTotHoras, x[3] ), NIL)})
		EndIf
		
		cTotHoras := cValtoChar(nTotHoras)
		nTotHoras := val(substr(cTotHoras, 1, at(".", cTotHoras) + 2)) //truncando na marra 2 casas decimais
		If lMemoCalc .And. cTpHrExt == "E" .And. dUltimaD != dData
			//"Aplicada a TolerБncia Por PerМodo"
			If lMemoDia
				GravaMCalc("02", 3, "-fAvalTol")
				lMemoDia := .F.
			EndIf
			// "Aplicada a TolerБncia Por PerМodo"
			GravaMCalc("02", 4, STR0002 + " | " + dToC(dData) + ": " + Transform(StrZero(nTotHoras * 100, 4), "@R XX:XX") + " > " + Transform(StrZero(nTolerancia * 100, 4), "@R XX:XX") + " = " + cValToChar(nTotHoras > nTolerancia))
			dUltimaD := dData
		EndIf
		IF ( nTotHoras <= nTolerancia )
			
			If lVerHrNr .and. !Empty(::aHrNReal)
				For nX := 1 to Len(::aHrNReal)
					If !::aHrNReal[nX, 4] .and. ::aHrNReal[nX, 1] == dData
						aEval( ::aResult, { |x, y| If( ::aHrNReal[nX, 2] $ cEventos .And.  ( x[1] == ::aHrNReal[nX, 1]) .and. ( x[2] == ::aHrNReal[nX, 2] ) .and. ( If(!Empty(x[5]), x[5] == ::aHrNReal[nX, 6], .T. )), ::aResult[y, 3] := ::aHrNReal[nX, 5], NIL)})						
					EndIf
				Next nX
			ElseIf !(Empty(cTpHrExt))
				aEval( ::aMarClone, { |x| If(x[25] == dData, aadd(aMarcDia, aClone(x)), Nil)})				
				For nX := 1 To Len(aMarcDia)
					If (cTpHrExt == "S" .And. ! "S" $ aMarcDia[nX][12]) .Or. (cTpHrExt == "A" .And. ! "E" $ aMarcDia[nX][12])
						LOOP
					EndIf
					
					If (nPosTabCalend := aScan( ::aTabCalend, { |x| x[1] == aMarcDia[nX, 1] .And. x[4] == aMarcDia[nX, 12]})) > 0
	
						// Se for intervalo mСvel, altero o valor da saida entrada de intervalo para comparar futuramente com a tolerБncia
						If ::lIntMovFree
							nResultDiv := (Len(aMarcDia)/2)-1
							If dDataAnt <> aMarcDia[nX, 25]
								For nI:=1 To nResultDiv
									nPosSI := aScan( aClonTabPd, {|x| x[1] == aMarcDia[nX, 1] .And. x[4] == cValToChar(nI)+"S"} )
									nPosEI := aScan( aClonTabPd, {|x| x[1] == aMarcDia[nX, 1] .And. x[4] == cValToChar(nI+1)+"E"} )
									
									nPosSIMarc := aScan( aMarcDia, {|x| x[1] == aMarcDia[nX, 1] .And. x[12] == cValToChar(nI)+"S"} )
									
									If nPosSIMarc > 0 .And. nPosEI > 0 .And. nPosSI > 0
										aClonTabPd[nPosSI,3] := aMarcDia[nPosSIMarc,2] 
										aClonTabPd[nPosEI,3] := SomaHoras(aMarcDia[nPosSIMarc,2],aClonTabPd[nPosSI,9]) 
									EndIf
								Next
								dDataAnt := aMarcDia[nX, 25]
							EndIf
							nMinDif := fCalHoras(aClonTabPd[nPosTabCalend, 1], aClonTabPd[nPosTabCalend, 3], aMarcDia[nX, 1], aMarcDia[nX, 2])
						ElseIf !::lIntMovFree .And. ::nMovAint + ::nMovPint > 0
							nResultDiv := (Len(aMarcDia)/2)-1
							
							If cTpHrExt == "E" .And. ::aTabCalend[nPosTabCalend, 9] > 0
								For nI:=1 To nResultDiv
									nPosSI := aScan(aClonTabPd, {|x| x[1] == aMarcDia[nX, 1] .And. x[4] == cValToChar(nI)+"S"})
									nPosEI := aScan(aClonTabPd, {|x| x[1] == aMarcDia[nX, 1] .And. x[4] == cValToChar(nI+1)+"E"})
									
									nPosSIMarc := aScan(aMarcDia, {|x| x[1] == aMarcDia[nX, 1] .And. x[12] == cValToChar(nI)+"S"})
									
									If nPosSIMarc > 0 .And. nPosEI > 0 .And. nPosSI > 0
										nHrIntIni := fDHtoNS(aClonTabPd[nPosSI, 1], aClonTabPd[nPosSI, 3]) - fDHtoNS(, ::nMovAint)
										cHrIntIni := DataHora2Str(fNStoDH(nHrIntIni, "D"),fNStoDH(nHrIntIni, "H"))

										nHrIntFim := fDHtoNS(aClonTabPd[nPosSI, 1], aClonTabPd[nPosSI, 3]) + fDHtoNS(, ::nMovPint)
										cHrIntFim := DataHora2Str(fNStoDH(nHrIntFim, "D"), fNStoDH(nHrIntFim, "H"))

										cMarcHora := DataHora2Str(aMarcDia[nPosSIMarc, 1], aMarcDia[nPosSIMarc, 2])

										If cMarcHora >= cHrIntIni .And. cMarcHora <= cHrIntFim
											nTotInterv := fCalHoras(aClonTabPd[nPosSI, 1], aClonTabPd[nPosSI, 3], aClonTabPd[nPosEI, 1], aClonTabPd[nPosEI, 3])

											aRetFimInt := RetFimTab(aMarcDia[nPosSIMarc, 1], aMarcDia[nPosSIMarc, 2], nTotInterv)

											aClonTabPd[nPosSI,3] := aMarcDia[nPosSIMarc, 2]

											aClonTabPd[nPosEI,1] := aRetFimInt[1]
											aClonTabPd[nPosEI,3] := aRetFimInt[2]
										EndIf

										If cMarcHora < cHrIntFim .And. cTpHrExt == "E" .And. ::aTabCalend[nPosTabCalend, 9] > 0 .And. nX < Len(aMarcDia) .And. ::aTabCalend[nPosTabCalend, 9] <= fCalHoras(aMarcDia[nX, 1], aMarcDia[nX, 2], aMarcDia[nX+1, 1], aMarcDia[nX+1, 2])
											nMinDif := 0
											nX++
											lMarcVld := .F.
										EndIf
									EndIf
								Next
							EndIf

							If lMarcVld
								cMarcHora := DataHora2Str(aMarcDia[nX, 1], aMarcDia[nX, 2])
								cMarcTab := DataHora2Str(aClonTabPd[nPosTabCalend, 1], aClonTabPd[nPosTabCalend, 3])

								If ("E" $ aClonTabPd[nPosTabCalend, 4] .And. cMarcTab > cMarcHora) .Or. ("S" $ aClonTabPd[nPosTabCalend, 4] .And. cMarcTab < cMarcHora)
									nMinDif := fCalHoras(aClonTabPd[nPosTabCalend, 1], aClonTabPd[nPosTabCalend, 3], aMarcDia[nX, 1], aMarcDia[nX, 2])
								Else
									nMinDif := 0
								EndIf
							EndIf
							
							lMarcVld := .T.
						Else
							If cTpHrExt == "E" .And. ::aTabCalend[nPosTabCalend, 9] > 0 .And. ( nX < Len(aMarcDia) .And. ::aTabCalend[nPosTabCalend, 9] < fCalHoras(aMarcDia[nX, 1], aMarcDia[nX, 2], aMarcDia[nX+1, 1], aMarcDia[nX+1, 2]))
								nHrIntFim := fDHtoNS(Self:aTabCalend[nPosTabCalend, 1] , Self:aTabCalend[nPosTabCalend, 3]) + fDHtoNS(,0.15)

								cHrIntFim := DataHora2Str(fNStoDH(nHrIntFim, "D"),fNStoDH(nHrIntFim, "H"))
								cMarcHora := DataHora2Str(aMarcDia[nX, 1],aMarcDia[nX, 2])

								If cMarcHora >= cHrIntFim
									nMinDif := fCalHoras(::aTabCalend[nPosTabCalend, 1], ::aTabCalend[nPosTabCalend, 3], aMarcDia[nX, 1], aMarcDia[nX, 2])
								Else
									nMinDif := 0
									nX++
								EndIf
							Else
								nMinDif := fCalHoras(::aTabCalend[nPosTabCalend, 1], ::aTabCalend[nPosTabCalend, 3], aMarcDia[nX, 1], aMarcDia[nX, 2])
							EndIf
						EndIf
						
						If lTolMarc .And. nMinDif > fTolerancia( If(cTpHrExt == "E", ::cTolHex, ::cTolaSai), nX, 2, dData )
							lGeraExt := .T.
							Exit
						EndIf
					EndIf
				Next nX
				
				If !(lGeraExt)
					aEval( ::aResult, { |x, y| IF(( x[1] == dData ) .and. ( x[2] $ cEventos ), ::aResult[y, 3] := 0, NIL)})
				EndIf
			ElseIf lFalta
				IF ( Empty(aVerHrEx) )
					// Zera Quando for Menor ou Igual a Tolerancia, e Nao Existir Marcacao excedendo sua tolerancia.
					aEval( ::aResult, { |x, y| IF(  ( x[1] == dData ) .and. ( x[2] $ cEventos ), ::aResult[y, 3] := 0, NIL)})
				EndIf
			EndIf
		EndIf
	EndIf
	
return( NIL )

