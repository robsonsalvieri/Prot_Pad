Function fRd4HrRUS()
Local cTipe := GetMemVar("RDK_HIERAR")

	DBSELECTAREA("RD4")
	RD4->(DBGOTOP())
	While RD4->(!eof())

		IF (RD4->RD4_CODIGO == RDK->RDK_CODIGO .AND. EMPTY(RD4->RD4_HIERAR)  )
			RecLock("RD4",.F.)
			RD4->RD4_HIERAR := cTipe
			MsUnlock()
		Endif
   		RD4->( dbskip() )
  	Enddo
return

