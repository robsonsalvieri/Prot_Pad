Function Rd4ChecaEmp(cRd4EmpIDE)
	Local cTipo		:= GetMemVar("RDK_HIERAR")
	Local cIdent 	:= ""
	Local cEmpAux	:= ""
	Local aArea		:= GetArea()
	Local lRet		:= .T.

	dbSelectArea("RCL")
	dbSelectArea("SQB")
	dbSelectArea("SQ3")

	DEFAULT cRd4EmpIDE := ""

	If lCarga
		cEmpAux := cEmpCar
	Else
		If !Empty(cRd4EmpIDE)
			cEmpSel := cRd4EmpIDE
		Else
			cEmpSel := GetMemVar("RD4_EMPIDE")
		EndIf
		cEmpAux := cEmpSel
	Endif

	lEmpDif := ( cEmpTroca <> cEmpAux )

	If lEmpDif .Or. lClosed
		If lEmpDif
			cEmpTroca := cEmpAux
		EndIf
		lRet := Rd4TrocaEmp(cTipo,cEmpAux)
	EndIf

	If cTipo == "1"
		cIdent := "SQB"
		lShared := Empty(xFilial("SQB"))
	Else
		cIdent := "RCL"
		lShared := Empty(xFilial("RCL"))
		lSharedQ3 := Empty(xFilial("SQ3"))
	Endif
	
	RestArea(aArea)
	DbSelectArea(cIdent)
Return lRet

