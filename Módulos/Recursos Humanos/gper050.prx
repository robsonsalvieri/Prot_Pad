		If aVerba[03] == aCodFol[0983][1] //IR RRA 13o
			fSoma("13IR" ,aVerba[07], aVerba[03], aVerba[13], .T., aVerba[17], aVerba[12]+aVerba[21]+aVerba[22], aVerba[08], aVerba[11])
