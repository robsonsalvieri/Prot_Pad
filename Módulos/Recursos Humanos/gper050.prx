Static Function fSubOri(cData,cVerba,nChamada)
	Local nValor    := 0
	Local cAliasSRR := GetNextAlias()

	Default nChamada	:= 1 // 1= ProcRescisao, 2=ProcMovime

	If nChamada == 1
		BeginSql alias cAliasSRR
			SELECT SRR.RR_VALOR
			FROM %table:SRR% SRR
			WHERE SRR.%NotDel%
				AND SRR.RR_FILIAL  = %exp:SRR->RR_FILIAL%
				AND SRR.RR_MAT     = %exp:SRR->RR_MAT%
				AND SRR.RR_PERIODO = %exp:SRR->RR_PERIODO%
				AND SRR.RR_ROTEIR  = %exp:SRR->RR_ROTEIR%
				AND SRR.RR_DATA    = %exp:cData%
				AND SRR.RR_PD      = %exp:cVerba%
		EndSql

		If !(cAliasSRR)->(Eof())
			nValor := (cAliasSRR)->RR_VALOR
		Endif
		(cAliasSRR)->(dbCloseArea())

	ElseIf nChamada = 2
			BeginSql alias cAliasSRR
				SELECT SRR.RR_VALOR
				FROM %table:SRR% SRR
				WHERE SRR.%NotDel%
					AND SRR.RR_FILIAL  = %exp:SRR->RR_FILIAL%
					AND SRR.RR_MAT     = %exp:SRR->RR_MAT%
					AND SRR.RR_PERIODO = %exp:SRR->RR_PERIODO%
					AND SRR.RR_ROTEIR  = %exp:SRR->RR_ROTEIR%
					AND SRR.RR_DATA    < %exp:cData%
					AND SRR.RR_PD      = %exp:cVerba%
			EndSql

			If !(cAliasSRR)->(Eof())
				While (cAliasSRR)->(!Eof())
					nValor += (cAliasSRR)->RR_VALOR
				(cAliasSRR)->(DbSkip())
				EndDo
			EndIf

			(cAliasSRR)->(dbCloseArea())
	EndIf

Return(nValor)

