Static Function fVerTransf(lZera, aAuxTransf, cCompet, cCodEmp, cCodFil, cCodMat )
	Static cChaveAnt:= '!!'

	Default lZera	:= .F.

	If lZera
	cChaveAnt := Nil
	Else
		If cChaveAnt <> ( cCodEmp + cCodFil + cCodMat )
			cChaveAnt  := ( cCodEmp + cCodFil + cCodMat )
			aAuxTransf := {}
			fTransf(@aAuxTransf, , Nil, Nil, Nil, Nil, .F., .T.,,,,, cCodFil, cCodMat)
		Endif
	Endif
Return(.T.)

