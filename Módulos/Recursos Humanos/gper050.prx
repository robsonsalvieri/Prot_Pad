	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	SetMnemonicos(NIL,NIL,.T.)

	If nTpImpre == 1
		cPerg := 'GP050R'
		Pergunte(cPerg, .F.)
		oReport := ReportDef(cPerg)
		oReport:PrintDialog()
	Else
		cPerg := 'GPR050'
		Pergunte(cPerg, .F.)
		If Empty(Mv_Par13)
			Pergunte(cPerg, .T.)
		EndIf
		GR050Imp(1, .F., WnRel, "SRA", "1", oReport, "GPER050")
	EndIf

	RestArea( aArea )

Return()

