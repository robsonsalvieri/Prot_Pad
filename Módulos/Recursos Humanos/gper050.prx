/*/{Protheus.doc} fVldCNPJ
Valida raiz do CNPJ das Filiais selecionadas com a raiz do CNPJ da Filial Centralizadora
@author raquel.andrade
@since 23/02/2024
@version 1.0
/*/
Function fVldCNPJ(cFilCentral, cErro, cSolucao)
	Local aArea			:= GetArea()
	Local aCNPJCentral	:= {}
	Local lMsmRAIZ		:= .T.
	Local cCNPJFil 		:= ""
	Local cCNPJCentral 	:= ""

	Default	cFilCentral	:= ""
	Default	cErro		:= ""
	Default cSolucao	:= ""

	aCNPJCentral	:= FWSM0Util():GetSM0Data( cEmpant, cFilCentral, {"M0_CGC"})

	If Len(aCNPJCentral) > 0 .And. !Empty(aCNPJCentral[1][2])
		cCNPJCentral 	:= SubStr(aCNPJCentral[1][2],1,8)
		cQueryAlias 	:= GetNextAlias()
		BeginSql alias cQueryAlias
			Select SM0.M0_CODIGO, SM0.M0_CODFIL, SM0.M0_CGC
			From %Table:SM0% SM0
			WHERE  SM0.M0_CODIGO = %exp:cEmpant% AND
					 SM0.M0_CODFIL BETWEEN %exp:cFilDe% AND %exp:cFilAte% AND
					 SM0.%notDel%
			ORDER BY SM0.M0_CODIGO, SM0.M0_CODFIL
		EndSql

		If (cQueryAlias)->(!Eof())
			While  lMsmRAIZ .And. (cQueryAlias)->(!Eof()) 
				cCNPJFil	:= SubStr(FWSM0Util():GetSM0Data(,(cQueryAlias)->M0_CODFIL, {"M0_CGC"})[1][2],1,8)
				If !Empty(cCNPJFil) .And. cCNPJFil <> cCNPJCentral
					lMsmRAIZ	:= .F.	
					cErro 		:= OemToAnsi(STR0083) // "Encontrada filial com raiz diferente da Filial Centralizadora."
					cSolucao	:= OemToAnsi(STR0084) // "Verificar Filiais selecionadas nos campos Filial De/Até."
				EndIf
				(cQueryAlias)->( dbSkip() )
			EndDo
		EndIf
		(cQueryAlias)->(DbCloseArea())	
	Else
		lMsmRAIZ	:= .F.
		cErro 		:= OemToAnsi(STR0085) // "Filial Centralizadora não possui CNPJ cadastrado para ser realizada a validação."
		cSolucao	:= OemToAnsi(STR0086) // "Informe o CNPJ da Filial no cadastro de Filial no módulo SIGACFG."
	EndIf

	RestArea(aArea)

Return lMsmRAIZ
