										 NIL		,;				// udelOK
										 oFolders:aDialogs[ 02 ],;	// objeto de dialogo - oWnd
									     aHeader	,;				// Vetor com Header -  AparHeader
									     aColsTrf	 ; 				// Vetor com Colunas - AparCols
