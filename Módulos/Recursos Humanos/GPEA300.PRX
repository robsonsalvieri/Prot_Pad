Static Function ViewDef()

	Local oModel		:= FWLoadModel( 'GPEA300' )
	Local oStruRCA		:= FWFormStruct( 2, 'RCA' )
	Local oView

 	oView := FWFormView():New()
	oView:SetModel( oModel )

	If MV_MODFOL == "1"
		oStruRCA:RemoveField( "RCA_ALIAS" )
