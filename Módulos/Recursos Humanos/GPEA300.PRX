Function RcaConteuVld()

Local lRcaConteuVld := .T.
Local lRetExecForm  := .T.

Local cType
Local uChk

Begin Sequence

	cType	:= GetMemVar( "RCA_TIPO" )
	If !( cType $ "U/O/R/P/F" )
		uChk := GetMemVar( "RCA_CONTEU" )
		lRetExecForm := CheckExecForm( @uChk , .F. )
		If ValType(uChk) != cType .Or. !lRetExecForm .Or. ( cType == "C" .And. !(SubStr( M->RCA_CONTEU, 1, 1 ) $ "'" .Or. SubStr( M->RCA_CONTEU, 1, 1 ) $ '"' ) )
			lRcaConteuVld := .F.
            Help( Nil, Nil, OemToAnsi(STR0013), Nil, OemToAnsi(STR0024), 1, 0 )//"Atenção!"##"Tipo e conteúdo do mnemônico não são compatíveis"
		EndIf
	EndIf

End Sequence

Return( lRcaConteuVld )

