	EndIf

	oView:AddField( 'VIEW_RCA', oStruRCA, 'RCAMASTER' )

	oView:AddUserButton( STR0074, 'CLIPS', {|oView| fLocaliza(RCA_MNEMON, "RC3", {"RC3_RESULT","RC3_FORM01","RC3_FORM02" } )} )   //"Localizar Referência Cruzada"

