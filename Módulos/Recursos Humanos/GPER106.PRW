	lBrkCc		:= mv_par14 == 1 //Salta Pag. quebra C.Custo: 1=Sim; 2=Nao
	lBrkDp		:= mv_par15 == 1 //Salta Pag. quebra Depto:   1=Sim; 2=Nao
	lImpFil  	:= mv_par16 == 1 //Imprime totais por Filial: 1=Sim; 2=Nao
