	nTipDp		:= 1     		 //Tipo Impressao Depto: 1=Codigo; 2=Descricao; 3=Ambos
	lBrkPagFil	:= .F.			 //Salta Pag. quebra Filial 1=Sim; 2=Nao:
	lImpBase	:= mv_par16 == 1
	cRefOco		:= MV_PAR14
