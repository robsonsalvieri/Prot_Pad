	oView:CreateHorizontalBox("RU6", 100)
	oView:SetOwnerView("GPEA020_RU6", "RU6")

	oFWMVCWindow := FWMVCWindow():New()
	oFWMVCWindow:SetUseControlBar(.T.)
	oFWMVCWindow:SetView(oView)
	oFWMVCWindow:SetCentered(.T.)
	oFWMVCWindow:SetPos(0,0)
	oFWMVCWindow:SetSize(300,1400)
	oFWMVCWindow:SetTitle(STR0164 + cDepSRB + " - " + cNomeDep)//'Histórico do dependente: '
	oFWMVCWindow:Activate()

Return Nil

/*/{Protheus.doc} fVldHist
Verifica a existência de dados na tabela RU6 antes da exclusão via rotina GPEA020. 
@author raquel.andrade
@since 11/12/2023
/*/
Static Function fVldHist(nOperacao,cCodDep)
Local lRet 		:= .T.
Local cDtPgto  	:= ""
Local cCompet  	:= ""
Local aPerAtual := {}
Local cRot		:= IIf(SRA->RA_CATFUNC $ 'A|P',fGetCalcRot('9'),fGetCalcRot('1'))
	
Default nOperacao 	:= 3
Default cCodDep		:= ""

	DbSelectArea("RCH")
	If fGetPerAtual(@aPerAtual, xFilial("RCH"), SRA->RA_PROCES, cRot)
		cDtPgto := MesAno(aPerAtual[1,11])	// RCH_DTPGTO
		cCompet := aPerAtual[1,01]			// RCH_PER
		If cDtPgto == MesAno(dDataBase) .Or. cCompet == MesAno(dDataBase) // Regime Caixa, Regime Competência
			cChvRU6 := xFilial("RU6", SRA->RA_FILIAL) + SRA->RA_MAT + cCodDep
			RU6->(DbSetOrder(1)) // RU6_FILIAL+RU6_MAT+RU6_CODSRB+DTOS(RU6_DTMOV)+RU6_SEQMOV+RU6_TPREND   
			If RU6->(DbSeek(xFilial("RU6", SRA->RA_FILIAL) + SRA->RA_MAT + cCodDep))
				While RU6->(!Eof()) .And. RU6->(RU6_FILIAL+RU6_MAT+RU6_CODSRB) == cChvRU6 .And. lRet 
					If MesAno(RU6->RU6_DTPGTO) == cDtPgto .Or. RU6->RU6_COMPET == cCompet
						lRet :=	.F.	
					EndIf
				RU6->(dbSkip())
				Enddo				
			EndIf
		EndIf
	EndIf

Return lRet

/*/{Protheus.doc} fChkRU6
Função para verificar se base 12.1.33/12.1.2210 com índice desatualizado 
@author raquel.andrade
@since 30/04/2024
@version 1.0
/*/
Function fChkRU6()
Local oButton1
Local oCheckBo1
Local lCheckBo1 	:= .F.
Local oGroup1
Local oPanel1
Local oSay1
Local cSession		:= ""
Local cKey			:= ""
Local lChkMsg 		:= .T.
Local oDlg
Local cChvAtualizada:= "RU6_FILIAL+RU6_MAT+RU6_CODSRB+DTOS(RU6_DTMOV)+RU6_SEQMOV+RU6_TPREND+DTOS(RU6_DTPGTO)+RU6_COMPET+RU6_SEMANA"
Local cRelease		:= GetRPORelease() 
Local lChkRel		:= If (cRelease == "12.1.033" .Or. cRelease ==  "12.1.2210", .T.,.F.)


	cSession	:= "AlrtChkRU6Menu"
	cKey 		:= "AlrtChkRU6Menu_"
	lChkMsg 	:= fwGetProfString(cSession, cKey + cUserName,'',.T.) == ""

	If lChkMsg .And. lChkRel
	
		cSx2Unico := FWX2Unico( "RU6")

		If cSx2Unico == cChvAtualizada		

			DEFINE MSDIALOG oDlg TITLE OemToAnsi(STR0178) FROM 000, 000  TO 200, 500 COLORS 0, 16777215 PIXEL // "Aviso Importante"

				@ 000, 000 MSPANEL oPanel1 SIZE 300, 150 OF oDlg COLORS 0, 16777215 RAISED
				@ 005, 012 GROUP oGroup1 TO 065, 237 PROMPT  OF oPanel1 COLOR 0, 16777215 PIXEL
				//"Identificado que a chave única da tabela Histórico de Dependentes (RU6) não está atualizada e neste caso o processo de Migração de Releases"
				//"ou conferência de Duplicidade (CheckDupl) podem ser ser prejudicados."
				//"Aviso importante que deverá ser direcionado ao setor responsável, para maiores informações selecione Visualizar."
				@ 020, 017 SAY oSay1 PROMPT OemToAnsi(STR0179)  SIZE 215, 035 OF oPanel1 COLORS 0, 16777215 PIXEL
				@ 045, 017 SAY oSay1 PROMPT OemToAnsi(STR0180)  SIZE 215, 035 OF oPanel1 COLORS 0, 16777215 PIXEL
				@ 080, 012 CHECKBOX oCheckBo1 VAR lCheckBo1 PROMPT OEMToAnsi(STR0181) SIZE 067, 008 OF oPanel1 COLORS 0, 16777215 PIXEL //"Não exibir novamente"
				@ 070, 160 BUTTON oButton1 PROMPT (STR0005) SIZE 037, 012 OF oPanel1 PIXEL// "Visualizar"
				@ 070, 200 BUTTON oButton2 PROMPT "OK" SIZE 037, 012 OF oPanel1 PIXEL

				oButton1:bLClicked := {|| ShellExecute("open","https://tdn.totvs.com/pages/releaseview.action?pageId=841169932","","",1) }
				oButton2:bLClicked := {|| oDlg:End() }

			ACTIVATE MSDIALOG oDlg CENTERED

			If lCheckBo1
				fwWriteProfString(cSession, cKey + cUserName, 'CHECKED', .T.)
			EndIf

		EndIf

	EndIf

Return

/*/{Protheus.doc} Gp020DOk
Validação para exclusão da linha
@author raquel.andrade
@since 21/06/2024
@version 1.0
/*/
Static Function Gp020DOk(oGrid, nLine, cAction, cField)
Local lRet			:= .T.
Local oModel 		:= oGrid:GetModel()
Local nOperacao 	:= oModel:GetOperation()
Local lTabRU6		:= ChkFile("RU6")

If cAction == 'DELETE' .And. nOperacao == MODEL_OPERATION_UPDATE
	If cPaisLoc == "BRA" .And. lTabRU6 .And. oGrid:GetValue("RB_TIPIR") <> '4' 
		If !fVldHist(nOperacao,oGrid:GetValue("RB_COD"))
			// "Histórico de Cálculo de Dependentes"
			// "Não é possível realizar a exclusão quando existem cálculos para o funcionário no período aberto."
			// "Necessário fazer o Cancelamento do Cálculo."
			Help(" ", 1, OemToAnsi(STR0171),, OemToAnsi( STR0182 ),1,0,,,,,, { OemToAnsi( STR0173 )}) 
			lRet	:= .F.
		EndIf		
	EndIf
EndIf

Return lRet

/*/{Protheus.doc} fVldTpIr
Validação no campo RB_TIPIR
@author raquel.andrade
@since 21/06/2024
@version 1.0
/*/
Static Function fVldTpIr(oGrid)
Local lRet			:= .T.
Local oModel 		:= oGrid:GetModel()
Local nOperacao 	:= oModel:GetOperation()
Local lTabRU6		:= ChkFile("RU6")

If cPaisLoc == "BRA" .And. lTabRU6 .And. nOperacao == 4
	If oGrid:GetValue("RB_TIPIR") == '4' 
		If !fVldHist(nOperacao,oGrid:GetValue("RB_COD"))
			// "Histórico de Cálculo de Dependentes"
			// "Não é possível alterar o campo Tipo Dep. IRRF para  4-Não é dependente quando existem cálculos para o funcionário no período aberto."
			// "Necessário fazer o Cancelamento do Cálculo."
			Help(" ", 1, OemToAnsi(STR0171),, OemToAnsi( STR0172 ),1,0,,,,,, { OemToAnsi( STR0173 )}) 
			lRet	:= .F.
		EndIf
	EndIf
EndIf

Return lRet
