Local oGrid   	:= oModel:GetModel("GPEA020_SRB")
Local lRet 		:= .F.
Local nPos		:= fPosTab("S043", &(readvar()), "=", 4 )
Local cTpDoc 	:= oGrid:GetValue("RB_TPCIC")

If nPos > 0 .or. cTpDoc <> '07'
   	lRet:= .T.
EndIf

Return lRet

