Static Function G020VldIni(oModel, nOperacao)

	Local aArea     := GetArea()
	Local lRet 		:= .T.
	Local cMsgRet	:= ""
	Local cRetGPE 	:= ""

	Local lVerBlq   := .T.
	Local aRetGPE   := Array(2)
	Local aRetTAF   := Array(2)

	If nOperacao == 3 .And. xFilial("SRB", cFilAnt) <> xFilial("SRB", SRA->RA_FILIAL) //MANUTENCAO - Escolheu uma filial diferente da filial do Funcionario posicionado
		cMsgRet := OemToAnsi(STR0169) // "Não é permitido escolher uma filial diferente da filial do Funcionário."
		lRet := .F.
		//"Selecione a mesma filial do funcionário posicionado no browse (# ## ## )"
		IIf(lGp020Auto, AutoGrLog(cMsgRet), Help(,,'HELP',, cMsgRet,1,0,,,,,,{ OemToAnsi(STR0170) + " (" + AllTrim(xFilial("SRB", SRA->RA_FILIAL)) + ")." }))			
	EndIf
	
	If !(lGp020Auto)
		lVerBlq     := Iif(cPaisLoc == 'BRA' .And. FindFunction("fVersEsoc"), fVersEsoc( "S2300", .F., @aRetGPE, @aRetTAF, @cVersEnvio, @cVersGPE,,, ""), .T.)
	Endif

	If len(aRetGPE) >= 2
		cRetGPE := aRetGPE[2]
	EndIf

	cMsgRet	:= STRTRAN(STRTRAN(STR0099,"#GPE#",cRetGPE),"#TAF#",cRetGPE)

	If !(IsInCallStack("CFG600lmdl")) .And. nOperacao == 3
		oModel:SetOperation( 4 )
	EndIf

	//--------------------------------------------------------------
	//Verifica se o Arquivo Esta Vazio
	//--------------------------------------------------------------
	If !ChkVazio("SRA")
		Return .F.
	EndIf

	If nOperacao == MODEL_OPERATION_DELETE 
		If Gp020SrbNull()
			If lGp020Auto
				AutoGrLog(STR0044)
			Else
				Help(,,'HELP',, STR0044,1,0 ) //"Não existe nenhum dependente cadastrado para este funcionário."
			EndIf
			lRet := .F.
		EndIf
	EndIf

    RestArea( aArea )

Return lRet

