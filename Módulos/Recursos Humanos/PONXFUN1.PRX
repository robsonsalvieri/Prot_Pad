	If ValType(M->P8_MOTIVRG) == "C" .and. !Empty(M->P8_MOTIVRG)
	  	If MsgYesNo(STR0002)
			lMainRet := .T.
	    Endif
   	Endif
	lRet := lMainRet

Return lMainRet

/*/{Protheus.doc} fMigrRR2
Efetua migração dos registros da SYNC_PROTHEUS_TSA para a tabela RR2
@author allyson.mesashi
@since 07/10/2020
@version P12.1.27
/*/
Function fMigrRR2()

Local aArea			:= GetArea()
Local cAliasSYNC	:= GetNextAlias()
Local lMigra		:= .T.
Local lOk			:= .T.

If !TCCanOpen("SYNC_PROTHEUS_TSA")
	Return
Endif

BeginSql alias cAliasSYNC
	SELECT * FROM SYNC_PROTHEUS_TSA
EndSql

dbSelectArea( "RR2" )
RR2->( dbSetOrder(1) )
If (cAliasSYNC)->( !Eof() ) .And. RR2->( dbSeek( (cAliasSYNC)->ID + (cAliasSYNC)->COMPANY + (cAliasSYNC)->BRANCH_C + (cAliasSYNC)->BRANCH_E + (cAliasSYNC)->REGISTER ) )
	lMigra	:= .F.
EndIf

If lMigra
	While (cAliasSYNC)->( !Eof() )
		If !RR2->( dbSeek( (cAliasSYNC)->ID + (cAliasSYNC)->COMPANY + (cAliasSYNC)->BRANCH_C + (cAliasSYNC)->BRANCH_E + (cAliasSYNC)->REGISTER ) )
			If RR2->( RecLock("RR2", .T.) )
				RR2->RR2_ID			:= (cAliasSYNC)->ID
				RR2->RR2_COMPAN		:= (cAliasSYNC)->COMPANY
				RR2->RR2_BRANCC		:= (cAliasSYNC)->BRANCH_C
				RR2->RR2_BRANCE		:= (cAliasSYNC)->BRANCH_E
				RR2->RR2_REGIST		:= (cAliasSYNC)->REGISTER
				RR2->RR2_URL		:= (cAliasSYNC)->URL
				RR2->RR2_OBJECT		:= (cAliasSYNC)->TSAOBJECT
				RR2->RR2_METHOD		:= (cAliasSYNC)->TSAMETHOD
				RR2->RR2_SEQUEN		:= (cAliasSYNC)->TSASEQUENC
				RR2->RR2_COMMAN		:= (cAliasSYNC)->TSACOMMAND
				RR2->RR2_PROPER		:= (cAliasSYNC)->PROPERTY
				RR2->RR2_TYPEDA		:= (cAliasSYNC)->TYPE_DATA
				RR2->RR2_CONTEN		:= (cAliasSYNC)->TSACONTENT
				RR2->RR2_LENGTH		:= (cAliasSYNC)->TSALENGHT
				RR2->( MsUnlock() )
				TcSqlExec("DELETE FROM SYNC_PROTHEUS_TSA WHERE R_E_C_N_O_ = '" + cValToChar( (cAliasSYNC)->R_E_C_N_O_ ) + "'")
			Else
				lOk		:= .F.
			EndIf
		EndIf
		(cAliasSYNC)->( dbSkip() )
	EndDo
	If lOk
		TCSQLExec("DROP TABLE SYNC_PROTHEUS_TSA")
	EndIf
EndIf

(cAliasSYNC)->( dbCloseArea() )

Return

/*/{Protheus.doc} fMsgMCalc
Apresenta uma mensagem em tela informando sobre o impacto da geração da memória de cálculo na performance da rotina
@type  Static Function
@author Cícero
@since 23/08/2024
@param nTipo, number, Tipo de processamento 1 = Leitura; 2 = Apontamento; 3 = Ambos
@return lMemoCalc, logical, Determina se deve prosseguir com a geração da memória de cálculo
/*/
Function fMsgMCalc(nTipo)
	
	Local aButtons	:= {}
	Local bOpenTDN	:= {|| ShellExecute("open", "https://tdn.totvs.com/pages/releaseview.action?pageId=864346837", "", "", 1)}
	Local bOpenTDNA	:= {|| ShellExecute("open", "https://tdn.totvs.com/pages/viewpage.action?pageId=969971911", "", "", 1)}
	Local bCntNoLog	:= {|| lMemoCalc := .F., oModal:oOwner:End() }
	Local oModal
	Local oContainer
    Local oFont := TFont():New("Arial",,-12,.T.)
	
	DEFAULT nTipo := 3
	
	If IsBlind()
		lMemoCalc := .F.
		Return lMemoCalc
	EndIf
	
	If nTipo == 1 .Or. nTipo == 3
		aAdd(aButtons, {, STR0023, bOpenTDN, STR0007, , .T., }) // "Doc. Leitura" - "Documentação sobre a Memória de Cálculo no TDN"
	EndIf
	If nTipo == 2 .Or. nTipo == 3
		aAdd(aButtons, {, STR0024, bOpenTDNA, STR0007, , .T., }) // "Doc. Apontamento" - "Documentação sobre a Memória de Cálculo no TDN"
	EndIf
	
	//aAdd(aButtons, {, STR0006, bOpenTDN, STR0007, , .T., }) // "Saiba Mais" - "Documentação sobre a Memória de Cálculo no TDN"
	aAdd(aButtons, {, STR0008, bCntNoLog, STR0009, , .T., }) // "Desativar" - "Desativa a geração da Memória de Cálculo para essa execução"
	oModal := FWDialogModal():New()
	oModal:SetEscClose(.T.)
	oModal:setTitle(STR0010) // "Memória de Cálculo"
	
	oModal:setSubTitle(If(nTipo == 3, STR0011 + " | " + STR0025, If(nTipo == 1, STR0011, STR0025))) // "Leitura de Marcações" - "Apontamento de Horas Extras"
	
	//Seta a largura e altura da janela em pixel
	oModal:setSize(130, 240)
	
	oModal:createDialog()
	oModal:addCloseButton(NIL, STR0012) // "Continuar"
	oModal:addButtons(aButtons)
	oContainer := TPanel():New( ,,, oModal:getPanelMain() )
	oContainer:Align := CONTROL_ALIGN_ALLCLIENT
	
	TSay():New(3, 3, {||STR0013 + CRLF +; // "A Memória de Cálculo gera um log detalhado de todo o processo realizado pelo sistema."
	STR0014 + CRLF + CRLF +; // "A geração da Memória de Cálculo impacta negativamente na performance e é recomendada para casos específicos."
	STR0015}, oContainer,, oFont,,,, .T.,,, 235, 200,,,,,, .T.) // "Deseja continuar com a geração da Memoria de Cálculo?"
	
	oModal:Activate()
	
Return lMemoCalc

/*/{Protheus.doc}GravaMCalc
Grava em uma tabela temporária o texto da memória de cálculo
@type  Function
@author Cícero Alves
@since 23/08/2024
@param cTipo, character, Tipo do LOG ("01" = Leitura)
@param nTabs, numeric, Quantidade de TABs inseridos no início da linha para formatação
@param cTexto, character, Texto do Log com tamanho máximo de 255
@param lCRLF, logical, Define se adiciona uma quebra de linha no final do texto. Por padrão a quebra é inserida
@return NIL, NIL
/*/
Function GravaMCalc(cTipo, nTabs, cTexto, lCRLF)
	
	DEFAULT lCRLF := .T.
	
	Static nLinhaL := 0
	
	If !oTmpMCalc == NIL
		
		If cTipo = "01"
			nLinhaL++
		EndIf
		
		cTexto += If(lCRLF, CRLF, "")
		
		(cAliasMCalc)->(RecLock(cAliasMCalc, .T. ))
			(cAliasMCalc)->TIPO := cTipo
			(cAliasMCalc)->LINHA := StrZero(nLinhaL, 9)
			(cAliasMCalc)->NTAB := nTabs
			(cAliasMCalc)->TEXTO := cTexto
		(cAliasMCalc)->(MsUnlock())
		
	EndIf
	
Return

/*/{Protheus.doc} fTabMCalc
Cria uma tabela temporária para gravação da memória de cálculo
@type function
@author Cícero Alves
@since 23/08/2024
@return NIL, NIL
/*/
Function fTabMCalc()
	
	Local aStruct	:= {}
	Local aIndices	:= {}
	
	cAliasMCalc := GetNextAlias()
	
	Aadd(aStruct, {"TIPO" , "C",   2, 0})
	Aadd(aStruct, {"LINHA", "C",   9, 0})
	Aadd(aStruct, {"NTAB" , "N",   6, 0})
	Aadd(aStruct, {"TEXTO", "C", 255, 0})
	
	Aadd(aIndices, {"TIPO", "LINHA"})
	
	oTmpMCalc := RhCriaTrab(cAliasMCalc, aStruct, aIndices)
	
Return

