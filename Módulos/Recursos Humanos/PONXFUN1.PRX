	cWhere	:= "% (SRJ.RJ_FILIAL >= '" + xFilial("SRJ",cFilDe) + "' AND SRJ.RJ_FILIAL <= '" + xFilial("SRJ",cFilAte) + "') AND (SRJ.RJ_FUNCAO >= '" + cFuncDe + "' AND SRJ.RJ_FUNCAO <= '" + cFuncAte + "') %"
Else
	cWhere	:= "% SRJ.RJ_RHEXP = ' ' AND SRJ.RJ_FILIAL >= '" + xFilial("SRJ",cFilDe) + "' AND SRJ.RJ_FILIAL <= '" + xFilial("SRJ",cFilAte) + "' %"
