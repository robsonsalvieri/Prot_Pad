Function WSRHUserVacationAbsence(oProcess, cFilDe, cFilAte, cMatDe, cMatAte, dDataDe, dDataAte, lReproces)
Local cAliasSRH 	:= GetNextAlias()
Local cWhere		:= ""
Local cDateNow		:= DTOS(Date())
Local nTotReg		:= 0
Local nReg			:= 0
Local lRegua		:= .F.
Local aAreaSRA		:= {}

DEFAULT oProcess	:= NIL
DEFAULT cFilDe		:= Space(FWGETTAMFILIAL)
DEFAULT cFilAte		:= Replicate("Z",FWGETTAMFILIAL)
DEFAULT cMatDe		:= ""
DEFAULT cMatAte		:= Replicate("Z",TamSX3("RA_MAT")[1])
DEFAULT dDataDe		:= cToD(" / / ")
DEFAULT dDataAte	:= cToD(" / / ")
DEFAULT lReproces	:= .F.

lRegua	:= ValType(oProcess) == 'O'

Private oObjRep     := Nil

dbSelectArea("SRH")
SRH->(dbSetOrder(1))

dbSelectArea("SRA")
