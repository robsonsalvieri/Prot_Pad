//-------------------------------------------------------------------
//{Protheus.doc} getRegua
//Cria a regua de progressão
//-------------------------------------------------------------------

Static Function getRegua(cTab,oTProces)	
	Local nReg :=0 as Numeric
	//Cria regua de processamento
	if cTab == "SB1"
		DbSelectArea("SB1")
		nReg := SB1->(Reccount())
	Else
		DbSelectArea("SG1")
		nReg := SG1->(Reccount())
	EndIf
	If (oTProces<>Nil)
		oTProces:SetRegua1( nReg )
	else
		ProcRegua(nReg)
	EndIf
Return nReg
