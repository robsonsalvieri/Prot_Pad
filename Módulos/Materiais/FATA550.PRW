Function Fata550()

If !SX2->(DbSeek("ADS"))
	MsgStop(STR0001) //"Execute o update U_TKUPDADM antes de acessar esta rotina"
	Return Nil
EndIf

dbSelectArea("ADS")
dbSetOrder(1)

OBrowse := FWMBrowse():New()
oBrowse:SetAlias('ADS')
