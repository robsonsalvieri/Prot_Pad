Local lRet	:= .T.

DbSelectArea("ADT")
DbSetOrder(1) //ADT_FILIAL+ADT_CODTAR+ADT_CODCMP

If DbSeek(xFilial("ADT")+ADS->ADS_CODIGO)
	Help( ,, 'HELP',, STR0003, 1, 0)   //"Este registro encontra-se vinculado ao cadastro de Componentes x Tp. Tarefa, favor verificar."
