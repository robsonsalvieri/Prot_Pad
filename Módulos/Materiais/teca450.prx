Local lRet 		:= .T.					//Indica o retorno da funcao
Local aAreaAB6	:= AB6->(GetArea()) 	//Guarda a area corrente

If (AB6->AB6_MOEDA <> 0) 
	If AB6->AB6_TXMOED <= 0
