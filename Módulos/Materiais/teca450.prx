Static Function GetNumAB6()
Local aArea		:= GetArea()
Local aAreaAB6	:= AB6->(GetArea())
Local cNum		:= GetSx8Num("AB6","AB6_NUMOS")
Local nSaveSX8 := GetSX8Len()

dbSelectArea("AB6")
dbSetOrder(1)
While AB6->(DbSeek(xFilial("AB6")+cNum))
	While ( GetSX8Len() > nSaveSX8 )
		ConfirmSX8()
	EndDo
	cNum := GetSx8Num("AB6","AB6_NUMOS")
EndDo

RestArea(aAreaAB6)
RestArea(aArea)
Return(cNum)

