	cQuery += "D_E_L_E_T_='*' AND "		
	While ( nX < nY )
		TcSqlExec(cQuery+"R_E_C_N_O_>="+Str(nX,10,0)+" AND R_E_C_N_O_<="+Str(nX+1023,10,0)+"")
		nX += 1024
	EndDo
	
EndIf

RestArea(aArea)

Return(.T.)

