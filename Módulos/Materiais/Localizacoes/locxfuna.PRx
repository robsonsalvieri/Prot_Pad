/*/{Protheus.doc} LxEventIn
	Función para generación de evento para Notas Fiscales.
	@type  Function
	@author oscar.lopez
	@since 28/06/2022
	@version 1.0
	@param aCabNota, arreglo, Arreglo con información de encabezado del documento
	@param aCItems, arreglo, Arreglo con información del detalle del documento
	@param aCpItens, arreglo, Arreglo con información de los campos del detalle del documento
	@return Nil
	@example
		LxEventIn(aCabNota)
	/*/
Function LxEventIn(aCabNota, aCItems, aCpItens)

	Local cIdEvento := "" //Id el Evento
	Local cMensaje	:= ""
	Local cUsrEven	:= ""
	Local cInfor	:= ""
	Local nPos		:= 0
	Local aArea		:= {}
	Local aCampos	:= {}
	Local aInfo		:= {}
	Local nItera	:= 0
	Local aItemsE	:= {}
	Local nItemsE	:= 0
	Local nY		:= 0
	Local nPosItem  := 0
	Local nPosCod   := 0
	Local nPosCan   := 0
	Local nPosTot   := 0
	Local nPosNatu	:=0
	Default aCabNota := {}
	Default aCItems  := {}
	Default aCpItens := {}

	aArea		:= GetArea()
	cIdEvento	:= "030"
	nPosItem := Ascan(aCpItens,{|x| Alltrim(x)=="D1_ITEM"})
	nPosCod  := Ascan(aCpItens,{|x| Alltrim(x)=="D1_COD"})
	nPosCan  := Ascan(aCpItens,{|x| Alltrim(x)=="D1_QUANT"})
	nPosTot  := Ascan(aCpItens,{|x| Alltrim(x)=="D1_TOTAL"})
	
	For nItemsE := 1 To Len(aCItems)
		AADD(aItemsE, {/*Item*/IIf(nPosItem > 0,aCItems[nItemsE][nPosItem],""), ;
		           /*Producto*/IIf(nPosCod > 0,aCItems[nItemsE][nPosCod],""), ;
				   /*Cantidad*/IIf(nPosCan > 0,aCItems[nItemsE][nPosCan],0), ;
				  /*Val Total*/IIf(nPosTot > 0,aCItems[nItemsE][nPosTot],0)})
	Next nItemsE
	aCampos		:= {"F1_FORNECE", "F1_LOJA", "F1_SERIE", "F1_DOC", "F1_TIPODOC"}
	cMensaje	:= STR0145 + CRLF //"Inclusión de Factura de Entrada"
	cUsrEven	:= Subs(cUsuario,7,15)

	For nItera := 1 To Len(aCampos)
		nPos := AScan(aCabNota[1],{|x| x == aCampos[nItera] })
		If nPos > 0
			cInfor := AllTrim(aCabNota[2][nPos])
			AAdd(aInfo, cInfor)
			cMensaje += AllTrim(FWX3Titulo(aCampos[nItera])) + ": " + cInfor + CRLF
		EndIf
	Next nItera
	nPosNatu:= ASCAN(aCabNota[1], { |x| UPPER(x) == "F1_NATUREZ" })
	AAdd(aInfo, cUsrEven)
	cMensaje += STR0146 + cUsrEven + CRLF //"Por el usuario: "
	cMensaje += STR0157 + SA2->A2_NOME + CRLF //"Nom. Proveedor: "
	IF nPosNatu>0 
		IF !Empty(aCabNota[2][nPosNatu])
			cMensaje += STR0158 + aCabNota[2][nPosNatu] + CRLF //"Modalidad: "
		Endif
	EndIF
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³Envia e-mail do Evento 030                                              ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ

	// Mensagem Itens
	For nY:= 1 to Len(aItemsE)
		cMensaje += STR0153 + aItemsE[nY][1] + "|   "
		cMensaje += STR0154 + aItemsE[nY][2] + "|   "
		cMensaje += STR0155 + cValToChar(aItemsE[nY][3]) + "|   "
		cMensaje += STR0156 + cValToChar(aItemsE[nY][4]) + CRLF
	Next nY

	If FindFunction("COMTemSXI") .And. COMTemSXI(cIdEvento)
		EventInsert(FW_EV_CHANEL_ENVIRONMENT, FW_EV_CATEGORY_MODULES, cIdEvento, FW_EV_LEVEL_INFO,""/*cCargo*/, STR0145, cMensaje, .T./*lPublic*/) //"Inclusión de Factura de Entrada"
	Else
		//1-Num Docto, 2-Serie, 3-Proveedor, 4-Tienda, 5-Nom. Proveedor, 6-Inclusion, 7-Array Items, 8-Modalidad
		MEnviaMail(cIdEvento,{aInfo[4], aInfo[3], aInfo[1], aInfo[2], SA2->A2_NOME, 3, aItemsE, aCabNota[2][6] })
	Endif

	RestArea(aArea)

Return Nil

/*/{Protheus.doc} LxVldRut
Realiza validaciones para cuando MV_FEXML fue configurado con S (Generación de XML México a través de rutina)
@type function
@author luis.enríquez
@since 08/11/2022
@version 1.0
@param lSalida, lógico, .T. si es documento de Salida, caso contrario .F.
@return Nil
@example
LxVldRut(.T.)
@see (Usado en LOCXFUNA.PRW)
/*/
Function LxVldRut(lSalida)
	Local cRuta  := ""
	Local lRet   := .T.

	Default lSalida := .T.

	//Valida que exista la función fatsmex
	If lSalida .And. !FindFunction("fatsmex")
		MsgAlert(STR0148, STR0051) //"No existe compilada la rutina FATSMEX.PRW en el repositorio del ambiente." //"Comprobantes fiscales digitales"
		lRet := .F.
	ElseIf !lSalida .And. !FindFunction("fatemex")
		MsgAlert(STR0150, STR0051) //"No existe compilada la rutina FATEMEX.PRW en el repositorio del ambiente." //"Comprobantes fiscales digitales"
		lRet := .F.
	EndIf

	//Valida que exista el Certificado (.pem)
	cRuta  := &(GetNewPar("MV_CFDDIRS","")) + GetNewPar("MV_CFDI_CP","") //Ruta y Nombre del Ceritficado
	If !File(cRuta)
		MsgAlert(StrTran(STR0149,"###",cRuta) + " [MV_CFDDIRS/MV_CFDI_CP]",STR0051) //"No existe el archivo: ###" //"Comprobantes fiscales digitales"
		lRet := .F.
	EndIf
Return lRet

