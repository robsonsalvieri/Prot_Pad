/*/{Protheus.doc} LxChkNumNF
	Revisar que no salte la numeración de documentos fiscales
	Usar para numeración de documentos con formulario propio
	@type  Function
	@author ARodriguez
	@since 27/11/2023
	@version 1
	@param cSerie, caracter, serie
	@param cNum, caracter, consecutivo asignado
	@param cTabla, caracter, tabla de documentos
	@return cNum, caracter, consecutivo
	/*/
Function LxChkNumNF(cSerie, cNum, cTabla)
	Local aArea		:= GetArea()
	Local cAliasQry := GetNextAlias()
	Local lSX5Comp	:= (FwModeAccess("SX5",3) == "C")	// Tabla SX5 compartida a nivel sucursal?
	Local cWhereFil := ""

	Default cSerie	:= Space(TamSX3("F2_SERIE")[1])
	Default cNum	:= Space(TamSX3("F2_DOC")[1])
	Default cTabla	:= "SF2"

	// Obtiene último número asignado en la serie
	If cTabla == "SF1"
		cWhereFil := IIf(lSX5Comp, "% 1=1 %", "% SF1.F1_FILIAL = '" + xFilial("SF1") + "' %" )

		BeginSql Alias cAliasQry
			%noparser%
			SELECT MAX(F1_DOC) NUMDOC
			FROM
				%table:SF1% SF1
			WHERE
				%Exp:cWhereFil% AND
				SF1.F1_SERIE = %exp:cSerie% AND
				SF1.%notDel%
		EndSql
	Else
		cWhereFil := IIf(lSX5Comp, "% 1=1 %", "% SF2.F2_FILIAL = '" + xFilial("SF2") + "' %" )

		BeginSql Alias cAliasQry
			%noparser%
			SELECT MAX(F2_DOC) NUMDOC
			FROM
				%table:SF2% SF2
			WHERE
				%Exp:cWhereFil% AND
				SF2.F2_SERIE = %exp:cSerie% AND
				SF2.%notDel%
		EndSql
	EndIf

	If !(cAliasQry)->(Eof())
		//Se valida que haya registros previos con la serie seleccionada.
		If !empty((cAliasQry)->NUMDOC)
			// Próximo número
			If Len((cAliasQry)->NUMDOC) <> Len(cNum) .And. (cPaisLoc == 'PER' .Or. cPaisLoc == 'PAR')
				cConsec := Soma1(SubStr((cAliasQry)->NUMDOC,1,Len(cNum)),Len(cNum))
			Else
				cConsec := Soma1((cAliasQry)->NUMDOC,TamSX3("F2_DOC")[1])
			EndIf
			If cNum > cConsec
				// Si el asignado es mayor, forza consecutivo
				cNum := cConsec
				If SX5->(MsSeek(xFilial("SX5")+"01"+cSerie))
					RecLock("SX5",.F.)
					SX5->X5_DESCRI  := cConsec
					SX5->X5_DESCSPA := cConsec
					SX5->X5_DESCENG := cConsec
					If !InTransact() //Retorna se o sistema está em transação
						SX5->(MsRUnLock())
					EndIf
				EndIf
			EndIF
		EndIf
	EndIf

	(cAliasQry)->(dbCloseArea())
	RestArea(aArea)

Return cNum

/*/{Protheus.doc} LxChkAtF
	Valida si el ítem de una factura de entrada tiene relación con una remisión y si el ítem correspondiente creó activo inmovilizado
	@type  Function
	@author ARodriguez
	@since 08/01/2024
	@version 1.0
	@param n/a
	@return lRet, lógico, .T. = No tiene relación con remisión o no creó ATF, .F. = Relación con remisión y creó ATF
	@example
	GravaNfGeral() -> LxChkAtF()
	@see LOCXNF
	/*/
Function LxChkAtF()
	Local aArea	:= SD1->(GetArea())
	Local cKey	:= SD1->(D1_REMITO+D1_SERIREM+D1_FORNECE+D1_LOJA+D1_COD+D1_ITEMREM)
	Local lRet	:= .T.

	If !Empty(SD1->D1_REMITO)
		SD1->(dbSetOrder(1)) // D1_FILIAL+D1_DOC+D1_SERIE+D1_FORNECE+D1_LOJA+D1_COD+D1_ITEM

		If SD1->(MsSeek(xFilial("SD1") + cKey))
			lRet := Empty(SD1->D1_CBASEAF)
		EndIf

		SD1->(RestArea(aArea))
	EndIf

Return lRet

/*/{Protheus.doc} LxObtBanco
	Obtiene el banco con el cual fue generado el movimiento bancario
	@type  Function
	@author alfredo.medrano
	@since 02/05/2024
	@version 1.0
	@param n/a
	@return lRet, lógico, .T. = obtuvo registros .F. = no obtuvo registros
	@example
	Fc030Gera() -> LxObtBanco()
	@see FINC030
	/*/
Function LxObtBanco(cORDREC,cBanco,cAgencia,cConta)

	Local aArea	:= GetArea()
	Local cAliasSE5 := GetNextAlias()
	Local lRet	:= .F.
	Local cRecPag	:= Padr("P", TamSx3("E5_RECPAG")[1], " ") //Recibo o Pago
	Local cTpdocP   := Padr("VL", TamSx3("E5_TIPODOC")[1], " ") //Tipo de Doc.
	default cORDREC := "" 

	If !Empty(cORDREC)
			BeginSQL Alias cAliasSE5
			SELECT 
				E5_BANCO, E5_AGENCIA, E5_CONTA
			FROM
				%table:SE5% SE5
			WHERE
				E5_FILIAL 	=  %xFilial:SE5% 	AND
				E5_ORDREC   =  %Exp:cORDREC% 	AND
				E5_TIPODOC  =  %Exp:cTpdocP% 	AND
				E5_RECPAG   =  %Exp:cRecPag% 	AND
				SE5.%notDel%
		EndSQL
		
		If (!(cAliasSE5)->(EOF()))
			cBanco  := (cAliasSE5)->E5_BANCO
			cAgencia:= (cAliasSE5)->E5_AGENCIA
			cConta  := (cAliasSE5)->E5_CONTA
			lRet := .T.
		EndIf
		(cAliasSE5)->(DbCloseArea())
	EndIf
	RestArea(aArea)

Return lRet

/*/{Protheus.doc} LxValDupNF
	Valida si existe número de documento de la serie indicada
	Si SX5 es compartidaa nivel sucursal, validar número de documento sin filtrar filial
	@type  Function
	@author ARodriguez
	@since 10/06/2024
	@version 1.0
	@param cSerie, caracter, serie
	@param cNum, caracter, número de documento
	@param cTabla, caracter, tabla de documentos
	@return lRet, lógico, validación existosa? (no existe número de documento)
	/*/
Function LxValDupNF(cSerie, cNum, cTabla,cEspecie)
	
	Local lSX5Comp	:= (FwModeAccess("SX5",3) == "C")	// Tabla SX5 compartida a nivel sucursal?
	Local lRet		:= .T.
	Local cQuery	:=""
	Local cPreF     :=""
	Local nCampo    :=1
	Local nReg		:=0
	Local oExec 	:=Nil

	Default cSerie	:= Space(FWSX3Util():GetFieldStruct("F2_SERIE")[3]) 
	Default cNum	:= Space(FWSX3Util():GetFieldStruct("F2_DOC")[3])
	Default cTabla	:= "SF2"
	Default cEspecie:=""

	If lSX5Comp 
	
		
		IF cTabla=="SF1"
			cPreF:="1"
		ELSE
			cPreF:="2"
		ENDIF
		
		cQuery+= "  SELECT COUNT(1) NUMDOC "
		cQuery+= "  FROM   "+RetSqlName(cTabla)+" "+ cTabla 
		cQuery+= "  WHERE  "+cTabla+".F"+cPreF+"_SERIE  = ? "
		cQuery+= "  AND    "+cTabla+".F"+cPreF+"_DOC  = ? "
		IF ALLTRIM(cEspecie)!= ""
			cQuery+= "  AND    "+cTabla+".F"+cPreF+"_ESPECIE  = ? "
		ENDIF
		cQuery += " AND    "+cTabla+".D_E_L_E_T_ = ? "

		cQuery := ChangeQuery(cQuery)
			
		oExec := FwExecStatement():New(cQuery)
		

		
		oExec:SetString(nCampo++, cSerie) //Serie
		oExec:SetString(nCampo++, cNum) //Numero Doc
		IF ALLTRIM(cEspecie)!= ""
			oExec:SetString(nCampo++, cEspecie) //Especie(NF,NCC,etc)
		ENDIF
		oExec:SetString(nCampo++, " ") //D_E_L_E_T_

		nReg:=oExec:ExecScalar("NUMDOC")
		If nReg>0
			lRet:=.F.
		EndIF
		oExec:Destroy()
		oExec := nil 

		If !lRet
			Help("  ",1,"EXISTNF")
		EndIf

	EndIf

Return lRet

/*/{Protheus.doc} LxFunMerc
	Función encargada de llenar el arreglo de mercancias para generar lo nodos
	en el XML de carta porte
	@type  Function
	@author eduardo.manriquez
	@since 17/07/2025
	@version 1.0
	@param lTras, lógico , Variable que define si es un documento de traslado
	@return 
	@example
	LxFunMerc (lTras)
	@see
	/*/
Function LxFunMerc (lTras)
	Local lVlUnit := .T.
	Local lVlMerc := .T.
	Local nX      := 1
	Local lAgrCon := .T.
	Local cBienTrans := ""
	Local nCantidad := 0
	Local cClaveUnid := ""
	Local cDescrip := ""
	Local nValMerc := 0	
	Local lPECONCEP		:= ExistBlock("LOCXCONCEP")
	Local nTamano := iif(AllTrim(cEspecie)=="NCC",TamSX3("D1_ITEM")[1],TamSX3("D2_ITEM")[1])
	Local aConcepto := {}

	Default lTras   := .T.

	aMercCP := {}
	aSort(aDetFac,,,{|x,y| x[17] < y[17]}) //Ordena item ascendente
	For nX := 1 To Len(aDetFac)
		If(AllTrim(cEspecie) $ "NF" .And. !lTras .And. (!(aDetFac[nX][18] == "SV") .And. aDetFac[nX][7] == 0))
			lAgrCon := .F.
		Else
			lAgrCon := .T.
		Endif

		cBienTrans := aDetFac[nX][1]
		nCantidad := aDetFac[nX][3]
		cClaveUnid := aDetFac[nX][4]
		cDescrip := CFDCarEsp(aDetFac[nX][6])
		nValMerc := aDetFac[nX][8]

		If lPECONCEP
			aConcepto := ExecBlock("LOCXCONCEP",.F.,.F.,{SubStr(aDetFac[nX][2],nTamano+1),SubStr(aDetFac[nX][2],1,nTamano)})
			If !Empty(aConcepto[1]) .and. ValType(aConcepto[1]) == "C"
				cBienTrans := aConcepto[1]
			Endif

			If !Empty(aConcepto[3]) .and. ValType(aConcepto[3]) == "N"
				nCantidad := aConcepto[3]
			Endif

			If !Empty(aConcepto[4]) .and. ValType(aConcepto[4]) == "C"
				cClaveUnid :=  aConcepto[4]
			Endif

			If !Empty(aConcepto[6]) .and. ValType(aConcepto[6]) == "C"
				cDescrip := CFDCarEsp(aConcepto[6])				
			Endif

			If Len(aConcepto) >= 8 .And. ValType(aConcepto[8]) == "C" .And. !Empty(aConcepto[8])
				nValMerc := Val(aConcepto[8])
			Endif
		Endif
		lVlUnit := (lTras .Or. (!(aDetFac[nX][18] == "SV") .And. aDetFac[nX][7] > 0))
		lVlMerc := lAgrCon .and. lVlUnit .and.  aDetFac[nX][7] > 0
		if !lAgrCon .Or. lVlUnit
			aAdd(aMercCP,{cBienTrans, ; //1.BienesTransp
				Alltrim(Str(nCantidad)), ; //2.Cantiad
				cClaveUnid, ;               //3.ClaveUnidad
				Alltrim(CFDCarEsp(cDescrip,.T.)), ;//4.Descripcion
				aDetFac[nX][19], ; //5.FraccionArancelaria
				"MXN", ;           //6.Moneda
				aDetFac[nX][20], ; //7.PesoEnKg
				aDetFac[nX][21], ; //8.UUIDComercioExt
				IIf(lVlMerc,nValMerc,aDetFac[nX][22]), ; //9.ValorMercancia
				aDetFac[nX][23], ; //10.Cve Material Meligroso
				aDetFac[nX][24], ; //11.Embalaje
				aDetFac[nX][25], ; //12.IDOrigen
				aDetFac[nX][26], ; //13.IDDestino
				aDetFac[nX][13], ;  //14.Pedimento
				ObtColSAT("S019",aDetFac[nX][1],1,8,189,3),;//15.Material Peligroso "0", "1" o "0,1"
				aDetFac[nX][27], ; //16.Material Peligroso Si/No
				aDetFac[nX][30], ; //17. LoteMedicamento - FechaCaducidad
				aDetFac[nX][31], ; //18. Tipo Documento Aduanero
				aDetFac[nX][32], ; //19. RFC Importador
				aDetFac[nX][33], ; //20. Datos para Carta Porte
				aDetFac[nX][34], ; //21. Identificador Documento Aduanero
				})
		Endif
	Next nX	
	FwFreeArray(aConcepto)

Return 
