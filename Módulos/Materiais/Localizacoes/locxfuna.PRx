Function CFDCarEsp(cTexto,lCodHtml)
	Local cRet := ""
	Local nChar := 0
	Local aCarEsp := {}
	Default lCodHtml := .T.

	If !Empty(cTexto)
		cRet := cTexto
		If lCodHtml
			Aadd(aCarEsp,{"&","&amp;"})
			Aadd(aCarEsp,{'"',"&quot;"})
			Aadd(aCarEsp,{"<","&lt;"})
			Aadd(aCarEsp,{">","&gt;"})
			Aadd(aCarEsp,{"'","&apos;"})

			For nChar := 1 To Len(aCarEsp)
				cRet := StrTran(cRet,aCarEsp[nChar,1],aCarEsp[nChar,2])
			Next
		EndIf
	EndIf
	cRet := EncodeUtf8(cRet)

Return(cRet)

