Function LocXRelCOA()

//	F3_TIPOMOV = 'C'
//	F3_TIPO # 'D'
Local aCampos		:= {}
Local lQuery		:= .F.
Local nTotalQ		:= 0
Local cPerg			:= "RELCOA"
Local dPeriodoDe	:= DToS(dDataBase)
Local dPeriodoAte	:= DToS(dDataBase)
Local cFilDe		:= '  '
Local cFilAte		:= '99'
Local cIndTmp		:= ""

//Totalizadores para o cabeçalho, agrupamento por mês
Local nIMPORTE		:= 0
Local nTOTCOMP		:= 0
Local nTOTAGRU		:= 0
Local nTOTIMPO		:= 0
Local nTOTEXON		:= 0
Local dMesAgru

//agrupamento dos itens da nota fiscal SF3 (quando mais de um item na nota com TES diferentes)
