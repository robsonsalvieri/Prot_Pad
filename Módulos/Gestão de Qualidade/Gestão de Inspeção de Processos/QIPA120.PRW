FUNCTION A120CInf()
Local nInf

nInf := 0.00
DbSelectArea("QPX")
QPX->(Dbseek(xFilial("QPX")))
While ! Eof() .and. QPX_FILIAL == xFilial("QPX")
	RecLock("QPX",.F.)
	QPX->QPX_VLINF := nInf
	nInf := QPX->QPX_VLSUP + 0.01
	DbSkip()
Enddo
Return nil

