/*/{Protheus.doc} checkTiss

    @type  Class
    @author PLSTEAM
    @since 12/05/2022
/*/
//------------------------------------------------------------------------------------------
method checkTiss(cHealthProviderId, cTableCode, cProcCode, cTissVer) class PLSProcAuthDao

    local lRetorno      := .t.
    local cCodPro       := ''
    local cCodPad       := ''

    private aTabDup     := PlsBusTerDup( superGetMv("MV_TISSCAB",.f.,"87") )
    private aErrVarVin  := { .f., "", "", "" }

    BAU->(dbSetOrder(1))
    if (BAU->(msSeek(xFilial("BAU") + cHealthProviderId)))
        cTissVer := BAU->BAU_TISVER
    endIf

    cCodPad := allTrim( PLSVARVINC('87','BR4', allTrim(cTableCode)) )
    cCodPro := allTrim( PLSVARVINC(cTableCode, 'BR8', cProcCode, cCodPad + cProcCode,, aTabDup, @cCodPad) )
        
    if  ! PChkTabDup(@cCodPad, cCodPro, cTableCode)
        ::rejectionCauses( "De/Para nao realizado para -> (" + cTableCode + ") " + cProcCode )
        lRetorno := .f.
    endIf

    if lRetorno
        cTableCode := cCodPad
        cProcCode  := padR( allTrim(cCodPro), tamSX3("BR8_CODPSA")[1] ) 
    endIf

return(lRetorno)

method isExec(cAuthtype) class PLSProcAuthDao
local cRet := SOLICITACAO

if cAuthtype $ G_SADT_ODON + "|" + G_CONSULTA 
    cRet := EXECUCAO
endif

if self:getValue(self:oJson["journey"],'') == '2' .and. empty(self:getValue(self:oJson["mainAuthorizationCode"],''))
    cRet := SOLICITACAO
endif

return cRet

method getCodEsp(oJson) class PLSProcAuthDao
local cCodEsp   := ''
local cCbos     := ''

cCodEsp := self:getValue(oJson["specialtyCode"], '')
if empty(cCodEsp)
    cCbos := self:getValue(oJson["code"], '')
    if !empty(cCbos)
        cCodEsp := AllTrim(PLSVARVINC('24','BAQ',cCbos))
    endif
endif

return cvaltochar(cCodEsp)

method crit09L(aQtdBrw,aDadRDA) class PLSProcAuthDao
local lCritica  := .f.
local nX        := 1
local nY        := 1
local aItensPac := {}
local cCodOpe   := plsintpad()

for nX := 1 to len(aQtdBrw)
    aItensPac := PlRetPac(cCodOpe,aDadRDA[2],aQtdBrw[nX][1],alltrim(aQtdBrw[nX][2]),,aQtdBrw[nX][4])
    if len(aItensPac) > 0
        for nY := 1 to len(aQtdBrw)
            if nY <> nX // Não verifico o proprio pacote para não criticar indevidamente, pela TISS não pode um pacote dentro de outro, mas vai que...
                lCritica := iif(ascan(aItensPac, {|x| allTrim(x[1]) == allTrim(aQtdBrw[nY][1]) .and. allTrim(x[2]) == allTrim(aQtdBrw[nY][2]) }) > 0, .t., .f.)
            endif
            if lCritica
                exit
            endif
        next
    endif
    if lCritica
        exit
    endif
next

return lCritica

method calcCopartRda(aDadUsr, aDadRda, cTableCode, cProcCode, cSpecialtyCode, nReqQuantity, cAuthtype, cLocationCode) class PLSProcAuthDao
        
    Local lPagtoRDA := .F.
    Local aValor  := {}

    aValor := PLSCALCCOP(cTableCode, cProcCode, Substr(Dtos(dDataBase),5,2), Substr(Dtos(dDataBase),1,4), ;
						aDadRda[2], cSpecialtyCode,/*cSubEsp*/, cLocationCode, nReqQuantity, ;
						dDatabase, .T.,/*cObsoleto*/,/*nVlrEve*/,/*cGrpInt*/,aDadUsr, ;
						/*cPadInt*/, /*cPadCon*/, /*aQtdPer*/, iif(len(aDadUsr)>91,aDadUsr[92],"") ,/*nVlrApr*/, .T., ;
						/*lCompra*/, /*cHorPro*/,/*aRdas*/, /*cOpeRda*/, /*cTipPrefor*/, /*cProRel*/,/*nPrPrRl*/,;
						/*aValAcu*/, /*cNivAut*/, /*cChvNiv*/, /*dDatCir*/, /*cHorCir*/,/*cCid*/,;
						/*aUnidsBlo*/,cAuthtype, /*aCobAcu*/, /*nVlrAprPag*/,/*aVlBloq*/, /*cModCob*/,;
						/*nVlrPagBru*/, /*nRegBD6*/, /*lCirurgico*/, /*nPerVia*/, /*cRegPag*/,/*cRegCob*/,;
						/*nNOTUSED1*/, /*nNOTUSED2*/, /*aPacote*/,/*cChaveGui*/,/*cSequen*/, /*aRetCom*/,;
						/*cRegInt*/,/*cFinAte*/, /*aVetPag*/, /*cChaveLib*/,/*lAuditoria*/,/*cDente*/,/*cFaces*/,;
						.F.,/*cHorPro6C*/,/*lAneste*/, /*nVlrPagLiq*/,/*cTipAdm*/)

    lPagtoRDA := Len(aValor) >= 18 .and. valType(aValor[18]) == "C" .And. aValor[18] == "1" 

Return lPagtoRDA
