method new(cProcedureId, oJson) class PLSProcAuthDao

    ::cProcedureId      := cProcedureId
    ::oJson             := oJson
    ::aRejectionCauses  := {}
    ::lBCTMotNeg        := BCT->(FieldPos("BCT_MOTNEG")) > 0
