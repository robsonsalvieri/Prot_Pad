		aadd(aChave,{"BVI_MATRIC",M->BA3_MATRIC})
		// Alterado para ser feita a gravaГЦo pela funГЦo PLUPTCOLS, devido a funГЦo 'grava' esta se perdendo no RECNO(no array aVetTrab)
		// e com isso estava fazendo altereГУes em registros errado. Vinicius.Queiros 17/10/2019
		If Len(aDadBVI) > 0
			PLUPTCOLS("BVI",aDadBVI,aCabBVI,aVetBVI,nOpc,aChave,.T.)
