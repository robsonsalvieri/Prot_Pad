Function PLCONSREC(cAlias, oBrwPro)

	//зддддддддддддддддддддддддддддддддддддддддддддддддд©
	//ЁDefine variaveis para composicao da Matricula    Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддды
	Local oModelRec
	Local cCODINT := ""
	Local cCODEMP := ""
	Local cMATRIC := ""

	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//ЁFoi usado o FieldGet pq os outros campos da chave nao se encontram na MATRIZ aHeader               |
	//ЁPega o conteudo do campo informado na linha corrente 											  |
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	Local cTIPREG := ""
	Local cDIGITO := ""

	private aRotina := {}

	//Desativa o atalho de teclado para nЦo permitir abrir a rotina varias vezes enquanto ela esta aberta
	//pela rotina de reembolso
	SET KEY VK_F8 TO
	SET KEY VK_F2 TO
	SET KEY K_ALT_Z TO
	SET KEY VK_F4 TO
	SET KEY VK_F9 TO
	SET KEY VK_F10 TO



	if cAlias == "BA1"
		cCODINT := M->BA3_CODINT
		cCODEMP := M->BA3_CODEMP
		cMATRIC := M->BA3_MATRIC
		cTIPREG := oBrwUsr:FieldGet('BA1_TIPREG',oBrwUsr:Linha())
		cDIGITO := oBrwUsr:FieldGet('BA1_DIGITO',oBrwUsr:Linha())
		//ENVIA O CсDIGO DA FAMILIA PARA O FILTRO
		cMatric := cCODINT+cCODEMP+cMATRIC
	elseIf cAlias == "BOW"
		//ENVIA O CсDIGO DA FAMILIA PARA O FILTRO
		cMatric := SubStr(oBrwPro:acols[oBrwPro:obrowse:nAt][GdFieldPos("B1N_MATRIC",oBrwPro:aHeader)],1,14)
	endif

	//зддддддддддддддддддддддддддддддддддддддддддддддддд©
	//ЁSeta a ordem de procura	                        Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддды
	BA1->(DbSetOrder(2)) // BA1_CODINT+BA1_CODEMP+BA1_MATRIC+BA1_TIPREG+BA1_DIGITO
	if BA1->(msSeek(xFilial('BA1')+cMatric))
		aAdd( aRotina, { 'Pesquisar' , 				'PesqBrw'         , 0, 1, 0, .T. } )
		aAdd( aRotina, { 'Visualizar', 				'VIEWDEF.PLSCADREC', 0, 2, 0, NIL } )
		PLSCADREC(cMatric)
	Else
		MsgInfo(OemtoAnsi(STR0155)) //'UsuАrio nЦo ENCONTRADO')
	EndIf

	//reativa a tecla de atalho
	SetKey(VK_F8,  {|| PLCONSREC("BOW", oBrwPro)}) //Receitas
	SetKey(VK_F2,{|| PLBOWHIS("BOW",BOW->BOW_PROTOC)}) // Historico do reembolso
	SetKey(K_ALT_Z,{|| PLGRVBK6("MOA")}) // Verifica cadastro rede
	SetKey(VK_F4,{|| PLBrwRmbCan()}) // Protocolos cancelados
	SetKey(VK_F9   , {|| PLSEXITRE()}) // Glosar itens
	SetKey(VK_F10  , {|| VisMotExc()}) // Mot. Glosa
	//зддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Fim da funcao                                   Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддды
Return

