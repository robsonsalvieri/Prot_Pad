Function PL260VLD()
	Local dRet 	:= cTod('')
	LOCAL nCnt  := 0
	LOCAL nMeses:= GetNewPar("MV_PLSPRPF",12)
	LOCAL cDia 	:= SubStr(dtoc(dDataBase),1,2)
	LOCAL nMes 	:= Val(SubStr(dtoc(dDataBase),4,2))
	LOCAL nAno 	:= Val(StrZero(Year(dDataBase),4))

	For nCnt := 1 To nMeses
