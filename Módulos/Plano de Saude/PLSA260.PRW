*/
Function PIncLogin( cMatric , lIncDep , cTpPor, lMsg, cCpf, cEmail, lPortal, lMingle)

	LOCAL aArea 		:= GetArea()
	LOCAL aItens		:= {}
	LOCAL cCodAcPad	:= GetNewPar("MV_CDPBEN","")
	LOCAL cTpLogin    := GetNewPar("MV_LOGIN","1") // 1 - Matricula 2 - CPF  3 - E-mail
	LOCAL nI			:= 0
	LOCAL aItensAux	:= {}
	LOCAL cTit			:= ""
	LOCAL cLogin      := ""
	Local cPath			:= GetSrvProfString( "RootPath" , "" ) + "\LOGS\"
	Local cNomArq		:= "log_inc_usr" + DtoS(dDataBase) + "_" + StrTran(SubStr(Time(),1,5),":","")
	Local cPerAcePri	:= GetNewPar("MV_TPPERA","1")
	Local lInc	:=.T.
	Local lRet := .T.
	Local cMsg := ""
	Local lCripto := .F.
	Local cSenhaAux := ""
	Local lEnviaEmail := GetNewPar("MV_PLEMAIL",.F.)
	Local aFamilias := {}
	Local aItens := {}
	Local cChave := xFilial("BA1") + cCpf
	Local cChaveFamilia := ""
	Local nTamFam := 0
	Local nCodFan := 0
	Local lEhTitular := .F.
	Local nLenFam := 0
	Local nLenCodFam := 0
	Local nLenItens := 0
	Local cCriptoTISS := ""
	Local lUserExist := .F.
	Local cHolderType := SuperGetMv("MV_PLCDTIT", .F., "T")
	Local cCodMatric := ""

	DEFAULT lIncDep	:= .T. // Inclui os dependentes na B49
	DEFAULT cTpPor	:= "3"
	DEFAULT lMsg		:= .F.
	Default lPortal		:= .F.
	Default cEmail := ""
	Default lMingle  := .f.

	 lUserExist := userExist(cMatric, cCpf, cEmail) // Verifica se o usuario ja existe

	If PLSALIASEX("B49")
		If lUserExist // Se ja tem usuario criado sai da funГao
			If lPortal
				Return "BeneficiАrio jА existe, caso tenha esquecido a senha, solicite uma nova no link 'Esqueceu sua senha'"
			Else
				Return
			EndIf
		EndIf

		If Empty(cCodAcPad) //"NЦo foi possivel criar o acesso ao portal (NecessАrio definir o parametro MV_CDPBEN com o cСdigo de acesso padrЦo) "
			If lPortal
				cMsg := OemtoAnsi(STR0254)
				lRet := cMsg 
			Else
				MsgInfo(OemtoAnsi(STR0254))
				Return()
			EndIf
		EndIf

		If !lPortal .or. lMingle
		
			cCodUsr := A226USRCOD(.T.)
			
			BA1->(DbSetOrder(4))
			BA1->(MsSeek(cChave))
				
			While !BA1->(Eof()) .And. (xFilial("BA1") + BA1->BA1_CPFUSR) == cChave

				cChaveFamilia := BA1->(BA1_CODINT+BA1_CODEMP+BA1_MATRIC)

				If aScan(aFamilias, {|x| x == cChaveFamilia}) == 0
					aAdd(aFamilias, cChaveFamilia)
				EndIf

				BA1->(DbSkip())
			EndDo

			nLenFam := Len(aFamilias)

			For nTamFam := 1 To nLenFam
				cCodMatric := aFamilias[nTamFam]
				lEhTitular := .F.
				aCodigosDaFamilia := {}

				BA1->(DbSetOrder(2))
				If BA1->(MsSeek(xFilial("BA1") + cCodMatric))

					While !BA1->(Eof()) .And. ;
						BA1->(BA1_FILIAL+BA1_CODINT+BA1_CODEMP+BA1_MATRIC) == xFilial("BA1") + cCodMatric

						cCodBenef := BA1->(BA1_CODINT+BA1_CODEMP+BA1_MATRIC+BA1_TIPREG+BA1_DIGITO)

						If BA1->BA1_TIPUSU == cHolderType .And. BA1->BA1_CPFUSR == cCpf
							lEhTitular := .T.
						EndIf

						aAdd(aCodigosDaFamilia, cCodBenef)

						BA1->(DbSkip())
					End
				EndIf

				nLenCodFam := Len(aCodigosDaFamilia)

				For nCodFan := 1 To nLenCodFam
					cCodBenef := aCodigosDaFamilia[nCodFan]

					BA1->(DbSetOrder(2))
					If BA1->(MsSeek(xFilial("BA1") + cCodBenef))

						If lEhTitular
							aItensAux := {}
							aAdd(aItensAux, {"B49_CODUSR", cCodUsr, Nil})
							aAdd(aItensAux, {"B49_BENEF", cCodBenef, Nil})
							aAdd(aItens, AClone(aItensAux))

							If BA1->BA1_TIPUSU == cHolderType
								cTit := BA1->BA1_NOMUSR
							EndIf

						ElseIf BA1->BA1_CPFUSR == cCpf							
							cTit := BA1->BA1_NOMUSR							
							aItensAux := {}
							aAdd(aItensAux, {"B49_CODUSR", cCodUsr, Nil})
							aAdd(aItensAux, {"B49_BENEF", cCodBenef, Nil})
							aAdd(aItens, AClone(aItensAux))
							Exit
						EndIf

					EndIf
				Next
			Next
			
		Else

			cCodUsr := A226USRCOD(.T.) //O cadastro de usuАrios do portal nЦo usa o SXE pra gerar o sequencial

			BTS->(DbSetOrder(3))
			BTS->(DbSeek(xFilial("BTS")+cCpf))

			While !BTS->(Eof()) .AND. BTS->BTS_FILIAL == xFilial("BTS") .And. AllTrim(BTS->BTS_CPFUSR) == cCpf
				cTit := BTS->BTS_NOMUSR

				aItensAux	:= {}

				aAdd(aItensAux, {"B49_CODUSR" , cCodUsr , Nil}) // Codigo do usuario BSW
				aAdd(aItensAux, {"B49_BENEF" , BTS->BTS_CPFUSR , Nil}) // Matricula

				aAdd(aItens, AClone(aItensAux))
				BTS->(DbSkip())
			End

		EndIf
		lMsErroAuto := .F.
		lMsHelpAuto := .T.

		/*Preenchendo a Senha*/

		cSenhaOrg := alltrim(str(Randomize( 10, 100))) + alltrim(str(Randomize( 10, 100))) + alltrim(str(Randomize( 10, 100))) + alltrim(str(Randomize( 10, 100)))

		cSenhaOrg += GeneratePassword()
		cSenhaAux := cSenhaOrg

		cLogin := cMatric

		if cTpLogin = "3" .and. alltrim(cEmail) <> ""
			cLogin := cEmail
		else
			if cTpLogin = "2" .and. alltrim(cCpf) <> ""
				cLogin := cCpf
			endif
		endif

		//Verifica se o Login que irА ser utilizado jА esta cadastrado. Caso esteja, gravar a matrМcula.
		BSW->(DbSetOrder(1))
		if BSW->(dbSeek(xFilial("BSW")+ cLogin + (Space(TamSx3("BSW_LOGUSR")[1] - len(cLogin)))))
			cLogin := cMatric
		endif
		//Verificando por CPF cadastrado na BSW, pois pelo portal o CPF И cadastrado Primeiro que a matricula
		//NЦo permitindo a duplicaГЦo de usuarios iguais
		if BSW->(dbSeek(xFilial("BSW")+ cCpf + (Space(TamSx3("BSW_LOGUSR")[1] - len(cLogin)))))
			lInc:=.F.
		Endif

		lCripto := Len(BSW->(BSW_SENHA)) == 64 .And. FWAliasInDic("BSH", .F.) 

		cCriptoTISS := PLSCRIDEC(1, alltrim(cSenhaOrg))
		If !lInc 
			If Len(BSW->(BSW_SENHA)) == 64 .And. FWAliasInDic("BSH", .F.) 
				cSenhaOrg := SHA256(Trim(cSenhaOrg))
				PLStoragePass(cSenhaOrg,cLogin,cCodUsr,lPortal)
			Else
				cSenhaOrg := PLSCRIDEC(1,cSenhaOrg)
				
			Endif
		Else
			If FWAliasInDic("BSH", .F.) 
				cSenhaOrg := SHA256(Trim(cSenhaOrg))
				PLStoragePass(cSenhaOrg,cLogin,cCodUsr,lPortal)
			Else
				cSenhaOrg := PLSCRIDEC(1,cSenhaOrg)
			Endif
		Endif

		BSW->(RecLock("BSW",lInc))
		BSW->(BSW_FILIAL) := xFilial("BSW")
		BSW->(BSW_CODUSR) := cCodUsr
		BSW->(BSW_LOGUSR) := cLogin//cMatric
		BSW->(BSW_NOMUSR) := cTit
		BSW->(BSW_DTSEN)  := DATE()
		BSW->(BSW_SENHA)  := cSenhaOrg
		BSW->(BSW_CODACE) := cCodAcPad
		BSW->(BSW_TIPCAR) := "0"
		BSW->(BSW_TPPOR)  := cTpPor
		BSW->(BSW_EMAIL)  := upper(cEmail)
		BSW->(BSW_PRIACE) := .T.
		BSW->(BSW_PERACE) := cPerAcePri
		if BSW->(fieldPos("BSW_SENTIS")) > 0
			BSW->(BSW_SENTIS) := cCriptoTISS
		endif
		if BSW->(fieldPos("BSW_DTSNTI")) > 0
			BSW->(BSW_DTSNTI) := DATE()
		endif
		BSW->(MsUnlock())

		nLenItens := Len(aItens)

		B49->(dbSetOrder(2))
		For nI := 1 To nLenItens

			B49->(dbSeek(xFilial("B49") + aItens[nI][2][2]))
			B49->(RecLock("B49",.T.))
			B49->(B49_FILIAL) := xFilial("B49")
			B49->(B49_CODUSR) := aItens[nI][1][2]
			B49->(B49_BENEFI) := aItens[nI][2][2]
			B49->(MsUnlock())

			lMsg := .T.
	
		Next

		If ! lMsErroAuto

			If lMsg
				if(lMingle)
					if !PLMINGCRUD(K_Incluir, cLogin, cSenhaOrg , "3", .F.)
						return "Ocorreu um erro ao sincronizar usuАrio, tente novamente mais tarde."
					Else 
						If lEnviaEmail .And. lInc

							cSenMail := cSenhaAux
							cLogMail := cLogin					
							if FindFunction( "PLSNotifica") .and. FWAliasInDic("BQ7")
								PLSNotifica('000006', cMatric, cSenhaAux)
							endif
						EndIf
						
					EndIf
					
				else 
					BOJ->(DbSelectArea("BOJ"))
					BOJ->(DbSetOrder(3))
					BOJ->(MsSeek(xFilial("BOJ") + "PLSA260" + (Space(TamSx3("BOJ_ROTINA")[1] - 7))  + "1" + (Space(TamSx3("BOJ_VERSAO")[1] - 1 ))))

					PLSinaliza(BOJ->BOJ_CODSIN,nil,nil, alltrim(cEmail)/*"roberto.arruda@totvs.com"/*cEmailEnviar*/, "Envio email de anexo - Junta medica",,,,,, .F.,"",,,)
					If !lPortal //se rotina foi chamada pelo portal, nЦo deve ter alerta, portal jА possui os alertas prСprios
						MsgAlert(STR0253) //"Login no portal do beneficiАrio criado com sucesso!"
					Else
						lRet := .T.
					EndIf
				EndIf
			EndIf
		Else
			If lMsg .And. !lPortal
				MostraErro()
			Else
				MostraErro(cPath, cNomArq)
				lRet := .F.
			EndIf
		EndIf

	EndIf
	RestArea(aArea)
Return lRet

