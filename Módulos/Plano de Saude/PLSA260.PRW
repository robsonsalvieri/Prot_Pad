Function PLSVLDGPF(nVal)

	LOCAL cCdEmp := M->BA3_CODEMP
	LOCAL cMtr := BA3->(BA3_CODINT+BA3_CODEMP+BA3_MATRIC)

	If nVal == 1
		//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Retorna os titulares das famМlias do mesmo grupo/empresa para a
		//  consulta padrЦo PLSGPF, mas nЦo retorna o titular da famМlia que
		//  esta sendo alterada.
		//  Foi necessАrio efetuar o filtro nesta funГЦo, pois o tamanho do campo
		//  filtro da consuta padrЦo И insuficiente.
		//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		If BA1->BA1_CODEMP == cCdEmp .AND. BA1->BA1_TIPUSU == "T"  .AND. If(EMPTY(BA1->BA1_DATBLO) .OR. BA1->BA1_DATBLO >= dDataBase, .T., .F.) ;
				.AND. BA1->(BA1_CODINT+BA1_CODEMP+BA1_MATRIC) <> cMtr
			Return .T.
		Else
			Return .F.
		EndIf
	EndIf

	If nVal == 2
		//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Verifica se a famМlia que esta sendo inclusa em um grupo familiar
		//  nЦo И uma familia principal de um grupo familiar nao permitindo
		//  alterar o campo BA3_GRPFAM
		//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды

		If FunName() <> "PLSA124"
			cSQL := "SELECT BA3_CODINT,BA3_CODEMP, BA3_MATRIC, BA3_GRPFAM, BA3_BLOFAT,R_E_C_N_O_ FROM " + RetSQLName("BA3")
			cSQL  += "  WHERE " +  IIF(AllTrim(TCGetDB())$"ORACLE|POSTGRES", "SUBSTR(BA3_GRPFAM,1,14) = '", "SUBSTRING(BA3_GRPFAM,1,14) = '")
			cSQL  += + BA3->(BA3_CODINT+BA3_CODEMP+BA3_MATRIC) + "' AND D_E_L_E_T_ = ' '"
		Else
			cSQL := "SELECT BA3_CODINT,BA3_CODEMP, BA3_MATRIC, BA3_GRPFAM, BA3_BLOFAT,R_E_C_N_O_ FROM " + RetSQLName("BA3")
			cSQL  += " WHERE " + IIF(AllTrim(TCGetDB())$"ORACLE|POSTGRES", "SUBSTR(BA3_GRPFAM,1,14) = '", "SUBSTRING(BA3_GRPFAM,1,14) = '")
			cSQL  += Substr(M->BA3_GRPFAM,1,14) + "' AND D_E_L_E_T_ = ' '"
		EndIf

		dbUseArea(.T.,"TOPCONN",TCGENQRY(,,changeQuery(cSQL)),"TrbDesb",.F.,.T.)

		If !TrbDesb->(EOF())
			Alert(STR0251)
			M->BA3_GRPFAM := ""

			TrbDesb->(dbCloseArea())
			Return .T.
		EndIf

		TrbDesb->(dbCloseArea())

		If FunName() <> "PLSA124"
			cSQL := "SELECT BA3_CODINT,BA3_CODEMP, BA3_MATRIC, BA3_GRPFAM, BA3_BLOFAT,R_E_C_N_O_ FROM " + RetSQLName("BA3")
			cSQL  += " WHERE BA3_CODINT = '" + BA1->BA1_CODINT + "'"
			cSQL  += " AND BA3_CODEMP = '" + BA1->BA1_CODEMP + "'"
			cSQL  += " AND BA3_MATRIC = '" + BA1->BA1_MATRIC + "'"
			cSQL  += " AND D_E_L_E_T_ = ' '"
		Else
			cSQL := "SELECT BA3_CODINT,BA3_CODEMP, BA3_MATRIC, BA3_GRPFAM, BA3_BLOFAT,R_E_C_N_O_ FROM " + RetSQLName("BA3")
			cSQL += " WHERE BA3_CODINT = '"+Substring(M->BA3_GRPFAM, 1, 4)+"'"
			cSQL += " AND BA3_CODEMP = '"+Substring(M->BA3_GRPFAM, 5, 4)+"'"
			cSQL += " AND BA3_MATRIC = '"+Substring(M->BA3_GRPFAM, 9, 6)+"'"
			cSQL += " AND D_E_L_E_T_ = ' '"
		EndIf

		dbUseArea(.T.,"TOPCONN",TCGENQRY(,,changeQuery(cSQL)),"TrbDe",.F.,.T.)

		If !TrbDe->(EOF())
			If !EMPTY(TrbDe->BA3_GRPFAM)
				Alert(STR0252)

				TrbDe->(dbCloseArea())
				Return .F.
			EndIf
		EndIf

		TrbDe->(dbCloseArea())

	EndIf
Return .T.

