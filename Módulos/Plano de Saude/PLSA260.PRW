/*/
Function PLVERBLO(cChave, cTipReg, dData, cOp, cCpf)

	Local lRet		:= .F.
	Local aVgDatBlo	:= {}
	Local nTamVgBl	:= 0
	Local lIsBlock	:= .F.
	Local nSelAviso	:= 0
	Local dDatIniBlo := ctod("")
	Local dDatFinBlo := ctod("")

	Default cCpf 	:= ""

	// Retorna se o beneficiАrio estА bloqueado ou nЦo e o array com as datas de bloqueio e desbloqueio
	lIsBlock := PlChHiBlo("BCA", dDatabase, BA1->(BA1_CODINT+BA1_CODEMP+BA1_MATRIC), BA1->BA1_TIPREG, nil, nil, nil, nil, @aVgDatBlo, .F.)
	nTamVgBl := len(aVgDatBlo)
	If cOp == "0"// se for operaГЦo de bloqueio
		if lIsBlock
			nSelAviso := Aviso("AtenГЦo - PLVERBLO",;
							"NЦo И possМvel realizar o bloqueio pois o beneficiАrio estА dentro de um perМodo de bloqueio de: " + convDataStr(aVgDatBlo[nTamVgBl,1]) + " atИ " + convDataStr(aVgDatBlo[nTamVgBl,2]) + CHR(13)+CHR(10)+CHR(13)+CHR(10) +;
							"Consulte a documentaГЦo atravИs do botЦo abaixo ou utilize a rotina de histСrico de bloqueio para alterar a data de desbloqueio para a data atual e realize uma nova operaГЦo de bloqueio",;
							{"DocumentaГЦo", "Hist. Bloqueio", "OK"},;
							2)
			lRet := .T.

			if nSelAviso == 1
				ShellExecute("open", "https://tdn.totvs.com/pages/releaseview.action?pageId=910665757", "", "", 1)
			elseif nSelAviso == 2
				PLHistBlq()
			endif
		else
			// Tratamento para permitir lanГamento retroativo
			if dData < dDatabase
				return lRet
			endif
			
			dDatIniBlo := iif(nTamVgBl > 0, aVgDatBlo[nTamVgBl][1], ctod("")) // Data inicial do Зltimo desbloqueio
			dDatFinBlo := iif(nTamVgBl > 0, aVgDatBlo[nTamVgBl][2], ctod("")) // Data final do Зltimo desbloqueio
			// NЦo permite incluir um bloqueio caso a data esteja dentro de algum perМodo de bloqueio, mesmo que passado
			if dData >= dDatIniBlo .and. dData < dDatFinBlo
				help(nil, nil, "AtenГЦo", nil,;
					"NЦo И possМvel realizar o bloqueio pois a data informada estА dentro de um perМodo de bloqueio futuro",;
					1, 0, nil, nil, nil, nil, nil,;
					{"Reveja a data informada e insira uma data posterior a data de desbloqueio"})
				lRet := .T.
			else
				return lRet
			endif
		endif
	ElseIf cOp == "1" // se for operaГЦo de desbloqueio
		if !lRet
			if nTamVgBl > 0
				// Se for bloqueio futuro (ou seja, data de bloqueio maior que a database), entende-se que o bloqueio nЦo ocorreu ainda,
				// portanto o usuАrio deverА lanГar um bloqueio antes de lanГar um desbloqueio (isso poderА ser feito pela rotina de 'histСrico de bloqueio')
				if aVgDatBlo[nTamVgBl][1] > dDatabase
					// "NЦo И possМvel realizar o desbloqueio na data solicitada, pois existe um perМodo de bloqueio jА solicitado"
					help(nil, nil, "AtenГЦo", nil, STR0294, 1, 0, nil, nil, nil, nil, nil, {"Altere a data de bloqueio atravИs do histСrico de bloqueio ou revise a data de desbloqueio informada"})
					lRet := .T.
					return lRet
				endif
			endif
		endif

		if !lRet
			If CheckCPFFamilia(cCpf, cChave)
				Aviso(OemtoAnsi(" AtenГЦo "),OemtoAnsi("NЦo foi possМvel realizar o desbloqueio, pois existe outro BeneficiАrio Ativo da FamМlia com o mesmo nЗmero de CPF"), { "Ok" }, 2 )
				lRet := .T.
				Return lRet
			EndIf
		EndIf
	EndIf

return lRet

