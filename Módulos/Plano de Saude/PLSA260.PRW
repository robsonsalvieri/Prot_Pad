	LOCAL aAreaBCA	:= {}
	LOCAL cChaveHAT := ""
	LOCAL lProRat   := BG3->(Fieldpos("BG3_PRORAT")) > 0 // Verifica se o campo existe
	LOCAL oMulta    := Nil
