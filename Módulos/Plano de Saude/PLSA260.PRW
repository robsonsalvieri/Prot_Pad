	BI3->(DBSetOrder(1))
	If BI3->(MsSeek(xFilial("BI3")+cCodInt+cCodPro+cVersao))
		If BI3->BI3_ALLUSR == "1"
			If !lJuridico .and. !PLS260IsTit() .and. BI3->(FieldPos("BI3_SITANS")) > 0 .And. BI3->BI3_SITANS $ "2,3"
