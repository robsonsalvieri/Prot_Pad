*/
Function A260DELREG(cTab,aMat)
	LOCAL nI
	For nI := 1 to Len(aMat)
		&( cTab+'->( DbGoTo('+AllTrim(Str(aMat[nI]))+') ) ' )
		&( cTab+'->( RecLock("'+cTab+'",.F.) )' )
		&( cTab+'->( DbDelete() )' )
		&( cTab+'->( MsUnLock() )' )
	Next

Return


