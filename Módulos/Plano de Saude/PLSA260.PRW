/*/{Protheus.doc} PlBlcCanc
funГЦo que atualizara os campos na BA3 ao slecionar o cliente (SA1)
da funГЦo A260MOV (tela do cadastro de familia) quando chamada da rotina
de anАlise de beneficiАrios
/*/
//-------------------------------------------------------------------
Function Pl260CodMun(cCodMun,cBa3CodCli,nCamp)

	Local cEndCli	:= SA1->A1_END
	Local cEstCli	:= SA1->A1_EST
	Local cResulta	:= ''

	Default nCamp:= 0


	If nCamp = 1 // BA3_NUMERO
		iF AT( ",", cEndCli ) > 0

			ba3Num := StrTokArr(cEndCli, ",")
			//M->BA3_END 	  := ba3Num[1]
			cResulta := ba3Num[2]

		Else
			cResulta:= " "
		EndIF
	Endif


	If nCamp = 2 // BA3_CODMUN
		DbSelectArea("BID")
		BID->(DbSetOrder(3))

		If BID->(DbSeek(xFilial("BID")+cEstCli))
			cEstRef:= Substr(BID->BID_CODMUN,1,2)
			BID->(DbSetOrder(1))
			If BID->(DbSeek(xFilial("BID")+cEstRef+cCodMun))
				cResulta := BID->BID_CODMUN
			Endif

		Endif
	Endif

	If nCamp = 3 // BA3_END

		If AT( ",", cEndCli)
			ba3Num := StrTokArr(cEndCli, ",")
			//M->BA3_END 	  := ba3Num[1]
			cResulta := ba3Num[1]
		Else
			cResulta:=cEndCli
		Endif

	Endif

Return (cResulta)

