/*/{Protheus.doc} userExist
Retornar se o usuario ja tem cadastro na tabela BSW
@type function
@version  12.1.2510
@author diogo.sousa
@since 07/08/2025
@return logical
/*/
static function userExist(cMatric, cCpf, cEmail)
	Local cQuery := "" as character
	Local oExecStmt := Nil as object
	Local nOrder := 1 as numeric
	Local cAlias := "" as character
	Local lExist := .F. as logical	

	default cMatric := ""
	default cCpf 	:= ""
	default cEmail  := ""

	cQuery := "SELECT ? "
	cQuery += "FROM ? BSW "
	cQuery += "INNER JOIN ? B49 ON "
	cQuery += "B49.B49_FILIAL = ? AND "	
	cQuery += "B49.B49_CODUSR = BSW.BSW_CODUSR AND "
	cQuery += "B49.B49_BENEFI = ? AND "
	cQuery += "B49.D_E_L_E_T_ = ? "
	cQuery += "WHERE "
	cQuery += "BSW.BSW_FILIAL = ? AND "
	cQuery += "(BSW.BSW_LOGUSR = ? OR "
	cQuery += "BSW.BSW_LOGUSR = ? OR "
	cQuery += "BSW.BSW_LOGUSR = ? ) AND "
	cQuery += "BSW.D_E_L_E_T_ = ? "

	oExecStmt := FwExecStatement():new(cQuery)

	oExecStmt:setUnsafe(nOrder++, "BSW.BSW_LOGUSR")
	oExecStmt:setUnsafe(nOrder++, retSqlName("BSW"))
	oExecStmt:setUnsafe(nOrder++, retSqlName("B49"))
	oExecStmt:setString(nOrder++, xFilial("B49"))
	oExecStmt:setString(nOrder++, cMatric)
	oExecStmt:setString(nOrder++, " ")
	oExecStmt:setString(nOrder++, xFilial("BSW"))
	oExecStmt:setString(nOrder++, cMatric)
	oExecStmt:setString(nOrder++, cCpf)
	oExecStmt:setString(nOrder++, cEmail)
	oExecStmt:setString(nOrder++, " ")

	cAlias := oExecStmt:openAlias()
	if !(cAlias)->(eof())
		lExist = .T.
	endif

	(cAlias)->(dbCloseArea())

	oExecStmt:destroy()
    freeObj(oExecStmt)

return lExist

/*/{Protheus.doc} ManipulaBDQ
Verifica se existe registro na tabela BDQ relacionado a BDQ, realizando a exclusЦo
caso nЦo exista registro relacionado
@type function
@version  12.1.2510
/*/
Static Function ManipulaBDQ()
	
	Local cSqlBDQ := ""
	Local nIUsr   := 0
	Local nLnUsr  := LEN(oBrwusr:aCols)
	
	BDk->(DbSetOrder(1))
	BDQ->(DbSetOrder(1))

	For nIUsr := 1 To nLnUsr
		
		cSqlBDQ := " SELECT BDQ_CODINT, BDQ_CODEMP, BDQ_MATRIC, BDQ_TIPREG, BDQ_CODFAI "
		cSqlBDQ += " FROM " + RetSqlName("BDQ")
		cSqlBDQ += " WHERE BDQ_FILIAL = '" + xFilial("BDQ")  + "' "
		cSqlBDQ += "   AND BDQ_CODINT = '" + BA3->BA3_CODINT + "' "
		cSqlBDQ += "   AND BDQ_CODEMP = '" + BA3->BA3_CODEMP + "' "
		cSqlBDQ += "   AND BDQ_MATRIC = '" + BA3->BA3_MATRIC + "' "
		cSqlBDQ += "   AND BDQ_TIPREG = '" + oBrwUsr:FieldGet("BA1_TIPREG",nIUsr) + "'"
		cSqlBDQ += "   AND D_E_L_E_T_ = ' ' "
		cSqlBDQ += " GROUP BY BDQ_CODINT, BDQ_CODEMP, BDQ_MATRIC, BDQ_TIPREG, BDQ_CODFAI"

		dbUseArea(.T.,"TOPCONN",TcGenQry(,,cSqlBDQ),"TempBDQ",.T.,.T.)

		While !TempBDQ->(EOF())

			If !BDK->(MsSeek(xFilial("BDK") + TempBDQ->(BDQ_CODINT + BDQ_CODEMP + BDQ_MATRIC + BDQ_TIPREG + BDQ_CODFAI)))
				
				While BDQ->(MsSeek(xFilial("BDQ") + TempBDQ->(BDQ_CODINT + BDQ_CODEMP + BDQ_MATRIC + BDQ_TIPREG + BDQ_CODFAI)))
					BDQ->( RecLock("BDQ", .F.) )
					BDQ->( dbDelete() )
					BDQ->( MsUnlock() )

					BDQ->(DbSkip())
				EndDo
			EndIf

			TempBDQ->(DbSkip())
		EndDo

		TempBDQ->(DbCloseArea())
	Next 
Return

/*/{Protheus.doc} PlVldVcom
validaГЦo dos campos do envio contrato (VCOM) - Umimeds
@type method
@version 12.1.2510
@author giovanna.charlo
@return logical, sucesso ao validar campos
@since 16/04/2025
/*/
function PlVldVcom(cAlias)
	local lOK := .t. as logical 
	local cSendType := M->(&(cAlias + "_OPEVC")) as character
	local cFile := M->(&(cAlias + "_ACDVC")) as character
	local cNumber := M->(&(cAlias + "_NPROVC")) as character

	default cAlias := ""

	if M->(&(cAlias + "_ENVVC")) == "1" 
		do case 
			case empty(cSendType)
				fwAlertWarning("OperaГЦo do envio nЦo definida! Campo OpГЦo Envio ('" + cAlias + "'_OPEVC)", "Envio Contrato (VCOM)")
				lOK := .f.
			case cSendType == "1" .and. empty(cNumber) 
				fwAlertWarning("NЦo И possМvel realizar o encerramento, pois nЦo foi realizado o subcontato ainda nЦo foi cadastrado!", "Envio Contrato (VCOM)")
				lOK := .f.
			case empty(cFile)
				fwAlertWarning("Indicador de envio de arquivo nЦo definida! Campo Ind Acordo ('" + cAlias + "'_ACDVC)", "Envio Contrato (VCOM)")
				lOK := .f.
		endcase
	endif
return lOK
