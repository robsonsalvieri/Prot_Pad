Function PLS260EQP(cAlias)
	LOCAL lRet := .T.
	Local cEquipe := &("M->" + cAlias + "_EQUIPE")
	Local cCodVen := &("M->" + cAlias + "_CODVEN")
	Local cCodVe2 := &("M->" + cAlias + "_CODVE2")

