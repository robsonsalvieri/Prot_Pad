					lBloRN412 := .T.
					Exit
				EndIf
				BCA->(DbSkip())
			EndDo
		EndIf

		BC3->(DbSetOrder(1))//BC3_FILIAL+BC3_MATRIC+BC3_DATA+BC3_TIPO
