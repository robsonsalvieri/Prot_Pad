/*/{Protheus.doc} PLVLBLOPC
FunГЦo utilizada para controlar o tipo de bloqueio digitado no campo BF4_MOTBLO

@since		06/2016
@version	P12
/*/
//------------------------------------------------------------------------------------
Function PLVLBLOPC()

	LOCAL cTipoBloq := Posicione("BG3",1,xFilial("BR8") + M->BF4_MOTBLO,"BG3_TIPBLO")
	LOCAL lRet		:= .T.

	If !EMPTY(BF4->BF4_DATBLO) .AND. cTipoBloq == "0" //Bloqueado

		M->BF4_MOTBLO := BF4->BF4_MOTBLO
		M->BF4_DATBLO := BF4->BF4_DATBLO
		lRet := .F.

		MsgAlert(STR0268, STR0242) //"Este opcional jА estА bloqueado."##"AtenГЦo"

	ElseIf EMPTY(BF4->BF4_DATBLO) .AND. cTipoBloq == "1" //Desbloqueado

		MsgAlert(STR0269, STR0242) //"Este opcional jА estА desbloqueado.""##"AtenГЦo"

		M->BF4_MOTBLO := BF4->BF4_MOTBLO
		M->BF4_DATBLO := BF4->BF4_DATBLO
		lRet := .F.
	EndIf

Return lRet

