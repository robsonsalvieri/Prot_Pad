Function PLSA260VID
	Local I__f := 0

	LOCAL oDlg
	LOCAL nOpca     := 0
	LOCAL oEnc
	LOCAL aRet
	LOCAL bOK       := { || nOpca := 1,If( PLS955Obg() .And. Obrigatorio(oEnc:aGets,oEnc:aTela),oDlg:End(),nOpca:=2),If(nOpca==1,oDlg:End(),.F.) }
	LOCAL bCancel   := { || oDlg:End() }
	LOCAL cCadastro := OemtoAnsi(STR0120) //"AlteraГЦo Vida"
	LOCAL aOldTela  := aTela
	LOCAL aOldGets  := aGets
	LOCAL nOrdBA1   := BA1->(IndexOrd())
	LOCAL nRecBA1   := BA1->(Recno())
	LOCAL aButVid	:= {}

	PRIVATE aTela   := {}
	PRIVATE aGets   :={}

	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Define botoes para a enchoicebar do cadastro de usuarios...         Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	If ExistBlock("A260VDBT")
		If ValType( aButVid := ExecBlock("A260VDBT", .F., .F. ) ) <> "A"
			aButVid := {}
		EndIf
	EndIf

	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Define dialogo...                                                   Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	DEFINE MSDIALOG oDlg TITLE cCadastro FROM 0,0 TO (aPosObj[1,3]-50),(aPosObj[1,4]-450) OF GetWndDefault()
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Enchoice...                                                         Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	RegToMemory( "BTS", .F., .F. )

	oEnc := BTS->(MsMGet():New("BTS",BTS->(Recno()),K_Alterar,,,,,{015,004,450,600},,,,,,oDlg,,,.F.))
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Ativa o dialogo...                                                  Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	ACTIVATE MSDIALOG oDlg ON INIT EnchoiceBar(oDlg,bOK,bCancel,.F.,aButVid)
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Define tratamento de acordo com a opcao...                          Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	If nOpca == K_OK
		//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Inclui movimento...                                                 Ё
		//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		BTS->(PLUPTENC("BTS",K_Alterar))

		BA1->(DbSetOrder(7))
		IF BA1->(MsSeek(xFilial("BA1")+BTS->BTS_MATVID))
			While ! BA1->(Eof()) .And. BA1->(BA1_FILIAL+BA1_MATVID) == xFilial("BA1")+BTS->BTS_MATVID
				//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
				//Ё Atualiza os dados alterados...                                      Ё
				//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
				PLSESPVid(BA1->(Recno()),BTS->(Recno()),.T.)
				//здддддддддддддддддддддддддддддддддддддддддддд8©
				//ЁFunГЦo para espelhar as Tabelas PLS com RDC Ё
				//юдддддддддддддддддддддддддддддддддддддддддддд8ы
				Plsa260Esp(BTS->BTS_MATVID)
				//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
				//Ё Proximo registro...                                                 Ё
				//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
				BA1->(DbSkip())
			Enddo
		Endif
	Endif
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Restaura aGets e aTela e Arquivo de Usuarios...                     Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	aTela := aOldTela
	aGets := aOldGets

	BA1->(DbSetOrder(nOrdBA1))
	BA1->(DbGoTo(nRecBA1))
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Fim da Rotina...                                                    Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
Return(.F.)


