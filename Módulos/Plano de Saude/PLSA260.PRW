/*/{Protheus.doc} PLSULTTIP
FunГЦo para verificar tipo da Зltima operaГЦo de bloqueio/desbloqueio
@type  Function
@author Gabriel Lakatos
@since 04/07/2025
@version 12.1.2410
/*/
function PLSULTTIP(cChave, cTipReg, lFamilia)

	Local cQuery 	:= ""
	Local cRet		:= ""

	cQuery := " SELECT BCA_TIPO "
    cQuery += " FROM " + retSQLName("BCA") + " BCA "
    cQuery += " WHERE BCA_FILIAL = '" + xFilial("BCA") + "' "
    cQuery += "     AND BCA.D_E_L_E_T_  = ' ' "
    cQuery += "     AND BCA_MATRIC = '" + cChave + "' "
    cQuery += "     AND BCA_TIPREG = '" + cTipReg + "' "
    cQuery += " AND BCA.R_E_C_N_O_ = ( "
    cQuery += " SELECT MAX(BCA2.R_E_C_N_O_) "
    cQuery += " FROM " + retSQLName("BCA") + " BCA2 "
    cQuery += " WHERE BCA2.BCA_FILIAL = '" + xFilial("BCA") + "' "
    cQuery += " AND BCA2.D_E_L_E_T_  = ' ' "
    cQuery += " AND BCA_MATRIC = '" + cChave + "' "
    cQuery += " AND BCA_TIPREG = '" + cTipReg + "' ) "

	cQuery := changeQuery(cQuery)

	TCQUERY cQuery New Alias "TRBLAST"

	while !TRBLAST->(eof())

		cRet := TRBLAST->BCA_TIPO

		TRBLAST->(dbSkip())
	end

	// Caso nЦo tenha BCA, entende-se que nЦo hА histСrico portanto a prСxima operaГЦo deverА ser de bloqueio
	cRet := iif(empty(cRet), "1", cRet)

	if select("TRBLAST") > 0
		TRBLAST->(dbCloseArea())
	endif

return cRet

