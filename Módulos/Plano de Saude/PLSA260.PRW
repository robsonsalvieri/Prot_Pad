		cFiltro    += " AND BA1_CODEMP = '"+BG9->BG9_CODIGO+"' AND D_E_L_E_T_ = ' '"
	Endif

	DbSelectArea("BA1")
	SET FILTER TO &cFiltro

	PLSA260FAM()

	DbSelectArea("BA1")
	SET FILTER TO

	cCadastro := cOldCad

Return
