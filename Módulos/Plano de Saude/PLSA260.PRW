STATIC Function A260HIST(cTipoHis)

	//зддддддддддддддддддддддддддддддддддддддддддддддддд©
	//ЁDefine variaveis para composicao da Matricula    Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддды
	Local cCODINT 		:= M->BA3_CODINT
	Local cCODEMP		:= M->BA3_CODEMP
	Local cMATRIC       := M->BA3_MATRIC
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//ЁFoi usado o FieldGet pq os outros campos da chave nao se encontram na MATRIZ aHeader               |
	//ЁPega o conteudo do campo informado na linha corrente 											  |
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	Local cTIPREG       := oBrwUsr:FieldGet('BA1_TIPREG',oBrwUsr:Linha())
	Local cDIGITO       := oBrwUsr:FieldGet('BA1_DIGITO',oBrwUsr:Linha())
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//ЁEsta variavel e para verificar qual tipo de historicoЁ
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддды
	DEFAULT cTipoHis := 1

	//зддддддддддддддддддддддддддддддддддддддддддддддддд©
	//ЁSeta a ordem de procura	                        Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддды
	BA1->(DbSetOrder(2)) // BA1_CODINT+BA1_CODEMP+BA1_MATRIC+BA1_TIPREG+BA1_DIGITO
	if BA1->(DbSeek(xFilial('BA1')+cCODINT+cCODEMP+cMATRIC+cTIPREG+cDIGITO))

		//-------------------------------------------------------------------
		//  LGPD
		//-------------------------------------------------------------------
		objCENFUNLGP:useLogUser()

		If cTipoHis == 1
			//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
			//ЁChama o Historico de Transferencia										   Ё
			//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
			PL169HIST()
		Else
			//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
			//ЁChama o Historico de Desbloqueio											  Ё
			//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
			PL310HDE(xFilial('BA1')+cCODINT+cCODEMP+cMATRIC+cTIPREG)
		EndIf

	Else
		MsgInfo(OemtoAnsi(STR0155)) //'UsuАrio nЦo ENCONTRADO')
	EndIf

	//зддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Fim da funcao                                   Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддды
Return



