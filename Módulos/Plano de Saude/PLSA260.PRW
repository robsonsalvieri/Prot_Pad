			If !oGetBVC:aCols[nX][nUsado+1]
				If nX < n
					If M->BVC_VIGINI == CTOD(" / / ") .And. M->BVC_VIGFIN == CTOD(" / / ")
						If M->BVC_QTD == oGetBVC:aCols[nX][nQTD] .And. Empty(oGetBVC:aCols[nX][nPosIni]) .And. Empty(oGetBVC:aCols[nX][nPosFin])
