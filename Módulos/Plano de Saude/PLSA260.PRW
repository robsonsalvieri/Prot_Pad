Function PlsGrpFam(lDesb,lblqAut,cFam)
	LOCAL nCont

	If lBlqConf .AND. !EMPTY(cFam)

		If BG1->(FieldPos("BG1_BQGPFM")) > 0

			dbSelectArea("BG1")
			dbSetOrder(1)
			If dbSeek(xFilial("BG1")+cMtBlo)

				//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
				//Ё Verifica quais familias fazem parte do grupo familiar              Ё
				//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
				If BA3->(FieldPos("BA3_GRPFAM")) > 0
					cSQL := "SELECT BA3_CODINT,BA3_CODEMP, BA3_MATRIC, BA3_GRPFAM, BA3_BLOFAT,BA3_MOTBLO, R_E_C_N_O_ FROM " + RetSQLName("BA3")
					cSQL  += "  WHERE SUBSTRING(BA3_GRPFAM,1,14) = '" + cFam + "' AND D_E_L_E_T_ = ''"
					
					dbUseArea(.T.,"TOPCONN",TCGENQRY(,,changeQuery(cSQL)),"TrbDesb",.F.,.T.)

					Count To nQuant

					If nQuant > 0

						TrbDesb->(dbGoTop())

						dbSelectArea("BA1")
						dbSetOrder(1)

						//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
						//Verifica se o tipo do motivo И bloqueio e se este motivo de bloqueio permite bloquear
						//o grupo familiar
						//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
						If BG1->BG1_BQGPFM == "1" .AND. BG1->BG1_TIPBLO == "0"

							While !TrbDesb->(EOF())

								If dbSeek(xFilial("BA1")+TrbDesb->(BA3_CODINT+BA3_CODEMP+BA3_MATRIC))

									If EMPTY(TrbDesb->BA3_MOTBLO)
										//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
										//Ё faz o bloqueio do grupo familiar
										//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
										PL260BLOCO("BA3",TrbDesb->R_E_C_N_O_,4,.T.,cMtBlo,dBlqDat,"1",2,,,lblqAut,.F.)
									EndIf

									TrbDesb->(dbSkip())
								EndIf
							EndDo

							//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
							//Ё Verifica se o tipo do motivo И desbloqueio
							//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
						ElseIf  BG1->BG1_TIPBLO == "1"

							//здддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
							//Ё No caso de desbloqueio da familia principal, Verifica se o grupo familiar tambem esta
							//  bloqueado para efetuar o desbloqueio.
							//юдддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
							While !TrbDesb->(EOF())

								If dbSeek(xFilial("BA1")+TrbDesb->(BA3_CODINT+BA3_CODEMP+BA3_MATRIC))
									If !EMPTY(TrbDesb->BA3_BLOFAT)
										//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
										//Ё faz o desbloqueio do grupo familiar
										//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
										PL260BLOCO("BA3",TrbDesb->R_E_C_N_O_,4,.T.,cMtBlo,dBlqDat,"1",2,,,lblqAut, .F.)
									EndIf
									TrbDesb->(dbSkip())
								EndIf
							EndDo

						EndIf
					EndIf

					TrbDesb->(dbCloseArea())
				EndIf
			EndIf
		EndIf
	EndIf

Return

