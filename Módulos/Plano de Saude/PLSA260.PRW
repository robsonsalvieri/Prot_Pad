/*/
Function PLSA260BFB()

	LOCAL  lRet     := .F.
	LOCAL  cTipReg  := NIL
	STATIC __nLinha := 0

	//Vamos zerar a variavel pois ao entrar em um Beneficiario com o opcional desbloqueado a variavel ficava na memoria
	//e depois ao entrar em um opcional bloqueado, o F3 da lista de desbloqueio nao atualizava com as opcoes corretas.
	__lRetBlq := nil

	//inclusЦo de nova linha retorna os motivos de bloqueio
	If oBrwOUsu:nOpcx == 3 .OR. oBrwOUsu:nOpcx == 4 .AND. EMPTY(oBrwOUsu:aCols[oBrwOUsu:OBROWSE:nAt][oBrwOUsu:PLRETPOS("BF4_TIPBLO")])
		If BG3->BG3_TIPBLO == "0"
			lRet := .T.
		Endif
	Else

		//apСs a inclusЦo da nova linha no grid,o nOpcx fica com o mesmo valor de linhas que foram gravadas.
		//quando clicar na consulta padrЦo serА gravado a posiГЦo da linha que estА sendo chamada a consulta
		//padrЦo, se fechar a tela do opcional e clicar na mesma linha e chamar novamente a consulta padrЦo,
		//o __nLinha jА estarА preenchido e nЦo precisarА fazer novamente o seek na tabela.

		If __nLinha != oBrwOUsu:OBROWSE:nAt
			__nLinha  := oBrwOUsu:OBROWSE:nAt
			__lRetBlq := NIL
		EndIf

		//se a vАriavel __lRetBlq for nula, o sistema verica se as informaГУes da linha que esta chamando a
		//consulta padrЦo jА foi gravada na base, desta forma ira controlar o retorno da consulta padrЦo para
		//trazer o motivos de desbloqueio caso exista na base.

		If __lRetBlq == NIL

			cTipReg := oBrwUsr:aCols[oBrwUsr:OBROWSE:nAt][oBrwUsr:PLRETPOS("BA1_TIPREG")]

			BF4->(dbSetOrder(1))
			If BF4->(MsSeek(xFilial("BF4") + M->BA3_CODINT + M->BA3_CODEMP + M->BA3_MATRIC + cTipReg + M->BF4_CODPRO))

				If EMPTY(BF4->BF4_DATBLO)

					__lRetBlq := .T.
				Else

					__lRetBlq := .F.
				EndIf
			Else

				__lRetBlq := .T.
			EndIf
		EndIf

		If BG3->BG3_TIPBLO == "0" .AND. __lRetBlq
			lRet := .T.

		ElseIf BG3->BG3_TIPBLO == "1" .AND. !__lRetBlq
			lRet := .T.
		Endif
	Endif

Return(lRet)

