			BA3->(RecLock("BA3",.F.))
			BA3->BA3_CODPLA := BI1->BI1_PRONOV
			BA3->BA3_VERSAO := BI1->BI1_VERNOV
			BA3->BA3_DATBAS := BI1->BI1_DATA
			BA3->BA3_NUMCON := BI1->BI1_CONNOV
			BA3->(MsUnLock())
			//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
