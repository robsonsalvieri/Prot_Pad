Function PL260Forma()

	Local cAno			:= Str(Year(M->BA3_DATBAS), 4)
	Local cMes 			:= StrZero(Month(M->BA3_DATBAS), 2)
	Local nIncAutom 	:= GetNewPar("MV_PLCAROP", 1)
	Local lCobertFam	:= GetNewPar("MV_PLCOBFA",.F.) // Cobertura da Familia

	Private cCodFor 	:= alltrim(M->BA3_FORPAG)

	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Atualiza os gatilhos do BA3_CODPLA																  Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	RunTrigger(1,,,,"BA3_CODPLA")

	// Carrega a Cobertura da Familia
	If 	!Empty(M->BA3_CODPLA) .And. !Empty(M->BA3_VERSAO) .And. lCobertFam .And. nOpcx == K_Incluir
		PL260Cobert("1")
	EndIf

	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Consiste dados nescessarios...                                    Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	If 	Empty(M->BA3_CODPLA) .Or. Empty(M->BA3_VERSAO) .Or. Empty(cCodFor)
		Return .T.
	Endif

	if cCodFor == "108"
		//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Carrega por faixa salarial...                                                                     Ё
		//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		PL260FSal(cAno, cMes)
	else
		//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
		//Ё Carrega por faixa etaria...                                                                       Ё
		//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
		PL260FEta(cAno, cMes)
	endif

	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Carrega taxa de adesao...                                                                         Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	PL260Adesao()

	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Chamada da rotina que atualiza os opcionais do produto/subcontratos no ato da inclusao da familia Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	If nIncAutom > 0
		PL260Opc(.T.,nIncAutom == 2)
	Endif

