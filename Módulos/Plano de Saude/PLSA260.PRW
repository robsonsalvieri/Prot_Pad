		aDadBFG := PL260LICOL(oGetBFG:aCols, "BFG")
		aDadBW9 := PL260LICOL(oGetBW9:aCols, "BW9")
		aDadBWA := PL260LICOL(oGetBWA:aCols, "BWA")
