Function PL260CKSeq(xaCols, aCamp, aValores, nPos)

	Local lRet := .F.
	Local nX

	For nX := nPos+1 To Len(xaCols)

		If CompCols(xaCols[nX],aCamp,aValores) .And. Empty(xaCols[nX][AScan(oBrwOUsu:aHeader,{|x| x[2] == "BF4_DATBLO"})])
			lRet := .T.
		EndIf
	Next

Return lRet

