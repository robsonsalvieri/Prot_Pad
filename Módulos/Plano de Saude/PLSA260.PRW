*/
Function PL260FORREFR(nFolder, nPos, aOpts)
	LOCAL bForceRefresh
	DEFAULT nPos 	:= 6
	DEFAULT aOpts	:= {}

	If Len(aOpts) <> 0
		aOpcoes := aClone(aOpts)
	Endif
	bForceRefresh := aOpcoes[nFolder][nPos]

	If Valtype(&bForceRefresh) == 'B'
		Eval(&bForceRefresh)
	Endif

Return(NIL)


