Function PLSA260DAT(cCodInt,cCodEmp,dData,cNumCon)
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Define variaveis...                                                 Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	LOCAL lRet   := .F.
	LOCAL cChave := cCodInt+cCodEmp

	cNumCon := AllTrim(cNumCon)
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Verificar se a data base e maior que a data do servidor...          Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
	BG9->(DbSetOrder(1))
	If BG9->(MsSeek(xFilial("BG9")+cChave))

		If BG9->BG9_TIPO == "2"
			BT5->(DbSetOrder(1))
			If BT5->(MsSeek(xFilial("BT5")+cChave+cNumCon))
				BQB->(DBSetOrder(1))
				//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
				//Ё Pesquisa versao do contrato                                         Ё
				//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
				If BQB->(MsSeek(xFilial("BQB")+cChave+cNumCon))
					//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
					//Ё Verifica se a data do contrato da Familia esta no intervalo de datas da versao  Ё
					//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
					If dtos(dData) < dtos(BQB->BQB_DATINI)
						Help("",1,"PLSA260DAT")
					Else
						If Empty(dtos(BQB->BQB_DATFIN))
							lRet := .T.
						Else
							If dtos(dData) > dtos(BQB->BQB_DATFIN)
								Help("",1,"PLSA260D01")
							Else
								lRet := .T.
							Endif
						Endif
					Endif
				Endif
			Else
				Help("",1,"PLSA260BT5")
			Endif
		Else
			lRet := .T.
		Endif
	Else
		Help("",1,"RECNO")
	Endif

Return(lRet)

