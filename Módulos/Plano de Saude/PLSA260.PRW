	DEFAULT dDataPed := STOD("")

	PRIVATE oEncBlo
	PRIVATE oDlgBlo
	PRIVATE nOpcx   := nOpc
	PRIVATE cTipo
	PRIVATE aGets[0]
	PRIVATE aTela[0][0]
	PRIVATE cMatric := BA1->(BA1_CODINT+BA1_CODEMP+BA1_MATRIC)
