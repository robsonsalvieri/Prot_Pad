/*/
Function PLS260GRA(dDatLan,cAnoIni,cAnoFim,cMesIni,cMesFim)

	LOCAL cMes := StrZero(Month(dDatLan),2)
	LOCAL cAno := StrZero(Year(dDatLan),4)
	LOCAL lRet := .T.

	If cAnoIni < cAno
		MsgStop(OemtoAnsi(STR0230))//"Ano inicial nЦo pode ser inferior ao ano da data de lanГamento."
		lRet := .F.

	Elseif cAnoIni >= cAno .And. cAnoIni <= cAnoFim
		If cAno == cAnoIni .And. cMes > cMesIni
			MsgStop(OemtoAnsi(STR0231))//"MЙs e ano inicial nЦoo podem ser inferior ao mЙs e ano da data de lanГamento."
			lRet := .F.
		Endif
		If cAnoIni == cAnoFim .And. cMesIni > cMesFim
			MsgStop(OemtoAnsi(STR0232))//"O intervalo entre as datas И invalido! O mЙs e ano final nЦo podem ser inferior ao mЙs e ano inicial."
			lRet := .F.
		Endif
	Else
		MsgStop(OemtoAnsi(STR0233))//"Ano final nЦo pode ser inferior ao ano inicial."
		lRet := .F.
	Endif

Return(lRet)

