/*/{Protheus.doc} ClearBDQ
Limpa todas as faixas de desconto do plano x beneficiario
@type function
@version v1
@author Morgon
@since 6/5/2024
/*/
//--------------------------------------------------------------------
Static Function ClearBDQ(cChave)

	Default cChave	:= ""

	BDQ->(DbSetOrder(1)) //BDQ_FILIAL+BDQ_CODINT+BDQ_CODEMP+BDQ_MATRIC+BDQ_TIPREG+BDQ_CODFAI
	If !empty(cChave) .and. BDQ->(MsSeek(cChave))
		While !BDQ->(Eof()) .and. BDQ->(BDQ_FILIAL+BDQ_CODINT+BDQ_CODEMP+BDQ_MATRIC+BDQ_TIPREG) == cChave
			BDQ->(Reclock("BDQ", .F.))
				BDQ->(dbDelete())
			BDQ->(MsUnlock())

			BDQ->(dbSkip())
		Enddo
	Endif
Return

