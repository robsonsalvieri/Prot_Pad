Function PLSA920IdI(cCodSeq)
Local aArea := sGetArea()
Local nIdadeIni := 0
sGetArea(aArea,"BL9")
BL9->(DbSetOrder(1))
BL9->(MsSeek(xFilial()+cCodSeq,.T.))
BL9->(DbSkip(-1))
