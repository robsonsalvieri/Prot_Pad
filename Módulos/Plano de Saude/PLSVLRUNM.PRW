/*/
Function PLSUNIMED(cCodDoc,aParam)

LOCAL aRetExp := {}
LOCAL cMsg    := ""
LOCAL cCodCri := ""

BD3->(DBSetOrder(1))
If BD3->(DBSeek(xFilial("BD3")+cCodDoc))
   //Ŀ
   // Calculo o valor do porte baseado na unidade de medida  
   //
   aRetExp  := ExecBlock(BD3->BD3_RDMAKE,.F.,.F.,aParam)
   //Ŀ
   // Se nao ocorreu nenhum erro no calculo, verifico a porcentagem do valor  
   //
Endif

If Len(aRetExp) == 0
   PLSPOSGLO(PLSINTPAD(),__aCdCri043[1])
   cMsg    := PLSBCTDESC()
   cCodCri := __aCdCri043[1]
   aRetExp := {.F.,cMsg,{0,"",0},cCodCri}
Endif

Return(aRetExp)
