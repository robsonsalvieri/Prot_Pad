Local cCodPro  := ""
Local nPosChar := 0

if BRW->(FieldPos("BRW_TABDUT")) > 0 .And. BRW->(FieldPos("BRW_CODDUT")) > 0
	cAux     := Substr(aMatDad[oBrw:nAt,5],4,len(aMatDad[oBrw:nAt,5]))
