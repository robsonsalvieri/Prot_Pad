//---------------------------------------------------------------------------------
Function PLANEXRADI(cNumGui)
Local aValRet := {}
Local aDados := {}
Local aCabec := {}
Local nI := 0
LOCAL oDlg              
LOCAL nOpca      := 0
LOCAL bCancel    := { || nOpca := 0, oDlg:End() }  
LOCAL lVisGui    := B4A->(FieldPos('B4A_VISGUI')) > 0 
