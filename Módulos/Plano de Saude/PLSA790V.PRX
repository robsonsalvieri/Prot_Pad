cFilter2 := "B2I_FILIAL == '" + xFilial("B2I") + "' .AND. B2I_CODPRO == '" + cCodPro + "' .AND. B2I_CODPAD == '" + cCodTab + "' "
IIF (!(Empty(MV_PAR01)), cFilter2 += " .AND. DTOS (B2I_DTACAD) >= '" + DTOS(MV_PAR01) + "' ", "")
IIF (!(Empty(MV_PAR02)), cFilter2 += " .AND. DTOS (B2I_DTACAD) <= '" + DTOS(MV_PAR02) + "' ", "")

