	nQuant += 1
	B2I->(DbSkip())
EndDo
nValor := ( nValor / nQuant )
oModelVL:LoadValue( 'MD_VALOR', nValor )

	//Maior valor encontrado e guia
B2I->( DbGoBottom() )
oModelVL:LoadValue( 'GUI_MA_VALOR', B2I->B2I_NUMGUI )
oModelVL:LoadValue( 'MA_VALOR', B2I->B2I_VLNEGO )

