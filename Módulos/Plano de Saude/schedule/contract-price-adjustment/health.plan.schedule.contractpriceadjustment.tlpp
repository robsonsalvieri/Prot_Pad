#Include "tlpp-core.th"

namespace totvs.protheus.health.plan.schedule

/*/{Protheus.doc} contractpriceadjustment
Processamento do schedule para reajuste de preco de contrato do plano de saude  
@version 12.1.2510
@author diogo.sousa
@since 09/05/2025
/*/
Function contractpriceadjustment()

	Local jParameters  := JsonObject():new() As Json
	Local job          := .T. As Logical	

	jParameters["operatorCode"] := MV_PAR01
	jParameters["yearMonth"] := MV_PAR02
	jParameters["companyCodeFrom"] := AllTrim(MV_PAR03)
	jParameters["companyCodeTo"] := Alltrim(MV_PAR04)
	jParameters["contractType"] := cValToChar(MV_PAR05)
	jParameters["observation1"] := MV_PAR06
	jParameters["observation2"] := MV_PAR07
	jParameters["observation3"] := MV_PAR08
	jParameters["display"] := .F. //.T. Display(Tela) - .F. Background(Segundo plano)
	jParameters["yearMonthReadjustment"] := MV_PAR09
	jParameters["automatic"] := .T.
	jParameters["acceptsCriticism"] := MV_PAR10 == 1

	PLSA101(jParameters, job)	

	FreeObj(jParameters)
	
Return
