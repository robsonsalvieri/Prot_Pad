__cIdCtb := cIdContab

If nPosArotina > 0 .and. !Empty(aRotAuto)// Sera executada uma opcao diretamento de aRotina, sem passar pela mBrowse
	_lRotAuto := .T.
	_aRotAuto := aRotAuto
EndIf

if (!_lRotAuto)
	if (cRelease > "12.1.033" .And. cPaisLoc == 'BRA') .Or. (lPiloto .And. cPaisLoc != "BRA")
		FINA716()
		return
	endIf
endIf

If _lRotAuto
	dbSelectArea("SEH")
	posSEH(aRotAuto)
	bBlock := &( "{ |a,b,c,d,e| " + aRotina[ nPosArotina,2 ] + "(a,b,c,d,e) }" )
	Eval( bBlock, Alias(), (Alias())->(Recno()), nPosArotina)
	Return
EndIf
	mBrowse( 6, 1,22,75,"SEH",,,,,,FA183Legend())
Return(.T.)

