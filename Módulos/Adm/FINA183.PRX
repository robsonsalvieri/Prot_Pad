					Else
						SEI->EI_LA :=  "S"
					EndIf

					//Contabiliza pela variavel VALOR. Nao necessita de controle de flag.
					nTotal += DetProva( nHdlPrv,;
										cPadrao,;
										"FINA183" /*cPrograma*/,;
										cLote,;
										/*nLinha*/,;
										/*lExecuta*/,;
										/*cCriterio*/,;
										/*lRateio*/,;
										/*cChaveBusca*/,;
										/*aCT5*/,;
										/*lPosiciona*/,;
										@aFlagCTB,;
										/*aTabRecOri*/,;
										/*aDadosProva*/ )
				EndIf
			EndIf

			// atualiza a SEH para contabilizaГЦo do rendimento
			SEH->EH_VALIRF	:= 0
			SEH->EH_VALIOF	:= 0
			SEH->EH_VALJUR	:= nRendimento
			//здддддддддддддддддддддддддддддддддддддддддддддддддддд©
			//Ё Grava o Valor do Rendimento                        Ё
			//юдддддддддддддддддддддддддддддддддддддддддддддддддддды
			If nRendimento != 0
				Reclock("SEI",.T.)
				SEI->EI_FILIAL		:= xFilial("SEI")
				SEI->EI_NUMERO		:= SEH->EH_NUMERO
				SEI->EI_REVISAO		:= SEH->EH_REVISAO
				SEI->EI_TIPODOC		:= "I7" // Valor do rendimento na Retencao do IR
				SEI->EI_DATA		:= dDataBase
				SEI->EI_TIPO		:= SEH->EH_TIPO
				SEI->EI_APLEMP		:= SEH->EH_APLEMP
				SEI->EI_BANCO		:= cA183Bco
				SEI->EI_AGENCIA		:= cA183AGe
				SEI->EI_CONTA		:= cA183Cta
				SEI->EI_VALOR		:= nRendimento // Valor do rendimento
				SEI->EI_NATUREZ		:= cA183Nat
				SEI->EI_DTDIGIT		:= dDataBase
				SEI->EI_SEQ			:= cSequencia
				SEI->EI_VLMOED2		:= 0
				SEI->EI_HISTOR		:= cA183Histor
				SEI->EI_MOTBX		:= "APR"
				SEI->EI_TAXA		:= SEH->EH_TAXA
				SEI->EI_TXMOEDA		:= nA183VlMoed
				SEI->EI_IRACUM 		:= nIrAcum
				SEI->EI_VLRQTS      := nVlCotaAtu
				SEI->EI_VLRQTA      := nVlCotaAnt
				SEI->EI_QTDCOTA   	:= n183QtdCota
				SEI->EI_ULTAPR  	:= dDataBase
				cLctPco    := PcoItLanApr(SEI->EI_APLEMP, SEI->EI_TIPODOC, SEI->EI_MOTBX, .F.)
				If !Empty(cLctPco)
					//зддддддддддддддддддддддддддддддддддддддддддддддддддддд©
					//Ё Gera os lancamentos do SIGAPCO                      Ё
					//юддддддддддддддддддддддддддддддддддддддддддддддддддддды
					PcoDetLan("000011", cLctPco, "FINA183")
				EndIf
				If lPadrao
					If lUsaFlag  // Armazena em aFlagCTB para atualizar no modulo Contabil
						aAdd( aFlagCTB, { "EI_LA", "S", "SEI", SEI->( RecNo() ), 0, 0, 0} )
					Else
						SEI->EI_LA :=  "S"
					EndIf
				EndIf
			EndIf

			// Lancamento Contabil
			If lPadrao

				nTotal += DetProva( nHdlPrv,;
				                    cPadrao,;
				                    "FINA183" /*cPrograma*/,;
				                    cLote,;
				                    /*nLinha*/,;
				                    /*lExecuta*/,;
				                    /*cCriterio*/,;
				                    /*lRateio*/,;
				                    /*cChaveBusca*/,;
				                    /*aCT5*/,;
				                    /*lPosiciona*/,;
				                    @aFlagCTB,;
				                    /*aTabRecOri*/,;
				                    /*aDadosProva*/ )

				RodaProva(  nHdlPrv,;
							nTotal )

				cA100Incl( 	cArquivo,;
				           	nHdlPrv,;
				           	3 /*nOpcx*/,;
				           	cLote,;
				           	.T.,;
				           	.F.,;
				           	/*cOnLine*/,;
				           	/*dData*/,;
				           	/*dReproc*/,;
				           	@aFlagCTB,;
				           	/*aDadosProva*/,;
							/*aSeqDiario*/,;
							/*aTpSaldo*/,;
							/*lSimula*/,;
							/*cTabCTK*/,;
							/*cTabCT2*/,;
							__cIdCtb )

				aFlagCTB := {}  // Limpa o coteudo apos a efetivacao do lancamento
			EndIf

			SEH->EH_VALIOF	:= na183VlIof
			SEH->EH_VALIRF	:= na183VlIrf
			SEH->EH_VALJUR	:= nRendimento
			//здддддддддддддддддддддддддддддддддддддддддддддддддддд©
			//Ё Atualizacao da qtde de quotas para retencao do IR  Ё
			//юдддддддддддддддддддддддддддддддддддддддддддддддддддды
			DbSelectArea("SE9")
			SE9->(DbSetOrder(1))
			If DbSeek(xFilial()+SEH->EH_CONTRAT+SEH->EH_BCOCONT+SEH->EH_AGECONT)
				RecLock("SE9",.F.)
				SE9->E9_COTAANT := nVlCotaAnt
				SE9->E9_VLRCOTA := nVlCotaAtu
			EndIf

			DbSelectArea("SE0")
			SE0->(DbSetOrder(1))
			// Atualiza arquivo de cotacoes
			If !MsSeek(xFilial("SE0")+SE9->(E9_BANCO+E9_AGENCIA+E9_CONTA+E9_NUMERO)+Str(21001231-Val(DTOS(dDataBase)),11)+Space(3))
		  		RecLock("SE0",.T.)
	  			SE0->E0_FILIAL		:= xFilial("SE0")
	  			SE0->E0_BANCO		:= SE9->E9_BANCO
	  			SE0->E0_AGENCIA		:= SE9->E9_AGENCIA
	  			SE0->E0_CONTA		:= SE9->E9_CONTA
		  		SE0->E0_CONTRAT		:= SE9->E9_NUMERO
	  			SE0->E0_SUBCTA		:= Space(3)
	  			SE0->E0_DATA		:= dDataBase
	  			SE0->E0_ORDEM		:= Str(21001231-Val(DTOS(dDataBase)),11)
				SE0->E0_VALOR		:= nVlCotaAtu
  				SE0->E0_ORIGEM		:= "P" // Apropriacao
	  		Else
				If SE0->E0_VALOR <> nVlCotaAtu
	  				RecLock("SE0",.F.)
					SE0->E0_VALOR		:= nVlCotaAtu
  					SE0->E0_ORIGEM		:= "P" // Apropriacao
				EndIf
			EndIf

  			DbSelectArea("SE9")

			//зддддддддддддддддддддддддддддддддддддддддддддддддддддддддддд©
			//Ё Final  do bloco protegido via TTS                         Ё
			//юддддддддддддддддддддддддддддддддддддддддддддддддддддддддддды
			End Transaction

			lContinua := .F.
		OtherWise
			lContinua := .F.
	EndCase
EndDo

dbSelectArea("SEH")
MsUnLock()

If lPcoConf
	//зддддддддддддддддддддддддддддддддддддддддддддддддддддд©
	//Ё Gera os lancamentos do SIGAPCO  (referente SEH)     Ё
	//юддддддддддддддддддддддддддддддддддддддддддддддддддддды
	PcoDetLan("000011","01","FINA183")
EndIf

//зддддддддддддддддддддддддддддддддддддддддддддддддддддд©
//Ё Finaliza a geraГao de lancamentos do SIGAPCO        Ё
//юддддддддддддддддддддддддддддддддддддддддддддддддддддды
PcoFinLan("000011")

Return(.T.)

