*/
Static Function F378Invert(cAlias,cMarca,lTodos,oMark)
Local cCampo	:= ""
Local nReg 		:= (cAlias)->(Recno())

DEFAULT lTodos  := .T.

If MV_PAR07 == 1
	cCampo := 'E2_OK'//Emissão
Else
	cCampo := 'E5_OK'//baixa
EndIf

DbSelectArea(cAlias)

If lTodos
	DbGoTop()
Endif

While !lTodos .Or. !Eof()
	If (cAlias)->&cCampo == cMarca
		RecLock(cAlias)
		Replace &cCampo With Space(02)
	Else
		RecLock(cAlias)
		Replace &cCampo With cMarca
		(cAlias)->(MsUnlock())
	Endif
	If lTodos
		(cAlias)->(dbSkip())
	Else
		Exit
	Endif
Enddo

DbGoTo(nReg)

Return(NIL)
      
