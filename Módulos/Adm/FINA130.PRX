/*/{Protheus.doc} fA130Delet
	Programa de exclusao de Parametros Bancarios - CNAB  
	@type function
	@since 23/06/1992
	@author Wagner Xavier
	@param cAlias, character, Alias do arquivo
	@param nReg, numeric, Numero do registro
	@param nOpc, numeric, Numero da opcao selecionada
/*/
Function fA130Delet(cAlias as Character, nReg as Numeric, nOpc as Numeric)
	
	Local aRetInteg As Array
	Local bCampo    as Codeblock
	Local ni        as Numeric
	Local nOpca     as Numeric

	// Monta a entrada de dados do arquivo
	Private aTELA[0][0],aGETS[0]

	Inclui := .F.
	Altera := .F.
	Exclui := .T.

	// Verifica se o arquivo est† realmente vazio ou se est· posicionado em outra filial.
	If EOF() .or. SEE->EE_FILIAL # xFilial("SEE")
		HELP(" " , 1 , "ARQVAZIO")
		Return Nil
	Endif

	While .T.
		nOpca := 0
		dbSelectArea( cAlias )
		bCampo := {|nCPO| Field(nCPO) }
		FOR ni := 1 TO FCount()
			M->&(EVAL(bCampo,ni)) := FieldGet(ni)
		NEXT ni

		If !lF130Auto
			nOpca := AxVisual(cAlias,nReg,2,,,,,)
		Else
			nOpcA := 1
		EndIf

		If nOpcA == 1
			//Chamada da IntegDef para mensagem ˙nica
			If FWHasEAI('FINA130',.T.,,.T.)
				aRetInteg := FWIntegDef('FINA130')
				If ValType(aRetInteg) == "A" .AND. Len(aRetInteg) >= 2 .AND. !aRetInteg[1]
					Help( ,, "FINA130INTEGDEL",,AllTrim( aRetInteg[2] ), 1, 0,,,,,,)
					Exit
				EndIf
			EndIf
					
			Begin Transaction
				dbSelectArea( cAlias )
				RecLock(cAlias,.F.,.T.)
				dbDelete()
			End Transaction

		Else
			MsUnlock( )
		Endif
		Exit
	Enddo
	dbSelectArea( cAlias )
Return

