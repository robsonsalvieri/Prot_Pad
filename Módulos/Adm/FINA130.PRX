// FunÁ„o	| FA130ALT()
// Autor	| Pedro Pereira Lima
// Data		| 26/03/2014
// Uso		| Encapsulamento do AxAltera para mensagem ˙nica
//------------------------------------------------------------
Function FA130Alt(cAlias,nReg,nOpc)
	
	Local nOpca As Numeric
	Local aRetInteg As Array
	Local lDisarmTran As Logical

	Inclui := .F.
	Altera := .T.
	Exclui := .F.
	lDisarmTran := .F.

	Begin Transaction
		// Rotina Automatica
		If lF130Auto
			RegToMemory("SEE",.F.,.F.)
			If EnchAuto(cAlias,aAutoCab,'F130TudoOK(2)',nOpc)
				nOpcA := AxIncluiAuto(cAlias,,,4,SEE->(RecNo()))
			Endif
		Else
			nOpca := AxAltera(cAlias,nReg,nOpc,,,4,,'F130TudoOK(2)')
		EndIf

		If nOpca == 1
			//Chamada da IntegDef para mensagem ˙nica
			If FWHasEAI('MATA070',.T.,,.T.)
				aRetInteg := FWIntegDef('MATA070')
				If ValType(aRetInteg) == "A" .AND. Len(aRetInteg) >= 2 .AND. !aRetInteg[1]
					Help( ,, "FINA130INTEGALT",,AllTrim( aRetInteg[2] ), 1, 0,,,,,,)
					lDisarmTran := .T.
					RollBackDelTran()
				EndIf
			EndIf
		EndIf

	End Transaction
	
	If lDisarmTran
		Return .F.
	EndIf
Return

