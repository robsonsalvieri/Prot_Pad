// FunÁ„o	| FA130INC()
// Autor	| Pedro Pereira Lima
// Data		| 26/03/2014
// Uso		| Encapsulamento do AxInclui para mensagem ˙nica
//------------------------------------------------------------
Function FA130Inc(cAlias,nReg,nOpc)
	
	Local nOpca As Numeric
	Local aRetInteg As Array
	Local lDisarmTran As Logical

	Inclui := .T.
	Altera := .F.
	Exclui := .F.
	lDisarmTran := .F.
	nOpca  := 0

	Begin Transaction
		// Rotina Automatica
		If lF130Auto
			lMsErroAuto := .F.
			RegToMemory("SEE",.T.,.F.)
			If EnchAuto(cAlias,aAutoCab,,nOpc)
				nOpca := AxIncluiAuto(cAlias,'F130TudoOK(1)')
				If nOpca == 0
					lMsErroAuto := .T.
				EndIf
			Else
				lMsErroAuto := .T.
			Endif
		Else
			nOpca := AxInclui(cAlias,nReg,nOpc,,,,'F130TudoOK(1)')
		EndIf

		If nOpca == 1 //Confirmou inclusao
			FinaAtuSEB(SEE->EE_CODIGO)
			FinaAtuSEJ(SEE->EE_CODIGO)

			//Chamada da IntegDef para mensagem ˙nica
			If FWHasEAI('MATA070',.T.,,.T.)
				aRetInteg := FWIntegDef('MATA070')
				If ValType(aRetInteg) == "A" .AND. Len(aRetInteg) >= 2 .AND. !aRetInteg[1]
					Help( ,, "FINA130INTEGINC",,AllTrim( aRetInteg[2] ), 1, 0,,,,,,)
					lDisarmTran := .T.
					RollBackDelTran()
				EndIf
			EndIf
		EndIf

	End Transaction

	If lDisarmTran
		Return .F.
	EndIf

Return

