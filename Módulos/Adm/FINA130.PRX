/*/ {Protheus.doc} F130TudoOK
FunÁ„o para validaÁ„o do preenchimento da tela de Inclus„o/AlteraÁ„o (AxInclui)

@sample F130TudoOK()
@author Leonardo Castro
@since 15/12/18
@version 1.0

/*/
//---------------------------------------------------------------------------------------------------------
Function F130TudoOK(nTipo As Numeric)

	Local aArea   as Array
	Local aVldExt as Array
	Local cCampo  as String
	Local cDado   as String
	Local lRet    as Logical
	Local nX      as Numeric

	DEFAULT nTipo := 1 // 1 - Inclus„o / 2 - AlteraÁ„o

	aArea := GetArea()
	lRet  := .T.
	nX    := 0

	//VerificaÁıes de existÍncia da soluÁ„o de Job de ImportaÁ„o de Extratos Banc·rios
	If __lJobExt == NIL
		__lJobExt := SEE->(ColumnPos("EE_EXTJOB")) > 0
	Endif

	If __lJobExt
		aVldExt := {}
		AADD(aVldExt, {"EE_EXTCONA",M->EE_EXTCONA})
		AADD(aVldExt, {"EE_EXTDIRC",M->EE_EXTDIRC})
		AADD(aVldExt, {"EE_EXTDIRA",M->EE_EXTDIRA})
		AADD(aVldExt, {"EE_EXTDIRB",M->EE_EXTDIRB})
		AADD(aVldExt, {"EE_EXTDIRF",M->EE_EXTDIRF})
		AADD(aVldExt, {"EE_EXTARQC",M->EE_EXTARQC})
		AADD(aVldExt, {"EE_EXTARQR",M->EE_EXTARQR})
		AADD(aVldExt, {"EE_EXTFPRC",M->EE_EXTFPRC})
		If __lVldExt == NIL .AND. FindFunction("FA474OK")
			__lVldExt := .T.
		Endif
	ElseIf __lVldExt == NIL
		__lVldExt := .F.
	Endif

	If nTipo == 1
		lRet := !Empty(M->EE_SUBCTA)
	EndIf

	If lRet .and. __lVldExt .and. M->EE_EXTJOB == '1'
		For nX := 1 to Len(aVldExt) 
			cCampo := aVldExt[nX,1]
			cDado  := aVldExt[nX,2]
			lRet := FA474OK(cCampo,cDado, .T.)
			If !lRet
				Exit
			Endif
		Next
	Endif

	RestArea(aArea)
	FWFreeArray(aArea)
	FWFreeArray(aVldExt)

Return lRet
