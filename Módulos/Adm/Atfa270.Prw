Function Af270Del(cAlias, nReg, nOpc)
Local nOpca
Local oDlg
Local cCadastro := OemToAnsi(STR0006) //"Excluir"
Local aCamposSNG := {},bCampo,i
Local nValExclui := 0

Private aSize		:= {}
Private aObjects 	:= {} 
Private aPosObj  	:= {} 

Af270Help()

dbSelectArea("SX3")
dbSeek("SNG")

While !Eof() .And. (X3_ARQUIVO == cAlias)
	If X3Uso(X3_USADO) .And. cNivel >= X3_NIVEL
		AAdd(aCamposSNG,X3_CAMPO)
	Endif
	dbSkip()
Enddo

dbSelectArea(cAlias)
bCampo:= { |nCPO| Field(nCPO) }

For i:= 1 to FCount()
	M->&(Eval(bCampo,i)) := FieldGet(i)
Next i 

dbSelectArea("SN1")
dbSetOrder(1)
dbSeek(xFilial("SN1"))
While SN1->(N1_FILIAL == xFilial("SN1") .And. ! Eof())
	If SN1->N1_GRUPO == M->NG_GRUPO
		Help(" ",1,"AF270DEL")
		Return()
	Endif
	dbSkip()		                                                	
Enddo

//monta a entrada de dados do arquivo
Private aTELA[0][0], aGETS[0]

//envia processamento dos Gets
nOpca := 0
dbSelectArea(cAlias)
If !SoftLock(cAlias)
	Return
Endif 

//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
//ณ Calculo do tamanho dos objetos                                        ณ
//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
aSize := MsAdvSize()
aObjects := {}
AAdd( aObjects, { 100, 100, .T., .T. } )
aInfo := { aSize[ 1 ], aSize[ 2 ], aSize[ 3 ], aSize[ 4 ], 3, 3 }
aPosObj := MsObjSize( aInfo, aObjects )

DEFINE MSDIALOG oDlg TITLE cCadastro From aSize[7],0 to aSize[6],aSize[5] of oMainWnd PIXEL
nOpca := Enchoice(cAlias, nReg, nOpc,,,,,aPosObj[1])
nOpca := 1
ACTIVATE MSDIALOG oDlg ON INIT EnchoiceBar(oDlg,{|| nOpca := 2,oDlg:End()},{|| nOpca := 1,oDlg:End()})

If nOpca == 2
	Begin Transaction
		dbSelectArea(cAlias)
		RecLock(cAlias,.F.,.T.)
		dbDelete()
	End Transaction
Else
	MsUnlock()
End
dbSelectArea(cAlias)

Return()

