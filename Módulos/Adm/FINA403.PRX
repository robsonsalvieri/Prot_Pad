Function FINA403(lAutomato)
Local lPanelFin := IsPanelFin()
Local nOpca := 0
Local aButtons := {}
Local aSays := {}
Local lPCCBaixa := SuperGetMv("MV_BX10925",.T.,"2") == "1"
Local cPerg	:= "FIN403"

Local nRegEmp	:= SM0->(Recno())
Local nTamFil	:= IIf( lFWCodFil, FWGETTAMFILIAL, TamSX3( "E2_FILIAL" )[1] )
