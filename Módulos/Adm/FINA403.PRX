//------------------------------
If cAlias == "TMP"
	If _oFINA4031 <> Nil
		_oFINA4031:Delete()
		_oFINA4031 := Nil
	Endif
	_oFINA4031 := FWTemporaryTable():New( cAlias )  
	_oFINA4031:SetFields(aCampos) 	
	_oFINA4031:AddIndex("1", StrToKarr2( cChave, "+")) 	
	_oFINA4031:Create()
Else //TMP1
	If _oFINA4032 <> Nil
		_oFINA4032:Delete()
