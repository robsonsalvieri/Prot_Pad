Local aFornec := {}
Local bAscan
Local cFilAtu := cFilAnt
Local lTitPai 	:= .T.
Local cTitsSum	:= ""
Local lF401GRV := Existblock("lF401GRV")
Local lAS400 	:= TcSrvType() == "AS/400"

Local cDtPgt

Local lGerLog	:= (mv_par05 == 1)
Local lExistOri  := .T.
Local nRecMin := 0
Local nRecMax := 0

Local cArqTMP			:= ""
Local cArqINDEX		:= CriaTrab(,.F.)
Local cArqTMP1			:= ""
Local cArqINDEX1		:= CriaTrab(,.F.)
Local aTam := {}	
Local aCampos			:= {}
Local lTitINSS	:= .F.
Local lTitCheck:= .T.
Local cChvAux := ""

Local aLog		:= {}
Local cArqLog	:= ""
Local nPosLog	:= 0
Local nTotREG	:= 0
Local aCRIN480 := {}
Local lGrvDIRF := .T.
Local lIrBx	:=	.F.
Local aRecnoFon := {}
Local aAreaAux	:= {}
Local aRetAuto	:= {}

Local cTmpSA2Fil := "" //Gestao
Local cFilSA2	:= GetRngFil( aSelFil, "SA2", .T., @cTmpSA2Fil ) //xFilial("SA2")
Local cTpRen	:= ""
