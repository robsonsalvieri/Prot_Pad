Local cAliasPai
Local aRecno := {}
Local cRecno := ""
Local nX, nQt
Local nY
Local cAliasTrb
Local cChaveSr4
Local cQuery := ""
Local nI := 0
Local aStru := SE2->(dbStruct())
Local cIndex
Local aArea2
Local lDelFisico	:=	GetNewPar('MV_FIN401D',.T.)
Local lPlsAtiv	:= GetNewPar("MV_PLSATIV",.F.)
