Static Function cria_pn1( oPanel As Object )
	Local oFont As Object

	oFont := TFont():New(,,-25,.T.,.T.,,,,,)

	TSay():New(10,15,{|| STR0011},oPanel,,oFont,,,,.T.,,)// 'Envio de Cartas de Cobrança'
	TSay():New(35,10,{|| STR0012},oPanel,,,,,,.T.,,) 	// 'Clique em "Próximo" para:'
	TSay():New(45,15,{|| STR0013},oPanel,,,,,,.T.,,) 	// '- Definir os parâmetros da rotina;'
	TSay():New(55,15,{|| STR0014},oPanel,,,,,,.T.,,) 	// '- Selecionar layouts de Cartas de Cobrança;'
	TSay():New(65,15,{|| STR0015},oPanel,,,,,,.T.,,) 	// '- Selecionar clientes para evnio;'
	TSay():New(75,15,{|| STR0016},oPanel,,,,,,.T.,,) 	// '- Selecionar títulos para envio;'
	TSay():New(85,15,{|| STR0022},oPanel,,,,,,.T.,,) 	// '- Efetivar o envio das cartas conforme os dados selecionados.'
	TSay():New(95,10,{|| STR0017},oPanel,,,,,,.T.,,) 	// 'Ao final do processo, as cartas serão enviadas conforme os dados selecionados.'

