/*/{Protheus.doc} 
	Lida com a atualização da tabela temporária. Executa novamente apenas se a query sofreu alterações.
	@author guilherme.sordi
	@since 17/08/2022
	@version 12.1.33
/*/
Method hndRefresh() Class F811TmpDef
	Local cQuery := Self:getQuery()

	If cQuery != Self:cLastQuery
		Self:execQuery(cQuery)
	EndIf
Return

/*/{Protheus.doc} 
	Roda a query e coloca os dados na tabela temporária. 
	Não mostra tela com mensagem de progresso em execução automática.
	Reutiliza sempre a mesma tabela temporária, limpando apenas os dados, conforme últimas definições de boas práticas.
	@author guilherme.sordi
	@since 17/08/2022
	@version 12.1.33
/*/
Method execQuery(cQuery as Character) Class F811TmpDef
	Local nTcSql as Numeric

	nTcSql := 0

	If ::oTempTable <> Nil
		nTcSql := TcSQLExec("DELETE FROM " + ::oTempTable:GetRealName() )
		If nTcSql < 0 //Se ocorrer algum problema refaz a temporária
			::oTempTable:Delete()
			::oTempTable := Nil
		Else
			(::cAlias)->(dbGoTo(1))  //Necessário para atualização do Alias após deleção dos dados
		EndIf
	EndIf

	If ::oTempTable == NIL
		::oTempTable := FWTemporaryTable():New( ::cAlias )
		::oTempTable:SetFields(::aStruct)
		::setCustomIndex()
		::oTempTable:Create()
	EndIf

	If !l811Auto
		MsgRun(::cLoadMsg,STR0065,{||SqlToTrb(cQuery, ::aStruct, ::cAlias)}) // Cria arquivo temporario
	Else
		SqlToTrb(cQuery, ::aStruct, ::cAlias)
	Endif

	::cLastQuery := cQuery

	(::cAlias)->(DbSetOrder(0))
Return

