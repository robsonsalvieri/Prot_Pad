cCampos:="IE_MES,IE_EXERCIC,IE_INDICE,IE_DTAJUS,IE_MOEDA,IE_SEQ"
IF cPaisLoc == "ANG"
	cCampos +=",IE_NORMLEG"
EndIf

aCfg := { { "SIE", cCampos, {||MsExecAuto({|x,y| CTBA055(x,y)}, xAutoCab, 3)} } }

If ParamBox({	{6,STR0013,padr("",150),"",,"", 90 ,.T.,STR0014,"",GETF_LOCALHARD+GETF_LOCALFLOPPY+GETF_NETWORKDRIVE}},;	 //"Estrut. de taxas"###"Arquivo .CSV |*.CSV"
		STR0016,@aRet)  //"Importar para estrutura de taxas"

	oProcess:= MsNewProcess():New( {|lEnd| CTBImpCSV( lEnd, oProcess, aRet[1], aCfg )} )
	oProcess:Activate()
	lCTB055AUT := .F.

EndIf

RestInter()
RestArea(aArea)

Return .T.

