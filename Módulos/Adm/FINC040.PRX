                        { "MOTIVO    ", "C", 03, 0 },                    ;
                        { "HISTORICO ", "C", aTamSX3a[1]+1,aTamSX3a[2]}, ;
                        { "DATACONT  ", "D", 08, 0 },                    ;
                        { "DATADISP  ", "D", 08, 0 },                    ;
                        { "LOTE      ", "C", aTamSX3b[1],aTamSX3b[2]},   ;
                        { "BANCO     ", "C", aTamSX3d[1], 0 },           ;
                        { "AGENCIA   ", "C", aTamSX3e[1], 0 },           ;
                        { "CONTA     ", "C", aTamSX3f[1], 0 },           ;
