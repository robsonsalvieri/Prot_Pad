    aTamSX3e := TamSX3("A6_AGENCIA")
    aTamSX3f := TamSX3("A6_NUMCON")

    If cPaisLoc $ "BRA"
        aCampos	 :=    {{"OK","N",1,0},                                  ;
                        {"DATAX", "D", 08, 0 },                          ;
                        { "JUROS     ", "N", 16, 2 },                    ;
                        { "MULTA     ", "N", 16, 2 },                    ;
                        { "CORRECAO  ", "N", 16, 2 },                    ;
                        { "DESCONTOS ", "N", 16, 2 },                    ;
