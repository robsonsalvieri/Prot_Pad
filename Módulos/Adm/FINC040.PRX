                        {STR0019,"LOTE"},;  		// "Lote"
                        {STR0020,"BANCO"},;  		// "Banco"
                        {STR0021,"AGENCIA"},; 		// "Agˆncia"
                        {STR0022,"CONTA"},;  		// "Conta"
