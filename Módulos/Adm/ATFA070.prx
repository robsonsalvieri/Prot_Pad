//-----------------------------------------------------------------
/*/{Protheus.doc} VlMesAntMoe
Função criada para melhora de performance, carregando valores para todas as 
moedas uma unica vez ao invez de chamar a VlMesAn uma vez por moeda. 
@author Totvs
@since 10/09/2020
@version 1.0
/*/
//-----------------------------------------------------------------

Static Function VlMesAntMoe( cTab,cCampo,cOcorr )
Local dMesAnt		:= (FirstDay(dDataBase) - 1)
Local aArea			:= GetArea() 
Local aAreaSN0		:= SN0->(GetArea())
Local aAreaSN3		:= SN3->(GetArea())
Local nQtMoed		:= AtfMoedas() //Controle de multiplas moedas
Local cQry			:= ""
Local __oQry
Local nX			:= 0
Local cAliasQry 	:= GetNextAlias()
Local lCont			:= .F.
Local nValor 		:= 0 

Default cTab 	:= ""
Default cCampo 	:= ""
Default cOcorr 	:= ""


If cQryMoe == ""
	cQryMoe := "SELECT "
	For nX:= 1 to nQtMoed
		cQryMoe += "N4_VLROC"+ALLTRIM(STR(nX))

		If nX < nQtMoed
			cQryMoe += " ," +CRLF
		EndIf
	Next
Endif
//-------------------------------------------------------------------------
// Após a virada do ano (ATFA080) o campo N3_VRDMES1 é zerado
// Caso realize a depreciacao de janeiro e em seguida o descalculo
// nao pega a depreciacao de dezembro, respeitando a alteracao do ATFA080
// MV_ATFRSLD = .T.
//-------------------------------------------------------------------------
DBSelectArea("SN0") 

SN0->(DBSetOrder(1)) //N0_FILIAL+N0_TABELA+N0_CHAVE

If SN0->(msSeek(XFilial("SN0")+"13"+"VIRADAATIVO")) .And. STOD(SN0->N0_DESC01) < dMesAnt 

	cQry := cQryMoe
	cQry += " FROM ? SN4
	cQry += "WHERE N4_FILIAL = ?
	cQry += "AND N4_CBASE = ?
	cQry += "AND N4_ITEM = ?
	cQry += "AND N4_TIPO = ?
	cQry += "AND N4_DATA = ?
	cQry += "AND N4_OCORR = ?
	cQry += "AND N4_SEQ = ?
	cQry += "AND SN4.D_E_L_E_T_ = ' '

	If _cQry == Nil .or. _cQry != cQry
		_cQry := cQry
		cQry := ChangeQuery(cQry)
		_cQryModif := cQry
	EndIf
	
	__oQry := FWPreparedStatement():New(_cQryModif)
	
	__oQry:SetNumeric(1,RetSqlName("SN4"))
	__oQry:SetString(2,SN4->N4_FILIAL)
	__oQry:SetString(3,SN4->N4_CBASE)
	__oQry:SetString(4,SN4->N4_ITEM)
	__oQry:SetString(5,SN4->N4_TIPO)
	__oQry:SetString(6,DTOS(dMesAnt))
	__oQry:SetString(7,cOcorr)
	__oQry:SetString(8,SN4->N4_SEQ)

	cAliasQry := MPSYSOpenQuery(__oQry:GetFixQuery(),cAliasQry)

	DbSelectArea(cAliasQry)

	lCont :=  (cAliasQry)->(!Eof())

	For nX := 1 to nQtMoed
		If lCont
			nValor := (cAliasQry)->&("N4_VLROC"+ALLTRIM(STR(nX)))
		EndIf

		If Len(cCampo)==9 .and. nX>9
				(cTab)->(FieldPut(FieldPos(SubStr(cCampo,1,8) + Alltrim(Str(nX)) 	), nValor ))
		Else
				(cTab)->(FieldPut(FieldPos(cCampo + Alltrim(Str(nX)) 				), nValor ))
		EndIf
	Next

	(cAliasQry)->(dbCloseArea()) // Fecha a query
EndIf

RestArea( aAreaSN3)
RestArea( aAreaSN0 )
RestArea( aArea)

Return

