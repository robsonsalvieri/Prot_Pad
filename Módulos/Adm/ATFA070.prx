Function Af070Moeda(nQtdMoedas,cAlias,cGrpCampo)

	Local aValores	:= {}

	Local nI		:= 0

	Local cCampo	:= ""

	For nI := 1 to nQtdMoedas
		cCampo := Alltrim(cGrpCampo) + Alltrim(Str(nI))
		If (cAlias)->(FieldPos(cCampo)) > 0
			aAdd(aValores,(cAlias)->&(cCampo))
		Endif
	Next nI

Return (aValores)

