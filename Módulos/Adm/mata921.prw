Function a921Alter()
Local nReg	:=AHG->(Recno())
Local nOpc	:=4
Local cAlias:="AHG"
Local nX    :=0
Local aCpos :={}

For nX := 1 To FCount()
	M->&(FieldName(nX)) := CriaVar(FieldName(nX))
    // Campos nao editaveis
	If !(FieldName(nX)) $ "AHG_CODPUB/AHG_CODPRO"
		Aadd(aCpos,FieldName(nX))
	EndIf
Next nX

AxAltera( cAlias, nReg, nOpc,,aCpos)
Return(.t.)

