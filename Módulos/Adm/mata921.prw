Function a921Inclui()
Local cAlias	:="AHG"
Local nReg		:=AHG->(Recno())
Local nOpc		:=3
Local nGravou	:=0
Local nSaveSx8Len 	:= GetSx8Len()

nGravou :=AxInclui( cAlias, nReg, nOpc,,,,"A921ChkInc()")
if nGravou <> 1
	While GetSx8Len() > nSaveSx8Len
		RollBackSX8()
	End
endif
Return(.T.)


