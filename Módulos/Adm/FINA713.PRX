/*/{Protheus.doc} F713SMTFin
    Dispara os e-mails e atualiza a SEA para os boletos TechFin
    @type function
    @version 1.0
    @author daniel.moda
    @since 01/04/2025
    @param aBolTecFin, array, {character (json), SEA->RECNO }
    @param cEmail, character, endereço do destinatário
    @param oServMail, ojjeto, classe de envio de e-mail
/*/
Static Function F713SMTFin(aBolTecFin As Array, cEmail As Character, oServMail As Object)
    Local nBol      As Numeric
    Local cApiMail  As Character
    Local cApiMsg   As Character
    Local oLog      As Object
    Local oLogSEA   As Object
    Local aArea     As Array
    Local aAreaSEA  As Array
    Local nTotalBol As Numeric

    aArea     := GetArea()
    aAreaSEA  := GetArea('SEA')
    oLog      := JsonObject():new()
    oLogSEA   := JsonObject():new()
    cApiMsg   := STR0013 //"E-mail enviado com sucesso!"
    cApiMail  := '1'     // 0=Aguardando envio;1=Enviado;2=Falha;3=Não envia
    nTotalBol := Len( aBolTecFin )

    oServMail:setEmail(cEmail)   // e-mail do destinatário
    oServMail:setBills(aBolTecFin) // array com os dados do boletos
    oServMail:sendEmailTF()        // envia os e-mails
    If !oServMail:lSentMail
        cApiMail := '2' // Falha
        cApiMsg  := oServMail:getError()
    EndIf
    // cria o objeto com o log e adiciona no array
    oLog['date']    := FWTimeStamp(2, DATE(), TIME())
    oLog['message'] := cApiMsg
    // Atualiza o status na SEA da transmissão do e-mail
    For nBol := 01 To nTotalBol
        SEA->(DbGoTo(aBolTecFin[nBol][2]))
        // pega os logs armazenados
        oLogSEA:FromJSON(SEA->EA_APILOG)
        oLogSEA['mail'] := oLog

        RecLock('SEA', .F.)
        SEA->EA_APIMAIL := cApiMail
        SEA->EA_APILOG  := oLogSEA:ToJSON()
        SEA->(MsUnlock())
    Next
    oLog:FromJSON( '{}' )
    oLogSEA:FromJSON( '{}' )
    FreeObj(oLog)
    FreeObj(oLogSEA)

    RestArea(aAreaSEA)
    RestArea(aArea)
    aSize(aAreaSEA,0)
    aSize(aArea,0)
    FwFreeArray(aAreaSEA)
    FwFreeArray(aArea)
Return
