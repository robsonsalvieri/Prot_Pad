Function PCOA470DLG(cAlias,nRecno,nOpcx)
Local nOpca := 0

Do Case
	Case aRotina[nOpcx][4] == 2
		nOpca := AxVisual("ALQ",ALQ->(Recno()),2,,,,,,,,,,,,,,)
	Case aRotina[nOpcx][4] == 3
		nOpca := AxInclui("ALQ",ALQ->(Recno()),3,,,,,.F.,,,,,,,,,,,)
	Case aRotina[nOpcx][4] == 4
		nOpca := AxAltera("ALQ",ALQ->(Recno()),4,,,,,,,,,,,,,,,,,)
	Case aRotina[nOpcx][4] == 5
		nOpca := AxDeleta("ALQ",ALQ->(Recno()),5,,,,,,,,,,,)
EndCase

Return nOpca

