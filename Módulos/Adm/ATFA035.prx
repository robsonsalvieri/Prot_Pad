	If cFilialF < cFilialI
		lRet := .F.
	Else
		cFil := cFilialF
	Endif
Endif

If lRet	
	
	cFil := Substr(cFil,1,nTamFil)

	If nOpc == 1
		cFilIni := xFilial("SN1",cFil) 
	Else
		cFilFim := xFilial("SN1",cFil) 
	Endif
Endif

Return lRet

