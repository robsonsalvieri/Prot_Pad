*/
Function AE8Fill(aRecursos)
	Local aTemp := {} // utilizado temporariamente para preencher aRecursos

	// aRecursos passada por referencia
	aRecursos := {}
	
	dbSelectArea("AE8")
	dbGoTop()
		
	While !AE8->(EoF())

		// inicializa o item
		AE8InitItem(@aTemp)
		
		// 
		aTemp[SUB_AE8_FILIAL] := AE8->AE8_FILIAL
		aTemp[SUB_AE8_RECURS] := AE8->AE8_RECURS
		aTemp[SUB_AE8_DESCRI] := AE8->AE8_DESCRI
		aTemp[SUB_AE8_CALEND] := AE8->AE8_CALEND
		aTemp[SUB_AE8_CONFIR] := AE8->AE8_CONFIR
		aTemp[SUB_AE8_APTMRE] := AE8->AE8_APTMRE
		
		// utilizar a funcao aClone()
		aAdd(aRecursos, aClone(aTemp))
		
		AE8->(dbSkip())
	End
Return .T.

