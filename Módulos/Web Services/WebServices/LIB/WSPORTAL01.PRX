	aadd(::UserListPortal,WsClassNew("GenericStruct"))

	::UserListPorta[nX]:Code        := (cAlias)->AI9_PORTAL
	::UserListPorta[nX]:Description := (cAlias)->AI9_DESCRI

	dbSelectArea(cAlias)
	dbSkip()
