Local aArea  := GetArea()
Local lQuery := .F.
Local cAlias := "AI9"
Local nX     := 0 
#IFDEF TOP
Local cQuery := ""
#ENDIF

#IFDEF TOP
	cAlias := "PrtListPortals"
	lQuery := .T.

	cQuery := "SELECT * "
	cQuery += "FROM "+RetSqlName("AI9")+" AI9 "
