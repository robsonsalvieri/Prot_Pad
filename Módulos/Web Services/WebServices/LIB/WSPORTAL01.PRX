	EndIf
EndIf

AI8->(dbSetOrder(1))
If AI8->(dbSeek(xFilial("AI8")+"000006"))
	While !AI8->(Eof()) .And. AI8->(AI8_FILIAL+AI8_PORTAL) == xFilial("AI8")+"000006" 
		If Alltrim(AI8->AI8_TEXTO) == "Solicitacoes"
			RecLock("AI8",.F.)
			AI8->AI8_TEXTO := "Solicitações"
			AI8->(msUnlock())
