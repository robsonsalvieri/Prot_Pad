/*/
Static Function CalcSequen(cVar1)
Local cVar2	:= "001"
Local nVar		:= 0
Local nI		:= 0
   
For nI := 1 To 50000
	If cVar2 == cVar1
   		nVar := nI
   		Exit
	Endif
   	cVar2 := Soma1(cVar2)
Next nI

Return nVar

/*/{Protheus.doc} setaNoEmp
Atribui as propriedades de execauto do mata650 para não ocorrer a geração de empenhos.

@type  Static Function
@author lucas.franca
@since 16/01/2024
@version P12
@param aVetPar, Array, Array com os dados de execução automática do mata650
@return Nil
/*/
Static Function setaNoEmp(aVetPar)
	Local aParam := {}
	Local nIndex := 0
	Local nPos   := 0
	
	aAdd(aParam, {"GERAOPI"     ,"N"})
	aAdd(aParam, {"GERASC"      ,"N"})
	aAdd(aParam, {"AUTEXPLODE"  ,"N"})
	aAdd(aParam, {"C2_BATCH"    ,"S"})
	aAdd(aParam, {"GERAEMP"     ,"N"})
	aAdd(aParam, {"AUT_CHECKOPI","N"})

	For nIndex := 1 To Len(aParam)
		nPos := aScan(aVetPar, {|x| x[1] == aParam[nIndex][1]})
		If nPos == 0
			//Se a propriedade não existe no array aVetPar, cria ela
			aAdd(aVetPar, {aParam[nIndex][1], aParam[nIndex][2], NIL})
		Else
			//Propriedade já existe, apenas atualiza o valor.
			aVetPar[nPos][2] := aParam[nIndex][2]
		EndIf
		
	Next nIndex

Return Nil

/*/{Protheus.doc} processaEmp
Faz o processamento da criação dos empenhos recebidos pelo APS.

@type  Static Function
@author lucas.franca
@since 16/01/2024
@version P12
@param cNumOP , Caracter, Número da OP criada
@param oDataOP, Object  , Objeto com os dados da OP enviados pelo APS.
@return lSucesso, Logic, Indica se criou corretamente os empenhos
/*/
Static Function processaEmp(cNumOP, oDataOP)
	Local aCab     := {{"D4_OP", cNumOP, NIL}}
	Local aLinEmp  := {}
	Local aEmps    := {}
	Local cLocal   := ""
	Local cRoteiro := "01"
	Local lSucesso := .T.
	Local nIndex   := 0
	Local nTotal   := Len(oDataOP:listOfMaterialOrders)
	Local oEmp     := Nil

	Private lMsErroAuto    := .F.
	Private lAutoErrNoFile := .T.

	//Posiciona na SC2 da OP que foi criada.
	SC2->(dbSetOrder(1))
	SC2->(dbSeek(xFilial("SC2") + cNumOP))

	//Posiciona na SB1 do produto da ordem criada
	SB1->(dbSetOrder(1))
	SB1->(dbSeek(xFilial("SB1") + SC2->C2_PRODUTO))

	If !Empty(SB1->B1_OPERPAD)
		//Roteiro a ser registrado no empenho caso não exista na SC2 e tenha na SB1.
		cRoteiro := SB1->B1_OPERPAD
	EndIf

	If !Empty(SC2->C2_ROTEIRO)
		//Registra no empenho o mesmo roteiro da OP.
		cRoteiro := SC2->C2_ROTEIRO
	EndIf

	For nIndex := 1 To nTotal
		oEmp := oDataOP:listOfMaterialOrders[nIndex]

		//Ajusta tamanho de campos CHAR
		oEmp:MaterialCode       := PadR(oEmp:MaterialCode      , TamField("D4_COD"   ))
		oEmp:PertMaterialNumber := PadR(oEmp:PertMaterialNumber, TamField("D4_TRT"   ))
		oEmp:ActivityCode       := PadR(oEmp:ActivityCode      , TamField("D4_OPERAC"))
		oEmp:OriginMaterialCode := PadR(oEmp:OriginMaterialCode, TamField("D4_PRDORG"))

		//Valida campos obrigatórios: D4_COD, D4_QUANT e D4_DATA
		If Empty(oEmp:MaterialCode)
			SetSoapFault("PUTPRODUCTIONORDER", STR0022) //"Código do empenho (MaterialCode) não foi informado."
			lSucesso := .F.
			Exit
		EndIf

		If Empty(oEmp:MaterialQuantity)
			SetSoapFault("PUTPRODUCTIONORDER", STR0023) //"Quantidade do empenho (MaterialQuantity) não foi informado."
			lSucesso := .F.
			Exit
		EndIf

		If Empty(oEmp:MaterialDate)
			SetSoapFault("PUTPRODUCTIONORDER", STR0024) //"Data do empenho (MaterialDate) não foi informado."
			lSucesso := .F.
			Exit
		EndIf

		cLocal := getArmazem(oEmp:MaterialCode, oEmp:WarehouseCode)
		
		aAdd(aLinEmp, {"D4_OP"     , cNumOP                 , Nil})
		aAdd(aLinEmp, {"D4_COD"    , oEmp:MaterialCode      , Nil})
		aAdd(aLinEmp, {"D4_QTDEORI", oEmp:MaterialQuantity  , Nil})
		aAdd(aLinEmp, {"D4_QUANT"  , oEmp:MaterialQuantity  , Nil})
		aAdd(aLinEmp, {"D4_DATA"   , oEmp:MaterialDate      , Nil})
		aAdd(aLinEmp, {"D4_LOCAL"  , cLocal                 , Nil})
		aAdd(aLinEmp, {"D4_TRT"    , oEmp:PertMaterialNumber, Nil})
		aAdd(aLinEmp, {"D4_OPERAC" , oEmp:ActivityCode      , Nil})
		aAdd(aLinEmp, {"D4_PRODUTO", SC2->C2_PRODUTO        , Nil})
		aAdd(aLinEmp, {"D4_ROTEIRO", cRoteiro               , Nil})
		aAdd(aLinEmp, {"D4_PRDORG" , oEmp:OriginMaterialCode, Nil})

		aAdd(aEmps, aLinEmp)
		aLinEmp := {}
	Next nIndex

	//Executa inclusão via MATA381.
	If lSucesso
		MSExecAuto({|x,y,z| mata381(x,y,z)}, aCab, aEmps, 3)
		lSucesso := verErroAuto()
	EndIf
	
	aSize(aCab , 0)
	aSize(aEmps, 0)

Return lSucesso

/*/{Protheus.doc} verErroAuto
Verifica se a execução do ExecAuto teve algum erro
e caso tenha erro, atribui a mensagem de erro para retorno soap.

@type  Static Function
@author lucas.franca
@since 16/01/2024
@version P12
@return lSucesso, Caracter, Retorna se houve ou não erro no execauto
/*/
Static Function verErroAuto()
	Local aErro    := {}
	Local cErro    := ""
	Local lSucesso := lMsErroAuto == .F.
	Local nIndex   := 0

	If lMsErroAuto
		aErro := GetAutoGRLog()
		For nIndex := 1 To Len(aErro)
			cErro += aErro[nIndex]+Chr(13)+Chr(10)				
		Next nIndex
		aSize(aErro, 0)
		SetSoapFault("PUTPRODUCTIONORDER", cErro)
	EndIf

Return lSucesso

/*/{Protheus.doc} TamField
Retorna o tamanho de um campo

@type  Static Function
@author lucas.franca
@since 16/01/2024
@version P12
@param cField, Caracter, Nome do campo
@return _oTamField[cField], Numeric, Tamanho do campo (X3_TAMANHO)
/*/
Static Function TamField(cField)

	If _oTamField == Nil 
		_oTamField := JsonObject():New()
	EndIf

	If _oTamField:HasProperty(cField) == .F.
		_oTamField[cField] := GetSX3Cache(cField, "X3_TAMANHO")
	EndIf

Return _oTamField[cField]

/*/{Protheus.doc} getArmazem
Busca o armazém padrão do produto

@type  Static Function
@author lucas.franca
@since 19/01/2024
@version P12
@param cProduto, Caracter, Código do produto
@param cLocal  , Caracter, Código do armazém recebido para o produto
@return cLocal, Caracter, Código do armazém do produto
/*/
Static Function getArmazem(cProduto, cLocal)
	Local cAlias := ""

	//Se estiver vazio, busca o padrão do produto na SB1/SBZ.
	If Empty(cLocal)
		If _lUsaSBZ == Nil
			_lUsaSBZ := SuperGetMV("MV_ARQPROD", .F., "SB1") == "SBZ"
		EndIf

		If _lUsaSBZ
			//Configurado para usar SBZ, monta a query para buscar da SBZ/SB1.
			If _oQryArmzm == Nil
				_oQryArmzm := FwExecStatement():New("SELECT COALESCE(SBZ.BZ_LOCPAD, SB1.B1_LOCPAD) LOCPAD" +;
				                                     " FROM " + RetSqlName("SB1") + " SB1"                 +;
													 " LEFT JOIN " + RetSqlName("SBZ") + " SBZ"            +;
													   " ON SBZ.BZ_FILIAL  = ?"                            +;
													  " AND SBZ.BZ_COD     = SB1.B1_COD"                   +;
													  " AND SBZ.D_E_L_E_T_ = ' '"                          +;
													" WHERE SB1.B1_FILIAL  = ?"                            +;
													  " AND SB1.B1_COD     = ?"                            +;
													  " AND SB1.D_E_L_E_T_ = ' '"                          )
			EndIf

			_oQryArmzm:SetString(1, xFilial("SBZ"))
			_oQryArmzm:SetString(2, xFilial("SB1"))
			_oQryArmzm:SetString(3, cProduto)

			cAlias := _oQryArmzm:OpenAlias()
			cLocal := (cAlias)->LOCPAD
			(cAlias)->(dbCloseArea())
			
		Else
			//Não utiliza SBZ, sempre pega direto da SB1.
			SB1->(dbSeek(xFilial("SB1") + cProduto))
			cLocal := SB1->B1_LOCPAD
		EndIf
	EndIf

	cLocal := PadR(cLocal, TamField("D4_LOCAL"))
Return cLocal
