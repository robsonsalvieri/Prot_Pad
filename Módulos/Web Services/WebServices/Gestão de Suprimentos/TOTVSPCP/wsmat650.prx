Local aArea     := GetArea()
Local lRetorno  := .T.
Local cAliasSD3 := "SD3"
Local nY        := 0
Local cCliente  := SubStr(::CustomerID,1,Len(SA1->A1_COD))
Local cLoja     := SubStr(::CustomerID,Len(SA1->A1_COD)+1)
#IFDEF TOP
Local aStruSD3  := {}
