			EndIf
		Next nX
	#ELSE
		cQuery	+= "C2_FILIAL == '"+xFilial("SC2")+"' .And. "
		If !Empty(::PODateFrom)
			cQuery	+= "C2_EMISSAO>='"+Dtos(::PODateFrom)+"' .And. "
		EndIf
		If !Empty(::PODateTo)
			cQuery	+= "C2_EMISSAO<='"+Dtos(::PODateTo)+"' .And. "
