// Bugs in Payment Request
Static Function DMA3_payReq()
Local aF47 as Array
Local cTmpPos as Character
Local nReqTypPos as Numeric
Local nStatusPos as Numeric
Local nI as Numeric

//F47 Fields
    aF47 := {}
    DBSelectArea("SX3")
    SX3->(DBSetOrder(01)) //X3_ARQUIVO + X3_ORDEM
    If (SX3->(DBSeek("F47")))
        While (!SX3->(EOF()) .AND. SX3->X3_ARQUIVO == "F47")
            AAdd(aF47, {SX3->X3_CAMPO, SX3->X3_ORDEM})
            SX3->(DBSkip())
        EndDo
    EndIf

    //Sort
    ASort(aF47,,, { |x, y| y[02] > x[02] })

    //Seek field F47_REQTYP
    nStatusPos := AScan(aF47, {|x| AllTrim(x[01]) == "F47_DTREQ"})
    nReqTypPos := AScan(aF47, {|x| AllTrim(x[01]) == "F47_REQTYP"})

    //Verify the sequence
    For nI := nReqTypPos to (nStatusPos + 1) step -1
        //Change
        cTmpPos := aF47[nI-1][02]
        aF47[nI - 1][02] := aF47[nReqTypPos][02]
        aF47[nReqTypPos][02] := cTmpPos
    Next nI
    ASort(aF47,,, { |x, y| y[02] > x[02] })
    //Make Array
    For nI := 01 To Len(aF47)
        aAdd(aUpdate,{"SX3", aF47[nI, 01], "X3_ORDEM" , aF47[nI, 02]})
    Next nI
Return

