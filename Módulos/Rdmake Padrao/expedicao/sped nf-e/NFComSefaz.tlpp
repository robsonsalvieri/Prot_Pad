        cXML +=     "<qFaturada>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["qFaturada"],16,4) + "</qFaturada>"
        cXML +=     "<vProd>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["vProd"],21,4) + "</vProd>"
        if self:oDet["det"][nItem]["imposto"]["gProcRef"]["vDesc"] <> nil
            cXML +=     "<vDesc>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["vDesc"],16,2) + "</vDesc>"
        endIf
        if self:oDet["det"][nItem]["imposto"]["gProcRef"]["vOutro"] <> nil
            cXML +=     "<vOutro>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["vOutro"],16,2) + "</vOutro>"
        endIf
        if self:oDet["det"][nItem]["imposto"]["gProcRef"]["indDevolucao"] <> nil
            cXML +=     "<indDevolucao>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["indDevolucao"],16,2) + "</indDevolucao>"
        endIf
        if self:oDet["det"][nItem]["imposto"]["gProcRef"]["vBC"] <> nil
            cXML +=     "<vBC>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["vBC"],16,2) + "</vBC>"
        endIf
        if self:oDet["det"][nItem]["imposto"]["gProcRef"]["pICMS"] <> nil
            cXML +=     "<pICMS>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["pICMS"],16,2) + "</pICMS>"
        endIf
        if self:oDet["det"][nItem]["imposto"]["gProcRef"]["vICMS"] <> nil
            cXML +=     "<vICMS>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["vICMS"],16,2) + "</vICMS>"
        endIf
        if self:oDet["det"][nItem]["imposto"]["gProcRef"]["vPIS"] <> nil
            cXML +=     "<vPIS>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["vPIS"],16,2) + "</vPIS>"
        endIf
        if self:oDet["det"][nItem]["imposto"]["gProcRef"]["vCOFINS"] <> nil
            cXML +=     "<vCOFINS>" + self:formatValuePerType(self:oDet["det"][nItem]["imposto"]["gProcRef"]["vCOFINS"],16,2) + "</vCOFINS>"
        endIf
        //repeticao <gProc>
        If self:oDet["det"][nItem]["imposto"]["gProcRef"]["gProcRef"] <> NIL
            for nI := 1 to len(self:oDet["det"][nItem]["imposto"]["gProcRef"]["gProcRef"]["gProc"])
                cXML +=         "<tpProc>" + self:oDet["det"][nItem]["imposto"]["gProcRef"]["gProcRef"]["gProc"][nI]["tpProc"] + "</tpProc>"
                cXML +=         "<nProcesso>" + self:oDet["det"][nItem]["imposto"]["gProcRef"]["gProcRef"]["gProc"][nI]["nProcesso"] + "</nProcesso>"
            next
        EndIf
        cXML += "</gProcRef>"

    endIf
return cXML

