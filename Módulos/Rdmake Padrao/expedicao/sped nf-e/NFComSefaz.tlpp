            EndIf

            self:oDest["enderDest"]["xBairro"]  := AllTrim(SA2->A2_BAIRRO)

            self:oDest["enderDest"]["cMun"]     := self:aUF[aScan(self:aUF,{|x| x[1] == SA2->A2_EST})][02] + SA2->A2_COD_MUN
            self:oDest["enderDest"]["xMun"]     := AllTrim(SA2->A2_MUN)
