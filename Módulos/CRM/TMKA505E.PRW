Method load(obj) Class TSToolbarListTop  
Local lRet := .T.         
Local cAlias :=	GetNextAlias() 


BeginSQL alias cAlias   


	SELECT 	KJ_FILIAL,	KJ_CODIGO, 	KJ_CODTELE, 	KJ_TIPO, 
			KJ_NOME, 	KJ_POSICAO,	KJ_AJUDA, 		KJ_IMAGEM, 
			KJ_CDBLOCK,	KJ_USRLEVL				
	FROM	%table:SKJ% SKJ 
	WHERE	SKJ.KJ_FILIAL = %xFilial:SKJ% AND 	
			SKJ.%NOTDEL% AND
			SKJ.KJ_CODTELE = %EXP:obj:id%  AND
			SKJ.KJ_TIPO = %EXP:obj:listType%
EndSql                                  

If (cAlias)->(!EOF())
	Self:fillClass(obj, cAlias)
Else
	lRet := .F.
EndIF

DbSelectArea(cAlias)
DbCloseArea() 
Return lRet

