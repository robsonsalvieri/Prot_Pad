Method load(obj) Class WFInformationTop
Local lRet		:= .T.
Local cAlias	:= GetNextAlias()
Local cField 	:= ""

DbSelectArea("SKW")
cField += ", KW_LOTEAPR "

cField += ", KW_MODELO "

cField += ", KW_DATE "

cField += ", KW_TIME "

cField := "%" + cField + "%"

BeginSQL alias cAlias
	SELECT 	KW_FILIAL,	KW_CODIGO, 	KW_USER, 	KW_EMAIL, 
			KW_SUBJECT,	KW_CODEBLK, KW_INTERID	%Exp:cField%, 
			KW_AUTORIZ 
				
	FROM	%table:SKW% SKW 
	WHERE	SKW.KW_FILIAL = %xFilial:SKW% AND 
			SKW.%NOTDEL% AND
			SKW.KW_CODIGO = %EXP:obj:id%
EndSql

If (cAlias)->(!Eof())
	Self:fillClass(obj, cAlias)
	DbSelectArea(cAlias)
	DbCloseArea()
Else
	lRet := .F.
EndIf

Return lRet


