Function Tk130Grava()

Local aArea	:= GetArea()

DbSelectarea("SUR")
DbSetorder(1)
DbSeek(xFilial("SUR"))
While (!Eof()) .AND. (xFilial("SUR") == SUR->UR_FILIAL)
	
	//Atualiza a ocorrencia
	If !Empty(UR_ISTREE)
		//Se a ocorrencia for a mesma que sofreu alteracao
		If UR_CODREC == SU9->U9_CODIGO
			Reclock("SUR",.F.)
			Replace UR_DESC With SU9->U9_DESC
			MsUnlock()
		Endif
	Endif
	
	DbSelectarea("SUR")
	DbSkip()
End

RestArea(aArea)

Return(.T.)

