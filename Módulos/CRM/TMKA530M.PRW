Method load(obj) Class SlaTop
Local lRet	 := .T.         		//Retorno do metodo
Local cAlias :=	GetNextAlias() 		//Alias da query
Local cFields

DbSelectArea("SK6")
cFields := "%, 	K6_TURNO%"
	

BeginSQL alias cAlias
	SELECT 	K6_CODSLA,		K6_NOME, 		K6_DESCRI, 
			K6_PRIORID,		K6_CODRESP,		K6_MODWF,
			K6_INIVIG,		K6_FIMVIG,		K6_STATUS,
			K6_DSUTEIS	%EXP:cFields%

	FROM	%table:SK6% SK6
	WHERE	SK6.K6_FILIAL = %xFilial:SK6% AND 	
			SK6.%NOTDEL% AND
			SK6.K6_CODSLA = %EXP:obj:id%
EndSql                                  

If (cAlias)->(!Eof())
	obj:id				:= (cAlias)->K6_CODSLA
	obj:name			:= (cAlias)->K6_NOME
	obj:description		:= (cAlias)->K6_DESCRI
	obj:priorid			:= (cAlias)->K6_PRIORID
	obj:responsible		:= (cAlias)->K6_CODRESP	
	obj:codTemplate		:= (cAlias)->K6_MODWF
	obj:iniValidity		:= (cAlias)->K6_INIVIG	
	obj:finValidity		:= (cAlias)->K6_FIMVIG
	obj:status			:= (cAlias)->K6_STATUS
	obj:utilDates		:= (cAlias)->K6_DSUTEIS	
	DbSelectArea("SK6")
	obj:timeShift		:= (cAlias)->K6_TURNO
		
	DbSelectArea(cAlias)
	DbCloseArea()     	
Else
	lRet := .F.
EndIf

Return lRet


