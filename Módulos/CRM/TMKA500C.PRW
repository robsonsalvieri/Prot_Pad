Method execute(parameterList) Class TSToolbarButton
Local uRet                           
Local aArea := GetArea()
Local cFunName := AllTrim(Self:codeBlock)
Local cPosBracket := At("(", cFunName)

If cPosBracket > 0 
	cFunName := SubStr(cFunName, 1, cPosBracket - 1)   
EndIf
uRet := Eval(&("{|aParam|" + cFunName + "(aParam)}"), parameterList)
                        
RestArea(aArea)
Return uRet      

