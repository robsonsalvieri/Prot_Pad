Function TK520IVld()

Local lRet   := .T.
Local oModel := FWModelActive()

If FwFldGet("KY_TPWF") == "2" .And. FwFldGet("KY_AUTHSRV") $ "2 "
	oModel:LoadValue('SKYMASTER',"KY_AUTHSRV","1")
EndIf

Return lRet