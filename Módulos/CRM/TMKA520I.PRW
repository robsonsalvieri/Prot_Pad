Function RunWFTemplate(cCodTemplate)
Local oWFTemplate	:= Nil
Local aWFs			:= {}
Local lRet 			:= .F.                  
Local cCodUser 		:= RetCodUsr()
Local nCount		:= 0

oWFTemplate := WFTemplate():New()
lRet := oWFTemplate:load(cCodTemplate)
If lRet
	aWFs := oWFTemplate:buildWF(cCodUser)
	For nCount := 1 To Len( aWFs )
		aWFs[nCount]:start()
		aWFs[nCount]:save()
	Next
EndIf
Return lRet

