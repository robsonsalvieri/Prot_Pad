Function Tk400Hist(cCodLista,cCodItem,cStatus) 

Local aSaveArea := ""
Local lLock		:= .T.

If Empty(cCodLista) .OR. Empty(cCodItem)
	Return .F.
EndIf	

aSaveArea := GetArea()

DbSelectArea("SU8")
DbSetorder(1)	
If MsSeek(xFilial("SU8")+cCodLista+cCodItem)
	lLock := .F.
Else
	lLock := .T.
EndIf 

BEGIN TRANSACTION 
	RecLock("SU8",lLock)
	If lLock
		Replace U8_FILIAL  WITH xFilial("SU8")
		Replace U8_CRONUM  WITH cCodLista
		Replace U8_CONTATO WITH cCodItem
	EndIf	
	
	Replace U8_DATA    WITH dDataBase
	Replace U8_HROCOR  WITH Time()
	Replace U8_STATUS  WITH cStatus

	MsUnlock()
	DbCommit()
END TRANSACTION

RestArea(aSaveArea)
Return .T.