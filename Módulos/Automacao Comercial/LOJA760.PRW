Function LJ760VldFil()

	Local lRet := .F. //Retorno da Validacao
	Local aArea := GetArea() //Controde Area
	
	SM0->( dbGoTop() )
	While !SM0->( Eof() )
		If 	AllTrim(SM0->M0_CODFIL) == AllTrim(FwFldGet("MBS_CODFIL")) .Or. ;
			Empty(FwFldGet("MBS_CODFIL")) .OR. lRet
			lRet := .T.
			Exit
		EndIf
		SM0->( dbSkip() )
	EndDo
	RestArea(aArea)
Return lRet
