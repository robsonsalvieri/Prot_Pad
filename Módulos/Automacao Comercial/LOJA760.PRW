//"Forma de Pagamento de Entrada"
DEFINE MSDIALOG oDlg TITLE STR0015 FROM 000, 000  TO 308, 241 PIXEL

	aGetBrw := GetFmEnt()
    
	DbSelectArea("SX5")
	DbSetOrder(1)
