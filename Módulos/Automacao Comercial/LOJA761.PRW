*/
Method GetMultNeg(aItens) Class LJCMultNeg

	Local cRet := "" 			// Retorno da base de dados
	Local oRet := Nil			// Retorno tratado do Method
	Local cGrupoProd := ""		// Grupo de Produto	
	Local aBkArea := GetArea()	// Backup da area
	Local nX := 0				// Contador 			
	Local cProduto 		:= ""	// Produto		
	Local nValPro		:= 0	// Valor to item
	Local nPosProd		:= 0 	// Posicao da codigo do produto
	Local nPosVlrItem   := 0 	// Posicao do Valor Total do Item	
	Local nNumItens		:= 1	// Numero do itenns ser verificado
	Local cCodGru		:= "" 	// Grupo
	//As variแveis lEmitNfce - alimentada pela rotina LjEmitNFCe, lFtvdVer12 - alimentada pela rotina - LjFTVD() e cNomeProg - alimentada pela variแvel  lFtvdVer12  tiveram seu escopo alterado para variแveis locais, pois o cliente pode mudar de modulo para o SigaFAT atrav้s do SIGAADV e essas variแveis mudam o seu valor conforme o m๓dulo
	Local lFtvdVer12	:= LjFTVD()	//Verifica se ้ Release 11.7 e o FunName ้ FATA701 - Compatibiliza็ใo Venda Direta x Venda Assisitida
	
	Default aItens		:= {}
	
	If nModulo == 12 .OR. lFtvdVer12	
		nPosProd	:= aPosCpo[Ascan(aPosCpo,{|x| Alltrim(Upper(x[1])) == "LR_PRODUTO"})][2]	// Posicao da codigo do produto
		nNumItens	:= Len(aCols)
	Else
		nNumItens	:= Len(aItens)
	EndIf	                 
	
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณAdiciona os Itensณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	For nX := 1 To nNumItens
			cProduto 	:= ""
			If nModulo == 12 .OR. lFtvdVer12
				If !aCols[nX][Len(aCols[nX])]
					cProduto 	:= aCols[nX][nPosProd]		
				EndIf	
	   		Else
	   			cProduto :=	aItens[nX][3]
	   		EndIf
		
			If!Empty(cProduto)
				Self:IdProd(cProduto, @cCodGru)
				
				oRet := Self:ValidaNeg(cProduto, cCodGru)
				
				                                                
				If oRet:Count() > 0  
					Self:TrataRetNeg(oRet)
				EndIf                                                          
				
				cCodGru		:= ""
				oRet		:= Nil
			EndIf	
	Next nX
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณRemove os Itensณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	For nX := 1 To nNumItens
		cProduto := ""
		If nModulo == 12 .OR. lFtvdVer12
			If !aCols[nX][Len(aCols[nX])]
				cProduto 	:= aCols[nX][nPosProd]		
			EndIf	
   		Else
   			cProduto :=	aItens[nX][3]
   		EndIf
		
		If!Empty(cProduto)
			Self:IdProd(cProduto, @cCodGru)
			
			Self:RmNeg(cProduto, cCodGru)
			
			cCodGru		:= ""
			oRet		:= Nil
		EndIf
	Next nX	
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณAdiciona Multi Negocia็ใo Padrใoณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	DbSelectArea("MBS")
	MBS->( DbSetOrder(4) )	//MBS_FILIAL + MBS_PADRAO
	MBS->( DbSeek(xFilial("MBS") + "1") )
	
	MA7->( !EOF() )

	While MBS->( !EoF() ) .AND. MBS->MBS_FILIAL + MBS->MBS_PADRAO ==  xFilial("MBS") + "1"
		oRet := Self:AddMnPad()
		If oRet:Count() > 0  
			Self:TrataRetNeg(oRet)
		EndIf                                                          
		MBS->( DbSkip() )
	End

	RestArea(aBkArea)

Return oRet
