Method LJAXml(cPath, cNome) Class LJAXml

	::cPath		:= cPath
	::cNomeArq	:= cNome
	
	//Verifica se o path esta vindo com barra no final
	If Substr(::cPath, Len(::cPath), 1) == "\"
		::cArquivo	:= ::cPath + ::cNomeArq	
	Else
		::cArquivo	:= ::cPath + "\" + ::cNomeArq
	EndIf
	
	::oXml		:= Nil
	::cMsg		:= ""
	::cError	:= ""
	::cWarning	:= ""
Return Self

