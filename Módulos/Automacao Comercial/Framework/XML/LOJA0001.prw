Method Salvar(cPath) Class LJAXml

	Local lRetorno  := .T.							//Retorno do metodo
	
	DEFAULT cPath := NIL
	//Salva o arquivo no disco
	If cPath <> NIl
		SAVE ::oXml XMLFILE cPath
		
		//Verifica se o arquivo foi salvo
		lRetorno := ::XmlCriado(cPath)	
	Else
		SAVE ::oXml XMLFILE ::cArquivo
		
		//Verifica se o arquivo foi salvo
		lRetorno := ::XmlCriado()	
	EndIf

Return lRetorno

