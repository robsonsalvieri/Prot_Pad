Method Destruct() Class LJACollection
	
	Local nCount   	:= 0  	//Variavel contadora
	Local oObj		:= Nil  //Variavel que recebera objeto a ser destruido

	//Atribui ao array do novo objeto um clone do array original
	For nCount := 1 To ::Count()
		
		oObj := Self:Elements(nCount)
		
		If Valtype(oObj) == "O"
            
			oObj:Destruct()
			
			If Valtype(oObj) == "O"
				FreeObj(oObj)
				oObj := NIL
			EndIf
		
		EndIf
	Next nCount
    
Return Nil

