*/
Method Clonar() Class LJCQueue

	Local oRetorno := Nil				//Retorno do metodo
	Local nCount   := 0
	
	//Instancia o objeto LJCQueue (recursividade)
	oRetorno := LJCQueue():New()
	
	//Atribui ao array do novo objeto um clone do array original
	For nCount := 1 To ::Count()
		If Valtype(::Elements(nCount)) == "O"
			oRetorno:ADD(::Elements(nCount):Clonar())
		Else
			oRetorno:ADD(::Elements(nCount))
		EndIf
	Next
	
Return oRetorno

