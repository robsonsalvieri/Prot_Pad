Method New(cTabela, lCriaEstru) Class LJAEntidade
	
	Default lCriaEstru := .T.
	
	::oConexao 	:= Nil
	::oCampos	:= Nil
	::oIndices  := Nil
	::cTabela	:= cTabela
	::nIndice	:= 1
	
	//Verifica se ira criar a estrutura automaticamente
	If lCriaEstru
		//Carrega os campos da tabela
		::CarrCampos()
		
		//Carrega os indices da tabela
		::CarrIndice()
	EndIf
		
Return Self

