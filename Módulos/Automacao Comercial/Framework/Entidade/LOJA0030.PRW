Method Create( cTable ) Class LJCEntidadeFactory
	Local oLJCEntidade := Nil

	Do Case
		Case cTable == "SM0" .Or. cTable == "SX6" .Or. cTable == "SX3" .Or. cTable == "_SCHDTSK" .Or. cTable == "XX0" .Or. cTable == "XX1" .Or. cTable == "XX2" .Or. cTable == "XX4"
			oLJCEntidade := LJCEntidadeSistemica():New( cTable )
		Otherwise
			oLJCEntidade := LJAEntidade():New( cTable )
	EndCase
Return oLJCEntidade