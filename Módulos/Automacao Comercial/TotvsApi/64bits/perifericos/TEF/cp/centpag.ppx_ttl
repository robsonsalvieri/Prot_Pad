function ResCd(); CallDll("cpIntegracionEMV", getRspCdResponse, PCHAR, PASCAL, "COMPARAMS:","clsCpIntegracionEMV","_clsCpIntegracionEMV"); return nil
function ResRef(); CallDll("cpIntegracionEMV", getTx_Reference, PCHAR, PASCAL, "COMPARAMS:","clsCpIntegracionEMV","_clsCpIntegracionEMV"); return nil
function ResVoucher(); CallDll("cpIntegracionEMV", getRspVoucher, PCHAR, PASCAL, "COMPARAMS:","clsCpIntegracionEMV","_clsCpIntegracionEMV"); return nil


