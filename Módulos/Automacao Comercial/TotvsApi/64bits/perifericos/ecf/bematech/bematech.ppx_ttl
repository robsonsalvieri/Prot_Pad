	Local nParam5 	:= 0
	Local nParam6 	:= 0


	Do Case









		Case cFunction == Upper("Bematech_FI_AbreCupomMFD")
			cParam1 := DoEncode(aParams[3])
			cParam2 := DoEncode(aParams[4])
			cParam3 := DoEncode(aParams[5])
