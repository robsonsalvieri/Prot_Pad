Function ItautZpm(cFunction, aParams, cDelimit, cFimStr)

    Local oRet		:= Nil							//Retorno da dll
	Local cBuffer 	:= ""                       	//Retorno da funcao
	Local cParam1	:= ""							//Variavel auxiliar
	Local cParam2	:= ""                         	//Variavel auxiliar
	Local cParam3	:= ""							//Variavel auxiliar
	Local cParam4	:= ""							//Variavel auxiliar
	Local cParam5	:= ""							//Variavel auxiliar
	Local cParam6	:= ""							//Variavel auxiliar
	Local cParam7	:= ""							//Variavel auxiliar
	Local cParam8	:= ""							//Variavel auxiliar
	Local cParam9	:= ""							//Variavel auxiliar
	Local cParam10	:= ""							//Variavel auxiliar
	Local cParam11	:= ""							//Variavel auxiliar
	Local cParam12	:= ""							//Variavel auxiliar
	Local cParam13  := ""							//Variavel auxiliar
	Local cParam14	:= ""							//Variavel auxiliar
	Local cParam15	:= ""							//Variavel auxiliar
	Local nParam1 	:= 0							//Variavel auxiliar
	Local nParam2 	:= 0							//Variavel auxiliar
	Local nParam3 	:= 0							//Variavel auxiliar
	Local nParam4 	:= 0 							//Variavel auxiliar
	Local nParam5 	:= 0							//Variavel auxiliar
	Local nParam6 	:= 0							//Variavel auxiliar
	
	//Executa o comando selecionado
	Do Case

		Case cFunction == Upper("E4Open")
			nParam1 := Val(aParams[3])
			nParam2 := Val(aParams[4])
			oRet 	:= IZ_E4OPEN(nParam1, nParam2)
			cBuffer := AllTrim(Str(oRet)) + cFimStr

		Case cFunction == Upper("E4Close")  
			oRet 	:= IZ_E4CLOSE()
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4RdData") 
			cParam1 := DoEncode(aParams[3])
			cParam2 := DoEncode(aParams[4])
			nParam1 := Val(aParams[5])									
			cParam3 := DoEncode(aParams[6])
			nParam2 := Val(aParams[7])									
			oRet 	:= IZ_E4RDDAT(cParam1, cParam2, nParam1, cParam3, nParam2)
			cBuffer := AllTrim(Str(oRet)) + cDelimit + DoDecode(cParam1) + cDelimit + DoDecode(cParam2) + cDelimit + AllTrim(Str(nParam1)) + cDelimit + DoDecode(cParam3) + cDelimit + AllTrim(Str(nParam2)) + cFimStr
				
		Case cFunction == Upper("E4Verao")  
			nParam1 := Val(aParams[3])											
			oRet    := IZ_E4VERAO(nParam1)
			cBuffer := AllTrim(Str(oRet)) + cFimStr
			
		Case cFunction == Upper("E4IniCup")
			nParam1 := Val(aParams[3])	
			oRet    := IZ_E4INICU(nParam1)
			cBuffer := AllTrim(Str(oRet)) + cFimStr
								
		Case cFunction == Upper("E4IniCP") 
			nParam1 := Val(aParams[3])
			cParam1 := DoEncode(aParams[4])
			oRet 	:= IZ_E4INICP(nParam1, cParam1)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4IteCF")		
			nParam1 := Val(aParams[3])
			cParam1 := DoEncode(aParams[4])
			cParam2 := DoEncode(aParams[5])
			cParam3 := DoEncode(aParams[6])
			cParam4 := DoEncode(aParams[7])
			oRet 	:= IZ_E4ITECF(nParam1, cParam1, cParam2, cParam3, cParam4)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
										  
		Case cFunction == Upper("E4IteCFEsp")										
			nParam1 := Val(aParams[3])
			cParam1 := DoEncode(aParams[4])
			cParam2 := DoEncode(aParams[5])
			cParam3 := DoEncode(aParams[6])
			cParam4 := DoEncode(aParams[7])
			oRet 	:= IZ_E4ITESP(nParam1, cParam1, cParam2, cParam3, cParam4)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4OpeCNF")
			nParam1 := Val(aParams[3])
			cParam1 := DoEncode(aParams[4])
			cParam2 := DoEncode(aParams[5])
			cParam3 := DoEncode(aParams[6])
			oRet 	:= IZ_E4OPECN(nParam1, cParam1, cParam2, cParam3)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4Print")                                                      
			cParam1 := DoEncode(aParams[3])
			oRet 	:= IZ_E4PRINT(cParam1)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4DesIt")
			nParam1 := Val(aParams[3])
			nParam2 := Val(aParams[4])
			nParam3 := Val(aParams[5])
			cParam1 := DoEncode(aParams[6])
			cParam2 := DoEncode(aParams[7])
			oRet 	:= IZ_E4DESIT(nParam1, nParam2, nParam3, cParam1, cParam2)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr  
		
		Case cFunction == Upper("E4AcrIt")
			nParam1 := Val(aParams[3])
			nParam2 := Val(aParams[4])
			nParam3 := Val(aParams[5])
			cParam1 := DoEncode(aParams[6])
			cParam2 := DoEncode(aParams[7])
			oRet 	:= IZ_E4ACRIT(nParam1, nParam2, nParam3, cParam1, cParam2)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		                                                                                                                                            
		Case cFunction == Upper("E4CanIt")
			nParam1 := Val(aParams[3])
			nParam2 := Val(aParams[4])
			cParam1 := DoEncode(aParams[5])
			cParam2 := DoEncode(aParams[6])
			oRet 	:= IZ_E4CANCI(nParam1, nParam2, cParam1, cParam2)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4DesCup")
			nParam1 := Val(aParams[3])
			cParam1 := DoEncode(aParams[4])
			cParam2 := DoEncode(aParams[5])
			oRet 	:= IZ_DESCCUP(nParam1, cParam1, cParam2)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4AcrCup")
			nParam1 := Val(aParams[3])
			cParam1 := DoEncode(aParams[4])
			cParam2 := DoEncode(aParams[5])
			oRet 	:= IZ_E4ACRCU(nParam1, cParam1, cParam2)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4RegPag")                                                                   
			nParam1 := Val(aParams[3])
			cParam1 := DoEncode(aParams[4])
			oRet 	:= IZ_E4REPAG(nParam1, cParam1)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4RegPagEsp")																		           
			nParam1 := Val(aParams[3])
			cParam1 := DoEncode(aParams[4])
			cParam2 := DoEncode(aParams[5])
			oRet 	:= IZ_E4REPES(nParam1, cParam1, cParam2)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4Cons")																		
			cParam1 := DoEncode(aParams[3])
			cParam2 := DoEncode(aParams[4])
			cParam3 := DoEncode(aParams[5])
			oRet 	:= IZ_E4CONS(cParam1, cParam2, cParam3)  
			cBuffer := AllTrim(Str(oRet)) + cFimStr
		
		Case cFunction == Upper("E4FimCup")                                    
