	Local oGroupStatus				:= LJCInitialLoadGroupStatus():New()
	Local oLJCMessageManager	:= GetLJCMessageManager()
	
	cSerializedResult := Self:Comunicate( "GetILStatusLoad" , Self:oClient:ToXML(.F.) )
	
