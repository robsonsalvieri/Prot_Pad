Method Gravar(cTexto) Class LJCLogIntegracao

	Local cArq := "\INTEGRA\" + cEmpAnt + cFilAnt + "\" + dtos(Date()) + ".log" 	//Nome do arquivo
	Local nHandle := 0																//Handle do arquivo
		
	If !File( cArq )
		MakeDir( "\INTEGRA" )
		MakeDir( "\INTEGRA\" + cEmpAnt + cFilAnt )
		nHandle := FCreate( cArq )
		FClose( nHandle )
	EndIf  
	
	If File( cArq )
	    cTexto  := Time() + " " +cTexto
		nHandle := FOpen( cArq, 2 )
		FSeek ( nHandle, 0, 2 )											// Posiciona no final do arquivo
		FWrite( nHandle, cTexto + Chr(13) + Chr(10), Len(cTexto)+2 )
		FClose( nHandle) 
	EndIf

Return Nil 
