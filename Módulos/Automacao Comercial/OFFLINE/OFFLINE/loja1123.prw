Method New(cEmpTrb, cFilTrb) Class LJCGrvEntrada
	DEFAULT cEmpTrb	:= ""					//Empresa para processamento
	DEFAULT cFilTrb	:= ""					//Filial para processamento

	::cEmpTrb	:= cEmpTrb
	::cFilTrb	:= cFilTrb
	::lPrepEnv	:= !Empty(::cEmpTrb) .AND. !Empty(::cFilTrb)
	
	::PrepEnv()

	::lRepAut	:= SuperGetMV("MV_LJRPAUT", NIL, .F.)

	::Importa()

	::ReplicaEnt()

	::ClearEnv()	
	
Return Self

