*/
Method ConverCamp(cCampo, cValor, oCampo) Class LJAAdapXMLRec

	Local cTipo := ""						//Tipo do campo no dicionario
	
	cTipo := oCampo:ElementKey(cCampo):cTipo
	
	Do Case
        //Data
		Case cTipo == "D"
		
			cValor := STOD(cValor)
		
		//Numerico
		Case cTipo == "N"
			
			cValor := Val(cValor)
			
		//Caracter
		Case cTipo == "C"
			
			cValor := cValor
			
	End Case	
	
Return Nil

