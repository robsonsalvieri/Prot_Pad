	Local oParams 	:= Nil							//Objeto do tipo LJCParamsAPI
	Local oParam 	:= Nil                         	//Objeto do tipo LJCParamAPI
	Local nCount	:= 0
	
	oParams := LJCParamsAPI():New()
	
	For nCount := 1 To Len(aDados)
		
		oParam := LJCParamAPI():New(aDados[nCount])
		
		oParams:ADD(nCount, oParam)
	Next
	
Return oParams
