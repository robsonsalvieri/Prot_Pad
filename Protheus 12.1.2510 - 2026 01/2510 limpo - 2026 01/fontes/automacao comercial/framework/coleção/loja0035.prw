Method Sort(lCrescente, cAtributo, cExpr) Class LJCList

	Local 	cBloco 		:= ""    		//Variavel para montagem do bloco de filtro

	DEFAULT lCrescente 	:= .T.
    DEFAULT cExpr 		:= NIL 
    
	 
	If !Empty(cExpr)  
		aSort(::aColecao, NIL, NIL, &cExpr)
	Else
		If Valtype(::Elements(1)) == "O"
		    cAtributo := "nome"
		    
		    If lCrescente
		    	cBloco := "{|x,y| x:" + cAtributo + "< y:" + cAtributo + "}"
			Else		    
		    	cBloco := "{|x,y| x:" + cAtributo + "> y:" + cAtributo + "}"
		 	EndIf
		 	
			
			aSort(::aColecao, NIL, NIL, &cBloco)
		Else
			If lCrescente
				cBloco := "{|x,y| x < y}"
			Else
				cbloco := "{|x,y| x > y}"
			EndIf
			
			aSort(::aColecao, NIL, NIL, &cBloco)
		EndIf  
		
	EndIf
	
Return Nil 

