*/
Method Seek( cAtributo, cKey) Class LJCList

	Local nPos := 0    		//Variavel que retornara a posicao da pesquisa
	Local cBloco := ""      //Variavel que montara o bloco de pesquisa
	
	DEFAULT cKey := ""

	If Valtype(::Elements(1)) == "O" 
	
		If !Empty(cAtributo)	   
			cBloco := "{|x| x:" + cAtributo + " == cKey }"
			nPos := aScan(::aColecao,  &cBloco)
		EndIf
	Else
		nPos := aScan(::aColecao, { |x| x == cKey})
	EndIf

Return nPos    

