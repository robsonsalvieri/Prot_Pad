	If IsAutocom(EQUIP_IMPFISCAL) == EQUIP_DLL_AUTOCOM
		nRet := oAutocom:IFAlimProp(NumPdv, NumCaixa, NomeCaixa, NumCupom)
	Else
		If IsAutocom(EQUIP_IMPFISCAL) == EQUIP_DLL_SIGALOJA
