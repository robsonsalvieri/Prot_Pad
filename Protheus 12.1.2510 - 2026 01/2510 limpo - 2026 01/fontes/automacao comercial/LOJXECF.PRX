/*/
//-----------------------------------------------
Function INFCon(lAskTry)
Local lConnected := .T.
Local nRet := 0
Local cModelo		:= LjGetStation("IMPFISC")
Local cPorta		:= LjGetStation("PORTIF")

Default lAskTry := .F.
nRet := INFTexto(".")

If nRet <> 0 
	lConnected := .F.
	Do While lAskTry
		lAskTry := MSGYESNO( "Impressora não responde. Tentar novamente?", "ERRO" )
		If lAskTry
			INFFechar()
			nRet := INFAbrir( cModelo, cPorta )
			If nRet == 0
				nRet := INFTexto(".")
			EndIf
			lAskTry  := nRet <> 0
			lConnected := !lAskTry
		EndIf
	EndDo
EndIf

Return lConnected

