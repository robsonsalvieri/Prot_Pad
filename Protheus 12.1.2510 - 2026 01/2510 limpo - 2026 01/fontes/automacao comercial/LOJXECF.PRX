	If IsAutocom( EQUIP_IMPCHEQUE ) == EQUIP_DLL_AUTOCOM
		nRet := oAutocom:CHImprTransf( cBanco, cValor, cCidade, cData, cAgencia, cConta, cMensagem )
	Else
