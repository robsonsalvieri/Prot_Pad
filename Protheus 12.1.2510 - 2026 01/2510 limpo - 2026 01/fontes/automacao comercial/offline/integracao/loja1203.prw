Method Executar() Class LJCEstrutura

	Local nConta := 0										//Variavel de controle contador                                            
	Local aDados := ::oDadosExportacao:GetExportacao()		//Array com os dados da exportacao

	//Le a estrutura de cada tabela
	For nConta = 1 to len(aDados)              
        //Instancia o objeto LJCDadosTabela
		oDadosTabela := LJCDadosTabela():new(aDados[nConta]:GetTabela())
	    //Adiciona a estrutura no array
		AADD(::aEstru, oDadosTabela)
	Next

Return Nil 

