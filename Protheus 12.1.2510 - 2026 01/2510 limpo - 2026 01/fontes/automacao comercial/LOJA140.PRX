/*/{Protheus.doc} LjVeriDoc
Verifica se o Documento possui a chave da Nfce

@author 	Rene Julian 
@since 		09/12/2024
@version	P12
@param 		cDoc, 	 Caractere, Numero do documento.
@param 		cSerie,  Caractere, Serie da nota fiscal.
@param	 	cSitua,  Caractere, Situação de cancelamento do documento
@param      cKey,    Caractere,  
@return 
/*/
//-----------------------------------------------------------------------------------------
Static Function LjVeriDoc(cDoc, cSerie , cSitua , cKey )
Local cKeySF2  := ""
Local cModelo  := "65" // NFC-e
Local cXml     := ""
Local cXmlProt := ""
Local oXML		:= Nil 
Local lXMLOk   := .F.

If Empty(cKey)
	//Verifico se na SF2 foi gravado a chave 
	SF2->(DbSetOrder(1)) //F2_FILIAL+F2_DOC+F2_SERIE+F2_CLIENTE+F2_LOJA+F2_FORMUL+F2_TIPO                                                                                                  
	If SF2->(DbSeek(xFilial("SF2")+SL1->L1_DOC+SL1->L1_SERIE+SL1->L1_CLIENTE+SL1->L1_LOJA))
		cKeySF2 := SF2->F2_CHVNFE
		cModelo := SF2->F2_ESPECIE 
	EndIf
	If Empty(cKeySF2) 		

		//Chamo função para retornar o xml do TSS 
		oXML := TXMLManager():New()
		LjNfceGet( @cXml		, @cXmlProt	, cDoc		, cSerie	,;
 				   .F.			, 1			, cModelo 	)

		lXMLOk := oXML:Parse( cXML )
		
		If lXMLOk
			lXMLOk := oXML:XPathRegisterNS( "ns", "http://www.portalfiscal.inf.br/nfe" )
			If lXMLOk
				cKeySF2 := oXML:XPathGetAtt( "/ns:NFe/ns:infNFe", "Id" )
				cKeySF2 := SubStr(cKeySF2, 4 ,Len(cKeySF2) )
			EndIf 
		EndIf

		If oXML <> NIL
			oXML := FreeObj(oXML)
			oXML := NIL
		EndIf
	EndIf 	
	If !Empty(cKeySF2)
		RecLock('SL1',.F.)
		SL1->L1_KEYNFCE := cKeySF2
		SL1->( MsUnLock() )
	EndIf 
EndIf 

Return
