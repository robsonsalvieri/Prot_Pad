	ElseIf cPaisLoc == "GUA"
		cLabelOpcao := STR0065  // "Cancelar NF(NCC)"
	EndIf
	aRotina := MenuDef()
EndIf

DbSelectArea("SL1")
SL1->(DbSetOrder(1))

//Ponto de Entrada LJ7069 - que permite a alteração do mBrowser com o filtro retornado
