/*/{Protheus.doc} LjInNfPdvOn
Faz o ajuste da SF2 e SD2 para que funcione corretamente a inutilização da NF-e com TOTVS PDV Online.

@author 	Jeferson Mondeki
@since 		25/11/2024
@version	P12
@param 		cDoc, 	 Caractere , Numero do documento.
@param 		cSerie,  Caractere , Serie da nota fiscal.
@param 		cCodCli, Caractere , Código do cliente 
@param 		cCodLoja,Caractere , Codigo da loja 
@return 	Nil 
/*/
//-----------------------------------------------------------------------------------------

Static Function LjInNfPdvOn(cDoc, cSerie, cCodCli, cCodLoja)

Local aAreaSF2 	:= SF2->(GetArea()) 	// Area selecionada.
Local aAreaSD2 	:= SD2->(GetArea()) 	// Area selecionada. 

SF2->(DbSelectArea("SF2"))
SF2->(DbSetOrder(1))//F2_FILIAL+F2_DOC+F2_SERIE+F2_CLIENTE+F2_LOJA+F2_FORMUL+F2_TIPO

If SF2->(DbSeeK(xFilial("SF2")+cDoc+cSerie))
	cCodCli 	:= SF2->F2_CLIENTE
	cCodLoja 	:= SF2->F2_LOJA

	SD2->(DbSelectArea("SD2"))
	SD2->(DbSetOrder(3))//D2_FILIAL+D2_DOC+D2_SERIE+D2_CLIENTE+D2_LOJA+D2_COD+D2_ITEM
	If SD2->(DbSeeK(xFilial("SD2")+cDoc+cSerie)) 
		While !SD2->(Eof()) .And. xFilial("SD2") == SD2->D2_FILIAL .And. SD2->D2_DOC == cDoc .And. SD2->D2_SERIE == cSerie 
			If Reclock( "SD2", .F. )
				Replace SD2->D2_ORIGLAN with "LF"
				SD2->( MsUnlock() )
			EndIf	
			SD2->( dbSkip() )
		End	
	EndIf	
EndIf

RestArea(aAreaSF2)
RestArea(aAreaSD2)

Return Nil

