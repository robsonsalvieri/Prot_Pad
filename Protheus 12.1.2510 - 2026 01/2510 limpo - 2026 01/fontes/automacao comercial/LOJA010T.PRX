Static Function L010Dec2WChr( nVal )

Local cHexFirst:="00"
Local cHexSecond:=""
Local nTmp:=0
Local cRet
If nVal > 255
	nTmp := Int( nVal / 256 )
	cHexFirst := L010Dec2Hex( nTmp )
	nTmp := nTmp * 256
Endif

nVal := nVal - nTmp
nTmp := Int( nVal / 16 )
cHexSecond :=	Right( L010Dec2Hex( nTmp ) ,1 )
nTmp := nTmp * 16
nVal := nVal - nTmp
cHexSecond +=	Right( L010Dec2Hex( nVal ) ,1)

cRet := Chr( L010Hex2Dec(cHexSecond) ) + Chr( L010Hex2Dec(cHexFirst) )

Return( cRet )
