Static Function L010VldNsu(cContr)

Local lRet 		:= .T.			// Retorno da funcao

SLV->(DbSetOrder(3))
If !SLV->(DbSeek(xFilial("SLV") + cContr))
	lRet := .F.
	//"NSU nao encontrado! Processo cancelado."		
	MsgInfo(STR0051)
Else
	If !Empty(SLV->LV_DOCCANC)
	    //"Transação TEF já cancelada!"
		MsgInfo(STR0050)
		lRet := .F.
	Endif
Endif

Return(lRet)

