Function L010WaitWin(cFile)

Local lTimeOut := .F.
Local nSecIni := Seconds()

If ValType(ntTimeOut) <> "N"
	ntTimeOut := 20
Endif

While !File(cFile)
	If (( Seconds() - nSecIni ) > ntTimeOut)
		lTimeOut := .T.
		Exit
	Endif
	Sleep(50)											// Pausa para nao consumir muitos recursos do processador
End

Return( lTimeOut )

