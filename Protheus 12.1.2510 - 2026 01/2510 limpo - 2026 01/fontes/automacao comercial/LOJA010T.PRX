*/
Function L010SolicDados(nQtdGet,cModalidade,nformaDeb,lApresenta)
nQtdGet:=20
IF 	aBlocoResp[1][6]<>"0" .AND. lApresenta
	nQtdGet += 6
Endif
IF aBlocoResp[1][12]<>"0"
	nQtdGet += 6
Endif
IF cModalidade="D" .AND. aBlocoResp[1][13]<>"0"
	If nformaDeb=1
		nQtdGet += 12
	Endif
Endif
IF L010IsCashBack() .AND. lApresenta
	nQtdGet += 6
Endif
Return (nQtdGet<>20)

