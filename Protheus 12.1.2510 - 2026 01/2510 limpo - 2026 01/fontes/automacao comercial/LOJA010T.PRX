Static Function L010InstVal( cInstitu, lSemRede )

Local nReg	:= SAE->( RecNo() )
Local aInst := {}
Local aReg	:= {}
If Empty(cInstitu)
	Return (.T.)
Endif
SAE->( dbEval( { || aAdd( aInst, Upper(Left( AE_DESC, 16 ))), ;
aAdd(aReg , Recno()) } ) )
nElem := aScan( aInst, Upper(cInstitu) )
If (nElem > 0) .AND. lCdc
	SAE->( dbGoto( aReg[nElem] ) )
	cCartao := SAE->AE_COD
ElseIf lCdc
	cCartao := ""
Endif

lSemRede := ( nElem == 0 )

SAE->( dbGoto( nReg ) )
Return ( nElem <> 0 )

