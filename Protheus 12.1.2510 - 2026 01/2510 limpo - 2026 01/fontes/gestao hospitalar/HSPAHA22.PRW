*/
Static Function FS_GrvA22(nOpcE)

If nOpcE == 3 .Or. nOpcE == 4 //Incluir e Alterar
 If nOpcE == 3
  M->GFZ_CDFRQA := HS_VSxeNum("GFZ", "M->GFZ_CDFRQA", 1)
 Endif 
 RecLock("GFZ", (nOpcE == 3))
 HS_GrvCpo("GFZ")
 MsUnlock()	
ElseIf nOpcE == 5 //Excluir
 RecLock("GFZ", .F.)
 DbDelete()
 MsUnlock()
EndIf

Return(nOpcE)


