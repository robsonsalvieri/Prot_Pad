*/
Function HS_VldA22()
 Local lRet  := .T.
 Local aArea := GetArea()

 If lRet := IIF(Altera, FS_VldAlt(), .T.)
 
  If ReadVar() == "M->GFZ_UNFRQA"
   M->GFZ_DUFRQA := 0
   M->GFZ_PEFRQA := 0
 
  ElseIf ReadVar() == "M->GFZ_DUFRQA"
   If lRet := (M->GFZ_DUFRQA >= 0)
    If M->GFZ_UNFRQA == "H" .And. !(lRet := M->GFZ_DUFRQA <= 24)
     HS_MsgInf(STR0011, STR0008, STR0010) //"A Dura็ใo nใo pode ser maior que 24 horas!"###"Aten็ใo"###"Valida็ใo de Inclusใo"    		         
    ElseIf M->GFZ_UNFRQA == "M" .And. !(lRet := M->GFZ_DUFRQA <= 1440)
     HS_MsgInf(STR0012, STR0008, STR0010) //"A Dura็ใo nใo pode ser maior que 1440 minutos!"###"Aten็ใo"###"Valida็ใo de Inclusใo"    		         
    Else
     M->GFZ_PEFRQA := 0
    EndIf 
   EndIf
       
  ElseIf ReadVar() == "M->GFZ_PEFRQA"
   If lRet := (M->GFZ_PEFRQA >= 0)
    If M->GFZ_UNFRQA == "H" .And. !(lRet := M->GFZ_PEFRQA <= 24)
     HS_MsgInf(STR0013, STR0008, STR0010) //"O Perํodo nใo pode ser maior que 24 horas!"###"Aten็ใo"###"Valida็ใo de Inclusใo"    		      
    ElseIf M->GFZ_UNFRQA == "M" .And. !(lRet := M->GFZ_PEFRQA <= 1440)
     HS_MsgInf(STR0014, STR0008, STR0010) //"O Perํodo nใo pode ser maior que 1440 minutos!"###"Aten็ใo"###"Valida็ใo de Inclusใo"    		     
    ElseIf !(lRet := M->GFZ_PEFRQA <= M->GFZ_DUFRQA)
     HS_MsgInf(STR0015, STR0008, STR0010) //"O Perํodo nใo pode ser maior que a Dura็ใo!"###"Aten็ใo"###"Valida็ใo de Inclusใo"    		  
    ElseIf !(lRet := Mod(M->GFZ_DUFRQA, M->GFZ_PEFRQA) == 0)
     HS_MsgInf(STR0016, STR0008, STR0010) //"O valor informado no perํodo deve ser m๚ltiplo do valor informado na Dura็ao!"###"Aten็ใo"###"Valida็ใo de Inclusใo"    
    EndIf
   Endif 
  EndIf
  
 EndIf
 
 RestArea(aArea)

Return(lRet)    

