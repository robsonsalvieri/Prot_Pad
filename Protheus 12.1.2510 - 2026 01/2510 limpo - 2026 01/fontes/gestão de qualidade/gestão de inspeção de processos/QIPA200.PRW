/*/
FUNCTION A200CIQI()
Local nIqi_Inf := 0.00

DbSelectArea("QPG")
DbSetOrder(2)  // Chave: IQI Superior
DbSeek(xFilial("QPG"))
While QPG_FILIAL == xFilial("QPG") .and. ! Eof()
	RecLock("QPG",.F.)
	QPG->QPG_IQP_I := nIqi_Inf
	nIqi_Inf := QPG->QPG_IQP_S + 0.01
	DbSkip()
Enddo
DbSetOrder(1)
Return nil                      

