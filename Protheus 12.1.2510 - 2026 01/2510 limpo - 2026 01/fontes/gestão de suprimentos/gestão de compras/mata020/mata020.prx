/*/{Protheus.doc} M020APICGC
	TOTVS API CAROL - Função para preenchimento automatico dos campos da SA2, de acordo com o retorno JSON
	pela função APIFORCLI.
	@type  Function
	@author Fabiano Dantas
	@since 29/12/2020
	@param 
	@return return_var, return_type, return_description
	@example
	(examples)
	@see (links_or_references)
	/*/

Function M020APICGC()

	Local oModel 	  := FWModelActive()
	Local oField 	  := Nil
	Local cA2CGC   	  := FWFldGet("A2_CGC")
	Local nLenA2CGC   := Len(cA2CGC)
	Local oJson  	  := JsonObject():New()
	Local aRetJson 	  := {}
	Local lMVAPIFOR   := SuperGetMv("MV_APIFOR", .F., .F.)
	Local lFNAPIFOR   := FindFunction("APIFORCLI")
	Local cTel		  := ""
	Local aTel        := {}
	Local cRet        := ""
	Local lRelease	  := .F.
	Local cVersao	  := GetRpoRelease()

//Funcionalida a partir da release 12.1.27
	If cVersao >= "12.1.027"
		lRelease := .T.
	Endif

	If lRelease
		If Len(AllTrim(cA2CGC)) > 11 .And. lMVAPIFOR .And. lFNAPIFOR .And. !ISBLIND()

			aRetJson := APIFORCLI(cA2CGC)
			oJson:FromJson(aRetJson[2])
			cRet := oJson:GetJsonObject("hits")

			If cRet <> Nil

				If Len(cRet) > 0 .And. aRetJson[1]
					If oModel <> Nil
						oField := oModel:GetModel("SA2MASTER")

						If !Empty(oField:GetValue( 'A2_NREDUZ'))//Tratamento para limpar o campo caso houver uma nova consulta de CNPJ
							oField:ClearField('A2_NREDUZ')
						EndIf

						oField:SetValue( 'A2_NOME'  , PadR(Upper(DecodeUTF8(oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmname"])),TamSX3("A2_NOME")[1]))
						oField:SetValue( 'A2_NREDUZ', PadR(Upper(oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmdba"]),TamSX3("A2_NREDUZ")[1]))
						oField:SetValue( 'A2_EST'   , oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmaddress"][1]["mdmstate"])
						oField:SetValue( 'A2_CEP'   , oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmaddress"][1]["mdmzipcode"])

						If ValType(oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmaddress"][1]["mdmaddress3"]) == "C"
							oField:SetValue( 'A2_BAIRRO', PadR(Upper(oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmaddress"][1]["mdmaddress3"]),TamSX3("A2_BAIRRO")[1]))
						Endif

						If ValType(oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmaddress"][1]["mdmcity"]) == "C"
							oField:SetValue( 'A2_MUN'   , PadR(Upper(oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmaddress"][1]["mdmcity"]),TamSX3("A2_MUN")[1]))
						Endif

						If ValType(oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmaddress"][1]["mdmaddress1"]) == "C"
							oField:SetValue( 'A2_END'   , PadR(Upper(oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmaddress"][1]["mdmaddress1"]),TamSX3("A2_END")[1]))
						Endif

						oField:SetValue( 'A2_TIPO'  , Iif(nLenA2CGC == 11, "F", Iif(nLenA2CGC == 14, "J", "X")))
						oField:SetValue( 'A2_CNAE'  , oJson["hits"][1]["mdmGoldenFieldAndValues"]["cnaebr"])
						cTel := oJson["hits"][1]["mdmGoldenFieldAndValues"]["mdmphone"][2]["mdmphonenumber"]
						If !Empty(cTel)
							aTel := RemDddTel(cTel)
							oField:SetValue( 'A2_DDD'   , aTel[2])
							oField:SetValue( 'A2_TEL'   , aTel[1])
						EndIf

					EndIf
				Else
					If Len(cRet) == 0 .And. aRetJson[1]
						MsgAlert(STR0100)//"Não existe no Cadastro de Pessoas Jurídicas o número de CNPJ informado. Verifique se o mesmo foi digitado corretamente.
						lRet := .F.
					EndIf
				EndIf
			Else
				Alert(STR0098+aRetJson[2])//Erro ao carregar os dados do fornecedor
				lRet := .F.
			EndIf
		Else
			If !lFNAPIFOR
				Alert(STR0099)//Função APIFORCLI não compilada no RPO
				lRet := .F.
			EndIf
		Endif
	Endif

Return Nil

/*/{Protheus.doc} A020Commit
	Commit do cadastro de fornecedor

@Param oModel	Modelo - Fornecedor

@author rodrigo.mpontes
@since 25/05/2021
@return Nil, indefinido
/*/

Function A020Commit(oModel)

	Local lMVAPIFOR   := SuperGetMv("MV_APIFOR", .F., .F.)
	Local lFNAPIFOR   := FindFunction("APIFORCLI")
	Local cVersao	  := GetRpoRelease()
	Local cCGC		  := FWFldGet("A2_CGC")
	Local lAPI		  := .F.
	Local nOperation  := oModel:GetOperation()
	Local cEventID    := "036" //Inclusao de fornecedor
	Local cMensagem	  := ""
	Local lRet		  := .T.
	Local lDKE		  := ChkFile("DKE") .And. Len(COMXHDCO("DKE")) > 0
	Local lDHT		  := ChkFile("DHT")
	Local aAreaSA5 	  := SA5->(GetArea())
	Local lImpCadAut  := IsInCallStack("IMPCADAUTF") // Inclusão automatica, importador xml
	Local lDkeOrigem  := lDKE .And. DKE->(FieldPos("DKE_ORIGEM")) > 0
	Local cRelease      := GetRPORelease()
	Local cUpd          := ""
	Local nOrder        := 1
	Local oUpd    		:= Nil

	If nOperation == MODEL_OPERATION_INSERT

		If cVersao >= "12.1.027" .And. lMVAPIFOR .And. lFNAPIFOR .And. !ISBLIND() .And. Len(AllTrim(cCGC)) > 11
			lAPI := .T.
		Endif

		ComMetric("-inc",lAPI)
	Endif

//Gravação do campo DKE_ORIGEM 
	If nOperation == MODEL_OPERATION_INSERT .and. lDkeOrigem
		If lImpCadAut
			oModel:LoadValue("SA2DKE","DKE_ORIGEM","1") //Imp/Cadastro
		else
			oModel:LoadValue("SA2DKE","DKE_ORIGEM","3") //Manual
		Endif
	endif

	lRet := FWFormCommit(oModel)

//Se não houver atualização na SA2, mas sim na DHT ou DKE e tabela SA2 tiver STAMP
//campo sera atualizado - TAF (REINF)
	If lRet .And. !oModel:GetModel("SA2MASTER"):IsModified() .And. nOperation == MODEL_OPERATION_UPDATE .And. Ascan(TCStruct(RetSqlName("SA2")),{|x|x[1]=='S_T_A_M_P_'}) > 0 //Verifica se possui S_T_A_M_P_ na SA2

		If (lDHT .And. (oModel:GetModel("SA2DEP"):IsInserted() .Or. oModel:GetModel("SA2DEP"):IsUpdated() .Or. oModel:GetModel("SA2DEP"):IsDeleted())) .Or. ;
			(lDKE .And. oModel:GetModel("SA2DKE"):IsModified())

			//Tratamento para o STAMP da tabela SA2 ser atualizado.
			cUpd 	:= "UPDATE " + RetSqlName("SA2") + " SET D_E_L_E_T_ = ? WHERE A2_FILIAL = ? AND A2_COD = ? AND A2_LOJA = ? AND D_E_L_E_T_ = ? "
			nOrder	:= 1

			oUpd 	:= FWPreparedStatement():New()

			oUpd:SetQuery(cUpd)

			oUpd:SetString(nOrder++,Space(1))
			oUpd:SetString(nOrder++,FWXFilial("SA2"))
			oUpd:SetString(nOrder++,FwFldGet("A2_COD"))
			oUpd:SetString(nOrder++,FwFldGet("A2_LOJA"))
			oUpd:SetString(nOrder++,Space(1))

			cUpd := oUpd:GetFixQuery()

			TcSqlExec(cUpd)

			FreeObj(oUpd)
		Endif
	Endif

	If lRet .And. nOperation == MODEL_OPERATION_INSERT
		If ( FindFunction("COMTemSXI") .And. COMTemSXI( cEventId ) )
			cMensagem := STR0104+SA2->A2_COD+"/"+SA2->A2_LOJA+CHR(13)+CHR(10)+STR0105+SA2->A2_NOME+CHR(13)+CHR(10)+STR0106+Subs(cUsuario,7,15)+"." // "Incluído Fornecedor. Código/Loja: # Razão Social: # "Incluído no sistema pelo usuário: "
			EventInsert(FW_EV_CHANEL_ENVIRONMENT, FW_EV_CATEGORY_MODULES, cEventID,FW_EV_LEVEL_INFO,""/*cCargo*/,STR0107,cMensagem,.T./*lPublic*/) //"Inclusão de Fornecedor"
		ElseIf cRelease <= "12.1.2310"
			MEnviaMail(cEventID,{SA2->A2_FILIAL,SA2->A2_COD,SA2->A2_LOJA,SA2->A2_NOME,SA2->A2_NREDUZ,CUSERNAME})
		EndIf
	Endif

	If lRet .And. cPaisLoc == "COL" .And. FindFunction("FATXDELEMI")
		FATXDELEMI(oModel,nOperation)
	EndIf

//Atualiza o nome do fornecedor nos cadastros de produto x fornecedor.
	If lRet .and. nOperation == MODEL_OPERATION_UPDATE .and. oModel:GetModel("SA2MASTER"):IsFieldUpdated('A2_NOME')
		DbSelectArea("SA5")
		SA5->(DbSetOrder(1))//A5_FILIAL + A5_FORNECE + A5_LOJA
		If SA5->(Msseek(fwxFilial("SA5") + Fwfldget("A2_COD") + Fwfldget("A2_LOJA")))
			
			cUpd 	:= "UPDATE " + RetSqlName("SA5") + " SET A5_NOMEFOR = ? WHERE A5_FILIAL = ? AND A5_FORNECE = ? AND A5_LOJA = ? AND D_E_L_E_T_ = ? "
			nOrder	:= 1

			oUpd 	:= FWPreparedStatement():New()
			
			oUpd:SetQuery(cUpd)

			oUpd:SetString(nOrder++,Alltrim(FwFldGet("A2_NOME")))
			oUpd:SetString(nOrder++,FWXFilial("SA5"))
			oUpd:SetString(nOrder++,FwFldGet("A2_COD"))
			oUpd:SetString(nOrder++,FwFldGet("A2_LOJA"))
			oUpd:SetString(nOrder++,Space(1))

			cUpd := oUpd:GetFixQuery()

			TcSqlExec(cUpd)

			FreeObj(oUpd)
		EndIf
		RestArea(aAreaSA5)
	Endif

Return lRet

