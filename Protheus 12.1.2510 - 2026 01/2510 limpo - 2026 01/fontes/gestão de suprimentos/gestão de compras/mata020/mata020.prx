/*/{Protheus.doc} A020CarEsp()
Função que limpa os caracteres especiais dentro de um campo.

@author rodrigo.mpontes
@since 09/11/22
@version 12
/*/

Function A020CarEsp()

	Local aArea       := GetArea()
	Local cCampo      := ReadVar()
	Local cCpo		  := StrTran(cCampo,"M->")
	Local cConteudo   := &(cCampo)
	Local aCarEsp	  := {"'","#","%","*","&",">","<","!","@","$","(",")","_","=","+","{","}","[","]","?","\","|",":",";",'"','°','ª'}
	Local nI		  := 0
	Local nX  		  := 0
	Local oModSA2	  := FwModelActive()
	Local oViSA2      := FWViewActive()


	For nI := 1 To Len(cConteudo)
		If IsAlpha(SubStr(cConteudo,nI,1)) .Or. IsDigit(SubStr(cConteudo,nI,1)) ;
				.Or. SubStr(cConteudo,nI,1) == "."
			nX := nI
			Exit
		Endif
	Next nI

	If nX > 0
		cConteudo := SubStr(cConteudo,nX)
	Endif

	For nI := 1 To Len(aCarEsp)
		If aCarEsp[nI] == "&"
			cConteudo := StrTran(cConteudo,aCarEsp[nI],"E")
		Else
			cConteudo := StrTran(cConteudo,aCarEsp[nI]," ")
		Endif
	Next nI

//Adicionando os espaços a direita
	cConteudo := PadR(cConteudo,TamSx3(cCpo)[1])
//Definindo o conteúdo do campo
	If ValType(oModSA2) == "O"
		oModSA2:LoadValue("SA2MASTER",cCpo,cConteudo)
	Endif
	&(cCampo+" := '"+cConteudo+"' ")

	If ValType(oViSA2) == "O" .And. oViSA2:IsActive() .And. (aScan(oViSA2:GetModelSIds(),{|x| AllTrim(x) == 'SA2MASTER'})>0)
		oViSA2:Refresh('SA2MASTER')
		if ReadVar() <> cCampo
			__ReadVar := cCampo
		endif
	Endif

	RestArea(aArea)

Return .T.

/*/{Protheus.doc} A020OriInc
	(Validação do campo DKE_ORIGEM)
	@type  Function
	@author Thiago Rodrigues
	@since 07/02/2024
	@version version
	@see (links_or_references)
/*/
Function A020OriInc()
	Local lRet       := .T.
	Local oModelAtv  := FwModelActive()
	local nOperation := oModelAtv:GetOperation()
	local oModelSA2  := nil
	local cNewOri    := ""
	local cOriAnt    := ""

//Obtem o valor atual do campo Origem
	cNewOri := oModelAtv:Getmodel("SA2DKE"):getvalue("DKE_ORIGEM")

	if nOperation == 3
		//Operação de inclusão
		if cNewOri != "3"
			lRet := .f.
			Help(nil, nil , STR0011, nil, STR0113, 1, 0, nil, nil, nil, nil, nil, {} )//Atenção Para inclusão manual, utilize o valor 3.
		endif

	elseif nOperation == 4
		//Operação de alteração
		oModelSA2 := oModelAtv:getModel("SA2MASTER")
		cOriAnt := GetAdvFVal("DKE","DKE_ORIGEM",oModelSA2:Getvalue("A2_FILIAL") + oModelSA2:Getvalue("A2_COD") +;
			oModelSA2:Getvalue("A2_LOJA"),1)

		//Só permite alteração dos status de 1 para 2 e '' para 3
		if !( (cOriAnt == "1" .And. cNewOri == "2") .Or. (empty(cOriAnt) .And. cNewOri == "3") )
			Help(nil, nil , STR0011, nil, STR0112, 1, 0, nil, nil, nil, nil, nil, {} )//Atenção As alterações permitidas para este campo são: 1=Imp/Cadastro para 2=Imp/Revisado e em branco para 3=manual."
			lRet := .F.
		endif

	Endif

Return lRet

