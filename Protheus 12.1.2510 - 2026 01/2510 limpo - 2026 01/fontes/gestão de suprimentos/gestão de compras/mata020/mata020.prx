/*/
Static Function MATAFN()
	Local cTel := ""
	Local n := 0
	Local nTam := 0

	cTel := AllTrim(M->A2_DDD+M->A2_TEL)

	n := AT("-",cTel)
	If n > 0
		cTel := SubStr(cTel,1,n-1)+SubStr(cTel,n+1,Len(cTel))
	EndIf

	nTam := Len(AllTrim(cTel))
	If SubStr(cTel,1,1) == "0"
		cTel := SubStr(cTel,2,nTam)
	EndIf
	If nTam <= 8
		cTel := "xx"+cTel
	EndIf

Return cTel
