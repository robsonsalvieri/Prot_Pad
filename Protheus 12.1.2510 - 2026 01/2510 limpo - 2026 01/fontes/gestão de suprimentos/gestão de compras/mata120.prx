Static aDlgFields 	:= NIL
Static cCpsObrig  	:= ""
Static lPLSMT120  	:= FindFunction("PLSMT120")

Static lIsRussia  	:= (cPaisLoc $ "RUS")	 // Flag to indicate location, for Russia
