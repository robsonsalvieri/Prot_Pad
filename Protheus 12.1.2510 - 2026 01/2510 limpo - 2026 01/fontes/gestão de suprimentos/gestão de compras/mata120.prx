Local aArea	   := GetArea()
Local aGrupo   := {}
Local cGrupo   := ""
Local cUser    := A120ChUEAI()
Local nLoop    := 0
Local lRet     := .T.
Local lSolic   := .T.
Local lRestPed := If(SuperGetMv("MV_RESTPED")=="S", .T. , .F. )
Local lCnmdAlc := If(SuperGetMv("MV_CNMDALC")=="S", .T. , .F. )	 // Define se o controle de alçada é realizado na Medição de Contrato
Local lCN121Est := FwIsInCallStack("CN121Estorn")
Local lAjMsblq 	:= SAJ->(FieldPos("AJ_MSBLQL ")) > 0

aGrupo := UsrGrComp(cUser)

If aScan(aGrupo,"*") != 0
	lSolic := .F.
Else
	For nLoop := 1 to Len(aGrupo)
		cGrupo += aGrupo[nLoop]
	Next nLoop
EndIf

If !Empty(cUser) .And. lAjMsblq .And. lRestPed //Valida se o usuário está bloqueado para uso caso o campo AJ_MSBLQL tenha sido habilitado
	dbSelectArea("SAJ")
	dbSetOrder(1) // AJ_FILIAL, AJ_GRCOM, AJ_ITEM, R_E_C_N_O_, D_E_L_E_T_
	If dbSeek(xFilial("SAJ")+SC7->C7_GRUPCOM)
		While !Eof() .AND. SAJ->AJ_GRCOM == SC7->C7_GRUPCOM
			If SAJ->AJ_USER == cUser .And. !RegistroOk("SAJ",.F.)
				lRet := .F.
				Help(,,"A120REST",,STR0331+SAJ->AJ_US2NAME+STR0332, 1, 0,,,,,,{STR0333})
				Exit
			EndIf
			dbSkip()
		EndDo
	EndIf
EndIf

If ( lRet .And. lSolic .And. lRestPed .And. !(lCnmdAlc .And. lCN121Est ) .And. !(SC7->C7_GRUPCOM $ cGrupo) .And. SC7->C7_USER != cUser .And. !Empty(SC7->C7_USER))
	If Empty(SC7->C7_GRUPCOM)
		Help("  ",1,"A120RSPED",,UsrRetName(SC7->C7_USER),4,11)
	Else
		Help("  ",1,"USUNAOAUT",,SC7->C7_GRUPCOM,3,25)
	EndIf
	lRet := .F.
EndIf

RestArea(aArea)

Return lRet

