			FillGetDados(nOpcX,"SC7",1,cSeek,{|| &cWhile },,aNoFields,,,,,,,, {|aColsX| A120AfterC(aColsX,aRefImpos,l120Altera,l120Deleta,lCopia,@lContinua,@cItCop,lGrade,l120Inclui,lTrbGen,@aItOrig)},,IIF(cModulo == "EIC",{|aHeaderX| A120AfterH(l120Auto,aHeaderX)},),"SC7")
