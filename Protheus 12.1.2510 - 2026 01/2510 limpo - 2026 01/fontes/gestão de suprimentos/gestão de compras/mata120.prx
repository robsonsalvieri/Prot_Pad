Function AFwAllMark(oMark,cCampoOk)
Local lRet		:= .T.		  	 						// Retorno da rotina.
Local aAreaTmp	:= (oMark:Alias())->(GetArea())	  		// Guarda a area do browse.
Local lTop 		:= .T.	  								// Posiciona no primeiro registro.
Local cMsg		:= ""

Default cCampoOk := "C1_OK"

If EMPTY(aMarca)
	cMsg	:= STR0354 // "Deseja Selecionar Todos os Registros ?"
	lInverte := .F.
Else
	cMsg	:= STR0355 // "Deseja Inverter a Seleção de Todos os Registros ?"
	lInverte := .T.
EndIf

If MsgYesNo(cMsg,OemToAnsi(STR0356)) // "ATENÇÃO"

	(oMark:Alias())->(DbGoTop())
	While (oMark:Alias())->(!Eof())
		If lInverte
			If ( !oMark:IsMark() )
				RecLock(oMark:Alias(),.F.)
				(oMark:Alias())->&cCampoOk  := oMark:Mark()
				(oMark:Alias())->(MsUnLock())
				aAdd(aMarca, (oMark:Alias())->recno)
			Else
				RecLock(oMark:Alias(),.F.)
				(oMark:Alias())->&cCampoOk  := " "
				(oMark:Alias())->(MsUnLock())
				nPos := (aScan(aMarca, (oMark:Alias())->recno))
				If nPos > 0
					aDel(aMarca, nPos); aSize(aMarca, Len(aMarca)-1)
				EndIf
			EndIf
		Else
			RecLock(oMark:Alias(),.F.)
			(oMark:Alias())->&cCampoOk  := oMark:Mark()
			(oMark:Alias())->(MsUnLock())
			aAdd(aMarca, (oMark:Alias())->recno)
		EndIf
		(oMark:Alias())->(DbSkip())
	EndDo
Endif

RestArea(aAreaTmp)
oMark:oBrowse:Refresh()
Return lRet
