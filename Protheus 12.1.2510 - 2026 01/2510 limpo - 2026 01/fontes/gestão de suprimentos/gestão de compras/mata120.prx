		Endif
	Else
		If SoftLock("SC3") .And. Empty(cFlag)
			aArrayF4[oQual:nAt,1] := !aArrayF4[oQual:nAt,1]
			SimpleLock("SC3")
		ElseIf !Empty(cFlag)
			MsgInfo( STR0130 , STR0102 ) //Registro utilizado por outro usuário.
			If aArrayF4[oQual:nAt,1]
				aArrayF4[oQual:nAt,1] := !aArrayF4[oQual:nAt,1]
			Endif
		Endif
	Endif

	If !aArrayF4[oQual:nAt,1]
		If nTipoPed != 2
			If SimpleLock("SC1")
				SC1->(MsRUnlock())
			Endif
		Else
			If SimpleLock("SC3")
				SC3->(MsRUnlock())
			Endif
		Endif
	Endif
Endif

RestArea(aArea)
aSize(aArea,0)
aArea := nil

Return

