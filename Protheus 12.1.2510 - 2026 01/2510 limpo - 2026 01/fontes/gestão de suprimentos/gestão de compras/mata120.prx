Function A120BuRtSC(cChaveRat,aHeadSCH,aColsSCH)
Local aAreaSCX   := SCX->(GetArea())
Local nPRateio   := GDFieldPos("C7_RATEIO")
Local cItemSCH 	 := ""
Local nItemSCH   := 0
Local cCampoCH	 := ""
Local cCampoCx	 := ""
Local nY	     := 0
Local aHeadSCX	 := {}
Local nX 		As Numeric
Local aCampSCX 	As Array
Local aCampSCH 	As Array

If Type("n") == "U"
	n := 1
EndIf

aCampSCX := FWSX3Util():GetAllFields("SCX")
aCampSCH := FWSX3Util():GetAllFields("SCH")

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Montagem do aHeader SCX  ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
For nX := 1 to Len(aCampSCX)
	If X3Uso(GetSX3Cache(aCampSCX[nX],"X3_USADO")) .And. cNivel >= GetSX3Cache(aCampSCX[nX],"X3_NIVEL") .And. ;
		!Alltrim(GetSX3Cache(aCampSCX[nX],"X3_CAMPO")) $ "CX_CUSTO#CX_FILIAL#CX_SOLICIT" .And. AllTrim(GetSX3Cache(aCampSCX[nX],"X3_CAMPO")) != "CX_ITEMSOL"

			Aadd(aHeadSCX,{ Alltrim(GetSX3Cache(aCampSCX[nX], "X3_TITULO")),;
			GetSX3Cache(aCampSCX[nX], "X3_CAMPO")   	,;
			GetSX3Cache(aCampSCX[nX], "X3_PICTURE")  	,;
			GetSX3Cache(aCampSCX[nX], "X3_TAMANHO")  	,;
			GetSX3Cache(aCampSCX[nX], "X3_DECIMAL")  	,;
			GetSX3Cache(aCampSCX[nX], "X3_VALID")   	,;
			GetSX3Cache(aCampSCX[nX], "X3_USADO")   	,;
			GetSX3Cache(aCampSCX[nX], "X3_TIPO")   		,;
			GetSX3Cache(aCampSCX[nX], "X3_F3")			,;
			GetSX3Cache(aCampSCX[nX], "X3_CONTEXT") } )
	EndIf
Next nX

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Adiciona os campos de Alias e Recno ao aHeader para WalkThru.³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
ADHeadRec("SCX",aHeadSCX)

If Empty(aHeadSCH)
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Montagem do aHeader SCH ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	For nX := 1 to Len(aCampSCH)
		If X3Uso(GetSX3Cache(aCampSCH[nX],"X3_USADO")) .And. cNivel >= GetSX3Cache(aCampSCH[nX],"X3_NIVEL") .And.;
			!Alltrim(GetSX3Cache(aCampSCH[nX],"X3_CAMPO")) $ "CH_CUSTO#CH_FILIAL#CH_PEDIDO#CH_FORNECE#CH_LOJA" .And. AllTrim(GetSX3Cache(aCampSCH[nX],"X3_CAMPO")) != "CH_ITEMPED"

			Aadd(aHeadSCH,{ Alltrim(GetSX3Cache(aCampSCH[nX], "X3_TITULO")),;
				GetSX3Cache(aCampSCH[nX], "X3_CAMPO")   	,;
				GetSX3Cache(aCampSCH[nX], "X3_PICTURE")  	,;
				GetSX3Cache(aCampSCH[nX], "X3_TAMANHO")  	,;
				GetSX3Cache(aCampSCH[nX], "X3_DECIMAL")  	,;
				GetSX3Cache(aCampSCH[nX], "X3_VALID")   	,;
				GetSX3Cache(aCampSCH[nX], "X3_USADO")   	,;
				GetSX3Cache(aCampSCH[nX], "X3_TIPO")   		,;
				GetSX3Cache(aCampSCH[nX], "X3_F3")			,;
				GetSX3Cache(aCampSCH[nX], "X3_CONTEXT") } )
		EndIf
	Next nX

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Adiciona os campos de Alias e Recno ao aHeader para WalkThru.³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	ADHeadRec("SCH",aHeadSCH)
EndIf

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Montagem do aCols SCH  ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
SCX->(dbSetOrder(1))
SCX->(dbSeek(xFilial("SCX")+cChaveRat))
While SCX->(!Eof()) .And. SCX->(CX_FILIAL+CX_SOLICIT+CX_ITEMSOL) == xFilial("SCX")+cChaveRat
	If cItemSCH <> SCX->CX_ITEMSOL
		cItemSCH := SCX->CX_ITEMSOL
		aAdd(aColsSCH,{StrZero(N,Len(SC7->C7_ITEM)),{}})
		nItemSCH := Len(aColsSCH)
		aCols[N][nPRateio] := "1"
	EndIf

	aAdd(aColsSCH[nItemSCH][2],Array(Len(aHeadSCH)+1))
		For nY := 1 to Len(aHeadSCH)
			cCampoCH := Substr(aHeadSCH[nY][2],4,len(aHeadSCH[nY][2])-3)
			cCampoCx := "CX_"+cCampoCH
			If IsHeadRec(aHeadSCH[nY][2])
				aColsSCH[nItemSCH][2][Len(aColsSCH[nItemSCH][2])][nY] := 0
			ElseIf IsHeadAlias(aHeadSCH[nY][2])
				aColsSCH[nItemSCH][2][Len(aColsSCH[nItemSCH][2])][nY] := "SCH"
			ElseIf (aHeadSCH[nY][10] <> "V") .And. SCX->(FieldPos(cCampoCx)) > 0
				If  aHeadSCH[nY][8] == "M"
					aColsSCH[nItemSCH][2][Len(aColsSCH[nItemSCH][2])][nY] := SCX->&(cCampoCx)
				Else
					aColsSCH[nItemSCH][2][Len(aColsSCH[nItemSCH][2])][nY] := SCX->(FieldGet(FieldPos(cCampoCx)))
				Endif
			Else
				aColsSCH[nItemSCH][2][Len(aColsSCH[nItemSCH][2])][nY] := SCH->(CriaVar(aHeadSCH[nY][2]))
			EndIf
			aColsSCH[nItemSCH][2][Len(aColsSCH[nItemSCH][2])][Len(aHeadSCH)+1] := .F.
		Next nY
	SCX->(dbSkip())
EndDo

FwFreeArray(aCampSCX)
FwFreeArray(aCampSCH)

RestArea(aAreaSCX)
aSize(aAreaSCX,0)
aAreaSCX := nil

Return()

