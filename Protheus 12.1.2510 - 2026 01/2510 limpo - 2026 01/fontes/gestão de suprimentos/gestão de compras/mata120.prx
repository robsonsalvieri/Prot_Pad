		EndIf
	Else
		If !(lCopia .And. aArea[1] == 'SC7')
			RestArea(aArea)
			aSize(aArea,0)
			aArea := nil
		EndIf
	EndIf
EndIf
__lPedNFC := .F.
Return(nOpcA)

