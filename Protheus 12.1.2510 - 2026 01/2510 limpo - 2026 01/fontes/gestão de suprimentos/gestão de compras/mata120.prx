#DEFINE TOTPED		6  // Total do Pedido
#DEFINE SEGURO		7  // Valor total do seguro
#DEFINE TOTF3		8  // Total utilizado no Folder 3
#DEFINE IMPOSTOS	9  // Array contendo Os Valores de Impostos Exibidos no ListBox
