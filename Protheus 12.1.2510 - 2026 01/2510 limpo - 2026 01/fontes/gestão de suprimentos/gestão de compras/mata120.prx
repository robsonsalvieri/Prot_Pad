@param nTipoPed, numeric, 1 = pedido de compra; 2 = autorização de entrega
/*/
Function A120PCO553(cTipo, cNumPed, lDeleta, lSeekSCR, nTipoPed)

Local aAreaMem  := FWGetArea()
Local aAreaSCR  := SCR->(FWGetArea())
Local aAreaDBM  := DBM->(FWGetArea())
Local aAreaSC7  := SC7->(FWGetArea())
Local aAreaSCH  := SCH->(FWGetArea())
Local cQuery    := ""
Local cAliasSql := GetNextAlias()
Local oQry		:= Nil
Local nOrder 	:= 1

Default cTipo    := ""
Default cNumPed  := ""
Default lDeleta  := .F.
Default lSeekSCR := .F.
Default nTipoPed := 1

If lSeekSCR

	SCR->(DBSetOrder(1)) // Indice 1 - CR_FILIAL+CR_TIPO+CR_NUM+CR_NIVEL
	If nTipoPed == 2 .And. SCR->(DBSeek(FWxFilial('SCR')+'AE'+cNumPed))
		cTipo := SCR->CR_TIPO
	Else
		If nTipoPed == 1 .And. (SCR->(DBSeek(FWxFilial('SCR')+'PC'+cNumPed)) .Or. SCR->(DBSeek(xFilial('SCR')+'IP'+cNumPed)))
			cTipo := SCR->CR_TIPO
		EndIf
	EndIf

EndIf

If cTipo == "IP"

	cQuery += "SELECT SCR.R_E_C_N_O_ RECSCR, DBM.R_E_C_N_O_ RECDBM, SC7.R_E_C_N_O_ RECSC7, SCH.R_E_C_N_O_ RECSCH "
	cQuery += "FROM ? SCR "
	cQuery += "INNER JOIN ? DBM ON "
	cQuery += "DBM.D_E_L_E_T_ = ? "
	cQuery += "AND DBM.DBM_FILIAL = ? "
	cQuery += "AND DBM.DBM_TIPO = SCR.CR_TIPO "
	cQuery += "AND DBM.DBM_NUM = SCR.CR_NUM "
	cQuery += "AND DBM.DBM_GRUPO = SCR.CR_GRUPO "
	cQuery += "AND DBM.DBM_ITGRP = SCR.CR_ITGRP "
	cQuery += "INNER JOIN ? SC7 ON "
	cQuery += "SC7.D_E_L_E_T_ = ? "
	cQuery += "AND SC7.C7_FILIAL = ? "
	cQuery += "AND SC7.C7_NUM = DBM.DBM_NUM "
	cQuery += "AND SC7.C7_ITEM = DBM.DBM_ITEM "
	If !IsInCallStack("MATA120")
		cQuery += "AND SC7.C7_CONAPRO = ? "
	EndIf
	cQuery += "INNER JOIN ? SCH ON "
	cQuery += "SCH.D_E_L_E_T_ = ? "
	cQuery += "AND SCH.CH_FILIAL = ? "
	cQuery += "AND SCH.CH_PEDIDO = SC7.C7_NUM "
	cQuery += "AND SCH.CH_FORNECE = SC7.C7_FORNECE "
	cQuery += "AND SCH.CH_LOJA = SC7.C7_LOJA "
	cQuery += "AND SCH.CH_ITEMPD = SC7.C7_ITEM "
	cQuery += "AND SCH.CH_ITEM = DBM.DBM_ITEMRA "
	cQuery += "WHERE SCR.D_E_L_E_T_ = ? "
	cQuery += "AND SCR.CR_FILIAL = ? "
	cQuery += "AND SCR.CR_TIPO = ? "
	cQuery += "AND SCR.CR_NUM = ? "

	cQuery := ChangeQuery(cQuery)

	oQry := FWPreparedStatement():New(cQuery)

	oQry:SetNumeric(nOrder++, RetSqlName("SCR"))
	oQry:SetNumeric(nOrder++, RetSqlName("DBM"))
	oQry:SetString(nOrder++ , ' ')
	oQry:SetString(nOrder++ , FWxFilial("DBM"))
	oQry:SetNumeric(nOrder++, RetSqlName("SC7"))
	oQry:SetString(nOrder++ , ' ')
	oQry:SetString(nOrder++ , FWxFilial("SC7"))
	If !IsInCallStack("MATA120")
		If !lDeleta
			oQry:SetString(nOrder++ , 'L')
		Else
			oQry:SetString(nOrder++ , 'B')
		EndIf
	EndIf
	oQry:SetNumeric(nOrder++, RetSqlName("SCH"))
	oQry:SetString(nOrder++ , ' ')
	oQry:SetString(nOrder++ , FWxFilial("SCH"))
	oQry:SetString(nOrder++ , ' ')
	oQry:SetString(nOrder++ , FWxFilial("SCR"))
	oQry:SetString(nOrder++ , cTipo)
	oQry:SetString(nOrder++ , cNumPed)

Else

	cQuery += "SELECT SCR.R_E_C_N_O_ RECSCR, SC7.R_E_C_N_O_ RECSC7, SCH.R_E_C_N_O_ RECSCH "
	cQuery += "FROM ? SCR "
	cQuery += "INNER JOIN ? SCH ON "
	cQuery += "SCH.D_E_L_E_T_ = ? "
	cQuery += "AND SCH.CH_FILIAL = ? "
	cQuery += "AND SCH.CH_PEDIDO = SCR.CR_NUM "
	cQuery += "INNER JOIN ? SC7 ON "
	cQuery += "SC7.D_E_L_E_T_ = ? "
	cQuery += "AND SC7.C7_FILIAL = ? "
	cQuery += "AND SC7.C7_NUM = SCR.CR_NUM "
	cQuery += "AND SC7.C7_ITEM = SCH.CH_ITEMPD "
	cQuery += "AND SC7.C7_FORNECE = SCH.CH_FORNECE "
	cQuery += "AND SC7.C7_LOJA = SCH.CH_LOJA "
	If !IsInCallStack("MATA120")
		cQuery += "AND SC7.C7_CONAPRO = ? "
	EndIf
	cQuery += "WHERE SCR.D_E_L_E_T_ = ? "
	cQuery += "AND SCR.CR_FILIAL = ? "
	cQuery += "AND SCR.CR_TIPO = ? "
	cQuery += "AND SCR.CR_NUM = ? "

	cQuery := ChangeQuery(cQuery)

	oQry := FWPreparedStatement():New(cQuery)

	oQry:SetNumeric(nOrder++, RetSqlName("SCR"))
	oQry:SetNumeric(nOrder++, RetSqlName("SCH"))
	oQry:SetString(nOrder++ , ' ')
	oQry:SetString(nOrder++ , FWxFilial("SCH"))
	oQry:SetNumeric(nOrder++, RetSqlName("SC7"))
	oQry:SetString(nOrder++ , ' ')
	oQry:SetString(nOrder++ , FWxFilial("SC7"))
	If !IsInCallStack("MATA120")
		If !lDeleta
			oQry:SetString(nOrder++ , 'L')
		Else
			oQry:SetString(nOrder++ , 'B')
		EndIf
	EndIf
	oQry:SetString(nOrder++ , ' ')
	oQry:SetString(nOrder++ , FWxFilial("SCR"))
	oQry:SetString(nOrder++ , cTipo)
	oQry:SetString(nOrder++ , cNumPed)

EndIf

cAliasSql := MPSYSOpenQuery(oQry:GetFixQuery(), cAliasSql)

If (cAliasSql)->(!EOF())

	While (cAliasSql)->(!EOF())

		SCR->(DBGoTo((cAliasSql)->RECSCR))

		If cTipo == "IP"
			DBM->(DBGoTo((cAliasSql)->RECDBM))
		EndIf

		SC7->(DBGoTo((cAliasSql)->RECSC7))
		SCH->(DBGoTo((cAliasSql)->RECSCH))

		If IsInCallStack("MATA120")
			A120LanPCO("SC7", "000055", "03", "MATA097", lDeleta) // Tratamento para o parametro MV_CFILPCO.
		Else
			PcoDetLan("000055", "03", "MATA097", lDeleta)
		EndIf

		(cAliasSql)->(DBSkip())

	EndDo

EndIf

(cAliasSql)->(DBCloseArea())

FWRestArea(aAreaMem)
FWRestArea(aAreaSCR)
FWRestArea(aAreaDBM)
FWRestArea(aAreaSC7)
FWRestArea(aAreaSCH)

Return

/*/{Protheus.doc} A120RelAdT
Relacionamento de adiantamento com pedido de compra existe/valido .T. ou .F.
@type function
@version 12.1.2310
@author guilherme.futro
@since 18/04/2024
@param cFIL		, character, filial
@param cNumPed	, character, número do pedido de compra
@param cForn	, character, codigo do fornecedor
@param cLojFor	, character, código da loja do fornecedor
/*/

Function A120RelAdT(cFIL As Character, cNumPed As Character, cForn As Character, cLojFor As Character) As Logical

	Local aArea 	As Array
	Local aAreaSC7 	As Array
	Local lRet 		As Logical

	aArea  		:= FWGetArea()
	aAreaSC7  	:= SC7->(FWGetArea())
	lRet 		:= .T.

	Default cFIL    	:= ""
	Default cNumPed  	:= ""
	Default cForn  		:= ""
	Default cLojFor 	:= ""


	dbSelectArea("SC7")
	dbSetOrder(3)
	if !SC7->(MsSeek(cFIL + cForn + cLojFor + cNumPed ))
		if Str(FIE->FIE_SALDO,17,2) == Str(FIE->FIE_VALOR,17,2)
			If RecLock("FIE",.F.)
				FIE->(DbDelete())
				FIE->(MsUnLock())
			endif
		endif

		lRet := .F.
	endif

	FWRestArea(aArea)
	FWRestArea(aAreaSC7)

Return lRet

/*/{Protheus.doc} A120VALCF
	(Valid do campo C7_CF)
	@type  Static Function
	@author Thiago Rodrigues
	@since 25/06/2024
	@version version
	@see (links_or_references)
/*/
Function A120VALCF()
Local lRet 		As Logical
Local lRetCF 	As Logical
Local nPosTes 	As Numeric
Local cValTES 	As Character

lRet 	:= .T.
nPosTes := GdFieldPos("C7_TES",aHeader)

If nPosTes > 0  .And. Len(aCols) > 0

	cValTES := aCols[n,nPosTes]

	If Empty(cValTES) //Valida se a TES está preenchida
		Help(,,STR0141,,STR0352,1,0) //#Atenção | Por favor, preencha primeiro a TES antes de inserir o CFOP.
		lRet := .F.
	EndIf

	cValCF 	:= M->C7_CF
	lRetCF 	:= Empty(cValCF)

	If  (!lRet .Or. lRetCF) //Sem preenchimento de CFOP ou CFOP preenchido sem TES
		MaFisRef("IT_CF", "MT120", "000")
	Else
		lRet := AvalCfo("SC7", cValCF) //Valida o CFOP preenchido

		If lRet
			MaFisRef("IT_CF", "MT120", cValCF)
		EndIf
	Endif

EndIf

Return lRet

