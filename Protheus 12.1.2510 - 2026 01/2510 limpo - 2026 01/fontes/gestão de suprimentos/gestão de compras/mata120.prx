If ( ExistTemplate("MT120LEG") )
	aLegeUsr := ExecTemplate( "MT120LEG", .F., .F., { aLegenda } )
	If ( ValType( aLegeUsr ) == "A" )
		aLegenda := aClone(aLegeUsr)
	EndIf
