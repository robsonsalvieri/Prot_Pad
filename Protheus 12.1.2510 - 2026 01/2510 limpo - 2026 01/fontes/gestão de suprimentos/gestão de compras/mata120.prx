		lRet := MaAvalPerm(1,{cProduto,"MTA121",3},aGruposUsr)
	ElseIf nTipoPed == 2 //Autorizacao de Entrega
		lRet := MaAvalPerm(1,{cProduto,"MTA122",3},aGruposUsr)
