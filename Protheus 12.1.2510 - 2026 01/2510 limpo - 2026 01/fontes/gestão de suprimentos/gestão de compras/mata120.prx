If ExistBlock( "MT120ISC" )
	ExecBlock( "MT120ISC", .F., .F. )
EndIf

RestArea(aAreaSB1)
aSize(aAreaSB1,0)
aAreaSB1 := nil

RestArea(aArea)
aSize(aArea,0)
aArea := nil

Return NIL

