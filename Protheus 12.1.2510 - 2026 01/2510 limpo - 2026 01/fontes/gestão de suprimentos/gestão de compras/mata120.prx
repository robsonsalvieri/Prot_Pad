Function A120Cod()

Local cCodProd  := &(ReadVar())
Local lGrade    := MaGrade()
Local lRet      := .T.
Local nPProduto := aScan(aHeader,{|x| AllTrim(x[2]) == "C7_PRODUTO"})
Local nPItem    := aScan(aHeader,{|x| AllTrim(x[2]) == "C7_ITEM"})
Local cCodSIte  := ""
Local cGruIte   := ""
Local cCodite   := ""
Local cGIte     := ""
Local cCIte     := ""
Local cAliasSB1 := "SQLSB1"
Local lIntGC    := IIf((SuperGetMV("MV_VEICULO",,"N")) == "S",.T.,.F.) .and. (nModulo == 11 .or. nModulo == 14 .or. nModulo == 41)
Local oQry		:= NIL
Local nOrder	:= 1

Public lAltProd :=.T.

If lIntGC
	If !FM_PILHA("OFJD10I")
		cQuery    := "SELECT SB1.B1_GRUPO , SB1.B1_CODITE FROM "+RetSqlName("SB1")+" SB1 WHERE SB1.B1_FILIAL= ? AND SB1.B1_COD = ? AND SB1.D_E_L_E_T_= ? "
		oQry := FWPreparedStatement():New(cQuery)
		oQry:SetString(nOrder++, xFilial("SB1"))
		oQry:SetString(nOrder++, M->C7_PRODUTO)
		oQry:SetString(nOrder++, ' ')
		cQuery := oQry:GetFixQuery()
		dbUseArea( .T., "TOPCONN", TcGenQry( ,, cQuery ), cAliasSB1, .F., .T. )
		If !( cAliasSB1 )->( Eof() )
			cGruIte := ( cAliasSB1 )->B1_GRUPO
			cCodite := ( cAliasSB1 )->B1_CODITE
			cGIte   := ( cAliasSB1 )->B1_GRUPO
			cCIte   := ( cAliasSB1 )->B1_CODITE
		EndIf
		cCodSIte := FG_ITESUB(cGruIte+cCodite)
		SB1->(dbSetOrder(1))
		If ValType(cCodSIte) == "A"
			cGruIte := cCodSIte[1]
			cCodite := cCodSIte[2]
		Else
			cGruIte := ( cAliasSB1 )->B1_GRUPO
			cCodIte := cCodSIte
		EndIf
		( cAliasSB1 )->( dbCloseArea() )
		If cCodIte != cCIte .or. cGruIte != cGIte
			cSQL := "SELECT SB1.B1_COD FROM "+RetSqlName("SB1")+" SB1 "+"WHERE SB1.B1_FILIAL = ? AND SB1.B1_GRUPO = ? AND SB1.B1_CODITE = ? AND SB1.D_E_L_E_T_= ? "
			nOrder := 1
			oQry := FWPreparedStatement():New(cSQL)
			oQry:SetString(nOrder++, xFilial("SB1"))
			oQry:SetString(nOrder++, cGruIte)
			oQry:SetString(nOrder++, cCodIte)
			oQry:SetString(nOrder++, ' ')
			cSQL := oQry:GetFixQuery()
			nRet := FM_SQL(cSQL)
			M->C7_PRODUTO := nRet
		EndIf

		nPosAt := aScan(aCols,{|x| x[nPProduto] == M->C7_PRODUTO .and. x[len(aCols[1])] == .f.  } )
		If nPosAt > 0 .and. nPosAt <> n .and. !aCols[nPosAt,Len(aCols[nPosAt])]
			MsgStop(STR0215+CHR(13)+ CHR(10)+STR0216+Alltrim(aCols[nPosAt,nPItem]))
			Return(.f.)
		EndIf
	EndIf
EndIf


If Alltrim(cCodProd) == Alltrim(GdFieldGet("C7_PRODUTO")) // Nao alterei o produto
	lAltProd:=.F.
Endif

If !lGrade
	lRet := ExistCpo("SB1")
EndIf

Return lRet

