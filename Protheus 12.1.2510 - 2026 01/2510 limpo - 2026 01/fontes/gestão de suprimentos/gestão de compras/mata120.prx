	If lEdita .And. aScan(aNoAltEdi,{|x| x == Substr(ReadVar(),4)}) > 0 .And.;
			!Empty(Posicione("SC7",1,xFilial("SC7")+cA120Num+aCols[n,nPosItem],"C7_CODED")) .And.;
