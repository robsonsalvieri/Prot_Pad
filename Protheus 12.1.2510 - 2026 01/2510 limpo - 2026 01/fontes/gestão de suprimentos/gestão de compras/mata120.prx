Function A120Trigger(cCampo)

Local aArea 	:= GetArea()
Local aRefer    := {}
Local nPosCpo	:= aScan(aHeader,{|x| AllTrim(x[2])==AllTrim(cCampo)})
Local lRetorno	:= .T.

If nPosCpo > 0
	aRefer := MaFisGetRF(aHeader[nPosCpo][6])
	If !Empty(aRefer[1])
		MaFisRef(aRefer[1],aRefer[2],aCols[N,nPosCpo])
		If cPaisLoc <>"BRA"
			ModxAtuObj()
		EndIf
	EndIf
Else
	lRetorno := .F.
EndIf

If lRetorno .And. "C7_TOTAL" $ cCampo
	A120ArrTot()
EndIf

RestArea(aArea)
aSize(aArea,0)
aArea := nil

Return lRetorno

