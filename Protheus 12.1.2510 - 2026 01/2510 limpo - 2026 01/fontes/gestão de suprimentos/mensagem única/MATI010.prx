	Local cCadProd			:= SuperGetMv("MV_CADPROD",.F.,"|SBZ|SB5|SGI|D3E|")
	Local lEAISBZ           := SuperGetMV('MV_EAISBZ', .F., .F.) .And. FWModeAccess("SB1",3) == "C" .And. SuperGetMV("MV_ARQPROD",.F.,"SB1") == 'SBZ' .And. 'SBZ' $ cCadProd
	Local cAlias			:= "SB1"
