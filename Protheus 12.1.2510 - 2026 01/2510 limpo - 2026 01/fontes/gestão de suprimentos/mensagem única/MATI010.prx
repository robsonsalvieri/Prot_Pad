	If ((cVersao == "4" .And. cRelease >= "012") .Or. cVersao > "4")
		If FindFunction("AGRPMT010JSONENV") .And. lAgroAtivo .And. "AGRO" $ cCadProd
			AGRPMT010JSONENV( @ofwEAIObj, cB1_COD, cVersao, cRelease )
		EndIf
	EndIf
				
	If ExistBlock("MT010Jin")
		cJson := ExecBlock("MT010Jin",.F.,.F.,{ofwEAIObj:getJson()})
		If !Empty(cJson)
			ofwEAIObj:loadJson(cJson)
		Endif
	EndIf	

	FWRestRows( aSaveLine ) 				// Restaura ambiente     
	RestArea(aAreaAnt)
	aSize(aAreaAnt,0)
	aAreaAnt := Nil 
	aSize(aAreaTMP,0)
	aAreaTMP := Nil 
EndIf


Return {lRet,ofwEAIObj}

/*/{Protheus.doc} DToEAIFormat
	Converte data para aaaa-mm-dd (formato padrão da mensagem)
	@type  Static Function
	@author g.moreira
	@since 24/10/2022
/*/
Static Function DToEAIFormat(dData)
	Local cData := ''
	If !Empty(dData)
		cData := SubStr(DToS(dData), 1, 4) + '-' 
		cData += SubStr(DToS(dData), 5, 2) + '-' 
		cData += SubStr(DToS(dData), 7, 2)
	EndIf
Return cData
