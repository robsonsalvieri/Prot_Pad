/*/{Protheus.doc}A242Auto
Funcao utilizada para preenchimento da validação dos campos do cabeçalho
via rotina automática

@author Squad Entradas
@since Jul/2020
@version P12.1.27
/*/
//-------------------------------------------------------------------
Static Function A242Auto()
Local lSubLote:= .F.
Local lLote   := .F.
Local lLocaliz:= .F.
Local lMta242L:= ExistBlock("MTA242L")
Local nX      := 0
Local nY      := Len(aAutoCab)

For nX := 1 to nY
	Do Case
		Case AllTrim(aAutoCab[nX][1]) == "cProduto"
			aAutoCab[nX][3]:= "(NaoVazio() .And. Existcpo('SB1') .And. A242IniCpo() .And. If(lMta242C,ExecBlock('MTA242C',.F.,.F.,{cProduto,nQtdOrig,oGet}),.T.))"
			IIF(Rastro(aAutoCab[nX][2],"L"),lLote := .T.,)
			IIF(!lLote .And. Rastro(aAutoCab[nX][2],"S"),lSubLote := .T.,)
			IIF(Localiza(aAutoCab[nX][2]),lLocaliz := .T.,)
		Case AllTrim(aAutoCab[nX][1]) == "cLocOrig"
			aAutoCab[nX][3]:= "ExistCpo('NNR') .And. A242Local(&(ReadVar())) .And. NaoVazio()"
		Case AllTrim(aAutoCab[nX][1]) == "nQtdOrig"
			aAutoCab[nX][3]:= "(A242Quant() .And. nQtdOrig > 0 .And. IIf(lMta242Q,ExecBlock('MTA242Q',.F.,.F.,{cProduto,nQtdOrig,oGet}),.T.))"
		Case AllTrim(aAutoCab[nX][1]) == "nQtdOrigSe"
			aAutoCab[nX][3]:= "(Positivo() .And. A242PriUm() .And. IIf(lMta242S,ExecBlock('MTA242S',.F.,.F.,{cProduto,nQtdOrigSe,oGet}),.T.))"
		Case AllTrim(aAutoCab[nX][1]) == "cDocumento"
			aAutoCab[nX][3]:= "NaoVazio() .And. A240Doc() .And. VldUser('D3_DOC')"
		Case (AllTrim(aAutoCab[nX][1]) == "cNumLote" .And. lSubLote) .Or. (AllTrim(aAutoCab[nX][1]) == "cNumLote" .And. !Empty(aAutoCab[nX][2]))
			aAutoCab[nX][3]:= "A242Lote(2)"
		Case (AllTrim(aAutoCab[nX][1]) == "cLoteDigi" .And. (lLote .Or. lSubLote)) .Or. (AllTrim(aAutoCab[nX][1]) == "cLoteDigi" .And. !Empty(aAutoCab[nX][2]))
			aAutoCab[nX][3]:= "A242Lote(1)"
		Case AllTrim(aAutoCab[nX][1]) == "cLocaliza"
			If lLocaliz
				aAutoCab[nX][3]:= "Empty(&(ReadVar())) .Or. ExistCpo('SBE',cLocOrig+&(ReadVar()))"
				If lMta242L
					aAutoCab[nX][3]+= " .And. ExecBlock('MTA242L',.F.,.F.,{cProduto,nQtdOrig,oGet,cLocOrig,cLocaliza})"
				EndIf
			Else
				aAutoCab[nX][2]:= " "
			EndIf
		Case AllTrim(aAutoCab[nX][1]) == "dEmis260"
			aAutoCab[nX][3]:= "A242Data()"
	EndCase
Next

Return


/*/{Protheus.doc} VldCabec
   Valida os campos padrão obrigatorios do cabecalho da dialog
   @type  Function
   @author Reynaldo
   @since 21/12/2020
   @version 1.0
   @param nenhum
   @return lRet, logico, Verdadeiro se os campos estiver ok
   /*/
Static Function VldCabec()
Local lRastroL
Local lRastroS
Local lRet
Local cSolucao

	cSolucao := ""

   If Empty(cProduto)
		cSolucao := I18N(STR0033 ,{STR0006}) // "Verifique o #1[Lote]#" // "Produto Origem"
		lRet := .F.
	Else
		lRet := .T.
   EndIf

   If lRet
		If Empty(cLocOrig)
			cSolucao := I18N(STR0033 ,{STR0007}) // "Verifique o #1[Lote]#" // "Armazem"
			lRet := .F.
		EndIf
   EndIf

   If lRet
      lRastroL := Rastro(cProduto,"L")
      lRastroS := Rastro(cProduto,"S")
      If lRastroL .OR. lRastroS
			If Empty(cLoteDigi)
				cSolucao := I18N(STR0033 ,{STR0011}) // "Verifique o #1[Lote]#" // "Lote"
	         lRet := .F.
			EndIf
      Else
         lRet := .T.
      Endif
   EndIf

   If lRet
      If lRastroS
			If Empty(cNumLote)
				cSolucao := I18N(STR0033 ,{STR0010}) // "Verifique o #1[Lote]#" // "Sub-Lote"
	         lRet := .F.
			EndIf
      EndIF
   EndIf

   If lRet
      If nQtdOrig <=0
			cSolucao := I18N(STR0034 ,{STR0014}) // "Verifique a #1[Quantidade]#" // "Quantidade"
			lRet := .F.
		EndIf
   EndIf

   If lRet
      If nQtdOrigSe < 0
			cSolucao := I18N(STR0034 ,{STR0015}) // "Verifique a #1[Quantidade]#" // "Quantidade Secund ria"
         lRet := .F.
      EndIf
   EndIf

   If lRet
		If Empty(dEmis260)
			cSolucao := I18N(STR0034 ,{STR0009}) // "Verifique a #1[Quantidade]#" // "Data"
         lRet := .F.
      EndIf
   EndIf

   If lRet
      If Empty(cDocumento)
			cSolucao := I18N(STR0033 ,{STR0016}) // "Verifique o #1[Lote]#" // "Documento"
         lRet := .F.
		EndIf
   EndIf

   If !Empty(cSolucao) .and. !lRet
		Help(" ", 1,"MA242PR", NIL, NIL, 1, 0, NIL, NIL, NIL, NIL, NIL, {cSolucao})
   EndIf

Return lRet

//-------------------------------------------------------------------
/*/{Protheus.doc}a242AfterC
Funcao responsavel validar e retirar se existe "produto pai/produto 
cabeçalho" no aCols

@author Miqueias L. Coelho
@since 30/03/2023
@version v1.0
/*/
//-------------------------------------------------------------------
Static Function a242AfterC(cDocumento, cSeq)
Local lRet		:= .T.
Local cQryACols := ""
Local nPosHdRec, nPosCsRec, nRecFather := 0

Static __lLibVer  := Nil
Static oQryRecSD3 := Nil

If __lLibVer == Nil
	__lLibVer := FWLibVersion() >= "20211116"
EndIf

If oQryRecSD3 == NIL
	cQryACols := ""
	cQryACols += "SELECT SD3.R_E_C_N_O_  "
	cQryACols += "FROM " + RetSQLName("SD3") + " SD3 "
	cQryACols += "WHERE SD3.D3_FILIAL = ? "
	cQryACols += "AND SD3.D3_NUMSEQ = ? "
	cQryACols += "AND SD3.D3_CHAVE = ? "
	cQryACols += "AND SD3.D_E_L_E_T_ = ? "
	cQryACols += "AND SD3.D3_DOC = ? "
	cQryACols += "AND SD3.D3_FATHER = ? "
	
	cQryACols := ChangeQuery(cQryACols)

	If __lLibVer
	    oQryRecSD3 := FwExecStatement():New(cQryACols)
	Else
	    oQryRecSD3 := FWPreparedStatement():New(cQryACols)
	EndIf
EndIf

oQryRecSD3:SetString(1,xFilial("SD3"))
oQryRecSD3:SetString(2,cSeq)
oQryRecSD3:SetString(3,'E9')
oQryRecSD3:SetString(4,' ')
oQryRecSD3:SetString(5,cDocumento)
oQryRecSD3:SetString(6,'F')

If __lLibVer
    nRecFather := oQryRecSD3:ExecScalar('R_E_C_N_O_')
Else
    cQryACols  := oQryRecSD3:GetFixQuery()
    nRecFather := MpSysExecScalar(cQryACols, 'R_E_C_N_O_')
EndIf

nPosHdRec := GDFieldPos("D3_REC_WT")
nPosCsRec := Ascan(aCols,{|x| x[nPosHdRec] == nRecFather})

If nPosCsRec > 0
	ADEL(aCols, nPosCsRec)
	ASIZE(aCols, Len(aCols)-1)
EndIf

Return lRet

/*/{Protheus.doc} A242When
	Verifica se o campo do cabeçalho pode ser editado
	conforme configurado no dicionário de dados
	@type  Static Function
	@author Gianluca Moreira
	@since 25/05/2023
/*/
Static Function A242When(cCampo)
	Local lEdit   := .T.
	Local cVisual := GetSX3Cache(cCampo, 'X3_VISUAL')
	Local cWhen   := GetSX3Cache(cCampo, 'X3_WHEN')

	lEdit := cVisual != 'V' .And. &cWhen

Return lEdit

