	Local lRet := .T.

	If lRet .And. nQtdOrig > 0 .And. Localiza(cProduto) .And. SaldoSBF(cLocOrig,cLocaliza,cProduto,cNumSerie,cLoteDigi,cNumLote) < nQtdOrig
		Help(" ",1,"SALDOLOCLZ")
		lRet := .F.
	EndIf

Return lRet

