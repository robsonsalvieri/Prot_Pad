If Ascan( aHeaderX, { | x | Alltrim( x[ 2 ] ) == "CP_SOLICIT" } ) > 0
	cTabSol := Eval( bGetSx3, "CP_SOLICIT", "X3_F3" )
EndIf

Return Nil

