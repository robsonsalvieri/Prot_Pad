ßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßßß
*/
static function CpoRat()

Local lCopSGS   := .F.
Local cQuery    := ""
Local cAliasSGS := GetNextAlias()
Local oQry
Local lM185bVLib := FWLibVersion() >= "20211116"

	cQuery := "select CP_NUM, GS_SOLICIT from "+RetSqlName("SCP")+" SCP " 
	cQuery += "inner join "+RetSqlName("SGS")+" SGS "
	cQuery += "on CP_FILIAL = GS_FILIAL "
	cQuery += "and CP_NUM = GS_SOLICIT "
	cQuery += "and SCP.D_E_L_E_T_ = ? " 
	cQuery += "where CP_FILIAL = ? "
	cQuery += "and CP_NUM = ? " 
	cQuery += "and SGS.D_E_L_E_T_ = ? "

	cQuery := ChangeQuery(cQuery)
	If lM185bVLib
		oQry := FwExecStatement():New(cQuery)
	Else
		oQry := FWPreparedStatement():New(cQuery)
	EndIf 

oQry:SetString(1,' ')
oQry:SetString(2,xFilial('SCP'))
oQry:SetString(3,cA105Num)
oQry:SetString(4,' ')

If lM185bVLib
	 oQry:OpenAlias(cAliasSGS)	
Else
	cAliasSGS := MPSYSOpenQuery(oQry:GetFixQuery())
EndIf 

While !(cAliasSGS)->(Eof()) 
	If !Empty((cAliasSGS)->GS_SOLICIT)
		lCopSGS := .T.
		Exit
	EndIf 
EndDo  

(cAliasSGS)->(DbCloseArea())

Return lCopSGS

