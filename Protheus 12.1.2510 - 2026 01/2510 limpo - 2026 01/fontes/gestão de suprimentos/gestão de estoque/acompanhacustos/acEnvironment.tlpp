oError      := ErrorBlock({|e| oRep900:setErrorInTransaction(cIdJob, cThread, cProc, e:Description, e:ErrorStack, cFil)})

LockByName(cFil + '_' + cProc + '_' +  cIdJob + '_' + cThread, .F. , .F.)

oD4A:SEEK(cIdJob, cThread, cProc)
oD4A:PATCH('D4A_STATUS', 'PROCESSING')
oD4A:LOCK()
Begin Sequence
    oService:setESTR900B(cEmp,cFil, cArm, cProd, nSaldoIni, nSaldoFim, nQuantIni, nQuantFin, nRangeIni, nRangeFim, lSecond, cIdJob, cIdConfig, lNotInvert, nRecStart, nRecEnd)
End Sequence

UnlockByName(cFil + '_' + cProc + '_' +  cIdJob + '_' + cThread, .F. , .F.)

ErrorBlock(oError)

Return

