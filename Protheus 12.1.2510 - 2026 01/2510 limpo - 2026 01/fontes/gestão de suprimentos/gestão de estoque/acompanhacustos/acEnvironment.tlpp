/*/{Protheus.doc} IniVarEnAc
    Inicializa variaveis publicas com informacoes do usuario logado no ambiente setado pelo Acompanha Custos
    @type Function
    @author Squad.Entradas
    @since 14/09/2022
    @version 1.0
    @param  cAcUserId , character, codigo do usuario
            cAcUsrName, character, nome do usuario
/*/
Function IniVarEnAc(cAcUserId, cAcUsrName)

If Type("__cUserId") == "C" .And. Empty(__cUserId)
    If FindFunction("totvs.framework.users.rpc.authByToken", .T. )
       	totvs.framework.users.rpc.authByToken(cAcUserId)
        FWXFilial('')// Valida que foi autenticado com sucesso
    else
        __cUserId := cAcUserId //DEBITO TECNICO 12.1.2510	
    EndIf
EndIf

If Type("cUserName") == "C" .And. Empty(cUserName)
    cUserName := cAcUsrName
EndIf

Return

/*/{Protheus.doc} ESTR900
    Inicializa variaveis publicas com informacoes do usuario logado no ambiente setado pelo Acompanha Custos
    @type Function
    @author Squad.Entradas
    @since 14/09/2022
    @version 1.0
    @param  cAcUserId , character, codigo do usuario
            cAcUsrName, character, nome do usuario
/*/
Function ESTR900(cEmp, cFil, cUserName, cId, cIdConfig, dDataIni, dDataFim, cParameters, aBranches, cPayload)

Local cProc     := "ESTR900"
Local cThread   := "01"

Local oRepository := Nil
Local oService    := Nil
Local oError      := Nil
Local oD4A        := Nil
Local oParameters := Nil

IniEnviron(cEmp,cFil, cProc)

oRepository   := totvs.protheus.backoffice.stock.trackcosts.ESTR900.repository.ESTR900Repository():New()
oService      := totvs.protheus.backoffice.stock.trackcosts.ESTR900.service.ESTR900Service():New()
oError        := ErrorBlock({|e| oRepository:setErrorInTransaction(cId, cThread, cProc, e:Description, e:ErrorStack, cFil)})
oD4A          := totvs.protheus.backoffice.stock.trackcosts.model.D4A.D4A():New()
oParameters   := JsonObject():New()

oParameters:FromJson(cParameters)

LockByName(cFil + '_' + cProc + '_' +  cId + '_' + cThread, .F. , .F.)

Begin Sequence    
    
    oD4A:SEEK(cId, cThread, cProc)
    oD4A:PATCH('D4A_STATUS', 'PROCESSING')
    oD4A:LOCK()
    oService:setESTR900(cId, cIdConfig, dDataIni, dDataFim, oParameters, aBranches, cUserName, cPayload)

End Sequence

UnlockByName(cFil + '_' + cProc + '_' +  cId + '_' + cThread, .F. , .F.)

ERRORBLOCK( oError )

Return

