            cAcUsrName, character, nome do usuario
/*/

Function ESTR900B(cEmp,cFil, cArm, cProd, nSaldoIni, nSaldoFim, nQuantIni, nQuantFin, nRangeIni, nRangeFim, lSecond, cIdJob, cUserName, nTotal, cIdConfig, lNotInvert, nRecStart, nRecEnd)

Local cProc         := "ESTR900B"
Local cThread       := ""
Local oService      := Nil
Local oRepository   := Nil  
Local oRep900       := Nil
Local oD4A          := Nil  
Local oError        := Nil

Default lNotInvert := .F. 
Default nRecStart  := 0
Default nRecEnd    := 0

IniEnviron(cEmp,cFil, cProc)

If lSecond
    cThread := "02"
Else 
    cThread := "01"
Endif

oService    := totvs.protheus.backoffice.stock.trackcosts.ESTR900B.service.ESTR900BService():New()
oRepository := totvs.protheus.backoffice.stock.trackcosts.ESTR900B.repository.ESTR900BRepository():New()
