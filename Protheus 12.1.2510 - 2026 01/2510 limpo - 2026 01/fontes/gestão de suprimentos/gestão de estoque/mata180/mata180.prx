Function A180TPSERV(cCampo)
Local lRet		 := .T.

If M->B5_TPISERV == "4" .OR. M->B5_TPISERV == "6"
	If cCampo == "B5_GSMI"
		M->B5_GSMI := "2"
		lRet := .F.
	ElseIf cCampo == "B5_GSMC"
		M->B5_GSMC := "2"
		lRet := .F.
	ElseIf cCampo == "B5_GSLE"
		M->B5_GSLE := "2"
		lRet := .F.
	EndIf
EndIf

Return lRet

