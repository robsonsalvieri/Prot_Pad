Local lFim	   := .F.

If cPaisLoc=="PTG"
	dbSelectArea("SF5")
	If dbSeek(xFilial()+cTm)
		If !Eof() .And. SF5->F5_PERDA$"OB|EX"
			DbSelectArea('SX6')
			If GetMV('MV_ABATEST', .T.)
    			If lGrava
					Do While !lFim
						If MSRLock(Recno())
						   cNumGuia := Right(Alltrim(X6Conteud()),8)
							PutMV("MV_ABATEST",Soma1(cNumGuia))
							MsRUnlock(Recno())
							lFim := .T.
						Endif
					EndDo
