Static Function A240IsPI(cProd)

Local lRet		:= .F.
Local aArea		:= GetArea()
Local cAliCOD	:= GetNextAlias()
Local cAliCMP	:= GetNextAlias()

BeginSql Alias cAliCMP

	SELECT
	COUNT(G1_COMP) AS COMP
	FROM
	%Table:SG1%
	WHERE
	G1_FILIAL = %xFilial:SG1% AND
	G1_COMP = %Exp:cProd% AND
	D_E_L_E_T_= ' '

EndSql

BeginSql Alias cAliCOD

	SELECT
	COUNT(G1_COD) AS COD
	FROM
	%Table:SG1%
	WHERE
	G1_FILIAL = %xFilial:SG1% AND
	G1_COD = %Exp:cProd% AND
	D_E_L_E_T_= ' '

EndSql

DbSelectArea(cAliCMP)
(cAliCMP)->(DbGoTop())

DbSelectArea(cAliCOD)
(cAliCOD)->(DbGoTop())
//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Se for COMPONENTE e CODIGO = PI                ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
If !Empty((cAliCMP)->COMP) .And. !Empty((cAliCOD)->COD)
	lRet := .T.
EndIf

(cAliCOD)->(DbCloseArea())
(cAliCMP)->(DbCloseArea())

RestArea(aArea)

Return lRet

