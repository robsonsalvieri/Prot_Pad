	//XXXXXX-XXXXXXXXXXXXXXX   XXXXXX-XXXXXXXXXXXXXXX    XXXXXX    99/99/99    XX  9999,999,999    9999,999,999     9999,999,999    9999,999,999
	//0                        26                        52        62          74  78              94               111             127
	li:=56
	_Tot1:=0
	_Tot2:=0
	_Tot3:=0
	_Tot4:=0
	_TotT1:=0
	_TotT2:=0
	_TotT3:=0
	_TotT4:=0
	DBSELECTAREA("TMP3")
	DBGOTOP()
	WHILE ! EOF()
		li :=1
		li :=cabec(titulo,cabec1,cabec2,nomeprog,tamanho)
