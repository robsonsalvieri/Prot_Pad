		SB1->(DBSEEK(XFILIAL("SB1")+TMP3->COD))
		@ LI,00 PSAY ALLTRIM(TMP3->COD)+" - "+SB1->B1_DESC
		li:=li+1
		_COD   :=TMP3->COD
		_TRANSP:=TMP3->TRANSP
