	Local lEnvBaseP		:=	.T. //Enviar ou nao base de Pis dependendo do codigo de CST.
	Local lEnvBaseC		:=	.T. //Enviar ou nao base de Cofins dependendo do codigo de CST.
	Local nBase			:=	0
