/*/{Protheus.doc} FINSPDF100

	Funcão para retornar os títulos que compoem o bloco F100 do SPED PIS/COFINS
	Arquivo anterior: MATXATU.PRX

	@Author	TOTVS
	@since	28/04/2011
/*/
//-----------------------------------------------------------------------------------------------------
Function FinSpdF100(nMesRef As Numeric, nAnoRef As Numeric, lTotF100 As Logical, cDataIni As Character, cDataFim As Character, cBloco As Character)

	Local aReturn    as Array
	Local aF100      as Array
	Local cAliasQry  as Character
	Local nA         as Numeric
	Local nX         as Numeric
	Local nJ         as Numeric
	Local nValorPis  as Numeric
	Local nValorCof  as Numeric
	Local cIndCmlt   as Character
	Local dLastDay   as Date
	Local cDocSpd    as Character
	Local cDcsSpd    as Character
	Local cMotBx     as Character
	Local lSpdMt100  as Logical
	Local lSpdMt460  as Logical
	Local aAreaSF1   as Array
	Local aAreaSF2   as Array
	Local lCodPart   as Logical
	Local cNOrigem   as Character
	Local cTipo      as Character
	Local cNome      as Character
	Local cOrigem    as Character
	Local cNomeApl   as Character
	Local cTpSel     as Character
	Local cTpApl     as Character
	Local lBxSped    as Logical
	Local cNatRec    as Character
	Local cNatDesp   as Character
	Local aNatNTrib  as Array
	Local lAplSped   as Logical
	Local cNatApl    as Character
	Local cTpSe5     as Character
	Local cTpVM		 as Character
	Local lEnvReg    as Logical
	Local aAreaSED   as Array
	Local nI         as Numeric
	Local aAlias     as Array
	Local aRegSped   as Array
	Local aAreaSEV   as Array
	Local lRenTribR  as Logical
	Local lRenTribD  as Logical
	Local cChave     as Character
	Local aMotBx     as Array
	Local lBxOk      as Logical
	Local nVrRateio  as Numeric
	Local cCstPis    as Character
	Local cCstCof    as Character
	Local lEnvBaseP  as Logical
	Local lEnvBaseC  as Logical
	Local nBase      as Numeric
	Local nMoeda     as Numeric
	Local lSEI_      as Logical
	Local nProp      as Numeric
	Local cChaveBx   as Character
	Local cChArray   as Character
	Local nDescBx    as Numeric
	Local nDescBxI   as Numeric
	Local nDescBxC   as Numeric
	Local aRecNTrib  as Array
	Local nTotF100   as Numeric
	Local lBaseImp   as Logical
	Local nValTit    as Numeric
	Local aTitRH     as Array
	Local aTamRH     as Array
	Local cChaveRH   as Character
	Local cArqRH     as Character
	Local cIndRH     as Character
	Local cAliasRH   as Character
	Local cNomeRH    as Character
	Local nBasePis   as Numeric
	Local aBasePis   as Array
	Local lTitRet    as Logical
	Local nVlMinImp  as Numeric
	Local cOriTit    as Character
	Local nAliasSe2  as Numeric
	Local cMoeda     as Character
	Local cNatSe2    as Character
	Local lDescDoc   as Logical
	Local cNatResg   as Character
	Local lNRastDSD  as Logical
	Local lIntPFS    as Logical
	Local nVrDescInc as Numeric
	Local cData      as Character
	Local cNro       as Character
	Local nVrJuros   as Numeric
	Local nRegApl    as Numeric
	Local cChaveApl  as Character
	Local nTxMoeda   as Numeric
	Local aRegApl    as Array
	Local nVrCanc    as Numeric
	Local aAreaSE5   as Array
	Local cChaveSE5  as Character
	Local lRecIss    as Logical
	Local lMVDescIss as Logical
	Local cNewDocSpd as Character
	Local cNewDcsSpd as Character
	Local lRetFat    as Logical

	Local cNatRcm    as Character
	Local cNatDcm    as Character
	Local lDescSe2   as Logical
	Local lDescSe5   as Logical
	Local nCm        as Numeric
	Local aSpd       as Array
	Local lEdCprb    as Logical
	Local cTpAplAnt  as Character
	Local lDbSkip    as Logical
	Local cNatRec1   as Character
	Local cNatDesp1  as Character
	Local cNatRcm1   as Character
	Local cNatDcm1   as Character
	Local c2DuPref   as Character
	Local cCprb      as Character
	Local cFilSED    as Character
	Local cFilSe1    as Character
	Local cFilSe2    as Character
	Local cFilSe5    as Character
	Local lPCCBx     as Logical
	Local lCalcIssBx as Logical
	Local nStatus    as Numeric
	Local aCodNatur  as Array
	Local aCodSeek   as Array
	Local nValCruz	 as Numeric

	Private lEnvDesd as Logical

	aReturn		:= {}
	aF100		:= {}
	cAliasQry	:= ""
	nA			:= 0
	nX			:= 0 
	nJ			:= 0 
	nValorPis	:= 0
	nValorCof	:= 0    
	cIndCmlt	:= ""
	cDocSpd		:= SuperGetMv("MV_DOCSPD",,"")	
	cDcsSpd		:= SuperGetMv("MV_DCSSPD",,"")	
	cMotBx		:= SuperGetMv("MV_MTBXSD",,"")	
	lSpdMt100	:= .F.
	lSpdMt460	:= .F.
	aAreaSF1	:= {}
	aAreaSF2	:= {}
	lCodPart	:= .T.
	cNOrigem	:= ""
	cTipo		:= ""
	cNome		:= ""
	cOrigem		:= ""
	cNomeApl	:= ""
	cTpSel		:= ""
	cTpApl		:= ""      
	lBxSped		:= SuperGetMv("MV_RENTRIB",,.F.) //Habilita o envio receitas financeiras não tributaveis 	
																	//,ou seja, dados da baixa (juros, descontos ou variacao cambial).
	cNatRec		:= SuperGetMv("MV_NATRNT",,"") 	//Codigo da Natureza de Receita Nao Tributavel.
	cNatDesp	:= SuperGetMv("MV_NATDNT",,"") 	//Codigo da Natureza de Despesa Nao Tributavel.
	aNatNTrib	:= {}
	lAplSped	:= SuperGetMv("MV_APLFIN",,.F.) //Habilita o envio das Aplicacoes financeiras.
	cNatApl		:= SuperGetMv("MV_APLNAT",,"") 	//Codigo da Natureza das Aplicacoes Financeiras.
	cTpSe5		:=	"'JR','CM','DC','MT'" //Juros, Variacao Cambial, Desconto e Multa.
	cTpVM		:= ""
	lEnvReg		:=	.F.
	aAreaSED	:=	{}
	nI			:=	0
	aAlias		:= If(lBxSped,{"SE5","SE1","SE2"},{"SE1","SE2"})
	aRegSped	:=	{}
	aAreaSEV	:=	{}
	lRenTribR	:=	.T. //Valida as naturezas de receitas nao tributaveis.
	lRenTribD	:=	.T. //Valida as naturezas de despesas nao tributaveis.
	cChave		:=	""  
	aMotBx 		:= ReadMotBx()
	lBxOk		:=	.T.   
	nVrRateio	:=	0
	cCstPis		:=	""
	cCstCof		:=	""
	lEnvBaseP	:=	.T. //Enviar ou nao base de Pis dependendo do codigo de CST.
	lEnvBaseC	:=	.T. //Enviar ou nao base de Cofins dependendo do codigo de CST.
	nBase		:=	0
	nMoeda		:= 0
	lSEI_		:= SuperGetMv("MV_APLAPRO",,.F.) //Habilita o envio das Aplicacoes financeiras.
	nProp		:= 0
	cChaveBx	:= ""
	cChArray	:= ""
	nDescBx		:= 0
	nDescBxI	:= 0
	nDescBxC	:= 0
	aRecNTrib 	:= {}
	nTotF100	:= 0
	lBaseImp	:= .T. 		// Nf possui valor de base de imposto.    
	nValTit		:= 0		//Campos para a geracao do arquivo para o RH.                
	aTitRH		:= {}
	aTamRH   	:= {}
	cChaveRH	:=	""
	cArqRH		:=	""
	cIndRH  	:=	""
	cAliasRH	:=	""   
	cNomeRH		:=	""   
	nBasePis	:=	0
	aBasePis	:=	{}                  
	lTitRet		:=	.F.		//Titulo retido em outro ou que retem outro.											
	nVlMinImp	:= IIF(dDataBase > dLastPcc, GetNewPar("MV_VL13137",10), GetNewPar("MV_VL10925",5000))
	cOriTit		:=	""		//Origem do titulo qdo fatura vou precisar utilizar...
	nAliasSe2	:=	0
	cMoeda		:=	""                        
	cNatSe2		:= SuperGetMv("MV_NATCP",,"") 	
	lDescDoc	:= SuperGetMv("MV_DESCDOC",,.F.) //Habilita o envio da descrição da operação/documento no bloco F100 do SPED PIS/COFINS. 	
	cNatResg	:= &(GetMv("MV_NATRAPL"))
	lNRastDSD	:= SuperGetMV("MV_NRASDSD",.T.,.F.)
	lIntPFS   	:= SuperGetMv("MV_JESCJUR",,.F.) //Integração PFS Juridico
	nVrDescInc	:=	0 //Valor do desconto incondicional referente ao campo E1_TPDESC (Legado)
	cData		:= ""	
	cNro		:= ""					                                                                           
	nVrJuros	:=	0
	nRegApl		:=	0
	cChaveApl	:=	""                             
	nTxMoeda	:=	0
	aRegApl		:= 	{}  
	nVrCanc		:= 	0
	aAreaSE5	:=	{}
	cChaveSE5	:= 	""
	lRecIss		:=	.F.
	lMVDescIss	:=	GetNewPar("MV_DESCISS",.F.)
	cNewDocSpd	:=","+STRTRAN(STRTRAN(STRTRAN(STRTRAN(STRTRAN(STRTRAN(cDocSpd,";",","),"/",","),"|",",")," ",","),"-",","),"_",",")+","
	cNewDcsSpd	:=","+STRTRAN(STRTRAN(STRTRAN(STRTRAN(STRTRAN(STRTRAN(cDcsSpd,";",","),"/",","),"|",",")," ",","),"-",","),"_",",")+","
	lRetFat		:=	.T.

	cNatRcm		:= SuperGetMv("MV_NATRCM",,"")	//Codigo da Natureza de Receita Nao Tributavel correcao monetaria
	cNatDcm		:= SuperGetMv("MV_NATDCM",,"")	//Codigo da Natureza de Despesa Nao Tributavel correcao monetaria
	lDescSe2	:= __lIsBra //Verifica campo TPDESC na tabela SE5 (<C>ondicional ou <I>ncondicional)
	lDescSe5	:= __lIsBra	//Verifica campo TPDESC na tabela SE5 (<C>ondicional ou <I>ncondicional)
	nCm			:= 0
	aSpd		:=	{}
	lEdCprb		:= .F.
	cTpAplAnt	:= ""
	lDbSkip 	:= .T.
	cNatRec1	:= SuperGetMv("MV_NATRNT1",,"") 	//Codigo da Natureza de Receita Nao Tributavel do parâmetro complementar do MV_NATRNT.
	cNatDesp1	:= SuperGetMv("MV_NATDNT1",,"") 	//Codigo da Natureza de Despesa Nao Tributavel do parâmetro complementar do MV_NATDNT.
	cNatRcm1	:= SuperGetMv("MV_NATRCM1",,"")		//Codigo da Natureza de Receita Nao Tributavel correcao monetaria do parâmetro complementar do MV_NATRCM.
	cNatDcm1	:= SuperGetMv("MV_NATDCM1",,"")		//Codigo da Natureza de Despesa Nao Tributavel correcao monetaria do parâmetro complementar do MV_NATDCM.
	c2DuPref  	:= SuperGetMv("MV_2DUPREF",,"")
	cCprb		:= ""
	cFilSED		:=xFilial("SED")
	cFilSe1		:=xFilial("SE1")
	cFilSe2		:=xFilial("SE2")
	cFilSe5		:=xFilial("SE5")
	lPCCBx 		:= SuperGetMv("MV_BX10925",.T.,"2") == "1" 
	lCalcIssBx	:= GetNewPar("MV_MRETISS","1") == "2"  	//Retencao do ISS pela emissao (1) ou baixa (2)
	nStatus 	:= 0
	aCodNatur 	:={}
	aCodSeek  	:={}
	nValCruz	:= 0

	lEnvDesd	:= .F. //Diferencia query da tabela SE2 que seleciona somente desdobramentos.						

	Default nMesRef  := Month(dDataBase)
	Default nAnoRef  := Year(dDataBase)
	Default lTotF100 := .F.
	Default cBloco   := ""

	If __lIsBra

	If __cNatRNTM == Nil
		__cNatRNTM 	:= SuperGetMv("MV_NTRNTM",,"") 	//Codigo da Natureza de Receita Nao Tributavel (multa).
	EndIf
	If __lVM2F100 == Nil
		__lVM2F100 := SuperGetMV('MV_VM2F100',,.F.) == .T.
	EndIf
	If __lVM2F100
		cTpSe5		+= ",'VM'"
		cTpVM		:= "_VM"
	EndIf
	
	cDataIni := If(cDataIni	= Nil, StrZero(nAnoRef,4)+StrZero(nMesRef,2)+"01", cDataIni)
	dLastDay := LastDay(Ctod("01/"+StrZero(nMesRef,2)+"/"+StrZero(nAnoRef,4))) 
	cDataFim := If(cDataFim	= Nil, StrZero(nAnoRef,4)+StrZero(nMesRef,2)+StrZero(Day(dLastDay),2), cDataFim)

	//Naturezas não tributaveis
	Aadd(aCodNatur, {cNatRec ,cNatRec1, __cNatRNTM})//Natureza de Receita Nao Tributavel
	Aadd(aCodNatur, {cNatRcm ,cNatRcm1 })//Natureza de Receita Nao Tributavel de correcao monetaria 
	Aadd(aCodNatur, {cNatDesp,cNatDesp1})//Natureza de Despesa Nao Tributavel
	Aadd(aCodNatur, {cNatDcm ,cNatDcm1 })//Natureza de Despesa Nao Tributavel de correcao monetaria 

	If !Empty(cNatRec1)
		cNatRec += ";"+cNatRec1
	EndIf
	If !Empty(cNatDesp1) 
		cNatDesp += ";"+cNatDesp1
	EndIf
	If !Empty(cNatRcm1) 
		cNatRcm += ";"+cNatRcm1
	EndIf
	If !Empty(cNatDcm1) 
		cNatDcm += ";"+cNatDcm1
	EndIf

	If lTotF100 //Caso seja realizada a totalizacao para o RH um arquivo DBF vai ser criado para o Relatorio de Desoneracao dos Dados do Financeiro.
		//Criando arquivo para o Relatorio de Desoneracao dos Dados do Financeiro.
		aTitRH := {}
		aTamRH:=TamSX3("E5_FILIAL")                         
		AADD(aTitRH,{"FILIAL"  ,"C",aTamRH[1],aTamRH[2]})      			
		aTamRH:=TamSX3("E5_PREFIXO")
		AADD(aTitRH,{"PREFIXO"    ,"C",aTamRH[1],aTamRH[2]})
		aTamRH:=TamSX3("E5_NUMERO")
		AADD(aTitRH,{"NUMERO"   ,"C",aTamRH[1],aTamRH[2]})
		aTamRH:=TamSX3("E5_PARCELA")
		AADD(aTitRH,{"PARCELA" ,"C",aTamRH[1],aTamRH[2]})
		aTamRH:=TamSX3("E5_TIPO")                                                     
		AADD(aTitRH,{"TIPO"   ,"C",aTamRH[1],aTamRH[2]})
		aTamRH:=TamSX3("E5_CLIFOR")
		AADD(aTitRH,{"CLIENTE" ,"C",aTamRH[1],aTamRH[2]})
		aTamRH:=TamSX3("E5_LOJA")
		AADD(aTitRH,{"LOJA"   ,"C",aTamRH[1],aTamRH[2]})
		aTamRH:=TamSX3("E5_DATA")
		AADD(aTitRH,{"DTMOV" ,"C",8,0})
		aTamRH:=TamSX3("E1_NATUREZ")
		AADD(aTitRH,{"NATUREZ"   ,"C",aTamRH[1],aTamRH[2]})
		aTamRH:=TamSX3("E5_VALOR")
		AADD(aTitRH,{"VALOR"   ,"N",aTamRH[1],aTamRH[2]})
		AADD(aTitRH,{"TABELA" ,"C",8,0})

		//Nome do arquivo FIN+Nome Filial+DES+mesano.
		cAliasRH	:= "FINDESRH" 
		cNomeRH	:= "FIN" + Alltrim(xFilial("SE1")) + "DES" + Alltrim(Str(nMesRef)) + Alltrim(Str(nAnoRef)) + ".DBF"	
		
		cIndRH		:=	CriaTrab(Nil,.F.)
		
		cChaveRH   := "FILIAL+PREFIXO+NUMERO+PARCELA+TIPO+CLIENTE+LOJA" 
		
		If _oFINXSPD1 <> Nil
			nStatus := TCSQLEXEC("DELETE FROM "+__cFin1Name)

			if nStatus < 0 .Or. SELECT(cAliasRH)<=0 
				_oFINXSPD1:Delete()
				_oFINXSPD1 	:= Nil
				__cFin1Name := ""
			Else
				(cAliasRH)->(DBGOTO(1))
			Endif
		Endif

		If _oFINXSPD1 == Nil
			_oFINXSPD1 := FWTemporaryTable():New( cAliasRH )  
			_oFINXSPD1:SetFields(aTitRH) 	
			_oFINXSPD1:AddIndex("1", StrToKarr2( cChaveRH, "+")) 	
			_oFINXSPD1:Create()

			__cFin1Name := _oFINXSPD1:GetRealName()
		EndIf
		DbSetOrder(1) 					
	Endif

	dbSelectArea("SED")

	//Identifico se tenho unidade de negócios.
	If __lUnidNeg == Nil
		__lUnidNeg := FWSizeFilial() > 2	// Indica se usa Gestao Corporativa
	EndIf
	If lEdF100 == Nil
		lEdF100 := SED->(ColumnPos("ED_F100"))	> 0
	EndIf

	cTipo	:=	"" //Tratamentos para o ponto de entrada SPDF1002
	If lSpdTipo
		cTipo := ExecBlock ("SPDF1002",.F.,.F.)	
	Endif
	If !Empty(cTipo) // Inserindo aspas e virgulas para o select da query.
		cNome:="'"
		For nA:=1 To Len(cTipo)
			If Subst(cTipo,nA,1) $ ";,-_.|/"
			cNome+="','"
		Else
			cNome+=Subst(cTipo,nA,1)
		Endif
		Next
	Endif
	cNome	+=	"'"
	If !Empty(cTipo) //Verificando o tamanho dos tipos de titulos fornecidos pelo usuario.
		cTpSel:=""
		cTipo:=""
		For nA:=1 To Len(cNome)
			If Subst(cNome,nA,1) $ "'"
				If Len(cTipo) > 1 .And. Len(cTipo) < 3
					cTpSel+=Space(1)
				Endif
					cTpSel+=Subst(cNome,nA,1)
				cTipo:=""
		ElseIf Subst(cNome,nA,1) $ ","
			cTpSel+=Subst(cNome,nA,1)
			Else
			cTpSel+=Subst(cNome,nA,1)
				cTipo+=Subst(cNome,nA,1)
		Endif
		Next
	Endif

	cOrigem	:=	"" //Tratamentos para o ponto de entrada SPDF1003
	If lSpdOrig
		cOrigem := ExecBlock ("SPDF1003",.F.,.F.)
	Endif		

	cNOrigem	:=	"" //Ponto de Entrada que das origens que nao devem ser checadas para o envio no bloco F100.
	If lSpdNOrig
		cNOrigem := ExecBlock ("SPDF1005",.F.,.F.)	 
		cNOrigem :=	SpdVldVar( cNOrigem )  
	Endif

	If lAplSped .And. !Empty(cNatApl) // Tratamento para o parametro MV_APLNAT.
		Aadd(aAlias,"SEI")	
		cNomeApl:="'"	// Inserindo aspas e virgulas para o select da query.
		For nA:=1 To Len(cNatApl)
			If Subst(cNatApl,nA,1) $ ";,-_|/" 				 
			cNomeApl+="','"
		Else
			cNomeApl+=Subst(cNatApl,nA,1)  
		Endif
		Next
		
		cNomeApl	+=	"'"
		cTpApl	:=""
		cNatApl	:=""
		For nA:=1 To Len(cNomeApl)			
			If Subst(cNomeApl,nA,1) $ "'" 				 
			If Len(cNatApl) > 1 .And. Len(cNatApl) < 10
				cTpApl+=Space(1)    	  	      
			Endif  	  	         
			cTpApl+=Subst(cNomeApl,nA,1)  	  	   
			cNatApl:=""	  	   
		ElseIf Subst(cNomeApl,nA,1) $ "," 
			cTpApl+=Subst(cNomeApl,nA,1)  
		Else
			cTpApl	+=Subst(cNomeApl,nA,1) 
				cNatApl	+=Subst(cNomeApl,nA,1) 	   	 
		Endif
		Next	
	Endif
	cTpAplAnt	:= cTpApl
	aRecNTrib	:=	{}	
	nAliasSe2	:=	0
				
	For nA:= 1 to Len(aAlias)
		If aAlias[nA] == "SE5"
			cAliasQry	:= "SE5QRY"
		Elseif aAlias[nA] == "SEI"
			cAliasQry	:= "SEIQRY"
		Elseif aAlias[nA] == "SE1"
			cAliasQry	:= "SE1QRY"
		Elseif aAlias[nA] == "SE2"
			cAliasQry	:= "SE2QRY"
		Endif

