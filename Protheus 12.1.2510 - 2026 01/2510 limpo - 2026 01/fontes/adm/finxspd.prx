nTamTit		:= TamSX3("E1_PREFIXO")[1]+TamSX3("E1_NUM")[1]+TamSX3("E1_PARCELA")[1]+1
nTamTipo	:= TamSX3("E1_TIPO")[1]

For nA:= 1 to Len(aAlias)             

	If aAlias[nA] == "SE5"
