Local aRotina	:= {}

Default cAlias	:= "SN3"

//Botıes da rotina de apontamentos
If cAlias == "SN3"
	aRotina	:= {{STR0001	, "AxPesqui"	,0 , 1, 			,.F.},;		//"Pesquisar"
				{STR0002	, "AxVisual"	,0 , 2					},;		//"Visualizar"
