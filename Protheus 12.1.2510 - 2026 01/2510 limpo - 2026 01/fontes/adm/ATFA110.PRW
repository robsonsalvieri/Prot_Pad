Local lRet			:= .F.
Local aParamBox		:= {}
Local cParTit		:= STR0027		//"Estornos m˙ltiplos"
Local bParOk		:= {|| AF110ValP1(cDataMov)}
Local aParamRet		:= {}
Local nTamFil		:= IIF(lFWCodFil, FWGETTAMFILIAL, 2)
Local nTamId		:= TamSX3("FNA_IDMOV")[1]
