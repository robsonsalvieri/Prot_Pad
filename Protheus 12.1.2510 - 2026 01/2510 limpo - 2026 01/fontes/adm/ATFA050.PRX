Static Function CalculoCM( cMoedaAtf, PE_lA30Embra )

	Local lResult	:= .T.

	Private lA30EMBRA	:= PE_lA30Embra


	If cPaisLoc == "CHI"
		lResult := CalcCMCHI( cMoedaAtf )
	ElseIf	cPaisLoc == "ARG"
		lResult := CalcCMARG( cMoedaAtf )
	Else	// Calculo Padrao
		lResult := CalcCMPADRAO( cMoedaAtf )
	Endif

Return ( lResult )






/*
