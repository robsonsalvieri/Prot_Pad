/*/{Protheus.doc} LoadSezTmp1

Responsável por carregar os dados da tabela SEZ na tabela 
temporária SEZTMP com base na tabela SE1/SE2 posicionada.

@author TOTVS
@since 09/09/2024
/*/
//-------------------------------------------------------------------
Static Function LoadSezTmp1(aCposDB as Array ,cCampo as Character)

	Local cAlias 		as Character
	Local cNextAlias 	as Character
	Local cCarteira		as Character
	Local nX 			as Numeric
	Local cQry  		as Character
	Local oQryCon		as Object
	Local acCpoEC		as Array
	Local cCpoEC		as Character
	Local nTotal		as Numeric
	Local cECqry		as Character
	Local lCachQry    	as Logical
	Local cCliFor		as Character
	Default aCposDB 	:= {}
	Default cCampo 		:= ""

	cAlias := "S" + cCampo
	cCarteira	:= 	If(cAlias=="SE1","R","P")
	nX := 0
	cQry := ""
	cNextAlias := GetNextAlias()
	acCpoEC := {}
	cCpoEC  := ""
	cECqry  := ""
	nTotal  := 0
	cCliFor := "_FORNECE"

	If( cAlias == "SE1" )
		cCliFor := "_CLIENTE"
	EndIf

	lCachQry := (FwLibVersion() >= "20211116")

	nTotal := Len(aCposDB)

	For nX:= 1 to nTotal
		If "EZ_EC0" $ Alltrim(aCposDB[nX][1]) 
			AAdd(acCpoEC,aCposDB[nX][1])
		EndIf
	Next nX

	nTotal := Len(acCpoEC)

	// Carregamento dos campos de novas entidades para query
	For nX:= 1 to nTotal		
		cECqry += "SEZ." + acCpoEC[nX] + ","
	Next nX

	cQry := " Select " 				
	cQry += " 	SEZ.EZ_NATUREZ," 	
	cQry += " 	SEZ.EZ_CCUSTO," 	
	cQry += " 	SEZ.EZ_CONTA," 		
	cQry += " 	SEZ.EZ_ITEMCTA," 	
	cQry += " 	SEZ.EZ_CLVL," 		
	cQry += " 	SEZ.EZ_VALOR," 		
	cQry += " 	SEZ.EZ_PERC, 	" 	
	cQry += " 	? 	" 				
	cQry += " 	SEZ.R_E_C_N_O_	" 	
	cQry += " From	" 				
	cQry += " 	? SEZ"
	cQry += " Where	" 				
	cQry += " 	SEZ.EZ_FILIAL  = ? AND"
	cQry += " 	SEZ.EZ_PREFIXO = ? AND"
	cQry += " 	SEZ.EZ_NUM	   = ? AND"
	cQry += " 	SEZ.EZ_PARCELA = ? AND"
	cQry += " 	SEZ.EZ_TIPO	   = ? AND"
	cQry += " 	SEZ.EZ_CLIFOR  = ? AND"
	cQry += " 	SEZ.EZ_LOJA	   = ? AND"	
	cQry += " 	SEZ.EZ_RECPAG  = ? AND"
	cQry += " 	SEZ.EZ_IDENT   = ? AND"
	cQry += " 	SEZ.D_E_L_E_T_ = ? "
	
	cQry := ChangeQuery(cQry)
	
	oQryCon := If ( lCachQry, FWExecStatement():New(cQry), FWPreparedStatement():New(cQry) )
	oQryCon:SetUnsafe(1,cECqry)
    oQryCon:SetUnsafe(2,RetSqlName("SEZ"))
    oQryCon:SetString(3,xFilial("SEZ"))	
	oQryCon:SetString(4,(cAlias)->&(cCampo + "_PREFIXO"))
	oQryCon:SetString(5,(cAlias)->&(cCampo + "_NUM"))
	oQryCon:SetString(6,(cAlias)->&(cCampo + "_PARCELA"))
	oQryCon:SetString(7,(cAlias)->&(cCampo + "_TIPO"))
	oQryCon:SetString(8,(cAlias)->&(cCampo + cCliFor))	
	oQryCon:SetString(9,(cAlias)->&(cCampo + "_LOJA"))
	oQryCon:SetString(10,cCarteira)
	oQryCon:SetString(11,"1")
	oQryCon:SetString(12," ")

	cNextAlias := IIf(lCachQry, oQryCon:OpenAlias(), MpSysOpenQuery(oQryCon:GetFixQuery()))

	(cNextAlias)->(DBGoTop())
    While (cNextAlias)->(!Eof())
		RecLock("SEZTMP", .T.)
		SEZTMP->EZ_NATUREZ	:= (cNextAlias)->EZ_NATUREZ
		SEZTMP->EZ_CCUSTO	:= (cNextAlias)->EZ_CCUSTO
		SEZTMP->EZ_CONTA	:= (cNextAlias)->EZ_CONTA
		SEZTMP->EZ_ITEMCTA	:= (cNextAlias)->EZ_ITEMCTA
		SEZTMP->EZ_CLVL   	:= (cNextAlias)->EZ_CLVL
		SEZTMP->EZ_VALOR	:= Round(NoRound((cNextAlias)->EZ_VALOR,3),2)
		SEZTMP->EZ_PERC		:= (cNextAlias)->EZ_PERC
		SEZTMP->EZ_RECNO	:= (cNextAlias)->R_E_C_N_O_
		SEZTMP->EZ_LA   	:= 'N'
		// Carregamento dos campos de novas entidades
		For nX:= 1 to Len(acCpoEC)		
			cCpoEC := acCpoEC[nX]
			SEZTMP->(&cCpoEC) := (cNextAlias)->(&cCpoEC)			
		Next nX
		MsUnLock()
        (cNextAlias)->(dbSkip())
    EndDo
    (cNextAlias)->(dbCloseArea())
	FwFreeObj(oQryCon)
	FwFreeArray(acCpoEC)	
return
