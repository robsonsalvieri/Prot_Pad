Static Function AdmAbreSM0()
	Local aArea			:= SM0->( GetArea() )
	Local aAux			:= {}
	Local aRetSM0		:= {}
	Local lFWLoadSM0	:= .T.
	Local lFWCodFilSM0 	:= .T.

	If lFWLoadSM0
		aRetSM0	:= FWLoadSM0()
	Else
		DbSelectArea( "SM0" )
		SM0->( DbGoTop() )
		While SM0->( !Eof() )
			aAux := { 	SM0->M0_CODIGO,;
						IIf( lFWCodFilSM0, FWGETCODFILIAL, SM0->M0_CODFIL ),;
						"",;
						"",;
						"",;
						SM0->M0_NOME,;
						SM0->M0_FILIAL }

			aAdd( aRetSM0, aClone( aAux ) )
			SM0->( DbSkip() )
		End
	EndIf

	RestArea( aArea )
Return aRetSM0

Static Function FindSN1(cBase,cItem)
Local cQuery 	:= ""
Local cAliasTMP := GetNextALias()
Local nCount    := 0
Local nTamFil   := TamSx3("N1_FILIAL")[1]

DEFAULT cBase := ""
DEFAULT cItem := ""

cQuery := " SELECT "
cQuery += "   COUNT(N1_FILIAL) COUNT "
cQuery += " 	FROM "
cQuery +=  		RetSQLName("SN1")+" SN1 "
cQuery += " 		WHERE "
cQuery += " 			N1_FILIAL BETWEEN '"+Space(nTamFil)+"' AND '"+Replicate("Z",nTamFil)+"' AND "
cQuery += " 			N1_CBASE = '"+cBase+"' AND "
cQuery += " 			N1_ITEM = '"+cItem+"' AND "
cQuery += " 			N1_STATUS <> '4' AND "
cQuery += " 			SN1.D_E_L_E_T_ = ' ' "

dbUseArea( .T. , "TOPCONN" , TcGenQry(,,cQuery) , cAliasTMP , .T. , .T.)

nCount := (cAliasTMP)->COUNT
(cAliasTMP)->(DBCloseArea())

Return nCount

Static Function FindSN3(cBase,cItem,cTipo)
Local cQuery 	:= ""
Local cAliasTMP := GetNextALias()
Local nCount    := 0
Local nTamFil   := TamSx3("N1_FILIAL")[1]
Local cFilAux	:= ""

DEFAULT cBase := ""
DEFAULT cItem := ""
DEFAULT cTipo := ""

cQuery := " SELECT "
cQuery += "  	N3_FILIAL, N3_CBASE, N3_ITEM, N3_BAIXA "
cQuery += " 	FROM "
cQuery +=  		RetSQLName("SN3")+" SN3 "
cQuery += " 		WHERE "
cQuery += " 			N3_FILIAL BETWEEN '"+Space(nTamFil)+"' AND '"+Replicate("Z",nTamFil)+"' AND "
cQuery += " 			N3_CBASE = '"+cBase+"' AND "
cQuery += " 			N3_ITEM = '"+cItem+"' AND "
cQuery += " 			N3_TIPO = '"+cTipo+"' AND "
cQuery += " 			SN3.D_E_L_E_T_ = ' ' "

dbUseArea( .T. , "TOPCONN" , TcGenQry(,,cQuery) , cAliasTMP , .T. , .T.)

While !(cAliasTMP)->(Eof()) 
	If cFilAux <> (cAliasTMP)->N3_FILIAL
		cBaixa := (cAliasTMP)->N3_BAIXA //Só verifico do primeiro para manter a regra do seek anterior a esta alteração		
		If  !(cBaixa=='1' .AND. SN1->(dbSeek((cAliasTMP)->(N3_FILIAL+N3_CBASE+N3_ITEM))) .AND. SN1->N1_STATUS=='4')			
    		nCount++
		Endif				
		cFilAux := (cAliasTMP)->N3_FILIAL
	EndIf
	(cAliasTMP)->(dbSkip())
EndDo
(cAliasTMP)->(DBCloseArea())

Return nCount
