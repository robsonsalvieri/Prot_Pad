/*/{Protheus.doc} F340GrvLiq
Função para gravação da SE5 e FK's do TX 

@Param nValorBx, Valor que sera baixado
@Param nSaldoTx, Saldo do TX que sera gravado
@Param nRecBx, Recno do TX para posicionamento na SE2

@Return lRet, Se conseguiu efetuar a gravação na tabelas
@author Vitor Duca
@since 11/12/2019
@version P12
/*/
//-----------------------------------------------------------------------
Static Function F340GrvLiq(nValorBx As Numeric, nSaldoTx As Numeric, cSeqBx As Character, nRecBx As Numeric, cDocTXA As Character)
	Local lBaixou 	As Logical 
	Local cLog		As Character		
	Local oBxImp	As Object
	Local lRet		As Logical
	Local aAreaSE2	As Array

	Default cSeqBx 		:= "00"
	Default nValorBx 	:= 0
	Default nSaldoTx	:= 0
	Default nRecBx		:= 0
	Default cDocTXA		:= ""

	//Inicialização das variaveis
	lBaixou 	:= .F.
	cLog		:= ""
	oBxImp		:= NIL
	lRet		:= .T.
	aAreaSE2	:= SE2->(GetArea())

	SE2->(DbGoTo(nRecBx))
	RecLock("SE2",.F.)
		SE2->E2_SALDO 	:= nSaldoTx
		SE2->E2_BAIXA 	:= dBaixa
		SE2->E2_VALLIQ 	:= nValorBx
		SE2->E2_MOVIMEN := dBaixa
	SE2->(MsUnLock())

	oBxImp := BxImposto():new()
	oBxImp:ModelOper(3)
	oBxImp:SetMotBx("IPA")
	oBxImp:SetSeqBx(cSeqBx)
	oBxImp:SetDocumen(cDocTXA)
	oBxImp:SetVlrBx(nValorBx)
	oBxImp:SetHistorico(STR0116)//"Imposto pago antecipadamente via TXA"
	oBxImp:Activate()
	lBaixou := oBxImp:Grava()
	
	If !lBaixou
		cLog := oBxImp:GetErrorGrava()
		Help( ,,"M340VALRE",,cLog, 1, 0 )
		lRet := .F.
	Endif
	
	oBxImp:Limpa()
	oBxImp := Nil

	RestArea(aAreaSE2)
Return lRet		

/*/{Protheus.doc} F340GRVK34
	Função para definir os valores da FK3 e FK4, quando houver retenção da baixa da 
	nf na compensação, via modelo FINM020

	@param aImpFK3FK4, array, Vetor com os impostos que geraram TX para a NF na compensação
	@sample aImpFK3FK4[1] = Código do imposto
			aImpFK3FK4[2] = Valor do imposto calculado
			aImpFK3FK4[3] = Natureza do imposto
			aImpFK3FK4[4] = IDRET do imposto
			aImpFK3FK4[5] = Imposto retido
			aImpFK3FK4[6] = Base de cálculo do imposto
			aImpFK3FK4[7] = Base de Retenção do imposto
	@param cFunName, char, Nome da função de onde a compensação está sendo chamada
	@param oSubFK3, object, Objeto do submodelo de gravação da FK3
	@param oSubFK4, object, Objeto do submodelo de gravação da FK4
	@param cIdFK2, char, Id da baixa
	@param aTitCalc, array, Recnos dos títulos que compõem a base de retenção
	@return Nil

	@author pedro.alencar
	@since 23/09/2019
	@version 12.1.27
	@type Static Function
/*/
Static Function F340GRVK34(aFK3FK4 As Array, cFunName As Char, oSubFK3 As Object, oSubFK4 As Object, cIdFK2 As Char, aTitCalc As Array, aCFGTribut As Array)
	Local lFKCodRet  As Logical
	Local nX 		 As Numeric		
	Local nQtdImpos  As Numeric
	Local nCFGTribut As Numeric
	Local nPosicao   As Numeric
	Local cIdFK4     As Character
	
	//Parâmetros de entrada
	Default aFK3FK4    := {}
	Default cFunName   := FunName()
	Default oSubFK3    := Nil
	Default oSubFK4    := Nil
	Default cIdFK2 	   := ""
	Default aTitCalc   := {}
	Default aCFGTribut := {}
	
	//Inicializa variáveis.
	lFKCodRet  := (cPaisLoc == "BRA" .and. FK3->(FieldPos("FK3_CODRET")) > 0 .and. FK4->(FieldPos("FK4_CODRET")) > 0)
	nX         := 0
	nQtdImpos  := Len(aFK3FK4) 
	nCFGTribut := Len(aCFGTribut)
	
	If __nDedSimpl == Nil
		__nDedSimpl  := SuperGetMV("MV_FVL1171", .F., 528) //Valor do desconto simplificado do IRPF
	EndIf
	
	If __lDicSimpl == NIL
		__lDicSimpl := (AliasInDic("DKE") .And. DKE->(FieldPos("DKE_IRSIMP")) > 0) .And. FK4->(FieldPos("FK4_DEDIRS")) > 0
    Endif
	
	For nX := 1 To nQtdImpos
		If aFK3FK4[nX][2] > 0 //Imposto calculado			
			If aFK3FK4[nX][5] > 0
				cIdFK4 := FINFKSID("FK4", "FK4_IDFK4")
				
				If __lCFGTrib .And. nCFGTribut .And. !Empty(aFK3FK4[nX,8]) .And. (nPosicao := AScan(aCFGTribut, {|CFG| AllTrim(CFG[1]) == aFK3FK4[nX,8]})) > 0
					aCFGTribut[nPosicao,6] := cIdFK4
				EndIf
				
				aFK3FK4[nX,4] := cIdFK4
			EndIf
			
			If !oSubFK3:IsEmpty()
				oSubFK3:AddLine()
			EndIf
			
			oSubFK3:GoLine(oSubFK3:Length())			
			oSubFK3:SetValue("FK3_IDFK3",  FINFKSID("FK3", "FK3_IDFK3"))
			oSubFK3:SetValue("FK3_DATA",   dBaixa)
			oSubFK3:SetValue("FK3_ORIGEM", cFunName)
			oSubFK3:SetValue("FK3_IMPOS" , aFK3FK4[nX][1])
			oSubFK3:SetValue("FK3_RECPAG", "P")
			oSubFK3:SetValue("FK3_MOEDA",  "01")
			oSubFK3:SetValue("FK3_VALOR",  aFK3FK4[nX][2])
			oSubFK3:SetValue("FK3_NATURE", aFK3FK4[nX][3])
			oSubFK3:SetValue("FK3_FILORI", SE2->E2_FILORIG)
			oSubFK3:SetValue("FK3_BASIMP", aFK3FK4[nX][6])
			oSubFK3:SetValue("FK3_IDORIG", cIdFK2 )
			oSubFK3:SetValue("FK3_TABORI", "FK2")
			oSubFK3:SetValue("FK3_IDRET",  aFK3FK4[nX,4])
			
			If __lTemMR
				oSubFK3:SetValue("FK3_CODFKM", aFK3FK4[nX,8])
				oSubFK3:SetValue("FK3_CLIFOR", aFK3FK4[nX,9])
				oSubFK3:SetValue("FK3_LOJA",   aFK3FK4[nX,10])
				oSubFK3:SetValue("FK3_CGC",    aFK3FK4[nX,11])
				oSubFK3:SetValue("FK3_RAICGC", Substr(aFK3FK4[nX,11], 1, 8))
			EndIf
			
			If lFKCodRet
				oSubFK3:SetValue("FK3_CODRET", aFK3FK4[nx,12])
			EndIf
			
			If aFK3FK4[nX][5] > 0 //Imposto retido
				If !oSubFK4:IsEmpty()
					oSubFK4:AddLine()
				EndIf
				
				oSubFK4:GoLine( oSubFK4:Length() )
				oSubFK4:SetValue("FK4_IDFK4",  aFK3FK4[nX,4])
				oSubFK4:SetValue("FK4_DATA",   dBaixa )
				oSubFK4:SetValue("FK4_ORIGEM", cFunName )
				oSubFK4:SetValue("FK4_IMPOS",  aFK3FK4[nX][1])
				oSubFK4:SetValue("FK4_RECPAG", "P" )
				oSubFK4:SetValue("FK4_MOEDA", "01" )
				oSubFK4:SetValue("FK4_VALOR",  aFK3FK4[nX][5])
				oSubFK4:SetValue("FK4_NATURE", aFK3FK4[nX][3])
				oSubFK4:SetValue("FK4_FILORI", SE2->E2_FILORIG)
				oSubFK4:SetValue("FK4_BASIMP", aFK3FK4[nX][7])
				
				If __lTemMR
					oSubFK4:SetValue("FK4_CODFKM", aFK3FK4[nX,8])
					oSubFK4:SetValue("FK4_CLIFOR", aFK3FK4[nX,9])
					oSubFK4:SetValue("FK4_LOJA",   aFK3FK4[nX,10])
					oSubFK4:SetValue("FK4_CGC",    aFK3FK4[nX,11])
					oSubFK4:SetValue("FK4_RAICGC", Substr(aFK3FK4[nX,11], 1, 8))
				EndIf
				
				If lFKCodRet
					oSubFK4:SetValue("FK4_CODRET", aFK3FK4[nx,12] )
				EndIf
				
				If Len(aFK3FK4[nX]) > 12 .And. aFK3FK4[nX][13] .And. __lDicSimpl
					oSubFK4:SetValue("FK4_DEDIRS", __nDedSimpl )
				Endif
			EndIf
		EndIf
	Next nX
	
	//Gravo o relacionamento de retenção dos títulos que tiveram impostos retidos na baixa atual
	FinFk3BCR(aTitCalc, aFK3FK4)
	FZerSFQIr()
Return Nil

/*/{Protheus.doc} F340DicBx
