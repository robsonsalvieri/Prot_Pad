							nTotalCmp += If(Val(SE5->E5_MOEDA) == 1, SE5->E5_VALOR, SE5->E5_VLMOED2)
						EndIf

						/*------------------------------+
						| Integração SIGAFIN x SIGAGFR. |
						+------------------------------*/
						If lIntMNT .And. SubStr( SE2->E2_ORIGEM, 1, 4 ) == 'MNTA'

							/*--------------------------------------+
							| Realiza o processo de baixa da multa. |
							+--------------------------------------*/
							NGBAIXASE2( 1 )

							/*-----------------------------------------+
							| Variável utilizada no processo de multa. |
							+-----------------------------------------*/
							lPagAutFin := .F.

							/*----------------------------+
							| Gera APD referente a multa. |
							+----------------------------*/
							MNT765CONF( 3 )

