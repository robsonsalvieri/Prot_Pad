	Default nRecTmpSE2 := 0
	Default nAcresc    := 0
	Default nDecres    := 0
	Default aBaixaSE5  := {}
	Default nVlrImpBx  := 0
	Default nPosTit    := 1
	Default lAutomato  := .F.
	Default aFK3FK4    := {}
	Default aBStExpPrm := {}
