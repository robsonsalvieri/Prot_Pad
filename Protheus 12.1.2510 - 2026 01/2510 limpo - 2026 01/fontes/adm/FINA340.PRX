/*/{Protheus.doc}FRetImpPa
	Retorna um array com os impostos devidos que não foram calculados (PA Bruto).

	A função irá verificar se o total de impostos gerados vs total ja compensado e diferente. 
	Caso o total compensado for maior que o gerado, sera calculado a diferença e criado os TXs.

	OBS: A consulta foi criada a partir da SE5, pois não foi possível validar os VRETS através das FKs 
		 para controlar quando deve ser gerada a diferença de impostos.

	@author Cesar Almeida
	@since 17/12/2025

	@return aRetorno, Array, Vetor com os valores de base e impostos, PCC, IRRF e ISS.
/*/
Static Function FRetImpPa(cChavePa As Character, nVLrPA As Numeric, nVlrNF As Numeric, nImpPA As Numeric) AS Array

	Local cQuery		As Character
	Local cAliasQry 	As Character
	Local cChaveDoc		As Character
	Local cTpDocEst 	As Character
	Local cMotBx 		As Character
	Local cDelete   	As Character
	Local nOrdem    	As Numeric
	Local nValIrf		As Numeric
	Local nValIss		As Numeric
	Local nDifCmp   	As Numeric
	Local nBaseCmp  	As Numeric
	Local nPropoc		As Numeric
	Local lDifCmp   	As Logical 
	Local aVlrPcc		As Array
	Local aRetDifCmp 	As Array

	Default cChavePa 	:= ""
	Default nVLrPA		:= 0
	Default nVlrNF 		:= 0
	Default nImpPA   	:= 0

	cQuery     	:= ""
	cAliasQry   := ""
	cChaveDoc	:= PadR(cChavePa, TamSx3("E5_DOCUMEN")[1], " ")
	cTpDocEst	:= "ES"
	cMotBx 		:= "CMP"
	cDelete   	:= " "
	nOrdem		:= 1
	nValIrf		:= 0
	nValIss		:= 0
	nDifCmp		:= 0
	nBaseCmp	:= 0
	nPropoc		:= 0
	lDifCmp		:= .F.
	aVlrPcc		:= {}
	aRetDifCmp 	:= {}
	
	If __oRtImpPa == Nil

		cQuery := "SELECT ISNULL(SUM(SE5.E5_VRETPIS+SE5.E5_VRETCOF+SE5.E5_VRETCSL+SE5.E5_VRETIRF+SE5.E5_VRETISS), 0) TOTRET "
		cQuery += ",ISNULL(SUM(SE5.E5_BASEIRF), 0) AS BASEIMP "
		cQuery += "FROM "  + RetSqlName("SE5") + " SE5 "
		cQuery += "WHERE SE5.E5_DOCUMEN = ? "
		cQuery += "AND SE5.E5_RECPAG = ? "
		cQuery += "AND SE5.E5_TIPODOC IN (?) AND SE5.E5_MOTBX =  ? " 
		cQuery += "AND SE5.E5_ORIGEM NOT IN (?) "
		cQuery += "AND (SE5.E5_VRETPIS > ? OR SE5.E5_VRETCOF > ? OR SE5.E5_VRETCSL > ? OR SE5.E5_VRETIRF > ? OR SE5.E5_VRETISS > ?) " 
		cQuery += "AND SE5.D_E_L_E_T_ = ? "
		cQuery += "AND NOT EXISTS (SELECT EST.E5_SEQ FROM " + RetSqlName("SE5") + " EST "
		cQuery += "WHERE EST.E5_SEQ = SE5.E5_SEQ "
		cQuery += "AND EST.E5_DOCUMEN = ? "
		cQuery += "AND EST.E5_RECPAG = ? AND EST.E5_TIPODOC = ? "
		cQuery += "AND EST.E5_ORIGEM NOT IN (?) "
		cQuery += "AND EST.E5_MOTBX = ? "
		cQuery += "AND EST.D_E_L_E_T_ = ? ) "

		__oRtImpPa := FwExecStatement():New(ChangeQuery(cQuery))

	EndIf

	__oRtImpPa:SetString(nOrdem++, cChaveDoc)
	__oRtImpPa:SetString(nOrdem++, 'P')
	__oRtImpPa:SetIn(nOrdem++, {'BA','CP'})
	__oRtImpPa:SetString(nOrdem++, cMotBx)
	__oRtImpPa:SetIn(nOrdem++, {'MATA103 ', 'MATA103'} )
	__oRtImpPa:SetNumeric(nOrdem++, 0)
	__oRtImpPa:SetNumeric(nOrdem++, 0)
	__oRtImpPa:SetNumeric(nOrdem++, 0)
	__oRtImpPa:SetNumeric(nOrdem++, 0)
	__oRtImpPa:SetNumeric(nOrdem++, 0)
	__oRtImpPa:SetString(nOrdem++, cDelete)
	__oRtImpPa:SetString(nOrdem++, cChaveDoc)
	__oRtImpPa:SetString(nOrdem++, 'R')
	__oRtImpPa:SetString(nOrdem++, cTpDocEst)
	__oRtImpPa:SetIn(nOrdem++, {'MATA103 ', 'MATA103'} )
	__oRtImpPa:SetString(nOrdem++, cMotBx)
	__oRtImpPa:SetString(nOrdem++, cDelete)

	cAliasQry := __oRtImpPa:OpenAlias()

	If !(cAliasQry)->(Eof())

		nDifCmp := nImpPA - (cAliasQry)->TOTRET
		lDifCmp := (NPIS340+NCOF340+NCSL340+NIRF340+NISS340) > nDifCmp 

		If lDifCmp

			nBaseCmp := Round((((cAliasQry)->BASEIMP + __nBase) - (nVlrPA - nImpPA)),2)		

			If nBaseCmp > 0

				If __lPccBaix .And. (NPIS340+NCOF340+NCSL340 > 0)
					
					aVlrPcc := NewMinPcc(dBaixa, nBaseCmp, SE2->E2_NATUREZ, "P", SE2->(E2_FORNECE+E2_LOJA), 0, 0, 0, Nil, .T., "CMP")

					If Len(aVlrPcc) >= 4
						aAdd(aRetDifCmp,{"PIS|COF|CSL", aVlrPcc[2], aVlrPcc[3], aVlrPcc[4]})
					EndIf

				EndIf

				If __lIrfBaix .And. NIRF340 > 0
					nValIrf := FCalcIRBx(nBaseCmp, SA2->A2_TIPO, dBaixa, dBaixa, Nil, Nil, Nil)	
					aAdd(aRetDifCmp,{"IRF", nValIrf})
				EndIF 

				If __lIssBaix .And. NISS340 > 0
					nPropoc := (nBaseCmp/nVlrNF)
					nValIss := Round((nPropoc * SE2->E2_ISS), 2)
					aAdd(aRetDifCmp,{"ISS", nValIss})
				EndIF 

			EndIf
		
		EndIf
	
	EndIf

	(cAliasQry)->(dbCloseArea())
	
Return aRetDifCmp



