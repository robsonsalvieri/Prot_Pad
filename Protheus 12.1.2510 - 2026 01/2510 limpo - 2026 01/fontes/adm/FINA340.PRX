			lIrfPA := SE2->E2_IRRF > 0
			lIssPA := SE2->(E2_ISS+E2_PRISS) > 0
			lInsPA := SE2->(E2_INSS+E2_PRINSS) > 0 
			nIssPr := SE2->E2_PRISS
			lBxPa  := SE2->E2_VALOR != SE2->E2_SALDO  
			nImpPA := SE2->(E2_PIS+E2_COFINS+E2_CSLL+E2_IRRF+E2_ISS+E2_PRISS+E2_INSS+E2_PRINSS)
			
			If !__lPaBruto .And. aTitulos[nTit,20] >= nSldPA
				nImposPA := F340VlrRet(nVlrPA, nImpPA, @nImpEmPA)
				nRetEmPA := SE2->(E2_INSS+E2_PRINSS) + If(__lPccBaix, 0, SE2->(E2_PIS+E2_COFINS+E2_CSLL)) + If(__lIrfBaix, 0, SE2->E2_IRRF) +;
													   If(__lIssBaix, 0, SE2->(E2_ISS+E2_PRISS)) 
			EndIf
		EndIf
		
		//Posiciona o título de NF
		SE2->(DbGoto(nRecNF))
		lRet     := !SE2->E2_TIPO $ MVPAGANT+"|"+MV_CPNEG+"|"+MVABATIM+"|INA|DIC" 
		cFilorig := SE2->E2_FILORIG
		
		If lRet .And. aTitulos[nTit,22] > SE2->E2_SDACRES .And. F340ImpBx(.F., "SE2")
			If __nVlLiqNF == 0
				__nVlLiqNF := SE2->E2_SALDO
				
				If __lPccBaix .And. SE2->(E2_PIS+E2_COFINS+E2_CSLL) > 0
					__nVlLiqNF -= SE2->(E2_PIS+E2_COFINS+E2_CSLL)
				EndIf
				
				If __lIrfBaix .And. SE2->E2_IRRF > 0
					__nVlLiqNF -= SE2->E2_IRRF 
				EndIf
				
				If __lIssBaix .And. SE2->E2_ISS > 0
					__nVlLiqNF -= Iif(__lBtrISS, SE2->(E2_ISS+E2_BTRISS), SE2->E2_ISS)
				EndIf 
			EndIf
			
			nVlrNF  := SE2->E2_VALOR
			nRetEmi := SE2->E2_INSS
			nLiqNF  := SE2->E2_SALDO
			lGerDif := !Empty(SE2->E2_CODRET)
			lIrrfPF := __lIrfBaix .And. (SA2->A2_TIPO == "F" .Or. SA2->A2_IRPROG == "1") .And. !SE2->E2_PRETIRF $ "4"
			
			If SE2->(E2_PIS+E2_COFINS+E2_CSLL) > 0
				If __lPccBaix
					nRetBai += SE2->(E2_VRETPIS+E2_VRETCOF+E2_VRETCSL) 
					nPenRet += SE2->(E2_PIS+E2_COFINS+E2_CSLL) - SE2->(E2_VRETPIS+E2_VRETCOF+E2_VRETCSL)
				Else
					nRetEmi += SE2->(E2_PIS+E2_COFINS+E2_CSLL)
					
					If (__lPaBruto .Or. !lPccPA)
						nRetNF += SE2->(E2_PIS+E2_COFINS+E2_CSLL)
					EndIf
				EndIf
			EndIf
			
			If SE2->E2_IRRF > 0 .Or. lIrrfPF
				If __lIrfBaix 
					nRetBai += SE2->E2_VRETIRF

					If lIrrfPF
						nBasIRPF := salRefPag(SE2->E2_FORNECE + SE2->E2_LOJA,,,,,,, .F.)
						nPenRet  += FCalcIRBx(nBasIRPF, SA2->A2_TIPO, dBaixa, dBaixa, Nil, Nil, Nil)
					Else
						nPenRet += (SE2->E2_IRRF - SE2->E2_VRETIRF)
					EndIf
				Else
					nRetEmi += SE2->E2_IRRF
					
					If (__lPaBruto .Or. !lIrfPA)
						nRetNF += SE2->E2_IRRF
					EndIf
				EndIf	
			EndIf
			
			If SE2->E2_ISS > 0
				If __lIssBaix 
					nRetBai += SE2->E2_VRETISS
					nPenRet += (SE2->E2_ISS - SE2->E2_VRETISS)
				Else
					nIssAux := Iif(__lBtrISS, SE2->E2_ISS + SE2->E2_BTRISS, SE2->E2_ISS )					
					nRetEmi += nIssAux

					If (__lPaBruto .Or. !lIssPA)
						nRetNF += nIssAux
					EndIf
				EndIf		
			EndIf
			
			If SE2->E2_INSS > 0 .And. (__lPaBruto .Or. !lInsPA)
				nRetNF += SE2->E2_INSS
			EndIf
			
			If nMoeda340 > 1
				nPenRet := xMoeda(nPenRet, 1, nMoeda340, dBaixa, __nCasDec, __nTxMoedP, aTitulos[nTit,21])
			Endif
			
			If (AllTrim(SE2->E2_FATURA) == "NOTFAT" .Or. !Empty(SE2->E2_NUMLIQ))
				If AllTrim(SE2->E2_FATURA) == "NOTFAT" 
					aRet := FinFatAbat(SE2->E2_PREFIXO,SE2->E2_NUM,SE2->E2_FORNECE,SE2->E2_LOJA)
				Else
					aRet := FinLiqAbat(SE2->E2_NUMLIQ, @__oPrepQry)
				EndIf			
				
				If !Empty(aRet)
					nTotIss := aRet[1]
					nTotIns := aRet[2]
					nTotIrf := aRet[3]
					nTotPis := aRet[4]
					nTotCof := aRet[5]
					nTotCsl := aRet[6]
				
					FwFreeArray(aRet)

					nRetEmi := nTotIss + nTotIns + nTotIrf + nTotPis + nTotCof + nTotCsl
				EndIf
			EndIf
			
			nVlrNF  += nRetEmi
			__nBase := (aTitulos[nTit,22] - SE2->E2_SDACRES)
			nLiqNF  -= (aTitulos[nTit,23] + SE2->E2_SDDECRE + nPenRet)

			//Abate o valor do DIC ref a comp anteriores quando compensados juntos
			If !Empty(aDicCMP) .And. !__lPaBruto
				nTamADIC := Len(aDicCMP)

				For nX := 1 To nTamADIC
					nDicAnt += aDicCMP[nX][2]
				Next nX

				If __nBase == (nLiqNF - nDicAnt) 
					nLiqNF -= nDicAnt
				EndIf
			EndIf
			
			__nVlCmpPA += aTitulos[nTit,20] 
			lBxTot     := (__nBase >= nLiqNF) .Or. (nTit == Len(aTitulos) .And. (__nVlLiqNF == __nVlCmpPA) .Or. (Abs(__nVlLiqNF - __nVlCmpPA) <= DIFCALC))  
			
			//Define a base de retenção.
			If __lPaBruto .And. lBxTot
				__nBase := (SE2->E2_SALDO + nRetBai + nRetEmi)
				__nBase += TotCmpDic(FINBuscaFK7((SE2->E2_FILIAL+"|"+SE2->E2_PREFIXO+"|"+SE2->E2_NUM+"|"+SE2->E2_PARCELA+"|"+SE2->E2_TIPO+"|"+SE2->E2_FORNECE+"|"+SE2->E2_LOJA), "SE2", SE2->E2_FILORIG))
				
				If nSldPA >= (aTitulos[nTit,20] + nRetEmi)
					aTitulos[nTit,20] += nRetEmi
				EndIf
			ElseIf !__lPaBruto
				If !lBxTot
					If nImposPA > 0 .And. ((__nBase + nImposPA) <= SE2->E2_SALDO)
						aTitulos[nTit,22] += nImposPA
						__nBase += (nImposPA+nImpEmPA) 	
					EndIf 
					
					If !lBxPa .And. aTitulos[nTit,20] == nSldPA .And. SE2->E2_SALDO >= nSldPA  
						__nBase := (nVlrPA - SE2->E2_SDACRES)
						aTitulos[nTit,22] := nVlrPA
						
						//Se o PA possui os mesmos impostos da NF, subtrai os impostos da emissão
						If ((lPccPA .And. SE2->(E2_PIS+E2_COFINS+E2_CSLL) > 0) .Or. (!lPccPA .And. SE2->(E2_PIS+E2_COFINS+E2_CSLL) == 0)) .And.; 
						((lIrfPA .And. SE2->E2_IRRF > 0) .Or. (!lIrfPA .And. SE2->E2_IRRF == 0)) .And.;
						((lIssPA .And. SE2->(E2_ISS+E2_PRISS) > 0) .Or. (!lIssPA .And. SE2->(E2_ISS+E2_PRISS) == 0)) .And.;
						((lInsPA .And. SE2->(E2_INSS+E2_PRINSS) > 0) .Or. (!lInsPA .And. SE2->(E2_INSS+E2_PRINSS) == 0))
						
						aTitulos[nTit,22] := (nVlrPA - nRetEmPA)
						EndIf
					EndIf
					
					If !lImpPA .And. aTitulos[nTit,20] == nSldPA .And. (nLiqNF - nPenRet) == nSldPA
						lBxTot := .T.
					EndIf
				EndIf
				
				//Baixa Total da NF
				If lBxTot
					__nBase := salRefPag(SE2->E2_FORNECE + SE2->E2_LOJA,,,,,,, .F.)
				EndIf	
			EndIf
			
			If lBxTot
				aTitulos[nTit,22] := (SE2->(E2_SALDO+E2_SDACRES) - (SE2->E2_SDDECRE+aTitulos[nTit,23]))
			Else 
				__nBase += SE2->E2_SDDECRE
			EndIf
			
			nPropoc := (__nBase/nVlrNF)
		ElseIf lRet .And. aTitulos[nTit,22] > SE2->E2_SDACRES
			IF SE2->E2_MOEDA == 1
				__nBase := (aTitulos[nTit,22] - SE2->E2_SDACRES) + (SE2->E2_SDDECRE+aTitulos[nTit,23])
			Else
				__nBase := (aTitulos[nTit,22] - xMoeda(SE2->E2_SDACRES, SE2->E2_MOEDA, 1, dDataBase, __nCasDec)) + (xMoeda(SE2->E2_SDDECRE, SE2->E2_MOEDA, 1, dDataBase, __nCasDec)+aTitulos[nTit,23])
			Endif
		EndIf
		
		If !Empty(nPropoc)
			If nMoeda340 > 1
				__nBase := Round(xMoeda(__nBase, nMoeda340, 1, dBaixa, __nCasDec, aTitulos[nTit,21], 0), 2)
			EndIf
			
			If __lPccBaix .And. !__lEmpPub
				aPCC := NewMinPcc(dBaixa, __nBase, SE2->E2_NATUREZ, "P", SE2->(E2_FORNECE+E2_LOJA), 0, 0, 0, Nil, .T., "CMP")
				
				If Len(aPCC) >= 4
					NPIS340 := aPcc[2]
					NCOF340 := aPcc[3]
					NCSL340 := aPcc[4]
					
					If Len(aPCC) > 4
						aTitCalc	:= aPCC[5]
					EndIf				
				EndIf
			ElseIf __lPccBaix .And. __lEmpPub
				nPis		:= 0
				nCofins		:= 0
				nCsll		:= 0
				nIrrf		:= 0
				nIss		:= 0
				nValPgto	:= __nBase
				nOldValPgto := __nBase
				f080TotMes(dBaixa,.T.)
				NPIS340	:= nPis
				NCOF340	:= nCofins
				NCSL340	:= nCsll
			EndIf		
			
			If __lIrfBaix
				If ( SE2->E2_MOEDA == 1 .And. SE2->E2_VLCRUZ <> SE2->E2_BASEIRF )
					__lBsIrRed	:= .T.
					cIdDocFK7	:= FinBuscaFK7(SE2->(E2_FILIAL+"|"+E2_PREFIXO+"|"+E2_NUM+"|"+E2_PARCELA+"|"+E2_TIPO+"|"+E2_FORNECE+"|"+E2_LOJA), "SE2", SE2->E2_FILORIG)
					If ( nBsRetAlrd := RetImp340(cIdDocFK7)[2][1] ) < SE2->E2_BASEIRF
						__nBsIrRed	:= ( min((SE2->E2_BASEIRF  - nBsRetAlrd), __nBase ) )
					EndIf
					NIRF340 := FCalcIRBx(__nBsIrRed, SA2->A2_TIPO, dBaixa, dBaixa, Nil, Nil, Nil)
				Else
					NIRF340 := FCalcIRBx(__nBase, SA2->A2_TIPO, dBaixa, dBaixa, Nil, Nil, Nil)
				EndIf
			EndIf
			
			If __lIssBaix .And. SE2->E2_ISS > 0
				NISS340 := Round((nPropoc * SE2->E2_ISS), 2)
				nIssBaseC := nIssBaseR := Round((nPropoc * SE2->E2_BASEISS), 2)
			EndIf
			
			aTitulos[nTit,19] := (NPIS340+NCOF340+NCSL340+NIRF340+NISS340)
			
			If nMoeda340 > 1
				aTitulos[nTit,19] := xMoeda((NPIS340+NCOF340+NCSL340+NIRF340+NISS340), 1, nMoeda340, dBaixa, __nCasDec, __nTxMoedP, aTitulos[nTit,21])
			Endif
			
			If aTitulos[nTit,19] > 0
				If __lPaBruto
					If lBxTot
						aTitulos[nTit,22] -= aTitulos[nTit,19]

						If nSldPA >= nVlrNF
							aTitulos[nTit,20] := aTitulos[nTit,22]
						EndIf
					EndIf 
					
					If lBxTot .Or. (nSldPA > aTitulos[nTit,20] .And. ((nSldPA - aTitulos[nTit,20]) >= aTitulos[nTit,19])) 
						lGerDic := .F.
					EndIf	
				Else
					aTitulos[nTit,22] -= aTitulos[nTit,19]
					
					If aTitulos[nTit,20] >= aTitulos[nTit,22] .And. (nSldPA > aTitulos[nTit,20] .Or. lBxTot)
						aTitulos[nTit,20] := aTitulos[nTit,22]
						lGerDic := .F.
					EndIf
				EndIf
			EndIf
			
			Reclock("SE2")
			SE2->E2_SALDO -= aTitulos[nTit,19]
			SE2->E2_DIRF := Iif(lGerDif, "2", SE2->E2_DIRF)
			SE2->(MsUnlock())
			
			//Gera na NF as retenções que não foram retidas no PA
			If __lPaBruto .Or. !lPccPA .Or. !lIrfPA .Or. (!lIssPA .Or. nIssPr > 0)

				If !__lBsIrRed
					If __lPaBruto .And. F340ImpBx(.F., "SE2") .And. nImpPA > 0 .And. !lIrrfPF
						aRetDifPA := FRetImpPa(cAdian, nVLrPA, nVlrNF, nImpPA)	
					EndIf
				EndIf

				If NPIS340+NCOF340+NCSL340+NIRF340+NISS340+nVlRetIrf > 0
					//PCC
					If (__lPaBruto .Or. !lPccPA) .And. (NPIS340+NCOF340+NCSL340) > 0
						If !lPccPA
							nPisNF  := NPIS340
							nCofNF  := NCOF340
							nCslNF  := NCSL340 
						ElseIf Len(aRetDifPA) > 0 .And. (nPos := aScan(aRetDifPA,{|x| AllTrim(Upper(x[1])) $ "PIS|COF|CSL"})) > 0
							nPisNF  := aRetDifPA[nPos][2]
							nCofNF  := aRetDifPA[nPos][3]
							nCslNF  := aRetDifPA[nPos][4]
						EndIf
						
						nVlrDic += (NPIS340+NCOF340+NCSL340) 
					EndIf
					
					//IRRF
					If (__lPaBruto .Or. !lIrfPA) .And. NIRF340 > 0
						If !lIrfPA
							nIrfNF  := NIRF340 	
						ElseIf Len(aRetDifPA) > 0 .And. (nPos := aScan(aRetDifPA,{|x| AllTrim(Upper(x[1])) $ "IRF"})) > 0
							nIrfNF  := aRetDifPA[nPos][2]
						EndIf
						
						nVlrDic += NIRF340
					EndIf 
					
					//ISS
					If (__lPaBruto .Or. (!lIssPA .Or. nIssPr > 0)) .And. NISS340 > 0
						If (!lIssPA .Or. nIssPr > 0) 
							nIssNF  := NISS340
						ElseIf Len(aRetDifPA) > 0 .And. (nPos := aScan(aRetDifPA,{|x| AllTrim(Upper(x[1])) $ "ISS"})) > 0
							nIssNF  := aRetDifPA[nPos][2]
						EndIf
						
						If(__lPaBruto .Or. !lIssPA)
							nVlrDic += NISS340
						EndIf
					EndIf
					
					//Impostos retidos na emissão da NF que não foram retidos no PA
					If nRetNF > 0 .And. lBxTot
						nVlrDic += nRetNF
					EndIf
					
					//Geração dos Tx's
					If (nPisNF+nCofNF+nCslNF+nIrfNF+nIssNF) > 0 
						aImpFK3FK4 := F340GrvImp(nPisNF, nCofNF, nCslNF, nIrfNF, nIssNF, lGerDif)				
					EndIf
				ElseIf (NPIS340+NCOF340+NCSL340+NIRF340+NISS340+nVlRetIrf == 0) .And. (nPisCalc+nCofCalc+nCslCalc+nIrfCalc+nIssCalc > 0)
					//Alimenta o array para gravar a FK3 com os valores calculados para quando nao houver retencao
					aImpFK3FK4 := F340GrvImp()
				EndIf
			EndIf     
			
			//Atauliza os Vrets e Prets da NF
			If (NPIS340+NCOF340+NCSL340+NIRF340+NISS340+nVlRetIrf) > 0
				FA340VPSE2()	
			EndIf
			
			//Gera DIC (Diferença de Imposto na Compensação)
			If lGerDic .And. nVlrDic > 0
				Aadd(__aDdsDic, nRecPA)
				Aadd(__aDdsDic, nVlrDic)
				Aadd(__aDdsDic, cAdian)
				Aadd(__aDdsDic, cFilorig)
				Aadd(__aDdsDic, cForn)
			EndIf		
		EndIf
	EndIf
	
	SE2->(DbGoto( __nRecPart ))
Return Nil

/*/{Protheus.doc}FinCFGTrib
	Configurador de Tributos, proporcionalização e geração dos impostos 
	configurados da baixa.
	
	@author Sivaldo Oliveira
	@since  01/08/2025
/*/
Static Function FinCFGTrib(nTit As Numeric, nRecPA As Numeric, nRecNF As Numeric, aFK3FK4 As Array, aCFGTribut As Array)
	Local lGeraDirf  As Logical
	Local lBxFinalNF As Logical
	Local lVldGrvTri As Logical
	Local cIdFK0     As Character
	Local cImpos     As Character
	Local cChaveK7NF As Character	
	Local cIdDocK7NF As Character
	Local cIdDocK7PA As Character
	Local nLinha     As Numeric
	Local nValorMov  As Numeric
	Local nBaseCTrib As Numeric
	Local nCFGRetido As Numeric
	Local nCFGCalcul As Numeric
	Local nCFGTribPA As Numeric
	Local nCFGTribNF As Numeric
	Local nSaldLiqNF As Numeric
	Local nTitulos   As Numeric
	Local nPosicao   As Numeric
	Local nDifCalc   As Numeric
	Local nQtdBasImp As Numeric
	Local nPosTrib   As Numeric
	Local aVetBase   As Array
	Local aBaseImp   As Array
	Local aGrossUp   As Array
	Local aCFGTribNF As Array
	Local aCFGTribPA As Array
	Local aAreaSE2   As Array
	Local aTribCalc  As Array
	Local oJSonDif   As JSon
	Local oTributoNF AS Json
	Local oTributoPA AS JSon
	
	//Parâmetros de entrada
	Default nTit       := 1
	Default nRecPA     := 0 
	Default nRecNF     := 0
	Default aFK3FK4    := {}
	Default aCFGTribut := {}
	
	//Inicializa variáveis local.
	lGeraDirf  := .F.
	lBxFinalNF := .F.
	lVldGrvTri := .F.
	cIdFK0     := ""
	cImpos     := ""
	cChaveK7NF := ""	
	cIdDocK7NF := ""
	cIdDocK7PA := ""
	nLinha     := 0
	nValorMov  := 0
	nBaseCTrib := 0		
	nCFGRetido := 0
	nCFGCalcul := 0
	nCFGTribPA := 0
	nCFGTribNF := 0
	nSaldLiqNF := 0
	nTitulos   := 0
	nPosicao   := 0
	nDifCalc   := 0
	nQtdBasImp := 0
	nPosTrib   := 0
	aVetBase   := Nil
	aBaseImp   := Nil
	aGrossUp   := Nil
	aCFGTribNF := Nil
	aCFGTribPA := Nil
	aAreaSE2   := Nil
	aTribCalc  := Nil
	oJSonDif   := Nil
	oTributoPA := Nil
	oTributoNF := Nil
	
	If nRecPA > 0 .And. nRecNF > 0
		aAreaSE2 := SE2->(FwGetArea())	
		SE2->(DbGoto(nRecPA))
		
		If SE2->E2_TIPO $ MVPAGANT
			cIdDocK7PA := FINBuscaFK7((SE2->E2_FILIAL+"|"+SE2->E2_PREFIXO+"|"+SE2->E2_NUM+"|"+SE2->E2_PARCELA+"|"+SE2->E2_TIPO+"|"+SE2->E2_FORNECE+"|"+SE2->E2_LOJA), "SE2", SE2->E2_FILORIG)
			aCFGTribPA := FinImpConf("1", SE2->E2_FILORIG, SE2->E2_FORNECE, SE2->E2_LOJA, SE2->E2_NATUREZ, cIdDocK7PA, SE2->E2_EMISSAO, @__oImpConf, @__oQryFKQ)
			
			If (nCFGTribPA := Len(aCFGTribPA)) > 0
				oTributoPA := JsonObject():New()
				
				For nLinha := 1 To nCFGTribPA
					If !Empty(cImpos := AllTrim(aCFGTribPA[nLinha,1]))
						If cImpos $ "PIS|COF|CSL"
							cImpos := "PIS-COF-CSL"
						EndIf
						
						If !oTributoPA:HasProperty(cImpos)
							oTributoPA[cImpos] := cImpos
						EndIf
					EndIf
				Next nLinha
			EndIf
			
			FwFreeArray(aCFGTribPA)
			
			SE2->(DbGoto(nRecNF))
			lGeraDirf  := !Empty(SE2->E2_CODRET)
			cChaveK7NF := (SE2->E2_FILIAL+"|"+SE2->E2_PREFIXO+"|"+SE2->E2_NUM+"|"+SE2->E2_PARCELA+"|"+SE2->E2_TIPO+"|"+SE2->E2_FORNECE+"|"+SE2->E2_LOJA)
			cIdDocK7NF := FINBuscaFK7(cChaveK7NF, "SE2", SE2->E2_FILORIG)
			aCFGTribNF := FinImpConf("1", SE2->E2_FILORIG, SE2->E2_FORNECE, SE2->E2_LOJA, SE2->E2_NATUREZ, cIdDocK7NF, SE2->E2_EMISSAO, @__oImpConf, @__oQryFKQ)
			nCFGTribNF := Len(aCFGTribNF)
			
			If nCFGTribNF > 0
				nTitulos   := Len(aTitulos)
				aBaseImp   := {}
				oTributoNF := JsonObject():New()
				
				For nLinha := 1 To nCFGTribNF
					cImpos := AllTrim(aCFGTribNF[nLinha,1])
					
					If cImpos $ "PIS|COF|CSL" .And. !oTributoNF:HasProperty("PIS-COF-CSL")
						oTributoNF["PIS-COF-CSL"] := aCFGTribNF[nLinha,2] == cREGIMECX
					EndIf
					
					If !oTributoNF:HasProperty(cImpos)
						oTributoNF[cImpos] := (aCFGTribNF[nLinha,2] == cREGIMECX)
						
						If oTributoNF[cImpos] .And. aCFGTribNF[nLinha,7] == cPRIMEIRBX
							nQtdBasImp ++
							AAdd(aBaseImp, {cImpos, 0})
						EndIf
					EndIf
				Next nLinha
				
				If (nValorMov := (aTitulos[nTit,22] - SE2->E2_SDACRES)) > 0
					__lCFGTrib := .T.
					aCFGTribut := {}
					lBxFinalNF := (nValorMov == SE2->E2_SALDO)
					aVetBase   := FinBaseMR(SE2->E2_SALDO, .F., SE2->E2_FILIAL, SE2->E2_PREFIXO, SE2->E2_NUM, SE2->E2_PARCELA, SE2->E2_TIPO, SE2->E2_FORNECE, SE2->E2_LOJA, oTributoNF:HasProperty("PIS-COF-CSL"), oTributoNF:HasProperty("IRF"),; 
								oTributoNF:HasProperty("INSS"), oTributoNF:HasProperty("ISS"), oTributoNF:HasProperty("CIDE"), oTributoNF:HasProperty("SEST"), SE2->E2_MOEDA, SE2->E2_TXMOEDA, dBaixa)
					nBaseCTrib := aVetBase[1]
					
					If !lBxFinalNF
						aCFGTribut := FinCalImp("1", SE2->E2_NATUREZ, SE2->E2_FORNECE, SE2->E2_LOJA, SE2->E2_FILORIG, nBaseCTrib, dBaixa, .T., aBaseImp, SE2->E2_TIPO, cChaveK7NF, cIdFK0, {}, .F., aGrossUp, .T.)
						AEval(aCFGTribut, {|aCFG| nCFGRetido += IIf(aCFG[9] == cREGIMECX .And. aCFG[13] == cSUBTRAI .And. aCFG[14] == cCARTEIMOV, aCFG[26], 0)})
						nSaldLiqNF := (SE2->E2_SALDO - Round(nCFGRetido, 2))
						nCFGRetido := 0
						
						If __lPaBruto
							nBaseCTrib := IIf(nValorMov >= nSaldLiqNF, nBaseCTrib, nValorMov)
						ElseIf !(lBxFinalNF := (nValorMov >= nSaldLiqNF))
							AEval(aCFGTribut, {|aCFG| nCFGCalcul += IIf(aCFG[9] == cREGIMECX .And. aCFG[13] == cSUBTRAI .And. aCFG[14] == cCARTEIMOV, aCFG[3], 0)})
							nBaseCTrib := Round((nValorMov / ((100 - ((nCFGCalcul/aVetBase[1]) * 100)) / 100)), 2)
						EndIf
					EndIf
					
					nQtdBasImp := IIf(nQtdBasImp > 0 .And. SE2->E2_VALOR == SE2->E2_SALDO, nQtdBasImp, 0)
					
					For nLinha := 1 To nQtdBasImp
						aBaseImp[nLinha, 2] := aVetBase[1]
					Next nLinha
					
					//Cálculo do imposto
					aCFGTribut := FinCalImp("1", SE2->E2_NATUREZ, SE2->E2_FORNECE, SE2->E2_LOJA, SE2->E2_FILORIG, nBaseCTrib, dBaixa, .T., aBaseImp, SE2->E2_TIPO, cChaveK7NF, cIdFK0, {}, .F., aGrossUp, .T.)
					
					If (nCFGTribNF := Len(aCFGTribut)) > 0
						aVetBase   := {}
						aTribCalc  := {}
						lVldGrvTri := (Len(aCFGTribut[1]) >= 27)
						
						For nLinha := 1 To nCFGTribNF
							If aCFGTribut[nLinha,9] == cREGIMECX .And. aCFGTribut[nLinha,13] == cSUBTRAI .And. aCFGTribut[nLinha,14] == cCARTEIMOV
								nPosTrib := 0
								cImpos   := AllTrim(aCFGTribut[nLinha,8])
								
								If lBxFinalNF .And. SE2->E2_VALOR > SE2->E2_SALDO
									oJSonDif := TributCalc({aCFGTribut[nLinha,8]}, Nil, Nil, SE2->E2_FILORIG, cIdDocK7NF)
									
									If oJSonDif != Nil .And. oJSonDif:HasProperty(aCFGTribut[nLinha,8]) .And. oJSonDif[aCFGTribut[nLinha,8]] > 0
										If Len(aVetBase) == 0
											aVetBase := FinBaseMR(SE2->E2_VALOR, .F., SE2->E2_FILIAL, SE2->E2_PREFIXO, SE2->E2_NUM, SE2->E2_PARCELA, SE2->E2_TIPO, SE2->E2_FORNECE, SE2->E2_LOJA, oTributoNF:HasProperty("PIS-COF-CSL"), oTributoNF:HasProperty("IRF"),; 
														oTributoNF:HasProperty("INSS"), oTributoNF:HasProperty("ISS"), oTributoNF:HasProperty("CIDE"), oTributoNF:HasProperty("SEST"),  SE2->E2_MOEDA, SE2->E2_TXMOEDA, dBaixa)
											
											aTribCalc := FinCalImp("1", SE2->E2_NATUREZ, SE2->E2_FORNECE, SE2->E2_LOJA, SE2->E2_FILORIG, aVetBase[1], dBaixa, .T., aBaseImp, SE2->E2_TIPO, cChaveK7NF, cIdFK0, {}, .F., aGrossUp, .F.)
										EndIf
										
										If Len(aTribCalc) > 0 .And. (nPosicao := AScan(aTribCalc, {|x| AllTrim(x[8]) == AllTrim(aCFGTribut[nLinha,8])})) > 0
											nDifCalc := (aCFGTribut[nLinha,3] + oJSonDif[aCFGTribut[nLinha,8]])
											nDifCalc := (aTribCalc[nPosicao, 3] - nDifCalc)
											
											If nDifCalc != 0 
												aCFGTribut[nLinha,3] := Round((aCFGTribut[nLinha,3] += nDifCalc), 2)
												aCFGTribut[nLinha,5] := Round((aCFGTribut[nLinha,26] + nDifCalc), 2)
											EndIf
										EndIf
									EndIf 
									
									FreeObj(oJSonDif)
								EndIf
								
								AAdd(aFK3FK4, {cImpos, aCFGTribut[nLinha,3], aCFGTribut[nLinha,10], "", aCFGTribut[nLinha,5], aCFGTribut[nLinha,2], aCFGTribut[nLinha,4], AllTrim(aCFGTribut[nLinha,1]), SE2->E2_FORNECE, SE2->E2_LOJA, SA2->A2_CGC, aCFGTribut[nLinha,23], .F.})								
								nPosTrib := IIf(!__lPaBruto .And. nQtdBasImp > 0, AScan(aBaseImp, {|Trib|Trib[1] == cImpos}), nPosTrib)								
								
								If nPosTrib == 0
									nCFGRetido += aCFGTribut[nLinha,5]
								Else
									__nTrib1Bx += aCFGTribut[nLinha,5]
								EndIf
								
								Do Case
									Case cImpos $ "PIS|COF|CSL" .And. oTributoNF["PIS-COF-CSL"]
										If cImpos == "PIS"
											NPIS340 += aCFGTribut[nLinha,5]
										ElseIf cImpos == "COF"
											NCOF340 += aCFGTribut[nLinha,5]
										Else
											NCSL340 += aCFGTribut[nLinha,5]
										EndIf
										
										cImpos	:= "PIS-COF-CSL"
									Case cImpos == "IRF" .And. oTributoNF[cImpos]
										NIRF340 += aCFGTribut[nLinha,5]
									Case cImpos == "ISS" .And. oTributoNF[cImpos]
										NISS340 += aCFGTribut[nLinha,5]
									OtherWise
										If oTributoNF[cImpos]
											NIMPCF340 += aCFGTribut[nLinha,5]
										EndIf
								EndCase
								
								If lVldGrvTri .And. oTributoNF[cImpos]
									aCFGTribut[nLinha,27] := !(nCFGTribPA > 0 .And. oTributoPA:HasProperty(cImpos))
								EndIf
							EndIf
							
							If nLinha == nCFGTribNF .And. nCFGRetido > 0							
								If __lPaBruto								
									If (lBxFinalNF .Or. (nValorMov > SE2->E2_SALDO))
										nValorMov := IIf(lBxFinalNF, aTitulos[nTit,22], SE2->E2_SALDO)
										nValorMov -= nCFGRetido
										
										aTitulos[nTit,22] := nValorMov
										aTitulos[nTit,20] := nValorMov
									EndIf
								ElseIf lBxFinalNF
									nValorMov := (SE2->E2_SALDO - nCFGRetido)
									aTitulos[nTit,22] := nValorMov
									aTitulos[nTit,20] := nValorMov
								EndIf
							EndIf
						Next nLinha
						
						//Atualiza os VR/PR ets
						If (NPIS340+NCOF340+NCSL340+NIRF340+NISS340) > 0
							FA340VPSE2()	
						EndIf
						
						//Dedução o impostos do Saldo da NF
						If nCFGRetido > 0
							SE2->(DbGoto(nRecNF))
							Reclock("SE2")
							SE2->E2_SALDO -= nCFGRetido
							SE2->E2_DIRF := IIf(lGeraDirf, "2", SE2->E2_DIRF)
							SE2->(MsUnlock())
						EndIf
						
						FwFreeArray(aVetBase)
						FwFreeArray(aTribCalc)
					EndIf
				EndIf
				
				FwFreeArray(aBaseImp)
				FWFreeObj(oTributoNF)
			EndIf
			
			FwFreeArray(aCFGTribNF)
			
			If oTributoPA != Nil
				FWFreeObj(oTributoPA)
			EndIf
		EndIf		
		
		FwRestArea(aAreaSE2)
		FwFreeArray(aAreaSE2)
		SE2->(DbGoto( __nRecPart))
	EndIf
Return Nil

/*/{Protheus.doc}F340GrvImp
	Gera na NF os impostos que não foram retidos no PA.

	@author Sivaldo Oliveira
	@since  12/06/2018
	@version 12
/*/
Static Function F340GrvImp(nTxPis, nTxCof, nTxCsl, nTxIrf, nTxIss, lGerDif)	
	Local aAreaSE2  As Array
	Local aAreaSA2  As Array
	Local aRetImp	As Array
	Local aIRGerar	As Array
	Local cFornImp  As Character
	Local cLojaImp  As Character
	Local cNmForn   As Character
	Local cNmRedu   As Character
	Local cParImp   As Character
	Local cTaxa     As Character
	Local cPrefPai  As Character 
	Local cNumPai   As Character
	Local cFilorig  As Character
	Local cCodAprov As Character
	Local cLa       As Character
	Local cFormPag  As Character
	Local cTipoPes  As Character
	Local cTitPai   As Character
	Local cCodPis   As Character
	Local cCodCof   As Character
	Local cCodCsl   As Character
	Local cCodIrf   As Character
	Local cNatImp	As Character
	Local cDesc		As Character
	Local cImpost	As Character
	Local cImpFK4	As Character
	Local cCodRet   As Character
	Local cFornPai  As Character
	Local cLojaPai	As Character
	Local cCNPJPai  As Character
	Local cNatIss   As Character
	Local cNatIrf   As Character
	Local cNatPis   As Character
	Local cNatCof   As Character
	Local cNatCsl   As Character
	Local cForMunic As Character
	Local dVenImp   As Date  
	Local nRecSA2   As Numeric
	Local nRecSED   As Numeric
	Local nX        As Numeric
	Local nVlrTrib  As Numeric
	Local nTrbCalc 	As Numeric
	Local nBasRet 	As Numeric
	Local nTitAGer	As Numeric
	Local nY        As Numeric
	Local lRatIRPF  As Logical
	Local nPisOri   As numeric
	Local nCofOri   As numeric
	Local nCslOri   As numeric
	Local lSimpIRAlu	As Logical
	Local lIntPFS   As Logical
	
	Default nTxPis 	:= 0
	Default nTxCof 	:= 0
	Default nTxCsl 	:= 0
	Default nTxIrf 	:= 0
	Default nTxIss 	:= 0
	Default lGerDif := .F.
	
	//Inicializa variáveis
	aAreaSE2  	:= SE2->(GetArea())
	aAreaSA2	:= {}
	aRetImp	  	:= {}
	cFornImp  	:= ""	
	cLojaImp  	:= PadR("00", __nTamLoja, "0")
	cNmForn   	:= ""
	cNmRedu   	:= ""
	nRecSA2   	:= SA2->(Recno())
	nRecSED   	:= SED->(Recno())	
	nX        	:= 0
	nY        	:= 0
	cParImp   	:= ""
	dVenImp   	:= dDataBase 
	cTaxa     	:= ""
	nVlrTrib  	:= 0
	nTrbCalc  	:= 0
	nBasRet   	:= 0
	cPrefPai  	:= SE2->E2_PREFIXO
	cNumPai   	:= SE2->E2_NUM	
	cOrigem   	:= "FINA340"
	cFilorig  	:= SE2->E2_FILORIG
	cCodAprov 	:= SE2->E2_CODAPRO
	cLa       	:= SE2->E2_LA
	cFormPag  	:= SA2->A2_FORMPAG
	cTipoPes  	:= SA2->A2_TIPO
	cTitPai   	:= SE2->(E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO+E2_FORNECE+E2_LOJA)
	cCodPis   	:= cCodCof := cCodCsl := "5952"
	cCodIrf   	:= Iif(lGerDif, SE2->E2_CODRET, "")
	aIRGerar	:= {}
	nTitAGer	:= 0
	cFornPai	:= SA2->A2_COD
	cLojaPai	:= SA2->A2_LOJA
	cCNPJPai	:= SA2->A2_CGC
	nPisOri		:= 0
	nCofOri		:= 0
	nCslOri		:= 0
	lSimpIRAlu	:= .F.
	lIntPFS     := FindFunction("JDesdFilho") .And. SuperGetMv("MV_JURXFIN",.T.,.F.)
	cNatIss     := &(SuperGetMV("MV_ISS"))
	cNatIrf     := &(SuperGetMV("MV_IRF"))
	cNatPis 	:= SuperGetMV("MV_PISNAT")
	cNatCof 	:= SuperGetMV("MV_COFINS")
	cNatCsl 	:= SuperGetMV("MV_CSLL")
	cForMunic   := SuperGetMV("MV_MUNIC")

	If cPaisLoc $ "BRA|MEX" .And. !Empty(SE2->(E2_CODRPIS+E2_CODRCOF+E2_CODRCSL))
		cCodPis   := SE2->E2_CODRPIS
		cCodCof   := SE2->E2_CODRCOF
		cCodCsl   := SE2->E2_CODRCSL
	EndIf
	
	SA2->(DbSetOrder(1))
	SED->(DbSetOrder(1))
	SE2->(DbSetOrder(1))
	cChave := xFilial("SA2", SE2->E2_FILORIG) + SE2->E2_FORNECE+E2_LOJA
	
	If SA2->(DbSeek(cChave))
		nRecSA2 := SA2->(Recno())
	Else
		SA2->(DbGoto(nRecSA2))
	EndIf
	
	cChave := xFilial("SED", SE2->E2_FILORIG) + SE2->E2_NATUREZ
	
	If SED->(DbSeek(cChave))
		nRecSED := SED->(Recno())
	Else
		SED->(DbGoto(nRecSED))
	EndIf
	
	If __lAglPcc .And. ((nTxPis + nTxCof + nTxCsl) > 0)
		nPisOri:= nTxPis
		nCofOri:= nTxCof
		nCslOri:= nTxCsl
		nTxPis += (nTxCof + nTxCsl)
		nTxCof := nTxCsl := 0
	EndIf	
	
	If (nTxPis+nTxCof+nTxCsl+nTxIrf) > 0
		cFornImp := SuperGetMv("MV_UNIAO", .T., "UNIAO")
		cNmForn  := "UNIAO"
		cChave   := xFilial("SA2", cFilorig) + PadR(cFornImp, __nTamForn, " ") + cLojaImp		
		
		aAreaSA2	:= SA2->(GetArea())
		If !SA2->(DbSeek(cChave))
			Reclock("SA2",.T.)
			SA2->A2_FILIAL := xFilial("SA2", cFilorig)
			SA2->A2_COD    := cFornImp
			SA2->A2_LOJA   := cLojaImp
			SA2->A2_NOME   := cNmForn
			SA2->A2_NREDUZ := cNmForn
			SA2->A2_BAIRRO := "."
			SA2->A2_MUN    := "."
			SA2->A2_EST    := SuperGetMv("MV_ESTADO")
			SA2->A2_End    := "."
			SA2->A2_TIPO   := "J"
			SA2->(MsUnlock())
		EndIf
	EndIf
	
	For nX := 1 To 5
		cTaxa    := MVTAXA
		cCodRet  := ""
		nTrbCalc := 0
		nBasRet  := 0
		lRatIRPF := .F.
		nTitAGer := 1
		
		Do Case 
			Case nX == 1	//-- PIS
				If nTxPis <= 0 .And. nPisCalc == 0
					Loop
				EndIf
				cNatImp  := Pad(cNatPis, __nTamNatu)
				cDesc    := "PIS"
				cImpost  := "PIS"
				cImpFK4  := "PIS"
				nVlrTrib := nTxPis 
				cCodRet  := cCodPis
				nTrbCalc := nPisCalc
				nBasRet	 := nPisBaseR
			
			Case nX == 2	//-- COFINS
				If nTxCof <= 0 .And. nCofCalc == 0
					Loop
				EndIf
				cNatImp  := Pad(cNatCof, __nTamNatu)
				cDesc    := "COFINS"
				cImpost  := "COFINS"
				cImpFK4  := "COF"					
				nVlrTrib := nTxCof 
				cCodRet  := cCodCof
				nTrbCalc := nCofCalc
				nBasRet  := nCofBaseR

			Case nX == 3	//-- CSLL
				If nTxCsl <= 0 .And. nCslCalc == 0
					Loop
				EndIf
				cNatImp  := Pad(cNatCsl, __nTamNatu)
				cDesc    := "CONTRIB.S/LUCRO LIQUIDO"				
				cImpost  := "CSLL"	
				cImpFK4  := "CSL"				
				nVlrTrib := nTxCsl
				cCodRet  := cCodCsl
				nTrbCalc := nCslCalc
				nBasRet  := nCslBaseR

			Case nX == 4	//-- IRRF
				If nTxIrf <= 0 .And. nIrfCalc == 0
					Loop
				EndIf
				cNatImp  := cNatIrf
				cDesc    := "IMPOSTO RENDA RETIDO NA FONTE"
				cImpost  := "IRRF"
				cImpFK4  := "IRF"
				nVlrTrib := nTxIrf
				cCodRet  := cCodIrf
				nTrbCalc := nIrfCalc
				nBasRet  := Iif( nIrfBaseR == 0 .And. nTxIrf > 0, __nBase, nIrfBaseR) // REVER

				If __oRatIRF != Nil .and. !Empty(__oRatIRF:aRatIRF)
					For nY := 1 To Len(__oRatIRF:aRatIRF)
						If __oRatIRF:aRatIRF[nY][6] != 0
							aAdd(aIRGerar,{"IRF", __oRatIRF:aRatIRF[nY][6], cCodRet})
						EndIf
						lSimpIRAlu := If(len(__oRatIRF:aRatIRF[nY])>10, __oRatIRF:aRatIRF[nY][11], .F.)	
						aAdd(aRetImp,{cImpFK4, __oRatIRF:aRatIRF[nY][6], cNatImp, "", __oRatIRF:aRatIRF[nY][6], __oRatIRF:aRatIRF[nY][5], __oRatIRF:aRatIRF[nY][5], "", SA2->A2_COD, SA2->A2_LOJA, __oRatIRF:aRatIRF[nY][3], cCodRet, lSimpIRAlu})
					Next nY
					lRatIRPF := .T.
					nTitAGer := Len(aIRGerar)
				EndIf

			Case nX == 5	//-- ISS
				If nTxIss <= 0 .And. nIssCalc == 0
					Loop
				EndIf
				
				cNmForn  := "MUNICIPIO"
				
				If SA2->(FieldPos("A2_MUNIC")) > 0 .And. !Empty(SA2->A2_MUNIC)
					cFornImp := SA2->A2_MUNIC
				Else
					cFornImp := cForMunic
				EndIf
				
				cChave := xFilial("SA2", cFilorig) + PadR(cFornImp, __nTamForn, " ") + cLojaImp	
				
				If !SA2->(DbSeek(cChave))
					Reclock("SA2",.T.)
					SA2->A2_FILIAL  := xFilial("SA2", cFilorig)
					SA2->A2_COD 	:= cFornImp
					SA2->A2_LOJA	:= cLojaImp
					SA2->A2_NOME	:= cNmForn
					SA2->A2_NREDUZ  := cNmForn
					SA2->A2_BAIRRO  := "."
					SA2->A2_MUN 	:= "."
					SA2->A2_EST 	:= SuperGetMv("MV_ESTADO")
					SA2->A2_End 	:= "."
					SA2->(MsUnlock())
				EndIf
				
				cNatImp  := cNatIss
				cDesc    := "IMPOSTO SOBRE SERVICOS"
				cImpost  := "ISS"
				cImpFK4  := "ISS"					
				nVlrTrib := nTxIss
				cTaxa    := MVISS
				nTrbCalc := nIssCalc
				nBasRet  := nIssBaseR
		EndCase
		
		//Cria a Natureza
		cChave  := (xFilial("SED", cFilorig) + cNatImp)
		
		If !SED->(DbSeek(cChave))
			RecLock("SED",.T.)
			SED->ED_FILIAL  := xFilial("SED", cFilorig)
			SED->ED_CODIGO  := cNatImp
			SED->ED_CALCIRF := "N"
			SED->ED_CALCISS := "N"
			SED->ED_CALCINS := "N"
			SED->ED_CALCCSL := "N"
			SED->ED_CALCCOF := "N"
			SED->ED_CALCPIS := "N"
			SED->ED_DESCRIC := cDesc
			SED->ED_TIPO	:= "2"
			MsUnlock()		
		EndIf			
		
		For nY := 1 to nTitAGer

			If nX == 4 .and. lRatIRPF
				cImpFK4  := aIRGerar[nY,1]
				nVlrTrib := aIRGerar[nY,2]
				cCodRet  := aIRGerar[nY,3]
			EndIf

			If nVlrTrib > 0
				dVenImp := F050VIMP(cImpost, dDataBase, dDataBase, dDataBase, cCodRet, cTipoPes, .T.)
				cParImp := FA340Parc(cPrefPai, cNumPai, cTaxa, cFornImp, cLojaImp, cFilorig)
				cNmForn := Posicione("SA2",1,xFilial("SA2",cFilorig) + cFornImp,"A2_NREDUZ")
				
				RecLock("SE2",.T.)
				SE2->E2_FILIAL  := xFilial("SE2", cFilorig)
				SE2->E2_PREFIXO := cPrefPai
				SE2->E2_NUM     := cNumPai
				SE2->E2_PARCELA := cParImp
				SE2->E2_TIPO    := cTaxa
				SE2->E2_EMISSAO := dBaixa
				SE2->E2_EMIS1   := dDataBase
				SE2->E2_VALOR   := nVlrTrib
				SE2->E2_VENCTO  := dVenImp
				SE2->E2_SALDO   := nVlrTrib
				SE2->E2_VENCREA := dVenImp
				SE2->E2_VENCORI := dVenImp
				SE2->E2_FORNECE := cFornImp
				SE2->E2_LOJA    := cLojaImp
				SE2->E2_NOMFOR  := cNmForn
				SE2->E2_MOEDA   := If(cPaisLoc == "BRA", 1, nMoeda340)
				SE2->E2_VLCRUZ  := Round(nVlrTrib, 2)
				SE2->E2_ORIGEM  := Upper(cOrigem)
				SE2->E2_NATUREZ := cNatImp
				SE2->E2_LA      := cLa
				
				If nX != 5 .And. cPaisLoc == "BRA"
					SE2->E2_CODRET := cCodRet
					SE2->E2_DIRF   := Iif(lGerDif, "1", "2")
				EndIf
				
				SE2->E2_FILORIG := cFilorig
				SE2->E2_TITPAI  := cTitPai
				SE2->E2_CODAPRO := cCodAprov
				SE2->E2_FORMPAG := cFormPag
				SE2->E2_SEQBX   := __cSequeBx
				SE2->(MsUnlock())

				FINGRVFK7("SE2", SE2->E2_FILIAL +"|"+ SE2->E2_PREFIXO +"|"+ SE2->E2_NUM +"|"+ SE2->E2_PARCELA +"|"+ SE2->E2_TIPO +"|"+ SE2->E2_FORNECE +"|"+ SE2->E2_LOJA,,cTitPai)

				// Integração SIGAPFS x SIGAFIN  - Inclui os desdobramentos para os titulos de impostos
				If lIntPFS
					JDesdFilho(3, Nil, {{"SE2", SE2->(Recno())}})
				EndIf
			EndIf
		Next nY
		
		//Trecho para gravação de moto unitário do PCC na tabela FK4, quando existir aglutinação de impostos (MV_AG10925 = 1)
		If __lAglPcc .And. cImpFK4 == "PIS" 
			nVlrTrib := nPisOri
		ElseIf __lAglPcc .And. cImpFK4 == "COF"
			nVlrTrib := nCofOri
		ElseIf __lAglPcc .And. cImpFK4 == "CSL" 
			nVlrTrib := nCslOri
		EndIf
		
		If !lRatIRPF		
			SA2->(DbGoto(nRecSA2))
			nTrbCalc := IIf(nTrbCalc <= 0, nVlrTrib, nTrbCalc)
			AAdd(aRetImp, {cImpFK4, nTrbCalc, cNatImp, "", nVlrTrib, If(__lBsIrRed, __nBsIrRed, __nBase), nBasRet, "", SA2->A2_COD, SA2->A2_LOJA, SA2->A2_CGC, cCodRet})
		EndIf
	Next nX
	
	If !Empty(aAreaSA2)
		RestArea(aAreaSA2)
	EndIf
	
	RestArea(aAreaSE2)
	FwFreeArray(aAreaSE2)
	FwFreeArray(aIRGerar)
Return aRetImp

/*/{Protheus.doc}FA340VPSE2
	Atualiza os vrets e prets da tabela SE2

	@author Sivaldo Oliveira
	@since  12/06/2018
	@version 12
/*/
Static Function FA340VPSE2()	
	Reclock("SE2")
	
	If (NPIS340+NCOF340+NCSL340) > 0 
		If NPIS340 > 0
			SE2->E2_VRETPIS	+= NPIS340
			SE2->E2_PRETPIS	:= "7"
		EndIf
		
		If NCOF340 > 0
			SE2->E2_VRETCOF	+= NCOF340
			SE2->E2_PRETCOF	:= "7"
		EndIf
		
		If NCSL340> 0
			SE2->E2_VRETCSL	+= NCSL340
			SE2->E2_PRETCSL	:= "7"
		EndIf	
	EndIf
	
	If NIRF340 > 0
		SE2->E2_VRETIRF	+= NIRF340
		SE2->E2_PRETIRF	:= "7"
	EndIf
	
	If NISS340 > 0
		SE2->E2_VRETISS	+= NISS340
	EndIf
	
	SE2->(MsUnlock())
Return Nil

/*/{Protheus.doc}FA340VPSE5
	Atualiza os vrets e prets da tabela SE5

	@author Sivaldo Oliveira
	@since  12/06/2018
	@version 12
/*/
Static Function F340VPSE5(cCamposE5 As Character)
	Default cCamposE5 := ""

	If !Empty(cCamposE5)
		If (NPIS340+NCOF340+NCSL340) > 0 
			cCamposE5 += ",{'E5_PRETPIS', '7'}"
			cCamposE5 += ",{'E5_PRETCOF', '7'}"
			cCamposE5 += ",{'E5_PRETCSL', '7'}"
			cCamposE5 += ",{'E5_VRETPIS', " + CVALTOCHAR(NPIS340) + "}"
			cCamposE5 += ",{'E5_VRETCOF', " + CVALTOCHAR(NCOF340) + "}"
			cCamposE5 += ",{'E5_VRETCSL', " + CVALTOCHAR(NCSL340) + "}"
		Endif
		
		If NIRF340 > 0
			cCamposE5 += ",{'E5_PRETIRF', '7'}"
			cCamposE5 += ",{'E5_VRETIRF', " + CVALTOCHAR(NIRF340) + "}"
		ElseIf nBaseIrpf > 0 
			cCamposE5 += ",{'E5_VRETIRF' , "+CVALTOCHAR(nVlRetIrf)+" }"
			cCamposE5 += ",{'E5_PRETIRF' , '1'}"
			cCamposE5 += ",{'E5_BASEIRF' , "+ CVALTOCHAR(nBaseIrpf)+" }"
		EndIf
		
		If NISS340 > 0
			cCamposE5 += ",{'E5_VRETISS' , " + CVALTOCHAR(NISS340) + "}"
		Endif
		
		If !__lBsIrRed
			If __nBase > 0
				cCamposE5 += ",{'E5_BASEIRF', " + CVALTOCHAR(__nBase) + "}"
			EndIf
		Else
			cCamposE5 += ",{'E5_BASEIRF', " + CVALTOCHAR(__nBsIrRed) + "}"
		EndIf
	EndIf
Return Nil

/*/{Protheus.doc}F340ValSel
	Não permite digitar um valor maior que olimite do título ou maior 
	que o total ser compensado

	@author Sivaldo Oliveira
	@since  12/06/2018
	@version 12
/*/
Static Function F340ValSel(nVlrSel As Numeric, nVlrLim As Numeric, nDif As Numeric, cTpTitSel As Char, nSldTitSel As Numeric, lRetImpBx As Logical) As Logical
	Local lRet As Logical
	Default cTpTitSel := ""
	Default nSldTitSel := 0
	Default lRetImpBx := .F. 
	
	//Inicializa variável
	lRet := .T.

	//Valida o título posicionado
	If nVlrSel <= 0 .Or. nVlrSel > nVlrLim
		lRet := .F.
	EndIf
	
	If lRet .And. (Empty(nDif) .Or. nVlrSel > nDif)
		lRet := .F.
	EndIf

	//Não deixa selecionar um valor de DIC que iria zerar o saldo da nota, para não impactar na retenção do imposto 
	If lRet
		If cTpTitSel == "DIC"
			If lRetImpBx .And. (nVlrSel + nValTot >= SE2->E2_SALDO)
				lRet := .F.
			EndIf
		ElseIf SE2->E2_TIPO == "DIC"
			If lRetImpBx .And. (nVlrSel >= nSldTitSel)
				lRet := .F.
			EndIf
		EndIf
		
		If !lRet
			MsgInfo(STR0126, STR0127) //"Para manter a integridade dos impostos, o sistema não permite zerar o saldo de títulos com retenções na baixa, através de compensações com títulos do tipo 'DIC'." ## "Compensação com DIC"
		EndIf
	EndIf
Return lRet

/*/{Protheus.doc}FA340Acres
	Define o valor do acréscimo a ser baixado e o valor a 
	a ser abatido do saldo da NF

	@author Sivaldo Oliveira
	@since  19/06/2018
	@version 12
/*/
Static Function FA340Acres(nVlrMov As Numeric, nAcresc)
	Default nVlrMov := 0
	Default nAcresc := 0
	
	If nAcresc > 0
		If nAcresc > nVlrMov
			nAcresc := nVlrMov 	
		EndIf
	
		//Caso o valor a ser compens seja -= ao valor do acrésc, 
		//não abate nada do saldo da nota
		If !SE2->E2_TIPO $ MVPAGANT+"|"+MVABATIM+"|INA"
			nVlrMov -= nAcresc 
		EndIf
	EndIf

Return nVlrMov 	  

