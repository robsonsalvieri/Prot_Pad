	__oDicBx:SetString(5, cForn)
	__oDicBx:SetString(6, cLoja)
	
	cQry := __oDicBx:GetFixQuery()
	cTblTmp := MpSysOpenQuery(cQry)	
	lRet := (cTblTmp)->(!Eof())	
	
Return lRet

