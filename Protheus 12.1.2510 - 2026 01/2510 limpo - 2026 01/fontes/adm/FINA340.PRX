								EndIf
							EndIf						
						EndIf
						
						Aadd(aRegSE2, aRecNo[nTit])
						REGVALOR := aRecNo[nTit] 
						nMoedSel := SE2->E2_MOEDA
						
						SE2->(dbGoTo(aRecNo[nTit]))
						cSequencia := FaNxtSeqBx("SE2", .T.)
						
						SE2->(dbGoTo(__nRecPart))
						__cSequeBx := FaNxtSeqBx("SE2", .T.)						
						__cSequeBx := IIf(cSequencia > __cSequeBx, cSequencia, __cSequeBx)
						
