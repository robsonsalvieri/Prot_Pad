	Local nLiqNF     As Numeric
	Local nRetNF     As Numeric
	Local nBasIRPF 	 As Numeric
	Local lIrrfPF	 As Logical
	Local nIssAux  	 As Numeric
	Local lGerDic    As Logical
	Local aPCC       As Array
	Local cFilorig   As Character
	Local lGerDif    As Logical
	Local nTamADIC	 As Numeric
	Local nX	 	 As Numeric
	Local cForn	 	 As Character
	Local cIdDocFK7	 As Character
	Local nDicAnt	 As Numeric
	Local aRet       As Array
	Local nTotIss	 As Numeric
	Local nTotIns 	 As Numeric
	Local nTotIrf	 As Numeric
	Local nTotPis	 As Numeric
	Local nTotCof	 As Numeric
	Local nTotCsl	 As Numeric
	Local nBsRetAlrd As Numeric
	Local nPos 		 As Numeric

	Default nTit   		:= 1
	Default nRecPA 		:= 0 
	Default nRecNF 		:= 0
	Default aImpFK3FK4 	:= {}
	Default aDicCMP	   	:= {}
	Default aTitCalc	:= {}

	//Variaveis para uso da função F080TOTMES
	Private nPis		As Numeric 
	Private nCofins		As Numeric
	Private nCsll		As Numeric
	Private nIrrf		As Numeric
	Private nIss		As Numeric
	Private nDescont	As Numeric
	Private nTotAbat	As Numeric
	Private nJuros		As Numeric
	Private nVA			As Numeric
	Private nMulta		As Numeric
	Private nAcresc		As Numeric
	Private nDecresc	As Numeric
	Private nValPgto	As Numeric
	Private nOldValPgto	As Numeric
	Private aDadosRef 	As Array
	Private aDadosRet 	As Array

	nPis		:= 0 
	nCofins		:= 0
	nCsll		:= 0
	nIrrf		:= 0
	nIss		:= 0
	nDescont	:= 0
	nTotAbat	:= 0
	nJuros		:= 0
	nVA			:= 0
	nMulta		:= 0
	nAcresc		:= 0
	nDecresc	:= 0
	nValPgto	:= 0
	nOldValPgto	:= 0
	nBsRetAlrd	:= 0
	aDadosRef 	:= Array(7)
	aDadosRet 	:= Array(7)
	
	//Inicializa variáveis.
	lRet    := .T.
	lBxTot  := .F.
	//Dados do PA
	nVlrPA   := 0 
	nSldPA   := 0
	lPccPA   := .F.
	lIrfPA   := .F.
	lIssPA   := .F.
	lInsPA   := .F.	
	lImpPA   := .F.
	nIssPr   := 0
	nImpPA   := 0
	nImposPA := 0
	nImpEmPA := 0
	lBxPa    := .T.
	cAdian   := ""
	aRetDifPA := {}
	//Dados da NF
	nVlrNF   := 0
	nPisNF   := 0
	nCofNF   := 0
	nCslNF   := 0
	nIrfNF   := 0
	nIssNF   := 0
	nInsNF   := 0   
	nPenRet  := 0
	nRetBai  := 0
	nRetEmi  := 0
	nPropoc  := 0
	nVlrDic  := 0
	nLiqNF   := 0
	nRetNF   := 0
	nBasIRPF := 0
	nTamADIC := 0
	nX		 := 0
	lIrrfPF  := .F.
	lGerDic  := .T.
	lGerDif  := .F.
	nDicAnt  := 0
	nVlRetIrf	:= 0
	nBaseIrpf	:= 0
	aRet        := {}
	cIdDocFK7	:= ""
	nPos		:= 0
	__lBsIrRed := .F.
	
	f340LRatIR(.T.)

	//Posiciona o título de PA
	SE2->(DbGoto(nRecPA))
	
	If SE2->E2_TIPO $ MVPAGANT .And. mv_par10 == 1
		If __lEmpPub == Nil 
			__lEmpPub := IsEmpPub()
		EndIf
		
		cAdian := SE2->(E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO+E2_FORNECE+E2_LOJA)
		nVlrPA := SE2->(E2_VALOR+E2_PIS+E2_COFINS+E2_CSLL+E2_IRRF+E2_ISS+E2_PRISS+E2_INSS+E2_PRINSS)
		nSldPA := SE2->E2_SALDO
		cForn  := SE2->E2_FORNECE + SE2->E2_LOJA
		
		//Verifica se o PA calcula impostos
		If (lImpPA := (SE2->(E2_PIS+E2_COFINS+E2_CSLL+E2_IRRF+E2_ISS+E2_PRISS+E2_INSS+E2_PRINSS) > 0))
