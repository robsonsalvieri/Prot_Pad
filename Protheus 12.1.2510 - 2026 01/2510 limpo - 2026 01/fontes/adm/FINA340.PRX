		cCamposE5 += ",{'E5_NUMERO'  , '" + SE2->E2_NUM		+ "'}"
		cCamposE5 += ",{'E5_PARCELA' , '" + SE2->E2_PARCELA	+ "'}"
		cCamposE5 += ",{'E5_TIPO'    , '" + SE2->E2_TIPO	+ "'}"
