//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
nOpcA := AxVisual(cAlias,nReg,nOpc,,,,,)

IF ( nOpcA == 1 .And. F019VldDel() )
	Begin Transaction
		RecLock(cAlias,.F.,.t.)
		dbDelete()
		MsUnLock()
	End Transaction
EndIf

RestArea(aArea)
Return(.T.)
   
