				If lIsRussia //Russia
					lRet := fA070Tit("SE1",SE1->(Recno()),4,,.T.,,lMov,lRusRevRa)
				Else
					lRet := fA070Tit("SE1",SE1->(Recno()),4,,.T.,lPix)			 
				Endif							
			EndIf
			
			If __nBxaPrin > 0 .And. (nPos := Ascan(xAutoCab, {|x| x[1] == "AUTREGBXPRIN"})) > 0
				xAutoCab[nPos, 2] := __nBxaPrin
			EndIf
			
			__nBxaPrin := 0
		ElseIf nOpc == 5	// Cancelamento
			If lIsRussia //Russia
				fA070Can("SE1",Recno(),5,,nOpbaixa,lMov)
			Else
				fA070Can("SE1",Recno(),5,,nOpbaixa)
			Endif
		ElseIf nOpc == 6		// Exclusão
			fA070Can("SE1",Recno(),6,,nOpbaixa)
		Endif
	EndIf
