	Dbselectarea('SA3')
	DbSetOrder(1)
	For nX:=1 to nVendedores

		cVendedor := SE1->(FieldGet( FieldPos("E1_VEND"+Alltrim( iif(nX < 10 ,Str(nX), CHR(nX+55)) )) ))
		If dbSeek(xFilial('SA3')+cVendedor)
			If SA3->A3_ALBAIXA > 0
				lProssegue := .F.
				HELP(" ",1, "FINCOMBX" )
				Exit
			Endif
		Endif

	Next nX
Endif

RestArea(aArea)

