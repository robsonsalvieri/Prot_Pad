						If FKD->( MsSeek( xFilial( 'FKD' ) + aAtuFKDID[ nLaco, 1 ] ) )
							While FKD->( !Eof() .And. FKD_IDDOC == aAtuFKDID[ nLaco, 1 ] ) 
