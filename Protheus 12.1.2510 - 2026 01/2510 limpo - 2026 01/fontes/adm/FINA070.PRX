/*/{Protheus.doc} f070AtCorr
	Função para atualizar o valor da correção monetaria quando executado gatilhos da tela de baixa

	@author Francisco Oliveira
	@since 21/08/2024
/*/

Static Function f070AtCorr()

	Local cTipoCM	 As Character
	Local lPccBxCr 	 As Logical
	Local lIrPjBxCr	 As Logical
	Local nPccBxCr	 As Numeric
	Local nIrPjBxCr	 As Numeric
	Local nTtlMov	 As Numeric
	Local nTtlCM	 As Numeric

	cTipoCM	 	:= SuperGetMv("MV_TIPOCM", .T., "T") 
	lPccBxCr 	:= FPccBxCr()
	lIrPjBxCr	:= FIrPjBxCr()
	nPccBxCr	:= 0
	nIrPjBxCr	:= 0
	nTtlMov		:= 0
	nTtlCM		:= 0

	If SE1->E1_MOEDA > 1 .And. nCm <> 0 .And. nDescont > 0 .And. __nVEstran <> Nil .And. __nVEstran > 0
		
		If lPccBxCr
			nPccBxCr	:= nPis + nCofins + nCsll
		Endif

		If lIrPjBxCr
			nIrPjBxCr	:= nIrrf
		Endif

		nTtlMov	:= nJuros + nVa + (nCm1 + nProRata) + nMulta - nDescont - nOtrga + nAcresc - nDecresc
		nTtlCM  := nValEstrang - (xMoeda(nTtlMov - nPccBxCr - nIrPjBxCr ,nMoedaBco,SE1->E1_MOEDA,,,,nTxMoeda))

		If cTipoCM == "O"
			Fa070Corr(nTtlCM, nTxMoeda)
		Else
			Fa070Corr(nValestrang, nTxMoeda)
		Endif
	Endif

Return

/*/{Protheus.doc} F070SUMSeq
	Busca a soma dos ultimos mesmo sequenciais do registro criado com o TIPODOC = "DB"

	@author  Francisco Oliveira
	@version 12.1.24
	@since   04/12/2024

	@return Numeric nSoma
/*/

Static Function F070SUMSeq(cSeqSE5DB As Character) As Numeric

	Local cFilTit		As Character
	Local cFilOrig		As Character
	Local cPrefTit		As Character 
	Local cNumTit		As Character  
	Local cParcTit		As Character 
	Local cTipoTit		As Character 
	Local cCliFor		As Character  
	Local cLjCliFor		As Character 
	Local cChvTitFK7	As Character
	Local cChaveTit		As Character
	Local nParam		As Numeric
	Local nSoma			As Numeric

	Default cSeqSE5DB := ""

	cFilTit		:= SE1->E1_FILIAL
	cFilOrig	:= SE1->E1_FILORIG
    cPrefTit	:= SE1->E1_PREFIXO
    cNumTit		:= SE1->E1_NUM
    cParcTit	:= SE1->E1_PARCELA
    cTipoTit	:= SE1->E1_TIPO
    cCliFor		:= SE1->E1_CLIENTE
    cLjCliFor	:= SE1->E1_LOJA
	nSoma		:= 0
	nParam		:= 1

	cChaveTit := cFilTit + "|" + cPrefTit + "|" + cNumTit + "|" + cParcTit + "|" + cTipoTit + "|" + cCliFor + "|" + cLjCliFor
	cChvTitFK7 := FINGRVFK7("SE1", cChaveTit, cFilOrig )

	If __oSeqBxDB == Nil

		cQuery := "SELECT SUM(FK5_VALOR) SUMTPDB "
		cQuery += "FROM ? FK5 "
		cQuery += "WHERE "
		cQuery += "FK5_TPDOC	   != ? AND "
		cQuery += "FK5_IDDOC		= ? AND "
		cQuery += "FK5_SEQ			= ? AND "
		cQuery += "FK5.D_E_L_E_T_	= ? "

		cQuery := ChangeQuery(cQuery)

        __oSeqBxDB := Iif(__lExecSt, FWExecStatement():New(cQuery), FWPreparedStatement():New(cQuery) )
	Endif

	__oSeqBxDB:SetUnsafe(nParam++, RetSqlName("FK5"))
	__oSeqBxDB:SetString(nParam++, "ES")
	__oSeqBxDB:SetString(nParam++, cChvTitFK7)
	__oSeqBxDB:SetString(nParam++, cSeqSE5DB)
	__oSeqBxDB:SetString(nParam++, " ")

	nSoma := Iif(__lExecSt, __oSeqBxDB:ExecScalar('SUMTPDB'), MpSysExecScalar(__oSeqBxDB:GetFixQuery(),"SUMTPDB") )

	If __oSeqBxDB != Nil
		__oSeqBxDB:Destroy()
		__oSeqBxDB := Nil
	Endif

Return nSoma

/*/{Protheus.doc} ValidVlLiq
	Valida se o valor digitado é maior que o valor liquido pendente de recebimento

	@author Sivaldo Oliveira
	@since 20/03/2025
	
	@Param nVlrDigita, Numeric, Valor digitado para baixa (pode ser baixa pracila ou total liquido)
	@Param lProcessa, Logical, Valor lógico que controla o processamento das funçoes do valid do campo
	@Param nVlrTotImp, Numeric, Valor somatório dos impostos a serem retidos
	@Return lRetorno, Logical, Valor lógico que controla a mudança de foco dos campos (nValRec e nValEstrang) 
/*/
Function ValidVlLiq(nVlrDigita As Numeric, lProcessa As Logical, nVlrTotImp As Numeric) As Logical
	Local lRetorno As Logical
	
	//Parâmetros de entrada
	Default nVlrDigita := 0	
	Default ___nVlrLiq := 0
	Default nVlrTotImp := 0
	
	//Inicializa variáveis
	lRetorno   := .T.
	__lDeduImp := .T.
	lProcessa  := .T.
	
	If cPaisloc == "BRA" .And. nVlrTotImp > 0 .And. SE1->E1_MOEDA == 1 .And. nVlrDigita > 0 .And. ___nVlrLiq > 0
		If !(lRetorno := !(nVlrDigita > ___nVlrLiq))
			Help(" ", 1, "ValorMaior")
		EndIf
		
		If (lProcessa  := (nVlrDigita <=___nVlrLiq))
			__lDeduImp := (nVlrDigita < ___nVlrLiq)
		EndIf
	EndIf

Return lRetorno
