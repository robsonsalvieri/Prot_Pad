Function F070DelTxBx(cFil as character)
	Local cChaveImp as character
	Local aAreaDTB  as Array
	Local aAreaSE1  as Array
	Local cTitPai   as character
	Local cNatur	as character
	Local cChave	as character
	Local cFilE2	as character
	Local aAreaSE5  as Array
	Local cChPix    as character

	Default cFil 	:= xFilial("SE1")

	cChaveImp := ""
	aAreaDTB := FwGetArea()
	aAreaSE1 := SE1->(FwGetArea())
	cTitPai := SE1->(E1_PREFIXO + E1_NUM + E1_PARCELA + E1_TIPO + E1_CLIENTE + E1_LOJA)
	cNatur := &(SuperGetMv("MV_IRF",,""))
	cChave := SE1->(E1_PREFIXO + E1_NUM) 
	cFilE2 := xFilial("SE2")
	aAreaSE5 := {}
	cChPix := "BA"+SE1->(E1_PREFIXO + E1_NUM + E1_PARCELA + E1_TIPO + dTos(E1_EMISSAO)+ E1_CLIENTE + E1_LOJA)

	If ( cPaisLoc == "BRA" )
		cChaveImp := SE1->(E1_CLIENTE + E1_LOJA + E1_PREFIXO + E1_NUM)
		SE1->(dbSetOrder(2))
		SE1->(MsSeek(cFil + cChaveImp))

		While SE1->(!Eof()) .And. SE1->(E1_FILIAL + E1_CLIENTE + E1_LOJA + E1_PREFIXO + E1_NUM) == cFil + cChaveImp
			IF SE1->E1_TIPO $ "PIS|COF|CSL|IRF" .And. ;
				(("FINA070" $ SE1->E1_ORIGEM .or. "FINA110" $ SE1->E1_ORIGEM) .and. ;
				SE5->E5_SEQ == SE1->E1_SEQBX .AND. RTrim(cTitPai) == RTrim(SE1->E1_TITPAI)) .Or.;
				("FINA890" $ SE1->E1_ORIGEM .And. RTrim(cTitPai) == RTrim(SE1->E1_TITPAI))  // Titulos transmitidos PIX
				FinGrvEx("R")
				// Deleto o imposto gerado pela transmissão PIX
				If "FINA890" $ SE1->E1_ORIGEM
					nValRec += SE1->E1_VALOR
					aAreaSE5  := SE5->(GetArea())
					SE5->(DbSetOrder(2))
					If SE5->(DbSeek(xFilial("SE5")+cChPix))
						F061DelSE5() 
					EndIf
					RestArea(aAreaSE5)
				EndIf
				FINDELFKs(SE1->E1_FILIAL+"|"+SE1->E1_PREFIXO+"|"+SE1->E1_NUM+"|"+SE1->E1_PARCELA+"|"+SE1->E1_TIPO+"|"+SE1->E1_CLIENTE+"|"+SE1->E1_LOJA,"SE1")
				RecLock("SE1",.F.,.T.)
				SE1->(dbDelete())
				SE1->(msUnLock())
			EndIf
			SE1->(dbSkip())
		EndDo

		SE2->(dbSetOrder(1))
		SE2->(MsSeek(cFilE2 + cChave))

		While !Eof() .and. Alltrim(SE2->(E2_FILIAL +E2_PREFIXO + E2_NUM )) == AllTrim(cFilE2 +cChave)
			IF SE2->E2_TIPO == MVTAXA .and. Alltrim(SE2->E2_NATUREZ) == Alltrim(cNatur) .And. Alltrim(cTitPai) == Alltrim(SE2->E2_TITPAI) .and.;
				(Alltrim(SE2->E2_ORIGEM) == "FINA070" .or. Alltrim(SE2->E2_ORIGEM) == "FINA110")  .AND. SE5->E5_SEQ == SE2->E2_SEQBX
				FinGrvEx("R")
				FINDELFKs(SE2->E2_FILIAL+"|"+SE2->E2_PREFIXO+"|"+SE2->E2_NUM+"|"+SE2->E2_PARCELA+"|"+SE2->E2_TIPO+"|"+SE2->E2_FORNECE+"|"+SE2->E2_LOJA,"SE2")			
				RecLock("SE2",.F.,.T.)
				SE2->(dbDelete())
				SE2->(msUnLock())
			EndIf
			SE2->(dbSkip())
		EndDo
	Endif

	FWRestArea(aAreaSE1)
	FWRestArea(aAreaDTB)

	FwFreeArray(aAreaSE5)
	FwFreeArray(aAreaSE1)
	FwFreeArray(aAreaDTB)

Return .T.

