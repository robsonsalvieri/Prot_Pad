Function FA070SITEF(cFilTit,cPrefixo,cNum,cParc,cTipo)
Local lRet    := .T.
Local aArea   := GetArea()
Local nIndFIF := Iif(FWSIXUtil():ExistIndex('FIF','8'),8,3)
Local cKeyFIF := ""

If AliasInDic("FIF")

	//-- Monta chave de busca de acordo com o indice utilizado
	cKeyFIF := Iif( nIndFIF==8 ,cFilTit,FWxFilial("FIF")) + cPrefixo + cNum + cParc + cTipo
	dbSelectArea("FIF")
	FIF->( dbSetOrder( nIndFIF ) ) //FIF_CODFIL+FIF_PREFIX+FIF_NUM+FIF_PARC+FIF_TIPO
	If dbSeek( cKeyFIF )
		BEGIN TRANSACTION

		RecLock( "FIF", .F. )
		FIF->FIF_STATUS := Iif(FIF->FIF_STATUS $ "7|8","6","1")
		FIF->FIF_PREFIX := Space( Len( FIF->FIF_PREFIX ) )
		FIF->FIF_NUM    := Space( Len( FIF->FIF_NUM ) )
		FIF->FIF_PARC   := Space( Len( FIF->FIF_PARC ) )
		FIF->FIF_TIPO   := Space( Len( FIF->FIF_TIPO ) )
		FIF->( MsUnlock() )

		END TRANSACTION
	EndIf
EndIf

RestArea(aArea)
Return lRet

