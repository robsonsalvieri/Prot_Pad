/*/{Protheus.doc}FBaseRPCC
Consiste valor base de calculo do PCC.
@author Leonardo Castro
@since  19/02/2016
@version 12
/*/
//-------------------------------------------------------------------
Static Function FBaseRPCC(nValorRec As Numeric, lCalcPCC As Logical)
	Local lFINA450  As Logical
	Local nValBase  As Numeric
	Local nBaseRet  As Numeric
	Local aBase     As Array
	Local nX        As Numeric
	Local cChaveAux As Char
	Local cCampo    As Char
	Local nParc     As Numeric
	Local nBase     As Numeric
	
	//Inicializa variáveis
	lFINA450  := FwIsInCallStack("FA450CMP")
	nValBase  := 0
	nBaseRet  := 0
	aBase 	  := {}
	nX 		  := 0
	cChaveAux := ""
	cCampo    := ReadVar() 
	nParc     := 0
	nBase     := If(SE1->E1_BASECSL > 0, SE1->E1_BASECSL, If(SE1->E1_BASEPIS > 0, SE1->E1_BASEPIS, If(SE1->E1_BASECOF > 0, SE1->E1_BASECOF, 0)))	
	nMoedaBco := IF(Type("nMoedaBco") != "N", 1, nMoedaBco)
	
	//Parâmetros de entrada.
	Default nValorRec	:= nBase
	Default lCalcPcc	:= .T.
	Default __nTxBxAnt  := 0
	
	If __lTemMR .And. __lPccMR .And. nBase == 0
		cChaveAux := FWxFilial( "SE1", SE1->E1_FILORIG ) + "|" + SE1->E1_PREFIXO + "|" + SE1->E1_NUM + "|" + SE1->E1_PARCELA + "|" + SE1->E1_TIPO + "|" + SE1->E1_CLIENTE + "|" + SE1->E1_LOJA

		aBase := FinImpFis(cChaveAux,SE1->E1_FILORIG,"SE1")
		For nX := 1 to Len(aBase)
			nBase := aBase[nX][2]
		Next
	Endif
	
	nParc := IF(Type("nParciais") != "N", 0, nParciais)// real

	If FindFunction("FxIsBxTotal")
		/*
			- Se o valor vai baixar Totalmente o título -> considerar base - Parciais (Parciais é considerado mais abaixo)
			- Ou se o valor não vai baixar totalmente -> considerar como base o valor passado (nValRec normalmente) - Valor Recebido
		*/	
		If SE1->E1_MOEDA == 1 .And. FxIsBxTotal( nValorRec , nParc , __nMRBxTot )	
			nValorRec := nBase
		EndIf
	Else
		nValorRec := IIf(cCampo $ "NVALREC|NVALESTRANG", nValorRec, nBase)
	EndIf

	nValBase := nBase

	If SE1->E1_MOEDA > 1
		nParc	:= xMoeda(nParc,nMoedaBco,SE1->E1_MOEDA,dDatabase,3,,IIf(__nTxBxAnt > 0, __nTxBxAnt, nTxMoeda))
	EndIf

	If (FwIsInCallStack("FA070VALREC") .Or. (lF070Auto .And. lAutValRec)) .and. SE1->E1_MOEDA > 1// Se for alteração o valor a ser baixado vem em dolar
		nValorRec	:= xMoeda(nValorRec,nMoedaBco,SE1->E1_MOEDA,dDatabase,3,,nTxMoeda)
	EndIf

	If lFINA450
		nValBase := nValorRec
	ElseIf nParc < nValBase // Se o valor das baixas parciais nao atingiram o valor da base.
		nValBase := nValBase - nParc
		If nValorRec < nValBase
			nValBase := nValorRec
		EndIf
	Else // Se ja alcançou a base de calculo.
		nBaseRet	:= 0
		lCalcPcc	:= .F.
	EndIf

	nBaseRet := nValBase
Return nBaseRet

