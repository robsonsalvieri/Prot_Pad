			nTxMoeda := SE1->E1_TXMOEDA
		ElseIf nMoedaBco == SE1->E1_MOEDA
			nTxMoeda := 1
        ElseIf nMoedaBco > 1 .AND. SE1->E1_MOEDA = 1
            nTxMoeda := RecMoeda(dBaixa,nMoedaBco)
        Else
            nTxMoeda := RecMoeda(dBaixa,SE1->E1_MOEDA)
   		EndIf
	EndIf
