/*/{Protheus.doc} F070VlFk5
	Pega valor do movimento bancário para atualização do saldo 
	
	@Type Static Function
	@author Fabio Zanchim
	@since 21/02/2022
	@version 1.0
	@return _nVal, Numeric
/*/
//-------------------------------------------------------------------
Static Function F070VlFk5() As Numeric

	Local _nVal 	As Numeric
	Local _cProc 	As Character 
	Local _aArea 	As Array
	Local _cAlias	As Character
	Local _cQuery	As Character
	
	_cQuery	:=""
	_cAlias :=""
	_nVal	:=0
	_aArea	:=GetArea()

	//-- Pega o codigo de processo relacionado a baixa
	_cProc := FINProcFKs(SE5->E5_IDORIG, 'FK1')

	If !Empty(_cProc)

		If __aTamX3 == Nil
			__aTamX3:=TamSx3("FK5_VALOR")
		EndIf
		_cAlias :=GetNextAlias()

		//-- Localiza o movimento bancario da baixa
		_cQuery := " SELECT FK5_VALOR "
		_cQuery += " FROM "+RetSQLName("FKA")+" INNER JOIN  "+RetSQLName("FK5")
		_cQuery += " 	ON FKA_FILIAL=FK5_FILIAL AND FKA_IDORIG=FK5_IDMOV "
		_cQuery += " 	AND FK5_RECPAG='R'"
		_cQuery += "    AND "+RetSQLName("FK5")+".D_E_L_E_T_=' '"
		_cQuery += " WHERE "
		_cQuery += " FKA_FILIAL='"+xFilial('FKA')+"' AND"
		_cQuery += " FKA_TABORI='FK5' "
		_cQuery += " AND FKA_IDPROC='"+_cProc+"'"
		_cQuery += " AND "+RetSQLName("FKA")+".D_E_L_E_T_=' '"
		_cQuery := ChangeQuery( _cQuery )
		dbUseArea( .T., "TOPCONN", TcGenQry(,,_cQuery), _cAlias, .F., .T. )

		DbSelectArea(_cAlias)
		DbGoTop()
		If !Eof()
			TCSetField(_cAlias, "FK5_VALOR" ,"N",__aTamX3[1],__aTamX3[2])
			_nVal := (_cAlias)->(FK5_VALOR)
		EndIF
		(_cAlias)->(dbCloseArea())
	EndIf 

	RestArea(_aArea)
Return(_nVal)

/*/{Protheus.doc} UpdF71Stat
Pega valor do movimento bancário para atualização do saldo 

@type	Static Function
@author Rafael Riego
@since	12/04/2022
@param  cFilOriFK1, character, filial de origem do título
@param	cIdDocFK1, character, id doc da baixa
@param	cSeqFK1, character, sequência da baixa na tabela FK1 (FK1_SEQ)
/*/
Static Function UpdF71Stat(cFilOriFK1 As Character, cIdDocFK1 As Character, cSeqFK1 As Character)

	Local aArea			As Array
	Local cIdFK1xF71	As Character
	Local cQuery		As Character

	Default cFilOriFK1	:= ""
	Default cIdDocFK1	:= ""
	Default cSeqFK1		:= ""

	aArea := GetArea()

	If !(Empty(cFilOriFK1)) .And. !(Empty(cIdDocFK1)) .And. !(Empty(cSeqFK1))
		If __oPrBxPIX == Nil
			cQuery := "SELECT FK1_IDFK1 IDFK1 FROM " + RetSQLName("FK1") + " FK1"
			cQuery += " WHERE FK1.FK1_FILIAL = ?"
			cQuery +=   " AND FK1.FK1_IDDOC = ?"
			cQuery +=   " AND FK1.FK1_SEQ = ?"
			cQuery +=   " AND FK1.FK1_TPDOC <> ?"
			cQuery +=   " AND FK1.D_E_L_E_T_ = ?"
			cQuery := ChangeQuery(cQuery)
			__oPrBxPIX := FwPreparedStatement():New(cQuery)
		EndIf

		__oPrBxPIX:SetString(1, FwXFilial("FK1", cFilOriFK1))
		__oPrBxPIX:SetString(2, cIdDocFK1)
		__oPrBxPIX:SetString(3, cSeqFK1)
		__oPrBxPIX:SetString(4, "ES")
		__oPrBxPIX:SetString(5, " ")

		If !(Empty(cIdFK1xF71 := MPSysExecScalar(__oPrBxPIX:GetFixQuery(), "IDFK1")))
			F71->(DbSetOrder(4)) //F71_FILIAL + F71_IDFK1
			If F71->(DbSeek(FwXFilial("F71", cFilOriFK1) + cIdFK1xF71))
				RecLock("F71", .F.)
				F71->F71_STATUS = "8"
				F71->(MsUnlock())
			EndIf
		EndIf
	EndIf

	RestArea(aArea)
	FwFreeArray(aArea)

Return Nil

