//-------------------------------------------------------------------
Static Function F070NotIN()
Local cTipos := MVPROVIS+"/"+MVRECANT+"/"+MV_CRNEG

//Destacar Abatimentos
If mv_par06 == 2
	cTipos += "/"+MVABATIM+"/"+MVFUABT +'")'//adicionado MVFUABT pois a variável MVABATIM não está retornando FU-
Endif

cTipos	:=	StrTran(cTipos,',','/')
