__lCalcImp := .F.
__lGeraF71 := .F.
nRecnoSE1  := SE1->(Recno())
__BaixSLD0 := IIf(__BaixSLD0 == Nil, FindFunction("BaixaSLD0"), __BaixSLD0)

If cPaisLoc == "BRA" .And. __lExcImpo .And. !(__lCalcImp := lPix) .And. !__lCnabImp .And. SE1->E1_SITUACA != "0"
	__lCalcImp := BorderoImp(SE1->E1_FILORIG, SE1->E1_NUMBOR, "R", SE1->E1_PREFIXO, SE1->E1_NUM, SE1->E1_PARCELA, SE1->E1_TIPO, SE1->E1_CLIENTE, SE1->E1_LOJA)	
	
	If !__lCalcImp		
		__lCalcImp := HistPagPix(SE1->E1_FILORIG, SE1->(E1_FILIAL+"|"+E1_PREFIXO+"|"+E1_NUM+"|"+E1_PARCELA+"|"+E1_TIPO+"|"+E1_CLIENTE+"|"+E1_LOJA), @__oPagPix)
		
		If !__lCalcImp			
			nRecnoF71 := F71Ativa(nRecnoSE1)
			
			If nRecnoF71 > 0 .And. TtBxImpPix(nRecnoSE1)
				If (lRetorno := ((lF070Auto .And. lAutBxPix) .Or. (!lF070Auto .And. MsgTtBxPix(.F.,.F.,.F.,@__lNExbMsg))))
					If (lRetorno := (ExcluiImpo(SE1->E1_FILIAL, SE1->E1_PREFIXO, SE1->E1_NUM, SE1->E1_PARCELA, SE1->E1_TIPO, SE1->E1_CLIENTE, SE1->E1_LOJA, nRecnoF71)))
						cChaveTit  := SE1->E1_FILIAL+"|"+SE1->E1_PREFIXO+"|"+SE1->E1_NUM+"|"+SE1->E1_PARCELA+"|"+SE1->E1_TIPO+"|"+SE1->E1_CLIENTE+"|"+SE1->E1_LOJA						
						If !Empty(cIdDocFK7 := FINBuscaFK7(cChaveTit, "SE1", SE1->E1_FILORIG))
							__lGeraF71 := PIXCancel(SE1->E1_FILIAL, cIdDocFK7, .T.)
						EndIf				
					EndIf
