									RecLock("FKD",.F.)
									FKD->FKD_VLINFO := 0
									FKD->(MsUnlock())

								EndIf

								FKD->( dbSkip() )
							end

						EndIf

					Next nLaco

					RestArea( aAreaFKDID )

				EndIf

				FWFreeArray( aAtuFKDID )
				aAtuFKDID	:=	Nil
				FWFreeArray( aAreaFKDID )
