	nTamSeq	   := TamSX3("EI_SEQ")[1]
	csequencia := Replicate("0",nTamSeq)
	n181Sequen := Replicate("0",nTamSeq)
	cNGFVersao := ""
	ca181Codigo:= SEH->EH_NUMERO+"/"+SEH->EH_REVISAO
