					SEI->EI_TXMOEDA := nA181VlMoed
					SEI->EI_VLRQTS  := na181VlCota
					SEI->EI_VLRQTA  := nVlCotaAnt
					SEI->EI_QTDCOTA := n181CtRg

					If lEiCalcUsr
