		Aadd(aExecAuto, {"E2_TIPO", SE2->E2_TIPO, NIL})
		Aadd(aExecAuto, {"E2_FORNECE", SE2->E2_FORNECE, NIL})
		Aadd(aExecAuto, {"E2_LOJA", SE2->E2_LOJA, NIL})
