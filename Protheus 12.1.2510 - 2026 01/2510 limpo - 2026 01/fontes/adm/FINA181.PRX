	nA181STOT1 := 0.00
	nA181STOT2 := 0.00
	nA181VPLP1 := 0.00
	nA181VPCP1 := 0.00
	nA181VPLP2 := 0.00
	nA181VPCP2 := 0.00
	nA181VJUR1 := 0.00
	nA181VJUR2 := 0.00
	nA181VVCLP := 0.00
	nA181VVCCP := 0.00
	nA181VVCJR := 0.00
	nA181VTOT1 := 0.00
	nA181VTOT2 := 0.00
	dA181DtApr := dDataBase
	nA181VlMoed:= RecMoeda(dA181DtApr,SEH->EH_MOEDA)
	aA181Reg   := {}
	aA181Estor := {}
	nPosicao   := 0
	cListBox   := ""
	cChave	   := ""
	aMovEst    := {}
	nCntFor    := 0
	nCntFor1   := 0
	UltApr     := Ctod("")
	lUltApr	   := .F. // Ultima apropriacao. (Unica restante)
	nRecnoSeh  := 0
	cLctPco    := ""
	lPcoConf   := .F.
	nTotalSEI  := 0
	nMoedaBco  := 1
	lFa181Est  := ( ExistBlock("FA181EST") )
	lAchouE2   := .T.
	cChaveSEH  := SEH->EH_APLEMP+SEH->EH_NUMERO+SEH->EH_REVISAO
	
	If FindFunction("F716Ver")
		cNGFVersao := F716Ver()
