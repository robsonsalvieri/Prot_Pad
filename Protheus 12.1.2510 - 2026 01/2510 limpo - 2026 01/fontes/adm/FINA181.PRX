/*/{Protheus.doc} F181Subst
	Realiza a substituição do titulo provisorio da parcela no emprestimo
	novo processo que so sera habilitado no novo gestor financeiro
	@type  Static Function
	@author Vitor Duca
	@since 08/04/2022
	@version 1.0
	@param cChavePR, Character, Chave do titulo provisorio que sera substituido
	@param nValor, Numeric, Valor do novo titulo que sera gerado
	@param cNatureza, Character, Natureza do novo titulo que será gerado
	@return lRet, Logical, Define se a substituição foi realizada com sucesso
/*/
Static Function F181Subst(cChavePR As Character, nValor As Numeric, cNatureza as Character) As Logical
	Local aExecAuto As Array
	Local aAuxTitPrv As Array
	Local aTitPrv As Array
	Local lRet As Logical
	Local nPosVenc As Numeric
	Local cHistorico As Character

	DEFAULT cChavePR := ""

	aExecAuto := {}
	aAuxTitPrv := {}
	aTitPrv := {}
	lRet := .F.
	nPosVenc := 0
	cHistorico :=  STR0106 + " - " + SEH->EH_NUMERO + SEH->EH_REVISAO

	SE2->(DbSetOrder(1))
	If SE2->(MsSeek(cChavePR))

		//Valor vem convertido para a moeda forte, pois é o valor de debito
		//apresentado em tela
		If SE2->E2_MOEDA > 1
			nValor := xMoeda(nValor, 1, SE2->E2_MOEDA)
		Endif

		If !Empty(SE2->E2_HIST)
			cHistorico := SE2->E2_HIST
		Endif

		lMsErroAuto := .F.

		Aadd(aExecAuto, {"E2_NUM"		,SE2->E2_NUM, NIL})
		Aadd(aExecAuto, {"E2_PREFIXO"	,SE2->E2_PREFIXO, NIL})
		Aadd(aExecAuto, {"E2_PARCELA"	,SE2->E2_PARCELA, NIL})
		Aadd(aExecAuto, {"E2_TIPO"		,__cMVFTPO, NIL})
		Aadd(aExecAuto, {"E2_NATUREZ"	,cNatureza, NIL})
		Aadd(aExecAuto, {"E2_FORNECE"	,SE2->E2_FORNECE, NIL})
		Aadd(aExecAuto, {"E2_LOJA"		,SE2->E2_LOJA, NIL})
		Aadd(aExecAuto, {"E2_EMISSAO"	,SE2->E2_EMISSAO, NIL})
		Aadd(aExecAuto, {"E2_VENCTO"	,SE2->E2_VENCTO, NIL})
		Aadd(aExecAuto, {"E2_VALOR"		,nValor, NIL})
		Aadd(aExecAuto, {"E2_HIST"		,cHistorico, NIL})
		Aadd(aExecAuto, {"E2_ORIGEM"	,"FINA181", NIL})

		//Informações contabeis do titulo
		Aadd(aExecAuto, {"E2_DEBITO"   	,SE2->E2_DEBITO	, NIL})
		Aadd(aExecAuto, {"E2_CREDIT"   	,SE2->E2_CREDIT	, NIL})
		Aadd(aExecAuto, {"E2_CCUSTO"	,SE2->E2_CCUSTO	, NIL})
		Aadd(aExecAuto, {"E2_CCD"   	,SE2->E2_CCD	, NIL} )
		Aadd(aExecAuto, {"E2_CCC"   	,SE2->E2_CCC	, NIL} )
		Aadd(aExecAuto, {"E2_ITEMD"   	,SE2->E2_ITEMD	, NIL} )
		Aadd(aExecAuto, {"E2_ITEMC"   	,SE2->E2_ITEMC	, NIL} )
		Aadd(aExecAuto, {"E2_CLVLDB"  	,SE2->E2_CLVLDB	, NIL} )
		Aadd(aExecAuto, {"E2_CLVLCR"  	,SE2->E2_CLVLCR	, NIL} )

		If lMovEnt05
			AADD(aExecAuto,{ "E2_EC05DB"  	, SE2->E2_EC05DB	, NIL })
			AADD(aExecAuto,{ "E2_EC05CR"  	, SE2->E2_EC05CR	, NIL })
		EndIf

		If lMovEnt06
			AADD(aExecAuto,{ "E2_EC06DB"  	, SE2->E2_EC06DB	, NIL })
			AADD(aExecAuto,{ "E2_EC06CR"  	, SE2->E2_EC06CR	, NIL })
		EndIf

		If lMovEnt07
			AADD(aExecAuto,{ "E2_EC07DB"  	, SE2->E2_EC07DB	, NIL })
			AADD(aExecAuto,{ "E2_EC07CR"  	, SE2->E2_EC07CR	, NIL })
		EndIf

		If lMovEnt08
			AADD(aExecAuto,{ "E2_EC08DB"  	, SE2->E2_EC08DB	, NIL })
			AADD(aExecAuto,{ "E2_EC08CR"  	, SE2->E2_EC08CR	, NIL })
		EndIf

		If lMovEnt09
			AADD(aExecAuto,{ "E2_EC09DB"  	, SE2->E2_EC09DB	, NIL })
			AADD(aExecAuto,{ "E2_EC09CR"  	, SE2->E2_EC09CR	, NIL })
		EndIf

		aAuxTitPrv := { { "E2_PREFIXO", SE2->E2_PREFIXO, NIL },;
						{ "E2_NUM", SE2->E2_NUM, NIL },;
						{ "E2_PARCELA", SE2->E2_PARCELA, NIL },;
						{ "E2_TIPO", "PR ", NIL },;
						{ "E2_FORNECE", SE2->E2_FORNECE, NIL },;
						{ "E2_LOJA", SE2->E2_LOJA, NIL }}

		Aadd(aTitPrv, aAuxTitPrv)

		If FindFunction("FSubCpoObg")
			FSubCpoObg(@aExecAuto, "SE2")
		EndIf

		//Retira E2_VENCREA do array, pois, caso esse campos esteja ordenado primeiro que
		//a emissão E2_EMISSAO execauto apresenta falha erroneamente
		If (nPosVenc := aScan(aExecAuto, {|x| x[1] == "E2_VENCREA" })) > 0
			aDel(aExecAuto, nPosVenc)
			aSize( aExecAuto, len( aExecAuto ) - 1 )
		Endif

		MsExecAuto({|a,b,c,d,e,f,g,h,i,j| FINA050(a,b,c,d,e,f,g,h,i,j)},aExecAuto,,6,,,,,,aTitPrv)

		If !lMsErroAuto
			lRet := .T.
		Endif
	Endif

Return lRet

/*/{Protheus.doc} F181BxParc
	Realiza a baixa do titulo efetivo referente a parcela do emprestimo
	novo processo de pagamento das parcelas no emprestimo
	disponivel apenas para o novo gestor
	@type  Static Function
	@author Vitor Duca
	@since 11/04/2022
