/*/{Protheus.doc} FCkNewProc
	Verificar se o Empréstimo foi criado no novo processo

	@type  Function
	@author Rodrigo Oliveira
	@since 27/04/2024
	@version 1.0

	@param cFil, Character, Filial de origem do título provisório
	@param cPrefix, Character, Prefixo do título provisório
	@param cNum, Character, Número do título provisório
	@param cParcel, Character, Parcela do título provisório

	@return lRet, Logical, Define se a inclusão do contrato foi realizada no novo processo
/*/
Function FCkNewProc(cFil As Character, aParc As Array) As Logical
	Local lRet 		As Logical
	Local cQuery 	As Character
	Local cSepProv 	As Character

	Default cFil	:= ""
	Default aParc	:= {}

	lRet		:= .F.

	If !Empty(aParc) .And. Len(aParc) >= 3
		If __oTitProv == Nil
			cSepProv	:= If("|"$MVPROVIS,"|",",")

			cQuery	:= "SELECT FK7_NUM TITPROV "
			cQuery	+= " FROM ? FK7 "
			cQuery	+= " JOIN ? SEI "
			cQuery	+= " ON EI_FILIAL = ? "
			cQuery	+= " AND EI_NUMERO = FK7_NUM "
			cQuery	+= " AND EI_TIPODOC = 'VL' "
			cQuery	+= " AND EI_PARCELA = FK7_PARCEL "
			cQuery	+= " AND SEI.D_E_L_E_T_ = ' ' "
			cQuery 	+= " WHERE FK7_FILIAL = ? "
			cQuery	+= " AND FK7_ALIAS = 'SE2' "
			cQuery  += " AND FK7_FILTIT  = ? "
			cQuery  += " AND FK7_PREFIX = ? "
			cQuery  += " AND FK7_NUM = ? "
			cQuery  += " AND FK7_PARCEL = ? "
			cQuery  += " AND FK7_TIPO IN " + FormatIn(MVPROVIS,cSepProv) + " "
			cQuery 	+= " AND FK7.D_E_L_E_T_ = ' ' "

			cQuery := ChangeQuery(cQuery)

			__oTitProv := FWPreparedStatement():New(cQuery)

		EndIf

		__oTitProv:SetNumeric(1, RetSqlName("FK7"))
		__oTitProv:SetNumeric(2, RetSqlName("SEI"))
		__oTitProv:SetString(3, xFilial("SEI"))
		__oTitProv:SetString(4, xFilial("FK7", cFil))
		__oTitProv:SetString(5, xFilial("SEI", cFil))
		__oTitProv:SetString(6, aParc[1])
		__oTitProv:SetString(7, aParc[2])
		__oTitProv:SetString(8, aParc[3])

		cQuery     	:= __oTitProv:GetFixQuery()
		cTitProv	:= MpSysExecScalar(cQuery,"TITPROV")
		lRet		:= !Empty(AllTrim(cTitProv))
	EndIf

Return lRet

/*/{Protheus.doc} F181VLIR
	Calcula o IR para aplicações financeiras,
	@type  Function
	@param nValBase, Numeric, Valor base para calculo do IR
	@param nDias   , Numeric, dias corridos da inclusão até o calculo
	@param nTaxaIR , Numeric, Taxa fixa de IR
	@return nValIR , Numeric, Valor do IR da aplicação
	@author Renato Ito
	@since 09/04/2024
/*/
Function F181VLIR(nValBase as Numeric, nDias as Numeric, nTaxaIR as Numeric) as Numeric
	Local nValIR	:= 0 as Numeric
	If Empty(nTaxaIR)
		If SX5->(MsSeek(xFilial("SX5") + "AR"))
			nTaxaIR := Val(TabelaIrf( "AR" , nDias ) )
		EndIf
	EndIf
	If !Empty(nTaxaIR)
		nValIR := ROUNDFIN((nValBase * (nTaxaIR/100)), 2, .T.)
		If nValIR < 0
			nValIR := 0
		EndIf
	EndIf
Return nValIR

/*/{Protheus.doc} F181CCBloc
	Valida se a conta bancária utilizada no resg/pag está bloqueada.

	@type  Function
	@return lIsBlock, logical, retorna verdadeiro caso a conta esteja bloqueada.
	@author Vinicius do Prado
	@since 29/04/2024
/*/
Function F181CCBloc() as logical
	Local nPos 		:= 0 as numeric
	Local lIsBlock 	:= .F. as logical
	Local aDataAut 	:= __aRetAuto as array
	Local cBank 	:= SEH->EH_BANCO as character
	Local cAgency 	:= SEH->EH_AGENCIA as character
	Local cAccount 	:= SEH->EH_CONTA as character

	If (nPos := aScan(aDataAut,{|x| x[1] == "cA181Bco"})) > 0
		cBank := Padr(aDataAut[nPos][2], TamSx3("A6_COD")[1])
	EndIf

	If (nPos := aScan(aDataAut,{|x| x[1] == "cA181Age"})) > 0
		cAgency := Padr(aDataAut[nPos][2], TamSx3("A6_AGENCIA")[1])
	EndIf

	If (nPos := aScan(aDataAut,{|x| x[1] == "cA181Cta"})) > 0
		cAccount := Padr(aDataAut[nPos][2], TamSx3("A6_NUMCON")[1])
	EndIf

	lIsBlock := CCBlocked(cBank,cAgency,cAccount)
Return lIsBlock

/*/{Protheus.doc} FValDtMov
	Valida se a data de movimento do estorno não é menor que o resgate/pagamento
	@type  Static Function
	@author Vitor Duca
	@since 14/06/2024
	@version 1.0
	@param nRecSEI, Numeric, R_E_C_N_O_ da SEI que será verificado o movimento
	@return lRet, Logical, Se a data é valida para o estorno
/*/
Static Function FValDtMov(nRecSEI As Numeric, dDataMov As Date) As Logical
	Local nRecSE5 As Numeric
	Local aAreaSE5 As Array
	Local aAreaSEI As Array
	Local aArea As Array
	Local cTpDoc As Character
	Local lRet As Logical
	Local cMsg As Character

	Default nRecSEI := 0
	Default dDataMov := dDataBase

	cTpDoc := "PE"
	lRet := .T.
	cMsg := STR0108 //"Não são permitidos estornos de pagamento em data anterior ao pagamento"

	If !Empty(nRecSEI)

		aArea := GetArea()
		aAreaSEI := SEI->(GetArea())
		aAreaSE5 := SE5->(GetArea())

		SEI->(DbGoto(nRecSEI))

		If SEI->EI_APLEMP == "APL"
			cTpDoc := "RF"
			cMsg := STR0107 //"Não são permitidos estornos de resgate em data anterior ao resgate"
		Endif

		If (nRecSE5 := FN181SE5( cTpDoc, SEI->EI_NUMERO + SEI->EI_REVISAO + SEI->EI_SEQ )) > 0
			SE5->(DbGoto(nRecSE5))
			If dDataMov < SE5->E5_DTDISPO
				help(, 1, "F181NOEST",, cMsg + Dtoc(SE5->E5_DTDISPO), 1, 0)
				lRet := .F.
			Endif
		Endif

		RestArea(aAreaSEI)
		RestArea(aAreaSE5)
		RestArea(aArea)
	Endif
Return lRet

/*/{Protheus.doc} F181IsMov
	Verifica se o movimento de baixa do titulo ocorreu pelo FINA181
	@type  Static Function
	@author Vitor Duca
	@since 15/07/2024
	@version 1.0
	@return lRet, Logical, Se a origem do movimento foi gerada pelo FINA181
/*/
Static Function F181IsMov() As Logical
	Local cQuery		as Character
	Local aBindParam	as Array
	Local lRet			as Logical

	aBindParam := 	{;
						SE2->E2_FILIAL,;
						SE2->E2_PREFIXO,;
						SE2->E2_NUM,;
						SE2->E2_PARCELA,;
						SE2->E2_TIPO,;
						SE2->E2_FORNECE,;
						SE2->E2_LOJA;
					}
	lRet := .T.

	cQuery := "SELECT COUNT(FK2.FK2_IDFK2) IDFK2 "
	cQuery += "FROM   " + RetSqlName("FK2") + " FK2 "
	cQuery += 	"INNER JOIN " + RetSqlName("FK7") + " FK7 "
	cQuery += 			"ON FK7.FK7_ALIAS = 'SE2' "
	cQuery += 				"AND FK7.FK7_FILTIT = ? "
	cQuery += 				"AND FK7.FK7_PREFIX = ? "
	cQuery += 				"AND FK7.FK7_NUM = ? "
	cQuery += 				"AND FK7.FK7_PARCEL = ? "
	cQuery += 				"AND FK7.FK7_TIPO = ? "
	cQuery += 				"AND FK7.FK7_CLIFOR = ? "
	cQuery += 				"AND FK7.FK7_LOJA = ? "
	cQuery += 				"AND FK7.FK7_IDDOC = FK2.FK2_IDDOC "
	cQuery += 				"AND FK7.D_E_L_E_T_ = ' ' "
	cQuery += "WHERE  FK2.D_E_L_E_T_ = ' ' "
	cQuery += 	"AND FK2.FK2_ORIGEM = 'FINA181' "
	cQuery += 	"AND NOT EXISTS(SELECT FK2EST.FK2_IDDOC "
    cQuery +=                   "FROM   " + RetSqlName("FK2") + " FK2EST "
    cQuery +=                   "WHERE  FK2EST.FK2_FILIAL = FK2.FK2_FILIAL "
    cQuery +=                          "AND FK2EST.FK2_IDDOC = FK2.FK2_IDDOC "
    cQuery +=                          "AND FK2EST.FK2_SEQ = FK2.FK2_SEQ "
    cQuery +=                          "AND FK2EST.FK2_TPDOC = 'ES' "
    cQuery +=                          "AND FK2EST.D_E_L_E_T_ = ' ') "

	If !(lRet := MpSysExecScalar( cQuery, "IDFK2", aBindParam ) > 0)
		help(, 1, "F181NOEST",, STR0109, 1, 0) //"O movimento de baixa do titulo foi originado por outra rotina, sendo assim o estorno deve acontecer na rotina geradora do movimento"
	Endif

Return lRet

/*/{Protheus.doc} F181BXAPR
	Calcular os valores apropriados retirando os resgatados para obter a diferença do total do resgate
	@author Renato Ito
	@since 25/06/2025
	@param cFilSEI, Character, Filial do SEI
	@param cAplEmp, Character, Aplicação ou Emprestimo
	@param cNumero, Character, Número do SEI
	@param cRevisao, Character, Revisão do SEI
	@return aRet, Array, [1]= JR [2]= I1
/*/
function F181BXAPR(cFilSEI as character, cAplEmp as character, cNumero as character, cRevisao as character)
	local cQuery     as character
	local oStatement as object
	local nValJRBX   as numeric
	local nValJRAPR  as numeric
	local nValIRBX   as numeric
	local nValIRAPR  as numeric
	local aRet       as array
	
	aRet := {}

	cQuery := "SELECT SUM(SEI.EI_VALOR) VALOR "
	cQuery += 	" FROM " + RetSqlName('SEI') + " SEI "
	cQuery += " WHERE "
	cQuery += 	" SEI.EI_FILIAL = ? "
	cQuery += 	" AND SEI.EI_APLEMP = ? "
	cQuery += 	" AND SEI.EI_NUMERO = ? "
	cQuery += 	" AND SEI.EI_REVISAO = ? "
	cQuery += 	" AND SEI.EI_MOTBX = ? "
	cQuery += 	" AND SEI.EI_TIPODOC = ? "
	cQuery += 	" AND SEI.EI_DATA < ? "
	cQuery += 	" AND SEI.EI_STATUS <> ? "
	cQuery += 	" AND SEI.D_E_L_E_T_ = ? "

	oStatement := FWExecStatement():New(ChangeQuery(cQuery))

	oStatement:setString(1, cFilial)  //EI_FILIAL
	oStatement:setString(2, cAplEmp)  //EI_APLEMP
	oStatement:setString(3, cNumero)  //EI_NUMERO
	oStatement:setString(4, cRevisao) //EI_REVISAO
	oStatement:setString(5, 'APR')    //EI_MOTBX
	oStatement:setString(6, 'JR')     //EI_TIPODOC
	oStatement:setDate(7, dDataBase ) //EI_DATA
	oStatement:setString(8, "C" )    //EI_STATUS
	oStatement:setString(9, ' ')      //D_E_L_E_T_

	// valor de JR da apropiacao
	nValJRAPR := oStatement:execScalar("VALOR")
	// valor de IR da apropiacao
	oStatement:setString(6, 'I1')     //EI_TIPODOC
	nValIRAPR := oStatement:execScalar("VALOR")
	// valor de IR dos resgates
	oStatement:setString(5, 'NOR')    //EI_MOTBX
	nValIRBX := oStatement:execScalar("VALOR")
	// valor de JR dos resgates
	oStatement:setString(6, 'JR')     //EI_TIPODOC
	nValJRBX := oStatement:execScalar("VALOR")

	aadd(aRet,{ "JR", (nValJRAPR - nValJRBX) * -1, 0 , (nValJRAPR - nValJRBX) * -1})
	aadd(aRet,{ "I1", (nValIRAPR - nValIRBX) * -1, 0 , (nValIRAPR - nValIRBX) * -1})
	oStatement:Destroy()
	oStatement := nil
return aRet
