	nDeprAc    := SN3->N3_VRDACM1 + SN3->N3_VRCDA1 + GetSldAcel('1')

	If nOpc = 5
		DbSelectArea("SN4")
		cNomeArq := CriaTrab(, .F.)
		IndRegua("SN4", cNomeArq, "N4_FILIAL+N4_CBASE+N4_ITEM+N4_TIPO+N4_OCORR",,;
