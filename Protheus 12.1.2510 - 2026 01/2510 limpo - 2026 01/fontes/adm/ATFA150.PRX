DEFAULT lRotAuto := .F.

nOpc := If(nOpc = 4, 5, nOpc)

If !FreeForUse("SN1",xFilial("SN1")+SN1->(N1_CBASE+N1_ITEM))
	Return
Endif

lMostra := Iif(Type("MV_PAR01") == "N",(MV_PAR01 == 1),.F.)
