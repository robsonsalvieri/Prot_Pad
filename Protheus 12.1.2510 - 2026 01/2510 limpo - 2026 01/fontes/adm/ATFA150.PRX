	Help(" ",1,"AF150JaBai")
	Return
Endif

If !Empty(SN1->N1_DTBLOQ)
	Help(" ",1,"AF150BLOQ")   		//Este bem esta bloqueado, nao pode sofrer ampliacao.
	Return
EndIf

If SN1->N1_STATUS == "2"
	Help(" ",1,"AF150BLOQ")   	//Este bem esta bloqueado, nao pode sofrer ampliacao.
	Return
EndIf

/*
 * Tratamento do Bloqueio de bens com classificação patrimonial como "Custos de Transação", não pode sofre depreciação.
 */
If SN1->N1_PATRIM == "T"
	Help(" ",1,"AF150CUSTR")
	Return
EndIf

/* ---------------------------------------------------------
   Nao permite ampliacões anteriores à data de Aquisição³
   --------------------------------------------------------- */
If  dDataBase < (SN1->(N1_AQUISIC))
	Help(" ",1,STR0033,,STR0034,1,0) //"Atenção!"##"A data da Ampliação deve ser maior que a data de aquisição do Bem "
	Return
EndIf

//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ NÆo permite que amplia‡äes possam se feitas ³
//³ antes da data do £ltimo c lculo.            ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
If	cCalcDep == cd_MENSAL .And.;
	(	dDataBase <= LastDay(dUltProc) .Or.;
	(Month(dDataBase) # Month(dUltProc + 1) .And.;
	Year(dDataBase) # Year(dUltProc)) )
	HELP(" ",1,"ATF150AD")
	Return
EndIf

If dDataBase < dUltProc .Or. (dDataBase > (LastDAy(dUltProc+1))) .OR. !(dDataBase >= dUltProc)		//Tipo Depreciacao diferente 02-Mes Subsequente
