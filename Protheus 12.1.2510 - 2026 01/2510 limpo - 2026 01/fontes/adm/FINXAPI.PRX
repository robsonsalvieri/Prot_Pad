/*/{Protheus.doc} FinCteSF8
    Armazena em um array os dados do CT-e que originou a substituição.

    @author Fabio Casagrande Lima
    @since  17/10/2024

    @param aSE2OriCTe, Array, Armazena os dados do CT-e substituido
    @param cNFiscal, Char, Numero do CT-e
    @param cSerie, Char, Serie do CT-e
    @param cFornece, Char, Serie do CT-e
    @param cLoja, Char, Serie do CT-e

	@return Nil
/*/
Function FinCteSF8(aSE2OriCTe, cNFiscal, cSerie, cFornece, cLoja)

    Local cAlisTmp As Char
    Local cQuery As Char
    Local nCont As Numeric

    Default aSE2OriCTe := {}
    Default cNFiscal := ""
    Default cSerie := ""
    Default cFornece := ""
    Default cLoja := ""

    nCont      := 1
    __lCachQry := IIf(__lCachQry == Nil, FwLibVersion() >= "20211116", __lCachQry)

    //Localiza CT-e de origem da substituição
    If __oQrySF8 == NIL
        cQuery := "SELECT F8_NFORIG,F8_FORNECE,F8_LOJA,F1_PREFIXO "
        cQuery += "FROM " + RetSQLName("SF8") + " SF8 "
        cQuery += "INNER JOIN " + RetSQLName("SF1") + " SF1 ON "
        cQuery += "SF1.F1_FILIAL = ? AND " //#1
        cQuery += "SF1.F1_DOC = SF8.F8_NFORIG AND "
        cQuery += "SF1.F1_SERIE = SF8.F8_SERORIG AND "
        cQuery += "SF1.F1_FORNECE = SF8.F8_FORNECE AND "
        cQuery += "SF1.F1_LOJA = SF8.F8_LOJA "
        cQuery += "WHERE SF8.F8_FILIAL = ? " //#2
        cQuery += "AND SF8.F8_NFDIFRE = ? " //#3
        cQuery += "AND SF8.F8_SEDIFRE = ? " //#4
        cQuery += "AND SF8.F8_TRANSP = ? " //#5
        cQuery += "AND SF8.F8_LOJTRAN = ? "//#6
        cQuery += "AND SF8.F8_TIPO = ? " //#7
        cQuery += "AND SF1.F1_TIPO = ? " //#8
        cQuery += "AND SF8.D_E_L_E_T_ = ? " //#9
        cQuery += "AND SF1.D_E_L_E_T_ = ? " //#10
        cQuery := ChangeQuery( cQuery )
        __oQrySF8 := IIf(__lCachQry, FwExecStatement():New(cQuery), FWPreparedStatement():New(cQuery))
    EndIf

    __oQrySF8:SetString(nCont++, FWxFilial("SF1")) //#1
    __oQrySF8:SetString(nCont++, FWxFilial("SF8")) //#2
    __oQrySF8:SetString(nCont++, cNFiscal)         //#3
    __oQrySF8:SetString(nCont++, cSerie)           //#4
    __oQrySF8:SetString(nCont++, cFornece )        //#5
    __oQrySF8:SetString(nCont++, cLoja )           //#6
    __oQrySF8:SetString(nCont++, "S" )             //#7
    __oQrySF8:SetString(nCont++, "C" )             //#8
    __oQrySF8:SetString(nCont++, Space(1) )        //#9
    __oQrySF8:SetString(nCont++, Space(1) )        //#10

    cAlisTmp := IIf(__lCachQry, __oQrySF8:OpenAlias(), MpSysOpenQuery(__oQrySF8:GetFixQuery()))

    While (cAlisTmp)->(!Eof())
        Aadd(aSE2OriCTe, (cAlisTmp)->F1_PREFIXO)
        Aadd(aSE2OriCTe, (cAlisTmp)->F8_NFORIG)
        Aadd(aSE2OriCTe, (cAlisTmp)->F8_FORNECE)
        Aadd(aSE2OriCTe, (cAlisTmp)->F8_LOJA)
        (cAlisTmp)->(dbSkip())
    Enddo

    (cAlisTmp)->(DbCloseArea())

Return

/*/{Protheus.doc} FinCTeSubs
    Efetua as gravações no SIGAFIN para as operações de inclusão/exclusão 
    do Conhecimento de Transporte Substituto (MATA103).

    Objetivo: 
        - Baixar saldo em aberto do título do CT-e que foi substituído;
        - Gerar NDF caso o título que foi substituído foi baixado;

    @author Fabio Casagrande Lima
    @since  11/10/2024

    @param aSE2OriCTe, Array, Dados do CT-e original da substituição
    @param nOper, Numeric, Identif. da operação (3-Inclusão/5-Exclusão)
    @param lBlind, Logical, Verdadeiro se o processamento não tem interface
    @param aCTeCtbFin, Array, Dados da contabilização online:
        [1] Verdadeiro se a contabilização será online
        [2] Verdadeiro se exibirá a tela dos lançamentos contábeis

	@return lRet, Logical, .T. se se não houve erro(s) nas inclusões/baixas
/*/

Function FinCTeSubs(aSE2OriCTe, nOper, lBlind, aCTeCtbFin)

    Local cMotBaixa As Char
    Local cNfOrig As Char
    Local cPrefOrig As Char
    Local cForOrig As Char
    Local cLojOrig As Char
    Local cFilSE2 As Char
    Local lRet As Logical
    Local aAreaAtual As Array
    Local aAreaSE2 As Array
    Local aTitulos As Array
    Local aMoviBx As Array

    Default aSE2OriCTe := {}
    Default nOper   := 3
    Default lBlind  := .F.
    Default aCTeCtbFin := {}

    aTitulos   := {}
    aMoviBx    := {}
    cMotBaixa  := AllTrim(SuperGetMV("MV_MTBXCTE", .F., "DAC"))
    lRet       := .T.

    If Len(aSE2OriCTe) > 0 
        aAreaAtual := FWGetArea()
        aAreaSE2  := SE2->(FWGetArea())
        cPrefOrig := aSE2OriCTe[1]
        cNfOrig   := aSE2OriCTe[2]
        cForOrig  := aSE2OriCTe[3]
        cLojOrig  := aSE2OriCTe[4]
        cFilSE2   := FWxFilial("SE2")
        SE2->(dbSetOrder(6)) // E2_FILIAL+E2_FORNECE+E2_LOJA+E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO
        IF SE2->(dbSeek( cFilSE2 + cForOrig + cLojOrig + cPrefOrig + cNfOrig ))			
            While SE2->(!Eof()) .And. SE2->(E2_FILIAL+E2_FORNECE+E2_LOJA+E2_PREFIXO+E2_NUM) == cFilSE2 + cForOrig + cLojOrig + cPrefOrig + cNfOrig
                If nOper == 3 
                    CTePreInc(@aTitulos, @aMoviBx, cMotBaixa) //Armazena os dados ref. a inclusão do CT-e Substituto
                ElseIf nOper == 5 //Exclusão
                    CTePreCan(@aTitulos, @aMoviBx, cMotBaixa) //Armazena os dados ref. a exclusão do CT-e Substituto
                EndIf
                SE2->(dbSkip())
            Enddo	
            If nOper == 3 //Efetiva inclusão de titulo e baixas do processo
                lRet := CTeGrvInc(aTitulos, aMoviBx, lBlind, cMotBaixa, aCTeCtbFin)
            ElseIf nOper == 5 //Efetiva exclusão de titulo e cancelamento de baixas do processo
                lRet := CTeGrvCan(aTitulos, aMoviBx, lBlind, cMotBaixa, aCTeCtbFin)
            EndIf
        EndIf
        If FindFunction("F080ClearM") 
            F080ClearM() //Efetua a exclusão do model - FINA080
        EndIF
        FWRestArea(aAreaSE2)
        FWRestArea(aAreaAtual)
        FwFreeArray(aAreaAtual)
        FwFreeArray(aAreaSE2)
    EndIf

    FwFreeArray(aTitulos)
    FwFreeArray(aMoviBx)

Return lRet

/*/{Protheus.doc} CTePreInc
    Armazena os dados que serão gravados no SIGAFIN durante a inclusão
    de um CT-e substituto.

    @author Fabio Casagrande Lima
    @since  17/10/2024

    @param aTitulos, Array, Armazena os dados para gerar as NDF's
    @param aMoviBx, Array, Armazena os dados para gerar as baixas nos títulos de origem
    @param cMotBaixa, Char, Motivo de baixa utilizado no processo (MV_MTBXCTE)
    @param c1Dup, Char, Código inicial de preenchimento da parcela da NDF

    @return Nil
/*/
Static Function CTePreInc(aTitulos, aMoviBx, cMotBaixa)

    Local aFina050 As Array
    Local aFina080 As Array
    Local nVlrNDF As Numeric
    Local cParcela As Char
    Local cTxtProbl As Char
    Local cTxtSoluc As Char

    Default aTitulos  := {}
    Default aMoviBx   := {}
    Default cMotBaixa := AllTrim(SuperGetMV("MV_MTBXCTE", .F., "DAC"))

    aFina050   := {}
    aFina080   := {}

    //Se tiver baixas no título de origem, gera título tipo NDF para o valor baixado 
    If SE2->E2_TIPO == MVNOTAFIS .And. SE2->E2_VALOR <> SE2->E2_SALDO .And. (nVlrNDF := SE2->E2_VALOR - SE2->E2_SALDO) > 0   
      
        //Caso já exista uma NDF no array aTitulos, soma o valor para gerar uma NDF totalizadora para a NF
        If Len(aTitulos) > 0
            aTitulos[1][10][2] += nVlrNDF  //Valor do título NDF
        Else
            cParcela := FinPcImp('SE2',SE2->E2_PREFIXO,SE2->E2_NUM,left(MV_CPNEG,3),SE2->E2_FORNECE,SE2->E2_LOJA)
            aAdd(aFina050, { "E2_PREFIXO" , SE2->E2_PREFIXO	  , Nil } )
            aAdd(aFina050, { "E2_NUM"     , SE2->E2_NUM       , Nil } )
            aAdd(aFina050, { "E2_PARCELA" , cParcela          , Nil } )
            aAdd(aFina050, { "E2_TIPO"    , left(MV_CPNEG,3)  , Nil } )
            aAdd(aFina050, { "E2_NATUREZ" , SE2->E2_NATUREZ   , Nil } )
            aAdd(aFina050, { "E2_FORNECE" , SE2->E2_FORNECE   , Nil } )
            aAdd(aFina050, { "E2_LOJA"    , SE2->E2_LOJA      , Nil } )
            aAdd(aFina050, { "E2_EMISSAO" , dDataBase         , Nil } )
            aAdd(aFina050, { "E2_VENCTO"  , dDatabase         , Nil } )
            aAdd(aFina050, { "E2_VALOR"   , nVlrNDF           , Nil } )
            aAdd(aFina050, { "E2_MOEDA"   , SE2->E2_MOEDA     , Nil } )
            aAdd(aFina050, { "E2_HIST"    , STR0090           , Nil } ) //"Crédito ref. CT-e subst. já baixado."
            aAdd(aFina050, { "E2_ORIGEM"  , FunName()         , Nil } )
            AAdd(aTitulos, aClone(aFina050) )

            cTxtProbl := STR0094 //"Existe uma ou mais parcelas vinculadas ao CT-e original que já se encontram baixadas."
            cTxtSoluc := STR0095 //"Será gerado um título de crédito (tipo NDF) no valor total baixado. Feche a janela para dar continuidade ao processo de inclusão do CT-e substituto."
            Help( ,, "FinCTeSubs1",, cTxtProbl, 1, 0,,,,,, {cTxtSoluc} )
        EndIf

    EndIf

    //Realiza a baixa do saldo em aberto no título de origem
    If SE2->E2_TIPO == MVNOTAFIS .And. SE2->E2_SALDO > 0
        Aadd(aFina080, {"E2_PREFIXO", SE2->E2_PREFIXO, NIL})
        Aadd(aFina080, {"E2_NUM"    , SE2->E2_NUM    , NIL})
        Aadd(aFina080, {"E2_PARCELA", SE2->E2_PARCELA, NIL})
        Aadd(aFina080, {"E2_TIPO"   , SE2->E2_TIPO   , NIL})
        Aadd(aFina080, {"E2_FORNECE", SE2->E2_FORNECE, NIL})
        Aadd(aFina080, {"E2_LOJA"   , SE2->E2_LOJA   , NIL})
        Aadd(aFina080, {"AUTMOTBX"  , cMotBaixa      , NIL})
        Aadd(aFina080, {"AUTHIST"   , STR0091        , NIL}) //"Baixa referente CT-e substituído."
        AAdd(aMoviBx, aClone(aFina080) )
    EndIf
    
    FwFreeArray(aFina080)
    FwFreeArray(aFina050)

Return

/*/{Protheus.doc} CTePreCan
    Armazena os dados que serão gravados no SIGAFIN durante a exclusão
    de um CT-e substituto.

    @author Fabio Casagrande Lima
    @since  17/10/2024

    @param aTitulos, Array, Armazena os dados para excluir as NDF's
    @param aMoviBx, Array, Armazena os dados para cancelar as baixas nos títulos de origem
    @param cMotBaixa, Char, Motivo de baixa utilizado no processo (MV_MTBXCTE)

    @return Nil
/*/
Static Function CTePreCan(aTitulos, aMoviBx, cMotBaixa)

    Local aFina050 As Array
    Local aFina080 As Array
    Local cTxtProbl As Char
    Local cTxtSoluc As Char

    Default aTitulos  := {}
    Default aMoviBx   := {}
    Default cMotBaixa := AllTrim(SuperGetMV("MV_MTBXCTE", .F., "DAC"))

    aFina050   := {}
    aFina080   := {}

    //Cancela a baixa automatica gerada na substituição
    If SE2->E2_TIPO == MVNOTAFIS .And. SE2->E2_SALDO == 0
        Aadd(aFina080, {"E2_PREFIXO", SE2->E2_PREFIXO, NIL})
        Aadd(aFina080, {"E2_NUM"	, SE2->E2_NUM    , NIL})
        Aadd(aFina080, {"E2_PARCELA", SE2->E2_PARCELA, NIL})
        Aadd(aFina080, {"E2_TIPO"	, SE2->E2_TIPO	 , NIL})
        Aadd(aFina080, {"E2_FORNECE", SE2->E2_FORNECE, NIL})
        Aadd(aFina080, {"E2_LOJA"	, SE2->E2_LOJA	 , NIL})
        AAdd(aMoviBx, aClone(aFina080) )
    EndIf
    //Cancela NDF que foi gerada na substituição
    If SE2->E2_TIPO == Left(MV_CPNEG,3) .And. AllTrim(SE2->E2_ORIGEM) $ "MATA103" 
        If SE2->E2_VALOR == SE2->E2_SALDO
            aAdd(aFina050, { "E2_PREFIXO" , SE2->E2_PREFIXO	  , Nil } )
            aAdd(aFina050, { "E2_NUM"     , SE2->E2_NUM       , Nil } )
            aAdd(aFina050, { "E2_PARCELA" , SE2->E2_PARCELA   , Nil } )
            aAdd(aFina050, { "E2_TIPO"    , Left(MV_CPNEG,3)  , Nil } )
            aAdd(aFina050, { "E2_NATUREZ" , SE2->E2_NATUREZ   , Nil } )
            aAdd(aFina050, { "E2_FORNECE" , SE2->E2_FORNECE   , Nil } )
            aAdd(aFina050, { "E2_LOJA"    , SE2->E2_LOJA      , Nil } )
            AAdd(aTitulos, aClone(aFina050) )
        Else
            cTxtProbl := STR0092+" Prefixo: "+AllTrim(SE2->E2_PREFIXO)+" / Número: "+AllTrim(SE2->E2_NUM) //"Existe um crédito (título NDF) vinculado a esse CT-e substituto que já foi utilizado e não poderá ser excluído."
            cTxtSoluc := STR0093 //"Clique em fechar para concluir o processo de inclusão do CT-e substituto."
            Help( ,, "FinCTeSubs2",, cTxtProbl, 1, 0,,,,,, {cTxtSoluc} )
        EndIf
    EndIf

    FwFreeArray(aFina080)
    FwFreeArray(aFina050)

Return

/*/{Protheus.doc} CTeGrvInc
    Realiza a MsExecAuto do FINA050 e FINA080 em uma 
    operação de inclusão de substituição de CT-e 
    através do módulo SIGACOM.

    @author Fabio Casagrande Lima
    @since  18/10/2024

    @param aTitulos, Array, Armazena os dados para excluir as NDF's
    @param aMoviBx, Array, Armazena os dados para cancelar as baixas nos títulos de origem
    @param lBlind, Logical, Verdadeiro se o processamento não tem interface
    @param cMotBaixa, Char, Motivo de baixa utilizado no processo (MV_MTBXCTE)
    @param aCTeCtbFin, Array, Dados da contabilização online:
            [1] Verdadeiro se a contabilização será online
            [2] Verdadeiro se exibirá a tela dos lançamentos contábeis

    @return lRet, Logical, .T. se se não houve erro(s) na MsExecAuto
/*/
Static Function CTeGrvInc(aTitulos, aMoviBx, lBlind, cMotBaixa, aCTeCtbFin)

    Local nX As Numeric
    Local nY As Numeric
    Local nOper As Numeric
    Local lRet As Logical
    Local lRetInc As Logical
    Local lRetBx As Logical
    Local lCtbOnline As Logical
    Local lExibeLanc As Logical
    Local aParams As Array
    
    Default aTitulos := {}
    Default aMoviBx := {}
    Default lBlind := .F.
    Default aCTeCtbFin := {}
    Default cMotBaixa := AllTrim(SuperGetMV("MV_MTBXCTE", .F., "DAC"))

    nOper := 3 // Oper = 3-Inclusão e 5-Exclusão
    lRetInc := .T.
    lRetBx := .T.
    lCtbOnline := .F.
    lExibeLanc := .F.
    aParams := {}

    If Len(aCTeCtbFin) > 1
        lCtbOnline := aCTeCtbFin[1]
        lExibeLanc := aCTeCtbFin[2]
    EndIf

    //Efetiva inclusão do(s) título(s) a pagar (tipo NDF)
    For nX := 1 to Len(aTitulos)
        aParams := {aClone(aTitulos[nX]), Nil, nOper, Nil, Nil, lExibeLanc, lCtbOnline }
        lRetInc := CTeExAuto("FINA050", aParams)
    Next nX

    //Efetiva baixa dos títulos substituídos 
    For nY := 1 to Len(aMoviBx)
        aParams := {aClone(aMoviBx[nY]), nOper, .F., Nil, lExibeLanc, lCtbOnline, {}}
        lRetBx := CTeExAuto("FINA080", aParams)
        If lRetBx
            //Realiza a baixa dos impostos vinculados ao CT-e substituído (gerados na emissão e/ou baixa)
            lRetBx := CTeBxImp(aMoviBx[nY], nOper, lBlind, cMotBaixa, aCTeCtbFin)
        EndIf      
    Next nY

    lRet := lRetInc .And. lRetBx

    FwFreeArray(aParams)

Return lRet

/*/{Protheus.doc} CTeGrvCan
    Realiza a MsExecAuto do FINA050 e FINA080 em uma 
    operação de exclusão de substituição de CT-e 
    através do módulo SIGACOM.

    @author Fabio Casagrande Lima
    @since  18/10/2024

    @param aTitulos, Array, Armazena os dados para excluir as NDF's
    @param aMoviBx, Array, Armazena os dados para cancelar as baixas nos títulos de origem
    @param lBlind, Logical, Verdadeiro se o processamento não tem interface
    @param cMotBaixa, Char, Motivo de baixa utilizado no processo (MV_MTBXCTE)
    @param aCTeCtbFin, Array, Dados da contabilização online:
        [1] Verdadeiro se a contabilização será online
        [2] Verdadeiro se exibirá a tela dos lançamentos contábeis

    @return lRet, Logical, .T. se se não houve erro(s) na MsExecAuto
/*/
Static Function CTeGrvCan(aTitulos, aMoviBx, lBlind, cMotBaixa, aCTeCtbFin)

    Local nX As Numeric
    Local nY As Numeric
    Local nOper As Numeric
    Local nOpBaixa As Numeric
    Local nScan As Numeric
    Local cPrefixo As Char
    Local cNumTit As Char
    Local cParcTit As Char
    Local cTipoTit As Char
    Local cFornece As Char
    Local cLoja As Char
    Local cChvSE2 As Char
    Local cIdDoc As Char
    Local cFiltro As Char
    Local lProcessa As Logical
    Local lRet As Logical
    Local lRetInc As Logical
    Local lRetBx As Logical
    Local lCtbOnline As Logical
    Local lExibeLanc As Logical
    Local aRecFK2 As Array
    Local aParams As Array

    Private aBaixaSE5 As Array //Utilizado na Sel080Baixa()

    Default aTitulos := {}
    Default aMoviBx := {}
    Default lBlind := .F.
    Default cMotBaixa := AllTrim(SuperGetMV("MV_MTBXCTE", .F., "DAC"))
    Default aCTeCtbFin := {}

    nOper := 5 // Oper = 3-Inclusão e 5-Exclusão
    lRetInc := .T.
    lRetBx := .T.
    aRecFK2 := {}
    aParams := {}
    lCtbOnline := .F.
    lExibeLanc := .F.

    If Len(aCTeCtbFin) > 1
        lCtbOnline := aCTeCtbFin[1]
        lExibeLanc := aCTeCtbFin[2]
    EndIf

    //Efetiva exclusão do(s) título(s) a pagar (tipo NDF)
    For nX := 1 to Len(aTitulos)
        aParams := {aClone(aTitulos[nX]), Nil, nOper, Nil, Nil, lExibeLanc, lCtbOnline }
        lRetInc := CTeExAuto("FINA050", aParams)
    Next nX

    //Efetiva exclusão da(s) baixa(s) dos títulos substituídos 
    For nY := 1 to Len(aMoviBx)
        nOpBaixa := 1
        aBaixaSE5 := {}
        lProcessa := .F.
        cPrefixo  := aMoviBx[nY][1][2]
        cNumTit	  := aMoviBx[nY][2][2]
        cParcTit  := aMoviBx[nY][3][2]
        cTipoTit  := aMoviBx[nY][4][2]
        cFornece  := aMoviBx[nY][5][2]
        cLoja	  := aMoviBx[nY][6][2]
        cChvSE2 := FWxFilial("SE2")+"|"+cPrefixo+"|"+cNumTit+"|"+cParcTit+"|"+cTipoTit+"|"+cFornece+"|"+cLoja
        cIdDoc  := FINBuscaFK7(cChvSE2, "SE2")
        cFiltro := "FK2_ORIGEM = 'MATA103' AND FK2_MOTBX = '"+cMotBaixa+"' "

        //Verifica qual a sequencia (FK2_SEQ) da baixa que deve ser cancelada
        aRecFK2 := GetPosts(cFilAnt, cIdDoc, cFiltro, .T.)
        If !Empty(aRecFK2)
            FK2->(dbGoTo(aRecFK2[1]))
            lProcessa := .T.

            SE2->(dbSetOrder(1)) //E2_FILIAL+E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO+E2_FORNECE+E2_LOJA
            SE2->(dbSeek(FWxFilial("SE2")+cPrefixo+cNumTit+cParcTit+cTipoTit+cFornece+cLoja))	
            
            //Carrega array aBaixaSE5 com as baixas do título
            Sel080Baixa("VL /BA /CP /",cPrefixo,cNumTit,cParcTit,cTipoTit,0,.F.,cFornece,cLoja,;
                .F.,.T.,.F.,0,.F.,.F.,.F.,0,SE2->E2_IDENTEE,.F.,SE2->E2_FILORIG)
            
            //Verifica qual é a posição da sequencia da baixa (FK2_SEQ) no "aBaixaSE5"
            If (nScan := aScan(aBaixaSE5,{|x|rTrim(x[9])==rTrim(FK2->FK2_SEQ)})) > 0
                nOpBaixa := nScan
            EndIf
        EndIf

        //Efetiva a exclusão da baixa dos títulos substituídos 
        If lProcessa

            //Cancela a baixa dos impostos vinculados ao CT-e substituído (gerados na emissão e/ou baixa)
            lRetBx := CTeBxImp(aMoviBx[nY], nOper, lBlind, cMotBaixa, aCTeCtbFin)
            
            If lRetBx
                aParams := {aClone(aMoviBx[nY]), nOper, .F., nOpBaixa, lExibeLanc, lCtbOnline, {}}
                lRetBx := CTeExAuto("FINA080", aParams)
            EndIf
        EndIF
    Next nY

    lRet := lRetInc .And. lRetBx

    FwFreeArray(aBaixaSE5)    
    FwFreeArray(aRecFK2)
    FwFreeArray(aParams)

Return lRet

/*/{Protheus.doc} CTeBxImp
    Realiza a MsExecAuto do FINA080 para baixar ou excluir a baixa
    dos impostos da	operação de substituição de CT-e 

    @author Fabio Casagrande Lima
    @since  21/10/2024

    @param aTitPai, Array, Dados do título pai dos impostos a serem baixados
    @param nOper, Numeric, Identif. da operação (3-Inclusão/5-Exclusão)
    @param lBlind, Logical, Verdadeiro se o processamento não tem interface
    @param cMotBaixa, Char, Motivo de baixa utilizado no processo (MV_MTBXCTE)
    @param aCTeCtbFin, Array, Dados da contabilização online:
        [1] Verdadeiro se a contabilização será online
        [2] Verdadeiro se exibirá a tela dos lançamentos contábeis

    @return lRetBx, Logical, .T. se se não houve erro(s) na MsExecAuto
/*/
Static Function CTeBxImp(aTitPai As Array, nOper As Numeric, lBlind As Logical, cMotBaixa As Character, aCTeCtbFin As Array) As Logical

    Local lCtbOnline As Logical
    Local lExibeLanc As Logical
    Local lRetBx As Logical
    Local aFina080 As Array
    Local aMoviImp As Array
    Local aParams As Array
    Local cTipoImp As Char
    Local nRecnoNF As Numeric
    Local nY As Numeric
    Local cChavePai As Character
    Local lFound As Logical
    Local jTitFilho As Json
    Local nLiObFilho As Numeric
    Local bWhile As Codeblock
    Local oBillRel As Object
    Local nIndice As Numeric

    Default aTitPai := {}
    Default nOper := 3
    Default lBlind := .F.
    Default aCTeCtbFin := {}
    Default cMotBaixa := AllTrim(SuperGetMV("MV_MTBXCTE", .F., "DAC"))

    lCtbOnline := .F.
    lExibeLanc := .F.
    lRetBx     := .T.
    cTipoImp   := MVTAXA+"|"+MVINSS+"|"+MVISS+"|"+MVTXA+"|"+"SES|CID|INP|IRF|PIS|COF|CSL"
    aMoviImp   := {}
    aParams    := {}
    lFound     := .F.
    jTitFilho  := JsonObject():New()
	nLiObFilho := 0
	bWhile     := {|| .F. }
    oBillRel   := Nil
    nIndice    := 17

    If __lUsaBill
        oBillRel := UsaBillRel()
    EndIf

    If Len(aCTeCtbFin) > 1
        lCtbOnline := aCTeCtbFin[1]
        lExibeLanc := aCTeCtbFin[2]
    EndIf

    //Guarda R_E_C_N_O_ da NF
    nRecnoNF := SE2->(Recno())

    If Len(aTitPai) >= 6
        cChavePai := FWxFilial("SE2")+aTitPai[1][2]+aTitPai[2][2]+aTitPai[3][2]+aTitPai[4][2]+aTitPai[5][2]+aTitPai[6][2]

        If oBillRel <> Nil
            nIndice := 1 // Indice 1 - E2_FILIAL+E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO+E2_FORNECE+E2_LOJA
        EndIf

        SE2->(DBSetOrder(nIndice))
        If (lFound := SE2->(DBSeek(cChavePai)))
            bWhile := {|| SE2->(!Eof()) .And. AllTrim(SE2->(E2_FILIAL+E2_TITPAI)) == AllTrim(cChavePai) }

            If oBillRel <> Nil
                jTitFilho  := oBillRel:getRelatedBills('SE2', .F.)
                nLiObFilho := 1
                bWhile     := {|| nLiObFilho <= Len(jTitFilho['document']) }
                lFound     := Len(jTitFilho['document']) > 0
            EndIf
        EndIf

        If lFound
            While Eval(bWhile)
                If oBillRel <> Nil
                    SE2->(DBSeek(FWxFilial("SE2") + jTitFilho['document'][nLiObFilho]['FK7_PREFIX'] +;
                                                    jTitFilho['document'][nLiObFilho]['FK7_NUM'] +;
                                                    jTitFilho['document'][nLiObFilho]['FK7_PARCEL'] +;
                                                    jTitFilho['document'][nLiObFilho]['FK7_TIPO'] +;
                                                    jTitFilho['document'][nLiObFilho]['FK7_CLIFOR'] +;
                                                    jTitFilho['document'][nLiObFilho]['FK7_LOJA']))
                EndIf
                //Valida se efetivará a baixa/cancelamento
                If SE2->E2_TIPO $ cTipoImp .And. ((nOper == 3 .And. SE2->E2_SALDO > 0) .Or. (nOper == 5 .And. SE2->E2_SALDO == 0))

                    //Valida se o imposto não está vinculado a uma baixa que não é do processo de substituição de CT-e.
                    If Empty(SE2->E2_SEQBX) .Or. (!Empty(SE2->E2_SEQBX) .And. SE2->E2_SEQBX == SeqBxFKs("FK2", cChavePai))

                        aFina080   := {}
                        Aadd(aFina080, {"E2_PREFIXO", SE2->E2_PREFIXO, NIL})
                        Aadd(aFina080, {"E2_NUM"    , SE2->E2_NUM    , NIL})
                        Aadd(aFina080, {"E2_PARCELA", SE2->E2_PARCELA, NIL})
                        Aadd(aFina080, {"E2_TIPO"   , SE2->E2_TIPO   , NIL})
                        Aadd(aFina080, {"E2_FORNECE", SE2->E2_FORNECE, NIL})
                        Aadd(aFina080, {"E2_LOJA"   , SE2->E2_LOJA   , NIL})
                        Aadd(aFina080, {"AUTMOTBX"  , cMotBaixa      , NIL})
                        Aadd(aFina080, {"AUTHIST"   , STR0091        , NIL}) //"Baixa referente CT-e substituído."
                        AAdd(aMoviImp, aClone(aFina080) )

                    EndIf
                EndIf

                nLiObFilho++

                SE2->(dbSkip())
            EndDo

            //Efetiva baixa dos títulos de impostos substituídos 
            For nY := 1 to Len(aMoviImp)
                aParams := {aClone(aMoviImp[nY]), nOper, .F., Nil, lExibeLanc, lCtbOnline, {}}
                lRetBx := CTeExAuto("FINA080", aParams)            
            Next nY

        EndIf
    EndIf

    //Restaura R_E_C_N_O_ da NF  
    SE2->(DbGoto(nRecnoNF))
    
    FwFreeArray(aFina080)
    FwFreeArray(aMoviImp)
    FwFreeArray(aParams)

    If oBillRel <> Nil
		FWFreeObj(oBillRel)
	EndIf

Return lRetBx

/*/{Protheus.doc} CTeExAuto
    Componente centralizador das operações envondo as 
    gravações via MsExecAuto das operações de substituição de CT-e.

    @author Fabio Casagrande Lima
    @since  10/12/2024

    @param cRotina, Char, Rotina a ser executada a MsExecAuto
    @param aParams, Array, Matriz contendo os parametros a serem passados p/ a MsExecAuto

    @return lRet, Logical, .T. se se não houve erro(s) na MsExecAuto
/*/
Static Function CTeExAuto(cRotina, aParams)

    Local lRet As Logical
    Local nTamPar As Numeric

    Private lMsErroAuto As Logical

    Default cRotina := ""
    Default aParams := {}

    lMsErroAuto := .F.
    lRet := .T.  
    nTamPar := Len(aParams)

    If cRotina == "FINA050" .And. nTamPar >= 7
        MsExecAuto({|a,b,c,d,e,f,g|FINA050(a,b,c,d,e,f,g)},aParams[1],aParams[2],aParams[3],aParams[4],aParams[5],aParams[6], aParams[7])
    ElseIf cRotina == "FINA080" .And. nTamPar >= 7
        MsExecAuto({|a,b,c,d,e,f,g|FINA080(a,b,c,d,e,f,g)},aParams[1],aParams[2],aParams[3],aParams[4],aParams[5],aParams[6], aParams[7])
    EndIf

    If lMsErroAuto
        lRet := .F.
        MostraErro()
    EndIf

Return lRet

