/*/{Protheus.doc}GetParcINP
Funcao utilizada para recuperar a parcela do INSS Patronal gerado	³±±
@type function
@return cParcINP - Numero da parcela do INSS Patronal

@author Rodrigo Oliveira
@since  10/08/2016
@obs Utilizado pelo módulo de Materiais e PLS
/*/
//-----------------------------------------------------------------------------------------------------
Function GetParcINP(cParcINP As Character, nValINSSPatr As Numeric, cIDDocPai As Character) As Logical

Local cQry As Character
Local cTitPai As Character
Local cChaveSE2 As Character
Local oQrySE2 As Object
Local nParam As Numeric
Local oBillRel As Object

Default cIDDocPai := ""

cQry      := " "
cTitPai   := SE2->(E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO+E2_FORNECE+E2_LOJA)
cChaveSE2 := ""
oQrySE2   := Nil
nParam    := 1
oBillRel  := Nil

If Empty(cIDDocPai)
    If __lUsaBill
        oBillRel := UsaBillRel()
    EndIf
EndIf

If oBillRel <> Nil
    cChaveSE2 := SE2->E2_FILIAL + "|" + SE2->E2_PREFIXO + "|" + SE2->E2_NUM + "|" + SE2->E2_PARCELA + "|" + SE2->E2_TIPO + "|" + SE2->E2_FORNECE + "|" + SE2->E2_LOJA
    cTitPai   := FinBuscaFK7(cChaveSE2, "SE2")
ElseIf !Empty(cIDDocPai)
    cTitPai := cIDDocPai
EndIf

cQry := " SELECT E2_PARCELA, E2_VALOR "
cQry += " FROM " + RetSqlName("SE2") + " SE2 "
If oBillRel <> Nil .Or. !Empty(cIDDocPai)
    cQry += "INNER JOIN " + RetSQLName("FK7") + " FK7 ON "
    cQry += "FK7.FK7_ALIAS = ? AND " // Amarração baseada no indice 4 - FK7_ALIAS+FK7_FILTIT+FK7_PREFIX+FK7_NUM+FK7_PARCEL+FK7_TIPO+FK7_CLIFOR+FK7_LOJA
    cQry += "FK7.FK7_FILTIT = SE2.E2_FILIAL AND "
    cQry += "FK7.FK7_PREFIX = SE2.E2_PREFIXO AND "
    cQry += "FK7.FK7_NUM = SE2.E2_NUM AND "
    cQry += "FK7.FK7_PARCEL = SE2.E2_PARCELA AND "
    cQry += "FK7.FK7_TIPO = SE2.E2_TIPO AND "
    cQry += "FK7.FK7_CLIFOR = SE2.E2_FORNECE AND "
    cQry += "FK7.FK7_LOJA = SE2.E2_LOJA AND "
    cQry += "FK7.D_E_L_E_T_ = SE2.D_E_L_E_T_ "
EndIf
cQry += " WHERE SE2.E2_FILIAL = ? "
cQry += " AND SE2.E2_TIPO = ? "
cQry += IIf(oBillRel == Nil .And. Empty(cIDDocPai), " AND SE2.E2_TITPAI = ? ", " AND FK7.FK7_IDPAI = ? ")
cQry += " AND SE2.D_E_L_E_T_ = ? "

cQry := ChangeQuery(cQry)

oQrySE2 := FwExecStatement():New(cQry)

If oBillRel <> Nil .Or. !Empty(cIDDocPai)
    oQrySE2:SetString(nParam++, "SE2")
EndIf

oQrySE2:SetString(nParam++, FWxFilial("SE2"))
oQrySE2:SetString(nParam++, 'INP')
oQrySE2:SetString(nParam++, cTitPai)
oQrySE2:SetString(nParam++, " ")

oQrySE2:OpenAlias("TMPINP")

If !("TMPINP")->(Eof())
    cParcINP 		:= TMPINP->E2_PARCELA
    nValINSSPatr	:= TMPINP->E2_VALOR
EndIf

TMPINP->(DbCloseArea())

oQrySE2:Destroy()
FWFreeObj(oQrySE2)

If oBillRel <> Nil
    FWFreeObj(oBillRel)
EndIf

Return .T.

