                                EndIf

                                If lUsaFlag .AND. lLctPad // Armazena em aFlagCTB para atualizar no modulo Contabil
                                    aAdd( aFlagCTB, {"FK1_LA", "S", "FK1", FK1->( Recno() ), 0, 0, 0} )
                                    aAdd( aFlagCTB, {"E5_LA", "S", "SE5", SE5->( Recno() ), 0, 0, 0} )
                                EndIf

