/*/{Protheus.doc}³ FilTitCmpR
    Retorna a filial do título da compensacao do titulo.
    Obs: Substitui a função FaPesqBx2, convertendo-a da SE5 para a FK1.

    @author fabio.casagrande
    @since  17/04/2025

	@param cFilComp, Character, Filial da compensação (FK1_FILIAL)
	@param cIdDoc, Character, ID do título compensado (FK1_IDDOC)
	@param dDataE, Date, Data da compensação (FK1_DATA)
	@param cSeq, Character, Sequencia de baixa (FK1_SEQ)  
	@param cTipoDoc, Character, Tipo de document o ser considerado (FK1_TPDOC)   

	@return cFilOrig, Character, Filial de origem do título a receber (FK1_FILORI)
/*/
//------------------------------
Static Function FilTitCmpR(cFilComp As Char,cIdDoc As Char,dDataE As Date,cSeq As Char,cTipoDoc As Char) As Char

    Local aArea As Array
    Local cQryAux As Character
    Local cFilOrig As Character
    Local cAlsTmp As Character
    Local nParam As Numeric

    Default cFilComp := cFilAnt
    Default cIdDoc := ""
    Default dDataE := ddatabase
    Default cSeq := ""
    Default cTipoDoc  := "CP"

    aArea := FwGetArea()
    cQryAux := ""
    cFilOrig := ''
    cAlsTmp := ''
    nParam := 1

    If __oQryFilT == Nil 
        cQryAux	:= "SELECT FK1.FK1_FILORI, FK1.FK1_FILIAL "
        cQryAux += "FROM "+RetSqlName("FK1")+" FK1 "
        cQryAux += "LEFT JOIN "+RetSqlName("FK1")+" FK1EST ON "
        cQryAux += "    FK1EST.FK1_FILIAL = FK1.FK1_FILIAL "
        cQryAux += "    AND FK1EST.FK1_IDDOC = FK1.FK1_IDDOC " 
        cQryAux += "    AND FK1EST.FK1_SEQ = FK1.FK1_SEQ "
        cQryAux += "    AND FK1EST.FK1_TPDOC = ? " //#1
        cQryAux += "    AND FK1EST.D_E_L_E_T_ = ? " //#2
        cQryAux += "WHERE FK1.FK1_FILIAL = ? " //#3
        cQryAux += "AND FK1.FK1_IDDOC = ? " //#4
        cQryAux += "AND FK1.FK1_SEQ = ? " //#5
        cQryAux += "AND FK1.FK1_RECPAG = ? " //#6
        cQryAux += "AND FK1.FK1_DATA = ? " //#7
        cQryAux += "AND FK1.FK1_MOTBX = ? " //#8
        cQryAux += "AND FK1.FK1_TPDOC = ? " //#9
        cQryAux += "AND FK1.D_E_L_E_T_  = ? " //#10
        cQryAux += "AND FK1EST.FK1_SEQ IS NULL "
        cQryAux := ChangeQuery(cQryAux)
        __oQryFilT := FwExecStatement():New(cQryAux)
    EndIf

    __oQryFilT:SetString(nParam++, 'ES') //#1
    __oQryFilT:SetString(nParam++, ' ') //#2
    __oQryFilT:SetString(nParam++, cFilComp) //#3
    __oQryFilT:SetString(nParam++, cIdDoc) //#4
    __oQryFilT:SetString(nParam++, cSeq) //#5
    __oQryFilT:SetString(nParam++, "R") //#6
    __oQryFilT:SetString(nParam++, dTos(dDataE)) //#7
    __oQryFilT:SetString(nParam++, 'CMP') //#8
    __oQryFilT:SetString(nParam++, cTipoDoc) //#9
    __oQryFilT:SetString(nParam++, ' ') //#10

    cAlsTmp := __oQryFilT:OpenAlias()

    If (cAlsTmp)->(!EOF())
        cFilOrig := (cAlsTmp)->FK1_FILIAL
        If !(Empty( (cAlsTmp)->FK1_FILORIG) .And. cPaisLoc == "MEX")
            cFilOrig := (cAlsTmp)->FK1_FILORIG
        EndIf
    EndIf

    (cAlsTmp)->(DbCloseArea())
    FWRestArea(aArea)
    FwFreeArray(aArea)

Return(cFilOrig)
