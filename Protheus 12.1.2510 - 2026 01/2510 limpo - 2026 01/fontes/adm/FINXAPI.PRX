                            aadd(aDelCmp,{FK1->FK1_DOC,If(lQuery,(cAliasSE5)->SE5RECNO,(cAliasSE5)->(RecNo())),FK1->FK1_SEQ,.T.,FK1->FK1_IDCOMP})
