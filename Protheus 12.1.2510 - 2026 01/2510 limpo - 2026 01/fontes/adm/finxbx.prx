/*/{Protheus.doc} BxValAce
Retorna array com os valores acessórios (FK6) baixados neste movimento para fazer estorno
@type function
@version 12
@author prime
@since 09/02/2023
@param cIdOri, character, E5_IDORIG
@return array, array contendo os valores acessórios
/*/
Function BxValAce(cIdOri As Character)

	Local aArea     As Array
	Local aValAcess As Array
	Local cTmp      As Character
	Local cQuery    As Character

	Default cIdOri := ""

	aArea          := GetArea()
	aValAcess      := {}
	cTmp           := ""
	cQuery         := ""

	If __oPrepSE5 == Nil
		cQuery := "SELECT FK6_CODVAL "
		cQuery += "FROM " + RetSqlName("FK6") + " FK6 "
		cQuery += "WHERE "
		cQuery += "FK6.FK6_IDORIG = ? "
		cQuery += "AND FK6.D_E_L_E_T_ = ' '"

		__oPrepSE5 := FWPreparedStatement():New(cQuery)
	EndIf

	__oPrepSE5:SetString(1, cIdOri)

	cQuery := __oPrepSE5:GetFixQuery()
	cTmp := MpSysOpenQuery(cQuery)

	dbSelectArea(cTmp)

	While !(cTmp)->(EoF())
		Aadd(aValAcess, (cTmp)->FK6_CODVAL)
		(cTmp)->(dbSkip())
	EndDo

	(cTmp)->(dbCloseArea())
	RestArea(aArea)

Return(aValAcess)

/*/{Protheus.doc} BaixaSLD0
	Ajusta  o valor recebido e/ ou valor líquido,
	valor do desconto e valor estrangeiro quando o 
	total do abatimento de imposto (IRRF) é maior
	que o valor do título para permitir efetivar a
	baixa do título
	
	@author Sivaldo Oliveira
	@since 05/06/2024
	
	@Param lSldInfAba, Logical, Indica que é uma baixa com valor de retenção (abatimentos de impostos) maior que o saldo do título
	@Param nVlrReceb, Numeric, Valor recebido ou líquido de recebimento
	@Param nAbatimImp, Numeric, Valor total de abatimento de imposto
	@Param nDesconto, Numeric, Valor do desconto concedido
	@Param nVlrEstrang, Numeric, Valor líquido de recebimento em outras moedas
	@Param nRecnoSE1, Numeric, Recno do cadastro do título
	@Param lVlrPadrao, Logical, Define se vai atualizar o valor padrão
/*/
Function BaixaSLD0(lSldInfAba As Logical, nVlrReceb As Numeric, nAbatimImp As Numeric, nDesconto As Numeric, nVlrEstrang As Numeric, nRecnoSE1 As Numeric, lVlrPadrao As Logical)
	Local cChaveSED  As Character
	Local cChaveSA1  As Character
	Local aAreaAtual As Array
	Local aAreaSED   As Array
	Local aAreaSA1   As Array
	Local aAreaSE1   As Array
	
	//Parâmetros de entrada
	Default lSldInfAba  := .F.
	Default nVlrReceb   := 0
	Default nAbatimImp  := 0
	Default nDesconto   := 0
	Default nVlrEstrang := 0
	Default nRecnoSE1   := 0
	Default lVlrPadrao  := .F.
	
	If cPaisLoc == "BRA" .And. nRecnoSE1 > 0 .And. nAbatimImp != 0
		//Inicializa variávels
		aAreaAtual := GetArea()
		aAreaSE1   := SE1->(GetArea())		
		
		SE1->(DbGoto(nRecnoSE1))	
		
		If nAbatimImp > SE1->E1_VALOR .And. (SE1->(E1_PIS+E1_COFINS+E1_CSLL+E1_ISS+E1_INSS) == 0) .And. SE1->E1_IRRF > 0 .And. SE1->E1_VRETIRF > 0 .And. "MATA" $ Upper(AllTrim(SE1->E1_ORIGEM))
			aAreaSED   := SED->(GetArea())
			aAreaSA1   := SA1->(GetArea())
			cChaveSED  := (FwXFilial("SED", SE1->E1_FILORIG) + SE1->E1_NATUREZ)
			cChaveSA1  := (FwXFilial("SA1", SE1->E1_FILORIG) + SE1->(E1_CLIENTE+E1_LOJA))			
			
			SED->(DbSetOrder(1))
			SA1->(DbSetOrder(1))
			
			lSldInfAba := (SED->(MsSeek(cChaveSED)) .And. SED->ED_CALCIRF == "S" .And. SA1->(MsSeek(cChaveSA1)) .And. SA1->A1_RECIRRF == "1" .And. SA1->A1_IRBAX == "2")
			
			If lSldInfAba .And. !lVlrPadrao
				If SE1->E1_SALDO > 0
					nVlrReceb   := IIf(nVlrReceb < 0, 0, nVlrReceb)
					nVlrEstrang := IIf(nVlrEstrang < 0, 0, nVlrEstrang)
					nDesconto   := IIf(nDesconto < 0, 0, nDesconto)
				EndIf
				
				nVlrReceb := IIf(SE1->E1_SALDO == 0 .And. nVlrReceb < 0, 0, nVlrReceb)
			EndIf
			
			RestArea(aAreaSED)
			RestArea(aAreaSA1)
			FwFreeArray(aAreaSED)
			FwFreeArray(aAreaSA1)
		EndIf
		
		RestArea(aAreaSE1)
		RestArea(aAreaAtual)
		FwFreeArray(aAreaSE1)
		FwFreeArray(aAreaAtual)
	EndIf		
Return Nil

/*/{Protheus.doc} RetencImp
	Retorna o um vetor bidimensional com o somatório da base e impostos retidos por título.
		
	@author Sivaldo Oliveira
	@since 10/01/2025
	
	@param cIdDocFK7,  Char, Identificador do registro de inclusão do título
	@param oRetencImp, Object, Objeto com a consulta preparada para listar todas
	baixa com onde houve retenção de impostos
	@return aRetorno, Array, Vetor bidimensional com os valores de base e impostos, PCC, e IRRF
	aRetorno[1,1] = Somatório da base dos impostos Pis, Cofins e Csll
	aRetorno[1,2] = Somatório do valor retido de Pis
	aRetorno[1,3] = Somatório do valor retido de Cofins
	aRetorno[1,4] = Somatório do valor retido de Csll
	aRetorno[2,1] = Somatório da base do imposto IRRF
	aRetorno[2,2] = Somatório do valor retido de IRRF
/*/
Function RetencImp(cIdDocFK7 As Char, oRetencImp As Object, cCart as Character) As Array
	Local lSomaBase   As Logical
	Local cQuery      As Character	
	Local cTblFK4     As Character
	Local cTblFK      As Character
	Local cTblTmp     As Character
	Local cDelete     As Character
	Local cImposto    As Character
	Local cChaveJson  As Character
	Local cCartOpost  As Character
	Local nArray      As Numeric
	Local aRetorno    As Array
	Local oJsIDFK     As JSon
	
	//Parâmetros de entrada
	Default cIdDocFK7  := ""
	Default oRetencImp := Nil
	Default cCart	   := 'R'
	
	//Inicializa variáveis.
	aRetorno := {{0, 0, 0, 0}, {0, 0}}
	
	If !Empty(cIdDocFK7)
		lSomaBase   := .T.
		cQuery      := ""		
		cTblFK4     := RetSqlName("FK4")
		cTblFK      := RetSqlName("FK1")
		cCartOpost	:= 'P'
		if cCart == 'P'
			cTblFK      := RetSqlName("FK2")
			cCartOpost	:= 'R'
		endIf
		cTblTmp     := ""
		cDelete     := " "
		cImposto    := ""
		cChaveJson  := ""
		nArray      := 0
		oJsIDFK    := Nil
		__lCachQry  := IIf(__lCachQry == Nil, FwLibVersion() >= "20211116", __lCachQry)
		

		If __oGetRImp == Nil
			__oGetRImp := JsonObject():New()
		EndIf

		If oRetencImp == Nil .Or. !(__oGetRImp:HasProperty(cCart))
			//Remove a propriedade anterior do JSON e insere a atual
			If Len(__oGetRImp:GetNames()) > 0
				__oGetRImp:DelName(__oGetRImp:GetNames()[1])
			EndIf
			__oGetRImp[cCart] := .T.

			cQuery := "SELECT FK1.FK1_IDFK1,"	
			if cCart == 'P'
				cQuery := "SELECT FK2.FK2_IDFK2,"
			endIf
			cQuery	+= " FK4.FK4_IMPOS Imposto, ISNULL(FK4.FK4_BASIMP, 0) Base, ISNULL(FK4.FK4_VALOR, 0) Valor "
			cQuery += "FROM ? "
			if cCart == 'R'
				cQuery += " FK1 JOIN ? FK4 " 		 
				cQuery += "ON (FK1.FK1_FILIAL = FK4.FK4_FILIAL AND FK1.FK1_IDFK1 = FK4.FK4_IDORIG "
				cQuery += "AND FK1.D_E_L_E_T_ = FK4.D_E_L_E_T_) "
				cQuery += "WHERE FK1.FK1_IDDOC = ? AND FK1.D_E_L_E_T_ = ? AND FK1.FK1_RECPAG = ? "
				cQuery += "AND NOT EXISTS ( "
				cQuery += "SELECT FK1ES.FK1_SEQ FROM ? FK1ES INNER JOIN ? FK4ES "
				cQuery += "ON (FK1ES.FK1_FILIAL = FK4ES.FK4_FILIAL AND FK1ES.FK1_IDFK1 = FK4ES.FK4_IDORIG " 
				cQuery += "AND FK1ES.D_E_L_E_T_ = FK4ES.D_E_L_E_T_ AND FK1ES.FK1_SEQ = FK1.FK1_SEQ ) " 
				cQuery += "WHERE FK1ES.FK1_IDDOC = ? AND FK1ES.D_E_L_E_T_ = ? AND FK1ES.FK1_RECPAG = ? ) "
			else
				cQuery += " FK2 JOIN ? FK4 "
				cQuery += "ON (FK2.FK2_FILIAL = FK4.FK4_FILIAL AND FK2.FK2_IDFK2 = FK4.FK4_IDORIG "
				cQuery += "AND FK2.D_E_L_E_T_ = FK4.D_E_L_E_T_) "
				cQuery += "WHERE FK2.FK2_IDDOC = ? AND FK2.D_E_L_E_T_ = ? AND FK2.FK2_RECPAG = ? "
				cQuery += "AND NOT EXISTS ( "
				cQuery += "SELECT FK2ES.FK2_SEQ FROM ? FK2ES INNER JOIN ? FK4ES "
				cQuery += "ON (FK2ES.FK2_FILIAL = FK4ES.FK4_FILIAL AND FK2ES.FK2_IDFK2 = FK4ES.FK4_IDORIG " 
				cQuery += "AND FK2ES.D_E_L_E_T_ = FK4ES.D_E_L_E_T_ AND FK2ES.FK2_SEQ = FK2.FK2_SEQ ) " 
				cQuery += "WHERE FK2ES.FK2_IDDOC = ? AND FK2ES.D_E_L_E_T_ = ? AND FK2ES.FK2_RECPAG = ? ) "
			endIf
			cQuery     := ChangeQuery(cQuery)
			oRetencImp := IIf(__lCachQry, FwExecStatement():New(cQuery), FWPreparedStatement():New(cQuery))
		EndIf
		
		oRetencImp:SetUnsafe(1, cTblFK)
		oRetencImp:SetUnsafe(2, cTblFK4)
		oRetencImp:SetString(3, cIdDocFK7)
		oRetencImp:SetString(4, cDelete)
		oRetencImp:SetString(5, cCart)
		oRetencImp:SetUnsafe(6, cTblFK)
		oRetencImp:SetUnsafe(7, cTblFK4)
		oRetencImp:SetString(8, cIdDocFK7)
		oRetencImp:SetString(9, cDelete)
		oRetencImp:SetString(10, cCartOpost)
		
		cTblTmp := IIf(__lCachQry, oRetencImp:OpenAlias(), MpSysOpenQuery(oRetencImp:GetFixQuery()))
		
		If !(cTblTmp)->(Eof())
			oJsIDFK := JsonObject():New()
			
			While !(cTblTmp)->(Eof())
				cImposto := Upper(AllTrim((cTblTmp)->Imposto))			
				
				If cImposto $ "PIS|COF|CSL|IRF"
					nArray := 1
					
					If cImposto == "IRF"
						nArray := 2
					EndIf
					
					if cCart == 'R'
						cChaveJson := (cTblTmp)->FK1_IDFK1 + cValToChar(nArray)
					else
						cChaveJson := (cTblTmp)->FK2_IDFK2 + cValToChar(nArray) 
					endIf
					
					If (lSomaBase := !oJsIDFK:HasProperty(cChaveJson))
						oJsIDFK[cChaveJson] := .T.
					EndIf
					
					If cImposto $ "PIS|COF|CSL"						
						If cImposto == "PIS"
							aRetorno[1,2] += (cTblTmp)->Valor
						ElseIf cImposto == "COF"
							aRetorno[1,3] += (cTblTmp)->Valor
						Else
							aRetorno[1,4] += (cTblTmp)->Valor
						EndIf
					Else
						aRetorno[2,2] += (cTblTmp)->Valor
					EndIf
					
					If lSomaBase							
						aRetorno[nArray,1] += (cTblTmp)->Base
					EndIf				
				EndIf
				
				(cTblTmp)->(DbSkip())
			EndDo
			
			FreeObj(oJsIDFK)
		EndIf
		
		(cTblTmp)->(DbCloseArea())
	EndIf
Return aRetorno

