/*/{Protheus.doc} GetPosts
Retorna array com recnos das baixas de um titulo

@param cFilTit - Filial a ser pesquisada na FK2 (conforme o parametro 4-lFilOrig)
@param cIdDoc - Chave unica do titulo (ID da FK7)
@param cFiltro - Filtro para baixas especificas
@param lFilOrig - .T. = Filtra por FK2_FILORI / .F. = Filtra por FK2_FILIAL

@return aRecFk2 - Recno(s) do(s) registro(s) da FK2 localizado(s) na query

@author Igor Sousa do Nascimento
@since  05/01/2018
/*/
//-------------------------------------------------------------------
Function GetPosts(cFilTit As Char, cIdDoc As Char, cFiltro As Char, lFilOrig As Logical) As Array

	Local aArea As Array
	Local aRecFk2 As Array
	Local cTmp As Char
	Local cQuery As Char
	Local cChaveJson As Char
	Local cAddFilter As Char
	Local nIndex As Numeric

	Default cFilTit  := xFilial("FK2")
	Default cIdDoc	 := ""
	Default cFiltro  := ""
	Default lFilOrig := .F.

	aArea      := GetArea()
	aRecFk2    := {}
	cTmp	   := ""
	cQuery     := ""
	nIndex     := 0
	cChaveJson := AllTrim(cFiltro)+cValToChar(lFilOrig)
	__lCachQry := IIf(__lCachQry == Nil, FwLibVersion() >= "20211116", __lCachQry)

	If !Empty(cFiltro)
		cAddFilter := "AND " + cFiltro
	EndIf

	If __oGetPost == Nil
		__oGetPost := JsonObject():New()
	EndIf

	If !(__oGetPost:HasProperty(cChaveJson))

		//Remove a propriedade anterior do JSON e insere a atual
		If Len(__oGetPost:GetNames()) > 0
			__oGetPost:DelName(__oGetPost:GetNames()[1])
		EndIf
		__oGetPost[cChaveJson] := .T.

		cQuery := "SELECT FK2.R_E_C_N_O_ RECNOBX "
		cQuery += "FROM " + RetSqlName("FK2") + " FK2 "
		If lFilOrig
			cQuery += "WHERE FK2.FK2_FILORI = ? " //#1
		Else
			cQuery += "WHERE FK2.FK2_FILIAL = ? " //#1			
		EndIf
		cQuery += 	"AND FK2.FK2_IDDOC = ? "      //#2
		cQuery += 	"AND FK2.FK2_RECPAG = ? "     //#3
		cQuery += 	"AND FK2.D_E_L_E_T_ = ? "     //#4
		If !Empty(cFiltro)
			cQuery += "? "                        //#5
		EndIf

		//Verifica se a baixa foi estornada
		cQuery 	+= " AND NOT EXISTS( "
		cQuery 	+= " 	SELECT FK2EST.FK2_IDDOC FROM " + RetSqlName("FK2") +" FK2EST"
		If lFilOrig
			cQuery += " WHERE FK2EST.FK2_FILORI = FK2.FK2_FILORI "
		Else
			cQuery += " WHERE FK2EST.FK2_FILIAL = FK2.FK2_FILIAL "			
		EndIf
		cQuery 	+= " 	AND FK2EST.FK2_IDDOC = FK2.FK2_IDDOC "
		cQuery 	+= " 	AND FK2EST.FK2_SEQ = FK2.FK2_SEQ "
		cQuery 	+= " 	AND FK2EST.FK2_TPDOC = ? "    //#6
		cQuery 	+= " 	AND FK2EST.D_E_L_E_T_ = ? ) " //#7 

		cQuery := ChangeQuery(cQuery)
		__oQryFK2 := IIf(__lCachQry, FwExecStatement():New(cQuery), FWPreparedStatement():New(cQuery))
	EndIf

	__oQryFK2:SetString(++nIndex, cFilTit) //#1
	__oQryFK2:SetString(++nIndex, cIdDoc)  //#2
	__oQryFK2:SetString(++nIndex, "P")     //#3
	__oQryFK2:SetString(++nIndex, " ")     //#4

	If !Empty(cFiltro)
		__oQryFK2:setUnsafe(++nIndex, cAddFilter)  //#5
	EndIf

	__oQryFK2:SetString(++nIndex, "ES")  //#6
	__oQryFK2:SetString(++nIndex, " ")   //#7

	cTmp := IIf(__lCachQry, __oQryFK2:OpenAlias(), MpSysOpenQuery(__oQryFK2:GetFixQuery()))
	
	While !(cTmp)->(EoF())
		Aadd(aRecFk2, (cTmp)->RECNOBX)
		(cTmp)->(dbSkip())
	EndDo
	(cTmp)->(dbCloseArea())

	RestArea(aArea)

Return aRecFk2

