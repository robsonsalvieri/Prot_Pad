/*/{Protheus.doc} FGrvFI2API
Função de gravação da tabela FI2 - para API de bancos
@example
	aFI2[x][1]: Ocorrencia
	aFI2[x][2]: Operação
	aFI2[x][3]: Valor anterior
	aFI2[x][4]: Novo valor
	aFI2[x][5]: Nome do campo
	aFI2[x][6]: Tipo do campo
	aFI2[x][7]: Carteira		'1' - Receber / '2' - Pagar
	aFI2[x][8]: FI2_HISTOR		jSon com EA_APIMSG (dados do boleto transmitido) para cancelamento de boleto
@author rafael.rondon
@since  27/01/2023
/*/
//-----------------------------------------------------------------------------------------------------
Function FGrvFI2API(aFI2 As Array)

	Local nX		As Numeric
	Local aArea 	As Array
	Local aAreaFI2 	As Array
	Local cNumBor 	As Character
	Local jCancel	As jSon
	Local jFI2_Hist	As jSon

	Default aFI2	:= {}

	nX			:=	1
	aAreaFI2 	:= FI2->(GetArea())
	aArea 		:= GetArea()

	If !Empty(SE1->E1_NUMBOR)
		cNumBor		:=  SE1->E1_NUMBOR
	Else
		cNumBor		:=  SEA->EA_NUMBOR
	EndIf

	For nX := 1 To Len(aFI2)

		// Para cancelamento, excluir as FI2 de alteração que não foram registradas (FI2_TRANSF <> 'S')
		If aFI2[nX][2] == 'C'
			DbSelectArea('FI2')
			FI2->(DbSetOrder(1)) // FI2_FILIAL+FI2_CARTEI+FI2_NUMBOR+FI2_PREFIX+FI2_TITULO+FI2_PARCEL+FI2_TIPO+FI2_CODCLI+FI2_LOJCLI+FI2_OCORR+FI2_GERADO
			If FI2->(DbSeek(xFilial('FI2',,SE1->E1_FILORIG) + aFI2[nX][7]	 + cNumBor + SE1->(E1_PREFIXO+E1_NUM+E1_PARCELA+E1_TIPO)))
				While 	xFilial('FI2',,SE1->E1_FILORIG) == FI2->FI2_FILIAL .AND. FI2->FI2_CARTEI == aFI2[nX][7]	 .AND. FI2->FI2_NUMBOR == cNumBor ;
						.AND. FI2->(FI2_PREFIX+FI2_TITULO+FI2_PARCEL+FI2_TIPO) == SE1->(E1_PREFIXO+E1_NUM+E1_PARCELA+E1_TIPO)

					If FI2->FI2_BORAPI == 'S' .AND. FI2->FI2_OPEAPI == 'A' .AND. FI2->FI2_TRANSF <> 'S'
						RecLock("FI2",.F.)
                        FI2->(DbDelete())
                        FI2->(MsUnlock())
					EndIf

					FI2->(DbSkip())

				EndDo
			EndIf

			// Adicionar no FI2_HISTOR dados do cancelamento para que não seja preciso nada da SEA/SE1	para transmissao/visualizacao de cancelamento
			jFI2_Hist := JsonObject():New()
			jFI2_Hist:FromJson(aFI2[nX][8])  

			jCancel := JsonObject():New()
			jCancel["ea_filial"] := SEA->EA_FILIAL
			jCancel["ea_filorig"] := SEA->EA_FILORIG
			jCancel["ea_portado"] := SEA->EA_PORTADO
			jCancel["ea_agedep"] := SEA->EA_AGEDEP
			jCancel["ea_subcta"] := SEA->EA_SUBCTA
			jCancel["ea_numcon"] := SEA->EA_NUMCON
			jCancel["e1_nomcli"] := SE1->E1_NOMCLI

			jFI2_Hist['dadosBorderoCancelamento'] := jCancel

			aFI2[nX][8] := jFI2_Hist:ToJson()

		EndIf

		RecLock('FI2',.T.)

			FI2->FI2_FILIAL 	:=  xFilial("FI2",SE1->E1_FILORIG)
			FI2->FI2_CARTEI 	:=  aFI2[nX][7]			// '1' - Receber / '2' - Pagar
			FI2->FI2_OCORR  	:=  aFI2[nX][1]
			FI2->FI2_GERADO 	:=  "2"
			FI2->FI2_NUMBOR		:=  cNumBor
			FI2->FI2_PREFIX	 	:=  SE1->E1_PREFIXO
			FI2->FI2_TITULO	 	:=  SE1->E1_NUM
			FI2->FI2_PARCEL	 	:=  SE1->E1_PARCELA
			FI2->FI2_TIPO  	 	:=  SE1->E1_TIPO
			FI2->FI2_CODCLI	 	:=  SE1->E1_CLIENTE
			FI2->FI2_LOJCLI	 	:=  SE1->E1_LOJA
			FI2->FI2_DTOCOR	 	:=  dDataBase
			FI2->FI2_VALANT	 	:=  aFI2[nX][3]
			FI2->FI2_VALNOV	 	:=  aFI2[nX][4]
			FI2->FI2_CAMPO 	 	:=  aFI2[nX][5]
			FI2->FI2_TIPCPO	 	:=  aFI2[nX][6]
			FI2->FI2_OPEAPI		:=	aFI2[nX][2] // 'A' Alteração ; 'C' Cancelamento
			FI2->FI2_BORAPI		:=	'S'
			If Len(aFI2[nX]) >= 8
				FI2->FI2_HISTOR		:=	aFI2[nX][8]
			EndIf

		FI2->(MsUnLock())

	Next nX

	FWRestArea(aAreaFI2)
	FWRestArea(aArea)

Return

//-----------------------------------------------------------------------------------------------------
/*/{Protheus.doc} FinGrvFI2
Função de gravação da tabela FI2 - Ocorrencias CNAB.
@example
	aItemsFI2[x][1]: Ocorrencia
	aItemsFI2[x][2]: Titulo do campo
	aItemsFI2[x][3]: Valor anterior
	aItemsFI2[x][4]: Novo valor
	aItemsFI2[x][5]: Nome do campo
	aItemsFI2[x][6]: Tipo do campo
@author Bruno Sobieski
@since  03/02/2005
/*/
//-----------------------------------------------------------------------------------------------------
Function FinGrvFI2()
Local nX		:=	1
Local aArea 	:= GetArea()
Local cChave 	:= ''
Local lF040GRCOM:= ExistBlock("F040GRCOM")

	lSubstFI2	:=	IIf(Type("lSubstFI2")=="L",lSubstFI2,.T.)

	FI2->(DbSetOrder(1)) // FI2_FILIAL+FI2_CARTEI+FI2_NUMBOR+FI2_PREFIX+FI2_TITULO+FI2_PARCEL+FI2_TIPO+FI2_CODCLI+FI2_LOJCLI+FI2_OCORR+FI2_GERADO
	If Type('aItemsFI2') == "A" .And. !Empty(aItemsFI2)

		For nX := 1 To Len(aItemsFI2)

			If !Empty(aItemsFI2[nX][1])

				cChave	:=	xFilial("FI2",SE1->E1_FILORIG) + "1" + SE1->(E1_NUMBOR + E1_PREFIXO + E1_NUM + E1_PARCELA + E1_TIPO +;
				 								   E1_CLIENTE + E1_LOJA) + aItemsFI2[nX][1] + "2"

				// Pesquisa pela ocorrencia nao gerada (FI2_GERADO = 2 - Ja esta na chave)
				If lSubstFI2 .And. FI2->(DbSeek(cChave))
					RecLock('FI2',.F.)
				Else
					RecLock('FI2',.T.)
				Endif
				Replace FI2_FILIAL 	WITH xFilial("FI2",SE1->E1_FILORIG)
				Replace FI2_CARTEI 	WITH "1"
				Replace FI2_OCORR  	WITH aItemsFI2[nX][1]
				Replace FI2_GERADO 	WITH "2"
				Replace FI2_NUMBOR 	WITH SE1->E1_NUMBOR
				Replace FI2_PREFIX	WITH SE1->E1_PREFIXO
				Replace FI2_TITULO	WITH SE1->E1_NUM
				Replace FI2_PARCEL	WITH SE1->E1_PARCELA
				Replace FI2_TIPO  	WITH SE1->E1_TIPO
				Replace FI2_CODCLI	WITH SE1->E1_CLIENTE
				Replace FI2_LOJCLI	WITH SE1->E1_LOJA
				Replace FI2_DTOCOR	WITH dDataBase
				Replace FI2_DESCOC 	WITH Posicione('SEB',1,xFilial('SEB',SE1->E1_FILORIG)+SE1->E1_PORTADO+Pad(FI2->FI2_OCORR,;
										 Len(SEB->EB_REFBAN))+"E","SEB->EB_DESCRI")
				Replace FI2_VALANT	WITH aItemsFI2[nX][3]
				Replace FI2_VALNOV	WITH aItemsFI2[nX][4]
				Replace FI2_CAMPO 	WITH aItemsFI2[nX][5]
				Replace FI2_TIPCPO	WITH aItemsFI2[nX][6]
				MsUnLock()

				IF lF040GRCOM
					ExecBlock( "F040GRCOM", .F., .F., {aItemsFI2} )
			    Endif

			Endif
		Next nX
	EndIf

	RestArea(aArea)

Return

