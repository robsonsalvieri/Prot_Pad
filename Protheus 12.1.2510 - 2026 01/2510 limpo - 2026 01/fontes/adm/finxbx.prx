									Endif
								Endif
							Next nX

							cCamposE5A := "{{'E5_PRETPIS' , '2'}"
							cCamposE5A += ",{'E5_PRETCOF' , '2'}"
							cCamposE5A += ",{'E5_PRETCSL' , '2'}}"

							oModelBxRA:SetValue("MASTER","E5_CAMPOS",cCamposE5A)
							//Grava os dados
