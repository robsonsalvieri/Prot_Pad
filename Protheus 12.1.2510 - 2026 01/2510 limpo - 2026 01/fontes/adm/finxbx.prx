/*/{Protheus.doc} FAtuFKDBx
Atualiza os valores de VA, informados na baixa, na tabela FKD
Acionada após a confirmação da baixa manual ou Lote.

@author pequim
@since 15/08/2016
@version undefined
@param lCancel
/*/
//--------------------------------------------------------------------------------------
Function FAtuFKDBx(lCancel As Logical, cCart As Character, cCodVa As Character, nVlVa As Numeric,;
        cIdFKD As Character)

    Local aArea      As Array
    Local aAreaFKC   As Array
    Local cChaveFK7  As Character
    Local cChaveFKD  As Character
    Local cChaveTit  As Character
    Local cFilOriTit As Character
    Local cModel     As Character
    Local dDtBaixa   As Date
    Local nRecSE1    As Numeric
    Local nRecSE2    As Numeric
    Local nTamFKD    As Numeric
    Local nX         As Numeric
    Local oModelVA   As Object
    Local oSubFKD    As Object
	Local aBxVlAcess As Array
	Local lF055Parc	 As Logical

    aArea      := GetArea()
    cChaveFK7  := ""
    cChaveFKD  := ""
    cChaveTit  := ""
    cFilOriTit := ""
    cModel     := ""
    dDtBaixa   := NIL
    nRecSE1    := SE1->(Recno())
    nRecSE2    := SE2->(Recno())
    nTamFKD    := 0
    nX         := 0
    oModelVA   := NIL
    oSubFKD    := NIL
	lF055Parc  := .F.

    Default lCancel := .F.
    Default cCart   := "R"
    Default cCodVa  := ""
    Default nVlVa   := 0
    Default cIdFKD  := ""

	If cCart == "R" .AND. Upper(AllTrim(SE1->E1_ORIGEM)) == "FINI055" .AND. SE1->E1_SALDO > 0
		lF055Parc := .T.
	EndIf

    If TableInDic('FKD')
        cOldVA := Iif(Type("cOldVA") == 'U', "" , cOldVA)
		nVA    := Iif(Type("nVA") == 'U', 0 , nVA)

        If lCancel
            dDTBaixa := CtoD("//")
        Else
            dDTBaixa := Iif(Type("dBaixa") == "U", dDataBase , dBaixa)
        EndIf

        If !Empty(cOldVA)
            cModel := If (cCart == 'R', "FINA070VA","FINA080VA")
            oModelVA := FWLoadModel(cModel)
            oModelVA:SetOperation( MODEL_OPERATION_UPDATE )
            oModelVA:Activate()
            oModelVA:LoadXMLData( cOldVA )

            //Atualiza Data da baixa
            oSubFKD := oModelVA:GetModel("FKDDETAIL")
            nTamFKD := oSubFKD:Length()

            For nX := 1 to nTamFKD
                oSubFKD:GoLine(nX)

				If oSubFKD:GetValue("FKD_VLINFO") <> 0 .OR. oSubFKD:GetValue("FKD_VLCALC") <> 0       //So grava data de baixa se existir valor calculado ou informado
                    oSubFKD:LoadValue("FKD_DTBAIX", dDTBaixa)
                EndIf

                If oSubFKD:GetValue("FKD_PERIOD") == "1" .AND. oSubFKD:GetValue("FKD_TPVAL") == "2" .And. Abs(oSubFKD:GetValue("FKD_SALDO")) + Abs(oSubFKD:GetValue("FKD_VLINFO")) <= Abs(oSubFKD:GetValue("FKD_VALOR"))		//Saldo só é controlado para FKC_TPVAL = 2 e FKC_PERIOD = 1
                    oSubFKD:LoadValue("FKD_SALDO", oSubFKD:GetValue("FKD_SALDO") + oSubFKD:GetValue("FKD_VLINFO")) //campo FKD_SALDO está armazenando o valor baixado.
                EndIf

				// Para titulos integracao TIN (FINI055), é possível baixar VA parcial e nao baixar algum VA. Se o título tiver saldo (E1_SALDO) deve deixar o VA ativo(sem FKD_DTBAIX) para que seja utilizado na proxima baixa. Se for para desabilitar o VA o RM deve mandar uma atualizacao do titulo pelo EAI FINI055 - FINANCING.
				If lF055Parc .AND. oSubFKD:GetValue("FKD_SALDO") == 0
					oSubFKD:LoadValue("FKD_DTBAIX", CtoD("//"))
				EndIf
            Next

            If oModelVA:VldData()
                FWFormCommit( oModelVA )
            EndIf
            oModelVA:Deactivate()
            oModelVA:Destroy()
            oModelVa := NIL
            cOldVA := ""
        Else
            If cCart == "R"
                cFilOriTit	:= xFilial("SE1",SE1->E1_FILORIG)
                cChaveTit := cFilOriTit + '|' + SE1->E1_PREFIXO + '|' + SE1->E1_NUM + '|' + SE1->E1_PARCELA + '|' + SE1->E1_TIPO + '|' + SE1->E1_CLIENTE + '|' + SE1->E1_LOJA
                cChaveFK7 := FINGRVFK7("SE1", cChaveTit, SE1->E1_FILORIG)
            Else
                cFilOriTit	:= xFilial("SE2",SE2->E2_FILORIG)
                cChaveTit := cFilOriTit + '|' + SE2->E2_PREFIXO + '|' + SE2->E2_NUM + '|' + SE2->E2_PARCELA + '|' + SE2->E2_TIPO + '|' + SE2->E2_FORNECE + '|' + SE2->E2_LOJA
                cChaveFK7 := FINGRVFK7("SE2", cChaveTit, SE2->E2_FILORIG)
            EndIf

            DbSelectArea("FKD")
            If !Empty(cIdFKD)
                FKD->( DbSetOrder(3) ) //FKD_FILIAL+FKD_IDFKD
                cChaveFKD := cFilOriTit+cIdFKD
            Else
                FKD->( DbSetOrder(2) ) // FKD_FILIAL+FKD_IDDOC+FKD_CODIGO
                cChaveFKD := cFilOriTit+cChaveFK7+cCodVa
            EndIf

            If !Empty(cCodVa) .Or. !Empty(cIdFKD)

                If FKD->(DbSeek(cChaveFKD))
                    If ABS(nVlVa) > ABS(FKD->FKD_SALDO)
                        nVlVa := FKD->FKD_SALDO  //Ajusta para que o saldo não fique negativo. Pode ocorrer quando o valor baixado é superior ao valor do VA.
                    EndIf
                    RecLock("FKD")
					FKD->FKD_DTBAIX := dDTBaixa
					//Saldo só é controlado para FKC_TPVAL = 2 e FKC_PERIOD = 1						
					If ABS(nVlVa) <> ABS(FKD->FKD_SALDO)
						FKD->FKD_SALDO -= nVlVa
					Else
						FKD->FKD_SALDO := 0 
					EndIf						
                    FKD->(MSUnlock())
                EndIf

            ElseIf FKD->(DbSeek(cFilOriTit+cChaveFK7))

				If lCancel
					aBxVlAcess := BxValAce(SE5->E5_IDORIG)
				EndIf

				While FKD->(!Eof()) .And. FKD->FKD_FILIAL == cFilOriTit .And. FKD->FKD_IDDOC == cChaveFK7
					//-- Gravo o valor calculado e o valor informado para que gravações da FK6					
					If (!lCancel .And. FKD->FKD_VALOR <> FKD->FKD_SALDO ) .OR. (lCancel .AND. aScan(aBxVlAcess , {|x| x= FKD->FKD_CODIGO }) > 0)
						RecLock("FKD")
						If !Empty(FKD_VLCALC)
							FKD->FKD_DTBAIX := dDTBaixa
						EndIf

						//-- Saldo só é controlado para FKC_TPVAL = 2 e FKC_PERIOD = 1
						aAreaFKC := FKC->(GetArea())
						IF POSICIONE("FKC",1,XFilial("FKC")+FKD->FKD_CODIGO,"FKC_TPVAL+FKC_PERIOD") == '21'
							IF lCancel
								FKD->FKD_SALDO := 0		
							ELSEIF FKD->FKD_VLINFO <> FKD->FKD_VLCALC						
								FKD->FKD_SALDO += FKD->FKD_VLINFO
							ELSEIF !EMPTY(FKD_VLCALC) .And. FKD->FKD_VALOR <> nVa
								FKD->FKD_SALDO += FKD->FKD_VLCALC
							ELSEIF !EMPTY(FKD_VLCALC)
								FKD->FKD_SALDO := FKD->FKD_VLCALC
							ENDIF
						ENDIF
						RestArea(aAreaFKC)

						FKD->(MSUnlock())
					EndIf
					FKD->(DbSkip())
				EndDo

			EndIf

			If cCart == "R"
				SE1->(dbGoTo(nRecSE1))
			Else
				SE2->(dbGoTo(nRecSE2))
			EndIf

		EndIf
	EndIf

	RestArea(aArea)

Return

