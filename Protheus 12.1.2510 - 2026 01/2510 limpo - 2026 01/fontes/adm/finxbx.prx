					cCamposE5 += ",{'E5_VRETCOF' , "+cValToChar(nVlRetCof)+"}"
					cCamposE5 += ",{'E5_VRETCSL' , "+cValToChar(nVlRetCsl)+"}"
					cCamposE5 += ",{'E5_PRETPIS' , '1'}"
					cCamposE5 += ",{'E5_PRETCOF' , '1'}"
					cCamposE5 += ",{'E5_PRETCSL' , '1'}"
