ElseIf i <> 0 //Gravo os valore acessorios ('D2', 'DC', 'J2', 'JR', 'M2', 'MT', 'C2', 'CM' ou 'TL')
	If __lFinm010
		cCamposE5 += "}"
		oModelBxR:SetValue( "MASTER", "E5_CAMPOS", cCamposE5 )
	Endif

