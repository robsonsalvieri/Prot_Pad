		If lRet

			If !lEstorno .AND.;
				((Alltrim(cTipoTran) == "CH" .AND. !IsCaixaLoja(cBcoOrig)) .OR. ((Alltrim(cTipoTran) == "TB") .AND. lGerChTB))
				nRegSEF := Fa100Cheq("FINA100TRF")
			Endif

			If lA100TR01
				ExecBlock("A100TR01",.F.,.F.,lEstorno)
