Static Function FA100Cpos(cModo)
Local aRet := {}
Local aStruct := SE5->(DbStruct())
Local aArea	 := GetArea()
Local cCampo := ""
Local lIncluiCpo := .T.
Local nX := 0

Default cModo := ""

For nX := 1 to Len(aStruct)
	cCampo := aStruct[nX,1]
	If cCampo == 'E5_TALAO'
		lIncluiCpo := cModo == "P"
	ElseIf cCampo $ 'E5_AGECHQ|E5_BCOCHQ|E5_CTACHQ|E5_POSTAL|E5_CGC'
		lIncluiCpo := cModo == "R"
	Else
		lIncluiCpo := .T.
	EndIf

	If lIncluiCpo
		AADD(aRet,cCampo)
	EndIf
Next nX
RestArea(aArea)

Return(aRet)


