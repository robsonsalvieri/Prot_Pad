Function Fa100Grv()
FJS->(dbSetOrder(1))
FJS->(dbSeek(xFilial("FJS")+M->E5_TIPO))

If cPaisLoc <> "BRA"
	If AllTrim(FJS->FJS_TERCEI) == "4"
		RecLock("SE5", .F.)
		Replace SE5->E5_TERCEIR	With "2"
		MsUnLock()
	Else
		RecLock("SE5", .F.)
		Replace SE5->E5_TERCEIR	With "1"
		MsUnLock()
	EndIf
EndIf

Return .T.

