/*/{Protheus.doc} fA100CliFor
Função gatilho para validar o código do Cliente/Fornecedor e preencher o campo loja.

@author Jose Lucas
@since  05/09/2011
@version 12
/*/
//-------------------------------------------------------------------------
Function fA100CliFor() As Logical

	Local aSavArea  As Array
	Local cCliFor   As Character
	Local cLoja     As Character
	Local lRet		As Logical

	aSavArea  := GetArea()
	cCliFor   := &(ReadVar())
	cLoja     := ""
	lRet      := .T.

	If Type('M->E5_LOJA') == 'U'
		cLoja := ALLTRIM(CriaVar('E5_LOJA'))
	ElseIf __cCliFor == cCliFor
		cLoja := AllTrim(M->E5_LOJA)
	EndIf

	If !Empty(cCliFor + cLoja)
		__cCliFor	:= cCliFor
		If lPagar
			SA2->(DbSetOrder(1))
			If !SA2->(dbSeek(xFilial("SA2")+ cCliFor + cLoja))
				Help(" ",1,"FA100FORN",,STR0088+CRLF,1,0)
				lRet := .F.
			EndIf
			cLoja := SA2->A2_LOJA
		ElseIf lReceber
			SA1->(DbSetOrder(1))
			If !SA1->(DbSeek(xFilial("SA1")+ cCliFor + cLoja))
				Help(" ",1,"FA100CLIE",,STR0089+CRLF,1,0)
				lRet := .F.
			EndIf
			cLoja := SA1->A1_LOJA
		EndIf
	EndIf

	If lRet
		M->E5_LOJA := cLoja
		nEndereco := aScan(aGets,{|x| ALLTRIM(Subs(x,9,10)) == "E5_LOJA"})
		If nEndereco > 0
			aTELA[Val(Subs(aGets[nEndereco],1,2))][Val(Subs(aGets[nEndereco],3,1))*2] := cLoja
		EndIf
		lRefresh := .T.
	EndIf

	RestArea(aSavArea)

Return lRet

