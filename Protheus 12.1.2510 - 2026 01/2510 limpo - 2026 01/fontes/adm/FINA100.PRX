			oSubFK5 := oModelMov:GetModel( "FK5DETAIL" )
			oSubFK5:SetValue( "FK5_DATA", dDataBase )
			oSubFK5:SetValue( "FK5_BANCO", cBcoDest )
			oSubFK5:SetValue( "FK5_AGENCI", cAgenDest )
			oSubFK5:SetValue( "FK5_CONTA", cCtaDest )
