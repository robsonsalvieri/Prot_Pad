					nTotal += DetProva( nHdlPrv, cPadrao, "FINA100" /*cPrograma*/, cLote, /*nLinha*/, /*lExecuta*/,;
					/*cCriterio*/, /*lRateio*/, /*cChaveBusca*/, /*aCT5*/, /*lPosiciona*/, @aFlagCTB,;
					/*aTabRecOri*/, /*aDadosProva*/ )
					If !lUsaFlag

