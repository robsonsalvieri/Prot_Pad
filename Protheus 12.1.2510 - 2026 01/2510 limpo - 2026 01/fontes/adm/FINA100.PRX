					{"EMISSAO"	, "", fA100X3Tit("EF_DATA")	  , ""},;
					{"VENCTO"	, "", fA100X3Tit("EF_VENCTO") , ""},;
					{"VENCREA"  , "", fA100X3Tit("E1_VENCREA"), ""},;
					{"STATUS"	, "", fA100X3Tit("EF_STATUS") , ""},;
