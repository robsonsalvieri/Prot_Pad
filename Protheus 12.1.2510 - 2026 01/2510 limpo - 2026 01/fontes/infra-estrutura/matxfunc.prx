Function FilBrowse(cAlias,aQuery,cFiltro,lShowProc)

Local cArqTmp	:= ""
Local nIndex	:= 0
Local nScan		:= 0
Local nOrdem   := 0
Local cKey		:= ""
Local nCntFor	:= 0

dbSelectArea(cAlias)
If Empty(IndexKey())
	dbSetOrder(1)
EndIf
If ( IndexOrd() != 0 )
	cKey	:= &(IndexKey())
EndIf

If ( !Empty(cFiltro) )

	dbSelectArea(cAlias)
	nOrdem := IndexOrd()

	nScan := aScan(aQuery,{|x| x[2] == nOrdem })


	If ( Len(dbFilter()) == 0 )

		cArqTmp := CriaTrab(,.F.)
		Set Filter To &cFiltro
		nIndex := IndexOrd()-1

		aadd(aQuery,{ cArqTmp , nOrdem })

	Else
		nIndex := IndexOrd()-1

	EndIf

	dbSetOrder(nIndex+1)

	If ( Len(aQuery) > 0 )
		dbSeek(cKey,.T.)
	EndIf

EndIf

Return(nIndex)


