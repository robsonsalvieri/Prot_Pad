Function VerPadrao(cPadrao)
Local aArea  := GetArea()
Local lRet   :=.F.
Local cChave := ""
Local nRecno := 0
DEFAULT cPadrao := ""

If __oVerPad == Nil
	__oVerPad := tHashMap():New()
EndIf

cChave := xFilial("CT5")+cPadrao

If __oVerPad:Get(cChave, @nRecno)     	
	CT5->(DbGoTo(nRecno)) //Garante o posicionamento correto na CT5
	lRet := .T.
Else
	CT5->(dbSetOrder(1))
	If CT5->(MsSeek(cChave))
		While CT5->(!Eof()) .And. CT5->(CT5_FILIAL+CT5_LANPAD) == cChave
			If CT5->CT5_STATUS <> '2'								
				lRet := .T.
				HMSet(__oVerPad,cChave, CT5->(RECNO()))
				Exit
			EndIf
			CT5->(dbSkip())
		EndDo
	EndIf
EndIf

RestArea(aArea)

Return lRet

