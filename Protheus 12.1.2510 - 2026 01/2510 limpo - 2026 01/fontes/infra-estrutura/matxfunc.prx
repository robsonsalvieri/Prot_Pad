Function MaDesTrava(aTravas)

Local aArea 	:= GetArea()
Local nCntFor	:= 0

If ( aTravas!=Nil )
	For nCntFor := 1 To Len(aTravas)
		dbSelectArea(aTravas[nCntFor,1])
		dbGoto(aTravas[nCntFor,2])
		MsUnLock()
	Next nCntFor
EndIf

RestArea(aArea)

Return(Nil)

