			cOldArea := "S"+SubStr(cCodAux,1,nPosUnder-1)
		Elseif nPosUnder == 4
			cOldArea := SubStr(cCodAux,1,nPosUnder-1)
		Endif
	Endif

	aOldArea	:= (cOldArea)->(GetArea())
	aAreaSB1	:= SB1->(GetArea())
	SB1->(dbSetOrder(1))
	SB1->(MsSeek(xFilial("SB1")+cCod))
	cReturn	:= If(Substr(cCodAux,3) == '_CODGRP', SB1->B1_GRUPO, SB1->B1_CODITE)

	oCodRet[cKeySeek] := cReturn 

	RestArea(aAreaSB1)
	RestArea(aOldArea)
	aSize(aOldArea,0)
	aSize(aAreaSB1,0)
	aOldArea	:= NIL
	aAreaSB1	:= NIL
EndIf 
Return (cReturn)

