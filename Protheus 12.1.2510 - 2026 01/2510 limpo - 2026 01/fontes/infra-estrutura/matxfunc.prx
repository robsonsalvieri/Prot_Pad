Function ConValor( cString, nLen )
Local cValor:=cString
Local cValorA:=""
Local k

For k:=1 To Len(cValor)
	IF !SubStr(cValor,k,1) $".,"
		cValorA+=SubStr(cValor,k,1)
	End
	IF SubStr(cValor,k,1) == ","
		cValorA+="."
	End
Next k
If Len(cValorA) <= nLen
	cValorA:=Space(nLen-Len(cValorA))+cValorA
Else
	cValorA:=Right(cValorA,nLen)
End
Return cValorA

