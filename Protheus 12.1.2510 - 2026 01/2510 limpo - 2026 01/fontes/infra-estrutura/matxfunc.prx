@return logical, .T. = se encontrou a amarração antiga e fez o ajuste; .F. = não encontrou a amarração antiga
/*/
Function FinAjusAC9(cEntidade As Character, cChvBusca As Character, cNewChave As Character) As Logical

	Local cQuery As Character
	Local oQryAC9 As Object
    Local nParam As Numeric
    Local cAliasAC9 As Character
	Local lRet As Logical

	Default cEntidade := ""
	Default cChvBusca := ""
	Default cNewChave := ""

	cQuery    := ""
	oQryAC9   := Nil
	nParam    := 1
    cAliasAC9 := GetNextAlias()
	lRet      := .F.

	If __lLibVer == Nil
        __lLibVer := FWLibVersion() >= "20211116"
    EndIf

	cQuery := "SELECT AC9.R_E_C_N_O_ RECAC9 "
	cQuery += "FROM " + RetSQLName("AC9") + " AC9 "
	cQuery += "WHERE AC9.AC9_FILIAL = ? " // WHERE seguindo a ordem do indice 2 da tabela AC9 = AC9_FILIAL+AC9_ENTIDA+AC9_FILENT+AC9_CODENT+AC9_CODOBJ
	cQuery += "AND AC9.AC9_ENTIDA = ? "
	cQuery += "AND AC9.AC9_FILENT = ? "
	cQuery += "AND AC9.AC9_CODENT = ? "
	cQuery += "AND AC9.D_E_L_E_T_ = ? "
	cQuery := ChangeQuery(cQuery)

	oQryAC9 := IIf(__lLibVer, FwExecStatement():New(cQuery), FWPreparedStatement():New(cQuery))
	
	oQryAC9:SetString(nParam++, FWxFilial("AC9"))
	oQryAC9:SetString(nParam++, cEntidade)
	oQryAC9:SetString(nParam++, FWxFilial(cEntidade))
	oQryAC9:SetString(nParam++, cChvBusca)
	oQryAC9:SetString(nParam++, " ")

	IIf(__lLibVer, oQryAC9:OpenAlias(cAliasAC9), MpSysOpenQuery(oQryAC9:GetFixQuery(), cAliasAC9))

	If (cAliasAC9)->(!EOF())
		While (cAliasAC9)->(!EOF())
			AC9->(DBGoTo((cAliasAC9)->RECAC9))

			RecLock("AC9", .F.)
			AC9->AC9_FILENT := FWxFilial("FK7")
			AC9->AC9_ENTIDA := "FK7"
			AC9->AC9_CODENT := cNewChave
			AC9->(MSUnlock())
			
			(cAliasAC9)->(DBSkip())
		EndDo

		lRet := .T.
	EndIf

	(cAliasAC9)->(DBCloseArea())
	oQryAC9:Destroy()
	FWFreeObj(oQryAC9)
	
Return lRet
