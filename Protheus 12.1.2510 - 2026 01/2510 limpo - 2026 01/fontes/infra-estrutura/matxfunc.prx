/*/{Protheus.doc} MatMailEv()
Rotina que retorna eventos disponiveis para envio de e-mail

@author Israel Escorizza
@since 08/12/2016
@version 2.0
@return aEventos
/*/
//--------------------------------------------------------------------
Function MatMailEv(cEvento)
Local aArea	:= GetArea()
Local aEventos:= {}

Default cEvento := ""

dbSelectArea("SX5")
dbSetOrder(1)

If TcSrvType() <> "AS/400"
		lQuery    := .T.
		cAliasSX5 := "MATMAILEV"

		cQuery := "SELECT * "
		cQuery += "FROM "+RetSqlName("SX5")+" SX5 "
		cQuery += "WHERE SX5.X5_FILIAL='"+xFilial("SX5")+"' AND "
		cQuery += "SX5.X5_TABELA	='E3' AND "
		If !Empty(cEvento)
			cQuery += "SX5.X5_CHAVE = '"+cEvento+"' AND "
		Endif
		cQuery += "SX5.D_E_L_E_T_=' ' "
		cQuery += "ORDER BY "+SqlOrder(SX5->(IndexKey()))
		cQuery := ChangeQuery(cQuery)

		dbUseArea(.T.,"TOPCONN",TcGenQry(,,cQuery),cAliasSX5)
Else
	dbSeek(xFilial("SX5")+"E3"+cEvento)
EndIf


While !Eof() .And. (cAliasSX5)->X5_FILIAL == xFilial("SX5") .And. (cAliasSX5)->X5_TABELA == "E3"

	SX5->(dbSeek( (cAliasSX5)->X5_FILIAL+(cAliasSX5)->X5_TABELA+(cAliasSX5)->X5_CHAVE ))
	AADD(aEventos,{Substr((cAliasSX5)->X5_CHAVE,1,3),X5Descri()})

	dbSelectArea(cAliasSX5)
	dbSkip()

EndDo

If lQuery
	dbSelectArea(cAliasSX5)
	dbCloseArea()
	dbSelectArea("SX5")
EndIf
RestArea(aArea)
Return aEventos

