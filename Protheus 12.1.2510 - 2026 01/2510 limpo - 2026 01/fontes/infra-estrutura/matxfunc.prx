	IF ( ( lDuplic ) .and. ( lEmpty ) )
		cMsgAviso := cString
		cMsgAviso += cCrLf
		cMsgAviso += cCrLf
		cMsgAviso += cEmptyString
	ElseIF ( lDuplic )
		cMsgAviso := cString
	ElseIF ( lEmpty )
