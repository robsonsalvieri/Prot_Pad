Function GetSAETipos()

Local aArea	:=	GetArea()
Local cRet	:=	Space(0)

DbSelectArea("SAE")
DbSetOrder(1)
DbSeek(xFilial())
While !EOF() .And. xFilial() == AE_FILIAL
	If !(AE_TIPO $ cRet)
		cRet +=	AE_TIPO + "|"
	Endif
	dbSkip()
Enddo

cRet	:=	IIf(!Empty(cRet),Substr(cRet,1,Len(cRet)-1),cRet)
RestArea(aArea)

Return(cRet)

