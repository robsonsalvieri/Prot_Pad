Function MsConHist(cAliasTab,cCliFor,cLoja,dDataIni,dDataFim,cProduto,cTransp)

Local aRet			:= {}
Local aAreaAIF		:= AIF->(GetArea())
Local cAliasAIF		:= "AIF"
Local cQuery		:= ""
Local cFilAlias 	:= FwxFilial(cAliasTab)
Local cFilAIF   	:= FwxFilial("AIF")

Default cCliFor	 	:= ""
Default cLoja	 	:= ""
Default dDataIni 	:= CTOD("01/01/00")
Default dDataFim 	:= CTOD("31/12/49")
Default cProduto 	:= ""
Default cTransp  	:= ""

If (cAliasTab $ "SA1/SA2")		//Cadastro de Clientes/Fornecedores
	dbSelectArea("AIF")
	dbSetOrder(1)

	cAliasAIF := "HISTTAB"
	cQuery := "SELECT AIF_FILIAL, AIF_FILTAB, AIF_TABELA, AIF_CODIGO, AIF_LOJA, AIF_DATA, AIF_CAMPO, AIF_CONTEU, AIF_HORA "
	cQuery += "FROM "+RetSqlName("AIF")+" AIF "
	cQuery += "WHERE AIF.AIF_FILIAL= ? AND "
	cQuery += "AIF.AIF_FILTAB= ? AND "
	cQuery += "AIF.AIF_TABELA= ? AND "
	cQuery += "AIF.AIF_CODIGO= ? AND "
	cQuery += "AIF.AIF_LOJA= ? AND "
	cQuery += "AIF.AIF_DATA>= ? AND "
	cQuery += "AIF.AIF_DATA<= ? AND "
	cQuery += "AIF.D_E_L_E_T_= ? "
	cQuery += "ORDER BY "+SqlOrder(AIF->(IndexKey()))

	If Empty(__oPrepSA)
		cQuery := ChangeQuery(cQuery)
		__oPrepSA := FwExecStatement():New(cQuery)
	Endif 

	__oPrepSA:SetString(1,cFilAIF)
	__oPrepSA:SetString(2,cFilAlias)
	__oPrepSA:SetString(3,cAliasTab)
	__oPrepSA:SetString(4,cCliFor)
	__oPrepSA:SetString(5,cLoja)
	__oPrepSA:SetString(6,DTOS(dDataIni))
	__oPrepSA:SetString(7,DTOS(dDataFim))
	__oPrepSA:SetString(8," ")

	__oPrepSA:OpenAlias(cAliasAIF)
	dbselectarea(cAliasAIF)
	
	TcSetField(cAliasAIF,"AIF_DATA","D",8,0)

	While !(cAliasAIF)->(Eof()) .And. (cAliasAIF)->AIF_FILIAL == cFilAIF .And. (cAliasAIF)->AIF_FILTAB == cFilAlias .And. (cAliasAIF)->AIF_TABELA == cAliasTab .And.;
		(cAliasAIF)->AIF_CODIGO == cCliFor .And. (cAliasAIF)->AIF_LOJA == cLoja .And. (cAliasAIF)->AIF_DATA >= dDataIni .And. (cAliasAIF)->AIF_DATA <= dDataFim

		AADD(aRet,{(cAliasAIF)->AIF_CAMPO,AllTrim((cAliasAIF)->AIF_CONTEU),(cAliasAIF)->AIF_DATA,(cAliasAIF)->AIF_HORA})

		dbSkip()
	EndDo

ElseIf (cAliasTab $ "SB1")		//Cadastro de Produtos
	dbSelectArea("AIF")
	If AIF->(FieldPos("AIF_CODPRO")) > 0
		dbSetOrder(2)

		cAliasAIF := "HISTTAB"
		cQuery := "SELECT AIF_FILIAL, AIF_FILTAB, AIF_TABELA, AIF_CODPRO, AIF_DATA, AIF_CAMPO, AIF_CONTEU, AIF_HORA "
		cQuery += "FROM "+RetSqlName("AIF")+" AIF "
		cQuery += "WHERE AIF.AIF_FILIAL= ? AND "
		cQuery += "AIF.AIF_FILTAB= ? AND "
		cQuery += "AIF.AIF_TABELA= ? AND "
		cQuery += "AIF.AIF_CODPRO= ? AND "
		cQuery += "AIF.AIF_DATA>= ? AND "
		cQuery += "AIF.AIF_DATA<= ? AND "
		cQuery += "AIF.D_E_L_E_T_= ? "
		cQuery += "ORDER BY "+SqlOrder(AIF->(IndexKey()))

		If Empty(__oPrepSB1)
			cQuery := ChangeQuery(cQuery)
			__oPrepSB1 := FwExecStatement():New(cQuery)
		Endif 

		__oPrepSB1:SetString(1,cFilAIF)
		__oPrepSB1:SetString(2,cFilAlias)
		__oPrepSB1:SetString(3,cAliasTab)
		__oPrepSB1:SetString(4,cProduto)
		__oPrepSB1:SetString(5,DTOS(dDataIni))
		__oPrepSB1:SetString(6,DTOS(dDataFim))
		__oPrepSB1:SetString(7," ")

		__oPrepSB1:OpenAlias(cAliasAIF)
		dbselectarea(cAliasAIF)
		
		TcSetField(cAliasAIF,"AIF_DATA","D",8,0)

		While !Eof() .And. (cAliasAIF)->AIF_FILIAL == cFilAIF .And. (cAliasAIF)->AIF_FILTAB == cFilAlias .And. (cAliasAIF)->AIF_TABELA == cAliasTab .And.;
			(cAliasAIF)->AIF_CODPRO == cProduto .And. (cAliasAIF)->AIF_DATA >= dDataIni .And. (cAliasAIF)->AIF_DATA <= dDataFim

			AADD(aRet,{(cAliasAIF)->AIF_CAMPO,AllTrim((cAliasAIF)->AIF_CONTEU),(cAliasAIF)->AIF_DATA,(cAliasAIF)->AIF_HORA})

			dbSkip()
		EndDo
	EndIf
ElseIf (cAliasTab == "SA4")		//Cadastro de Transportadora
	dbSelectArea("AIF")
	If AIF->(FieldPos("AIF_TRANSP")) > 0
		dbSetOrder(3)

		cAliasAIF := "HISTTAB"
		cQuery := "SELECT AIF_FILIAL, AIF_FILTAB, AIF_TABELA, AIF_TRANSP, AIF_DATA, AIF_CAMPO, AIF_CONTEU, AIF_HORA "
		cQuery += "FROM "+RetSqlName("AIF")+" AIF "
		cQuery += "WHERE AIF.AIF_FILIAL= ? AND "
		cQuery += "AIF.AIF_FILTAB= ? AND "
		cQuery += "AIF.AIF_TABELA= ? AND "
		cQuery += "AIF.AIF_TRANSP= ? AND "
		cQuery += "AIF.AIF_DATA>= ? AND "
		cQuery += "AIF.AIF_DATA<= ? AND "
		cQuery += "AIF.D_E_L_E_T_= ? "
		cQuery += "ORDER BY "+SqlOrder(AIF->(IndexKey()))

		If Empty(__oPrepSA4)
			cQuery := ChangeQuery(cQuery)
			__oPrepSA4 := FwExecStatement():New(cQuery)
		Endif 

		__oPrepSA4:SetString(1,cFilAIF)
		__oPrepSA4:SetString(2,cFilAlias)
		__oPrepSA4:SetString(3,cAliasTab)
		__oPrepSA4:SetString(4,cTransp)
		__oPrepSA4:SetString(5,DTOS(dDataIni))
		__oPrepSA4:SetString(6,DTOS(dDataFim))
		__oPrepSA4:SetString(7," ")

		__oPrepSA4:OpenAlias(cAliasAIF)
		dbselectarea(cAliasAIF)
		
		TcSetField(cAliasAIF,"AIF_DATA","D",8,0)

		While !Eof() .And. (cAliasAIF)->AIF_FILIAL == cFilAIF .And. (cAliasAIF)->AIF_FILTAB == cFilAlias .And. (cAliasAIF)->AIF_TABELA == cAliasTab .And.;
				(cAliasAIF)->AIF_TRANSP == cTransp .And. (cAliasAIF)->AIF_DATA >= dDataIni .And. (cAliasAIF)->AIF_DATA <= dDataFim

			AADD(aRet,{(cAliasAIF)->AIF_CAMPO,AllTrim((cAliasAIF)->AIF_CONTEU),(cAliasAIF)->AIF_DATA,(cAliasAIF)->AIF_HORA})

			dbSkip()
		EndDo
	EndIf
EndIf

dbSelectArea(cAliasAIF)
dbCloseArea()

RestArea(aAreaAIF)

Return( aRet )

