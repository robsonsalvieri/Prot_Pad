Function LerData(x,y)
Local cData
Local dDatBasOld := dDataBase  //salva database atual

If y == 6
	cData := Substr(xBuffer,x,2)+"/"+Substr(xBuffer,x+2,2)+"/"+Substr(xBuffer,x+4,2)
Elseif y == 8
	cData := Substr(xBuffer,x,2)+"/"+Substr(xBuffer,x+2,2)+"/"+Substr(xBuffer,x+4,4)
Endif
dDataBase := CtoD(cData,"ddmmyy")

If Empty(dDataBase) //se apos leitura data estiver em branco recupera database salva
	dDataBase := dDatBasOld  //pois nao pode contabilizar com data em branco
EndIf

Return " "


/*
ÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜ
±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
