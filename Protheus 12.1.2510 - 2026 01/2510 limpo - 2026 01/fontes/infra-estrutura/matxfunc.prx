Function MsDecimais( nMoeda )
DEFAULT nMOEDA := 1

If ( __cUltFilMVCent == Nil ) .Or. !(__cUltEmpMVCent+__cUltFilMVCent == cEmpAnt+cFilAnt ) .Or. ( __nMV9CENT == Nil ) .Or. ( __nMV1CENT == Nil ) .Or. ( __nMV0CENT == Nil )
	__cUltEmpMVCent := cEmpAnt
	__cUltFilMVCent := cFilAnt
	If nMoeda > 9
		__nMV9CENT := SuperGetMv("MV_CENT"+Str(nMoeda,2))
		Return __nMV9CENT
	ElseIf nMoeda > 1
		__nMV1CENT := SuperGetMv("MV_CENT"+Str(nMoeda,1))
		Return __nMV1CENT
	Else
		__nMV0CENT := SuperGetMv("MV_CENT")
		Return __nMV0CENT
	Endif
EndIf

If nMoeda > 9
	Return __nMV9CENT
ElseIf nMoeda > 1
	Return __nMV1CENT
Else
	Return __nMV0CENT
Endif

