	cPict := IF( cPict == NIL , "9999999999" , cPict )
	cPict := IF( Empty( cPict ) , "9999999999" , cPict )
	cRet  := Transform( xPar , ALLTRIM( cPict ) )
ElseIF ( cType == "C" )
	cPict := IF( cPict == NIL , "" , cPict )
	IF !Empty( cPict )
		cRet  := Transform( xPar , AllTrim( cPict ) )
	Else
		cRet  := xPar
	EndIF
ElseIF ( cType == "L" )
	cRet := IF( xPar , ".T." , ".F." )
ElseIF ( cType == "D" )
	IF ( lDtos )
		cRet := Dtos( xPar )
	Else
		cRet := DToC( xPar )
	EndIF
Else
	cRet := " "
EndIF

Return( cRet )

