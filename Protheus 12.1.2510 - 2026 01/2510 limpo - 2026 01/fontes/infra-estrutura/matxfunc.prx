	If !EMPTY(&('m->'+cCampo))
		cPict := Trim(POSICIONE('SBM',1,xFilial('SBM')+&('m->'+cCampo),'BM_PICPAD'))+'%C'
	EndIf
EndIf
Return RTrim(cPict)

