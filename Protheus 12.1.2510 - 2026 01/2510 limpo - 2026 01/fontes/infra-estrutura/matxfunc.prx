Function lIsDir(cCaminho)
Local lIsDir := .F.
If !empty(cCaminho) .and. !("\\"$cCaminho)
	cCaminho := alltrim(cCaminho)
	if Right(cCaminho,1) == "\"
		cCaminho := SubStr(cCaminho,1,len(cCaminho)-1)
	Endif
	lIsDir :=(Ascan( Directory(cCaminho,"D"),{|_Vet | "D" $ _Vet[5] } ) > 0)
EndIf
Return lIsDir



