/*/
Function CtRelation(cPadrao,lSeek,aSTRTRAN)

Local aSaveArea	:= GetArea()	
Local c_Alias
Local c_AliasAux := "AUX"
Local c_ChvAux	 := ""
Local c_Chave
Local n_Ordem
Local cChaveBusca := ""
Local aArea
Local nX := 0

Static __lExistCTL

DEFAULT lSeek 		:= .F.				/// RETIRADO O SEEK POIS TODOS OS PROCESSOS DEVEM ESTAR POSICIONADOS NO MOMENTO DA DETPROVA.
DEFAULT aSTRTRAN	:= {}				/// CRIADO TRATAMENTO PARA CTBANFE/CTBANFS PARA UTILIZAR O ALIAS DAS QUERIES
DEFAULT __lExistCTL := ChkFile("CTL")

If	__lExistCTL
	cChaveBusca := ""//- inicia a variavel 
	dbSelectArea("CTL")
	dbSetOrder(1)
	If DbSeek(xFilial("CTL")+cPadrao)
		c_Alias 		:= CTL->CTL_ALIAS
		c_Chave 		:= Alltrim(CTL->CTL_KEY)
		For nX := 1 To Len(aStrTran)
			If c_Alias == aStrTran[nX][2] .and. c_Alias <> aStrTran[nX][1]
				c_Alias := StrTran(c_Alias,aStrTran[nX][2],aStrTran[nX][1])
				c_Chave := StrTran(c_Chave,aStrTran[nX][2]+"->",aStrTran[nX][1]+"->")
			EndIf
		Next nX
		If lSeek
			aArea 	  	:= (c_Alias)->(GetArea())
			n_Ordem   	:= Val(CTL->CTL_ORDER)

			If c_Alias	= "SN3"
				c_ChvAux  	:= (c_Alias)->(&(c_Chave))
				c_AliasAux 	+= AllTrim(c_Alias)
				If ChkFile(c_Alias,.F.,c_AliasAux)
					(c_AliasAux)->(DbGoTop())
					(c_AliasAux)->(dbSetOrder(n_Ordem))
					If (c_AliasAux)->(DbSeek(c_ChvAux))
						cChaveBusca	:= c_ChvAux
					EndIf
					if Empty(cChaveBusca) .And. (c_Alias)->(DbSeek(c_ChvAux))
						cChaveBusca	:= c_ChvAux
					EndIf
					(c_AliasAux)->(dbCloseArea())
				EndIf
			Else
				If ChkFile(c_Alias)
					(c_Alias)->(dbSetOrder(n_Ordem))
					If (c_Alias)->(DbSeek(&(c_Chave)))
						cChaveBusca	:= (c_Alias)->(&(c_Chave))
					EndIf
				EndIf
			Endif
			(c_Alias)->(RestArea(aArea))

		Else
			cChaveBusca 	:= (c_Alias)->(&(c_Chave))
		Endif
	EndIf
EndIf
RestArea(aSaveArea)
aSize(aSaveArea,0)
aSaveArea := nil

Return cChaveBusca

