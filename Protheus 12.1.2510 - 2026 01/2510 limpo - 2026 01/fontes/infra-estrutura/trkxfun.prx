/*/{Protheus.doc} FindCNBbySC
	Procura o item do contrato(CNB) com base na solicitação de compra considerando a revisão
@author philipe.pompeu
@since 03/08/2023
@param cNumSC, caractere, numero da solicitação
@param cItem, caractere
@param cRevisa, caractere, revisão do contrato
@return lResult, lógico, se encontrou a CNB
/*/
Static Function FindCNBbySC(cNumSC , cItem, cRevisa)
	Local lResult := .F.
	Local cChave := ""

	CNB->(DbSetOrder(2))//CNB_FILIAL+CNB_NUMSC+CNB_ITEMSC
	cChave := xFilial( "CNB" ) + cNumSC + cItem	

	If (lResult := CNB->(DbSeek(cChave )))
		
		While CNB->(!Eof() .And. CNB_FILIAL+CNB_NUMSC+CNB_ITEMSC == cChave)
			If (CNB->CNB_REVISA == cRevisa)
				Exit
			EndIf			
			CNB->(DbSkip())
		EndDo

	EndIf
Return lResult

/*/{Protheus.doc} GetTitItNF
	Carrega os títulos da Nota Fiscal informada em <cChaveNF>
@author philipe.pompeu
@since 22/09/2023
@param cChaveNF	, caractere	, chave da nota fiscal
@param oTree	, objeto	, instância de DBTree
@param nLevel	, numerico	, nível atual
@param nMaxLevel, numerico	, nível máximo da árvore
@param cCargo	, caractere	, identificador do item da árvore
@param cTreeID	, caractere	, identificador da DBTree
@return Nil, nulo
/*/
Static Function GetTitItNF( cChaveNF, oTree, nLevel, nMaxLevel, cCargo, cTreeID )
	Local aAreas	:= {SF1->(GetArea()), GetArea()}
	LOCAL cFornece  := ""
	LOCAL cLoja     := ""
	LOCAL cDoc      := ""
	LOCAL cSerie    := ""
	LOCAL cImageSE2 := ""	
	LOCAL cTrkSeek	:= ""	
	LOCAL cQuery    := ""
	LOCAL cAliasQry := ""
	Local cText		:= ""
	Local cDefText	:= STR0113 + "#1/#2/#3/#4/#5/#6"  //"Titulo - Fornecedor/Loja/Prefixo/Numero/Parcela/Tipo - "
	Local nComeco	:= 1
	Local aParams	:= {}

	DEFAULT nMaxLevel := 1000000	
	DEFAULT cTreeID   := "000001"

	SF1->( DbSetOrder( 1 ) )
	
	cTrkSeek:= Left(cChaveNF,Len(SF1->(F1_FILIAL+F1_DOC+F1_SERIE+F1_FORNECE+F1_LOJA)))	
	
	If SF1->( DbSeek(cTrkSeek) )
		
		nComeco	:= FwSizeFilial() + 1 
		
		cDoc     := ExtraiStr(cChaveNF, @nComeco, Len( SF1->F1_DOC ))		
		cSerie   := ExtraiStr(cChaveNF, @nComeco, Len( SF1->F1_SERIE ))
		cFornece := ExtraiStr(cChaveNF, @nComeco, Len( SF1->F1_FORNECE ))		
		cLoja    := ExtraiStr(cChaveNF, @nComeco, Len( SF1->F1_LOJA ))

		nLevel++

		If nLevel <= nMaxLevel
			cAliasQry := GetNextAlias()			

			cQuery := "SELECT E2_FILIAL,E2_PREFIXO,E2_NUM,E2_PARCELA,E2_FORNECE,E2_LOJA,E2_TIPO"
			cQuery += " FROM " + RetSqlName( "SE2" )
			cQuery += " WHERE"
			cQuery += "     E2_FILIAL	= ?"
			cQuery += " AND E2_NUM		= ?"
			cQuery += "	AND E2_PREFIXO	= ?"
			cQuery += " AND E2_FORNECE	= ?"
			cQuery += " AND E2_LOJA		= ?"
			cQuery += " AND D_E_L_E_T_	=' '"

			cQuery := ChangeQuery( cQuery )

			aAdd(aParams,xFilial("SE2"))
			aAdd(aParams,cDoc)
			aAdd(aParams,&(SuperGetMv("MV_2DUPREF")))
			aAdd(aParams,cFornece)
			aAdd(aParams,cLoja)
			
			dbUseArea( .T., "TOPCONN", TcGenQry2(,,cQuery,aParams), cAliasQry, .F., .T. )

			oTree:TreeSeek( cCargo )

			cImageSE2 := MaEntImage( "SE2", 1 )

			While !( cAliasQry )->( Eof() )
				cKey := (cAliasQry)->(E2_FILIAL+E2_FORNECE+E2_LOJA+E2_PREFIXO+E2_NUM+E2_PARCELA+E2_TIPO)

				cKey := Pad( "SE2-" + cKey, 50 ) + cTreeID
				
				cText:= I18N(cDefText, (cAliasQry)->({ E2_FORNECE , E2_LOJA , E2_PREFIXO , E2_NUM , E2_PARCELA , E2_TIPO }))
				cText:= Pad(cText, 100)

				oTree:AddItem( cText,cKey, cImageSE2 ,cImageSE2,,,nLevel)

				(cAliasQry )->( dbSkip() )
			EndDo			
			(cAliasQry)->(dbCloseArea())
			
		EndIf
		nLevel--
	EndIf

	aEval(aAreas,{|x|RestArea(x)})
	FwFreeArray(aAreas)
	FwFreeArray(aParams)
Return Nil

//-------------------------------------------------------------------
/*/{Protheus.doc} GetQryTitMD
	Obtêm a query responsável por retornar os títulos gerados pela medição.
@author philipe.pompeu
@since 03/10/2023
@param cTabela	, caractere	, tabela que devem ser buscados os títulos
@return cQuery	, caractere	, consulta sql
/*/
//-------------------------------------------------------------------
Static Function GetQryTitMD(cTabela)
	Local cQuery := ""
	Default cTabela := "SE2"

	If (cTabela == "SE2")		
		cQuery := " SELECT "
		cQuery += " E2_FILIAL, "
		cQuery += " E2_PREFIXO, "
		cQuery += " E2_NUM, "
		cQuery += " E2_PARCELA, "
		cQuery += " E2_FORNECE, "
		cQuery += " E2_LOJA, "
		cQuery += " E2_TIPO "
		cQuery += " FROM "+ RetSQLName("SE2") + " SE2 "
		cQuery += " WHERE "
		cQuery += " SE2.E2_FILIAL = ? AND "
		cQuery += " SE2.E2_MDCONTR =  ?	AND "
		cQuery += " SE2.E2_MDREVIS =  ?	AND "
		cQuery += " SE2.E2_MDPLANI =  ?	AND "
		cQuery += " SE2.E2_MEDNUME =  ?	AND "
		cQuery += " SE2.E2_FORNECE =  ?	AND "
		cQuery += " SE2.E2_LOJA    =  ? AND "
		cQuery += " SE2.E2_TITPAI = '"	+ Space(Len(SE2->E2_TITPAI))+"' AND " //Nao trazer titulos de impostos
		cQuery += " SE2.E2_PREFIXO =  '"+ SuperGetMV("MV_CNPREMD",.F.,"MED") +"' AND"				
		cQuery += " SE2.E2_ORIGEM  IN  ('CNTA120', 'CNTA121') AND SE2.D_E_L_E_T_ = ' '"
	Else
		cQuery := " SELECT "
		cQuery += " E1_FILIAL, "
		cQuery += " E1_PREFIXO, "
		cQuery += " E1_NUM, "
		cQuery += " E1_PARCELA, "
		cQuery += " E1_CLIENTE, "
		cQuery += " E1_LOJA, "
		cQuery += " E1_TIPO "
		cQuery += " FROM "+ RetSQLName("SE1") + " SE1 "
		cQuery += " WHERE "
		cQuery += " SE1.E1_FILIAL = ? AND "
		cQuery += " SE1.E1_MDCONTR =  ?	AND "
		cQuery += " SE1.E1_MDREVIS =  ?	AND "
		cQuery += " SE1.E1_MDPLANI =  ?	AND "
		cQuery += " SE1.E1_MEDNUME =  ?	AND "
		cQuery += " SE1.E1_CLIENTE =  ?	AND "
		cQuery += " SE1.E1_LOJA    =  ? AND "
		cQuery += " SE1.E1_TITPAI = '"	+ Space(Len(SE1->E1_TITPAI))+"' AND " //Nao trazer titulos de impostos
		cQuery += " SE1.E1_PREFIXO =  '"+ SuperGetMV("MV_CNPREMD",.F.,"MED") +"' AND"
		cQuery += " SE1.E1_ORIGEM  IN  ('CNTA120', 'CNTA121') AND SE1.D_E_L_E_T_ = ' '"
	EndIf
Return cQuery
