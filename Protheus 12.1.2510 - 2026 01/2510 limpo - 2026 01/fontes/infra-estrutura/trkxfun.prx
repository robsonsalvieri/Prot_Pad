		oTree:AddItem( Pad( STR0014 + Transform(cNrCham,"@R XXXXXXXXXX/XX") + ; // "Atendimento Help Desk/Sequencia - "
		cAddString, 100 ), Pad( "ABK-" + cNrCham, 50 ) + cTreeID,cImageABK,cImageABK,,,nLevel)

	EndIf

	nLevel--

EndIf

Return( .T. )

