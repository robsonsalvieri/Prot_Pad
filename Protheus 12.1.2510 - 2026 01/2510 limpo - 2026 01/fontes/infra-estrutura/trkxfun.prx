Function MATRKSUB( cNumOrc, oTree, cAddString, nLevel, nMaxLevel,lItem,cTreeID )

Local aArea     := {}
Local cImageSUB := ""

DEFAULT nMaxLevel := 1000000
DEFAULT cTreeID   := "000001"

cAddString := If( ValType( cAddString ) == "C", cAddString, "" )

SUB->( DbSetOrder( 1 ) )
If SUB->( DbSeek( xFilial( "SUB" ) + cNumOrc ) )

	nLevel++

	If nLevel <= nMaxLevel

