				for nLacoIt := 1 to 2
					cCotFiap := GetNextAlias()
					cExpSql := "% SC8.C8_NUMPED " + iif( nLacoIt == 1, " = ", " <> ") + " '" + SC7->C7_NUM + "' %"
					BeginSQL Alias cCotFiap
						SELECT	C8_FILIAL,
							C8_NUM,
							C8_PRODUTO,
							C8_FORNECE,
							C8_LOJA,
							C8_NUMPED,
							C8_ITEM,
							C8_NUMSC
						FROM %Table:SC8% SC8 

						WHERE	SC8.C8_PRODUTO 		= %Exp:SC7->C7_PRODUTO%
							AND SC8.C8_FORNECE	= %Exp:SC7->C7_FORNECE%
							AND SC8.C8_LOJA 	= %Exp:SC7->C7_LOJA%
							AND %exp:cExpSql%
							AND SC8.C8_NUMSC    = %Exp:SC7->C7_NUMSC%
							AND SC8.C8_ITEMSC	= %Exp:SC7->C7_ITEMSC%
							AND SC8.%NotDel%
					EndSQL

					_aNoSC8Ped := iif( (cCotFiap)->(EOF()), {.T., SC7->C7_NUM}, _aNoSC8Ped)
					While (cCotFiap)->(!EOF())
						MaRetRoot( "SC8", ( cCotFiap )->C8_FILIAL + SC7->C7_NUMCOT + SC7->C7_PRODUTO + SC7->C7_FORNECE + SC7->C7_LOJA + /*SC7->C7_NUM*/( cCotFiap )->C8_NUMPED + SC7->C7_ITEM, @aRoot, @nMaxLevel, @nLevel,nTree )
						nTree++
						( cCotFiap )->( dbSkip() )
						lExitLoop := .T.
					End
					(cCotFiap)->(DbCloseArea())
					if lExitLoop
						exit
					endif
				next
			EndIf
		EndIf

