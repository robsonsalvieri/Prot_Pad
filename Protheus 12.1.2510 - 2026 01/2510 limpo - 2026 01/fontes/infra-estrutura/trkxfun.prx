/*/{Protheus.doc} MaTrkCN9H
	Monta as pastas de cabeçalho do contrato
@param cKey, caracter, chave de pesquisa
@param oTree, objeto, objeto Tree
@param cAddString, caracter, String a adicionar ao texto do item ( tree )
@param nLevel, numérico, nível
@param nMaxLevel, numérico, nível máximo
@param lItem, lógico, informe se exibe item
@param cTreeId, caracter, identificação da tree
@param nTipo, numérico, Tipo: 1-Pedido / 2-Autorização de Entrega
@author Sergio Silveira
@since 22/03/2006
@return Nil
-------------------------------------------------------------------------------------/*/
Function MaTrkCN9H( cKey, oTree, cAddString, nLevel, nMaxLevel,lItem,cTreeID, nTipo )

Local cContrato := ""
Local cRevisa   := ""
Local cImageCN9 := ""

Local cQuery    := ""
Local cAliasQry := ""
Local cFilCtr	:= ""

Local aAreas	:= {CN9->(GetArea()), GetArea()}

Local oStatement

DEFAULT nMaxLevel := 1000000
DEFAULT lItem     := .T.
DEFAULT nTipo     := 1
DEFAULT cTreeID   := "000001"

CN9->( DbSetOrder( 1 ) )// CN9_FILIAL+CN9_NUMERO+CN9_REVISA
If CN9->( DbSeek( xFilial( "CN9" ) + cKey ) )

	cContrato := CN9->CN9_NUMERO
	cRevisa   := CN9->CN9_REVISA
	cFilCtr	  := CN9->CN9_FILCTR

	cImageCN9 := MaEntImage( "CN9", 1 )

	nLevel++

	If nLevel <= nMaxLevel

		cAddString := If( ValType( cAddString ) == "C", cAddString, "" )

		If !oTree:AddItem( Pad( STR0107 + cContrato + "/" + cRevisa + ;  // "Contrato / Revisao - "
			cAddString, 100 ), Pad( "N9H-" + cKey, 50 )+cTreeID,cImageCN9,cImageCN9,,,nLevel)
			oTree:TreeSeek( Pad( "N9H-" + cKey, 50 )+cTreeID )

			//Alimenta as planilhas deste contrato
			cQuery := ""

			oStatement:= FWPreparedStatement():New()

			cQuery += " SELECT CNA_CONTRA,CNA_REVISA,CNA_NUMERO "
			cQuery += " FROM " + RetSqlName( "CNA" )
			cQuery += " WHERE CNA_FILIAL= ? "
			cQuery += " AND CNA_CONTRA= ? "
			cQuery += " AND CNA_REVISA= ? "
			cQuery += " AND D_E_L_E_T_= ' ' "
			cQuery += " ORDER BY " + SqlOrder( CNA->( IndexKey() ) )

			cQuery := ChangeQuery( cQuery )

			oStatement:SetQuery( cQuery )
			oStatement:SetString(1, xFilial("CNA", cFilCtr) )
			oStatement:SetString(2, cContrato )
			oStatement:SetString(3, cRevisa )

			cAliasQry:= MpSysOpenQuery( oStatement:GetFixQuery() )

			oTree:TreeSeek( Pad( "CN9-" + cKey, 50 )+cTreeID )

			( cAliasQry ) -> (dbGoTop())
			While !( cAliasQry )->( Eof() )

				MATRKCNA( cFilCtr + ( cAliasQry )->CNA_CONTRA + ( cAliasQry )->CNA_REVISA + ( cAliasQry )->CNA_NUMERO, @oTree, , @nLevel, nMaxLevel, lItem,cTreeID )
				oTree:TreeSeek( Pad( "N9H-" + cKey, 50 )+cTreeID )

				( cAliasQry )->( dbSkip() )

			End

			( cAliasQry )->( dbCloseArea())

		EndIf

	EndIf

	nLevel--
EndIf

aEval(aAreas,{|x|RestArea(x)})
FwFreeArray(aAreas)
FreeObj(oStatement)

Return

