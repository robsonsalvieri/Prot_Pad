/*/{Protheus.doc} MaTrkCNA
	Monta as pastas de cabecalho de planilhas
@param cPlanilha, caracter, chave de pesquisa da planilha
@param oTree, objeto, objeto Tree
@param cAddString, caracter, String a adicionar ao texto do item ( tree )
@param nLevel, numérico, nível
@param nMaxLevel, numérico, nível máximo
@param lItem, lógico, informe se exibe item
@param cTreeId, caracter, identificação da tree
@param nTipo, numérico, Tipo: 1-Pedido / 2-Autorização de Entrega
@author Sergio Silveira
@since 22/03/2006
@return Nil
-------------------------------------------------------------------------------------/*/

Function MaTrkCNA( cPlanilha, oTree, cAddString, nLevel, nMaxLevel,lItem,cTreeID, nTipo )

	Local cContrato := ""
	Local cRevisa   := ""
	Local cNumero   := ""
	Local cImageCNA := ""
	Local nTamFil 	:= GetSx3Cache('CND_FILIAL', 'X3_TAMANHO') +1
	Local cFilCtr	:= ""
	Local cQuery    := ""
	Local cAliasQry := ""

	Local oStatement
	Local aAreas	:= {CNA->(GetArea()), GetArea()}

	DEFAULT nMaxLevel := 1000000
	DEFAULT lItem     := .T.
	DEFAULT nTipo     := 1
	DEFAULT cTreeID   := "000001"

	cFilCtr   := ExtraiStr(cPlanilha, 1, FwSizeFilial())

	CNA->( DbSetOrder( 1 ) )// CNA_FILIAL+CNA_CONTRA+CNA_REVISA+CNA_NUMERO
	If CNA->( DbSeek( xFilial("CNA", cFilCtr) + Substr(cPlanilha, nTamFil) ) )

		cContrato := CNA->CNA_CONTRA
		cRevisa   := CNA->CNA_REVISA
		cNumero   := CNA->CNA_NUMERO

		cImageCNA := MaEntImage( "CNA", 1 )

		nLevel++

		If nLevel <= nMaxLevel

			cAddString := If( ValType( cAddString ) == "C", cAddString, "" )

			If !oTree:AddItem( Pad( STR0108 + cContrato + "/" + cRevisa + "/" + cNumero + ;  // "Planilha : Contrato / Revisao / Numero - "
				cAddString, 100 ), Pad( "CNA-" + cPlanilha, 50 )+cTreeID,cImageCNA,cImageCNA,,,nLevel)
				oTree:TreeSeek( Pad( "CNA-" + cPlanilha, 50 )+cTreeID )

				lFixo := CN300RetSt("FIXO",2)

				//Monta query para busca de dados relacionados ao cabeçalho da medição
				oStatement:= FWPreparedStatement():New()

				cQuery := ""

				cQuery += "SELECT CND_CONTRA,CND_REVISA,ISNULL(CXN.CXN_NUMPLA,CND.CND_NUMERO) AS CND_NUMERO,CND_NUMMED "
				cQuery += "FROM " + RetSqlName( "CND" ) + " CND "

				cQuery += " LEFT JOIN " + RetSqlName("CXN") + " CXN"
				cQuery += " ON CXN.CXN_FILIAL = CND.CND_FILIAL "
				cQuery += " AND CXN.CXN_CONTRA = CND.CND_CONTRA "
				cQuery += " AND CXN.CXN_REVISA = CND.CND_REVISA "
				cQuery += " AND CXN.CXN_NUMMED = CND.CND_NUMMED "
				cQuery += " AND ISNULL(CXN_CHECK, 'T') = 'T'"
				cQuery += " AND CXN.D_E_L_E_T_ = ' ' "


				cQuery += " WHERE CND.CND_FILCTR= ? "
				cQuery += " AND CND.CND_CONTRA= ? "
				cQuery += " AND CND.CND_REVISA= ? "
				cQuery += " AND ISNULL(CXN.CXN_NUMPLA,CND.CND_NUMERO)= ?"
				cQuery += " AND CND.D_E_L_E_T_=' ' "
				cQuery += " ORDER BY " + SqlOrder( CND->( IndexKey() ) )

				cQuery := ChangeQuery(cQuery)
				oStatement:SetQuery(cQuery)
				oStatement:SetString(1, cFilCtr)
				oStatement:SetString(2, cContrato)
				oStatement:SetString(3, cRevisa)
				oStatement:SetString(4, cNumero)

				cAliasQry := MpSysOpenQuery(oStatement:GetFixQuery())

				While!( cAliasQry )->( Eof() )
					MATRKCND( ( cAliasQry )->CND_CONTRA + ( cAliasQry )->CND_REVISA + ( cAliasQry )->CND_NUMERO + ( cAliasQry )->CND_NUMMED, @oTree, , @nLevel, nMaxLevel, lItem,cTreeID )
					oTree:TreeSeek( Pad( "CNA-" + cPlanilha, 50 )+cTreeID )
					( cAliasQry )->( dbSkip() )

				EndDo

				//Fecha a area de trabalho da query
				( cAliasQry )->(dbCloseArea())

			EndIf

		EndIf

		nLevel--
	EndIf

	aEval(aAreas,{|x|RestArea(x)})
	FwFreeArray(aAreas)
	FreeObj(oStatement)
Return

/*
ÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜÜ
±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±±
±±ÚÄÄÄÄÄÄÄÄÄÄÂÄÄÄÄÄÄÄÄÄÄÂÄÄÄÄÄÄÄÂÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÂÄÄÄÄÄÄÂÄÄÄÄÄÄÄÄÄÄ¿±±
